        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Lean Group",
                    "description": "Participants with body mass index 20-25 kg/m\\^2 will complete an outpatient study visit (body composition, blood draw, treadmill test, strength test) and an inpatient visit (muscle biopsies, fat biopsies, indirect calorimetry, exercise test).",
                    "interventionNames": [
                        "Diagnostic Test: Treadmill Test",
                        "Diagnostic Test: Strength Test",
                        "Diagnostic Test: Dual-Energy X-Ray Absorptiometry",
                        "Procedure: Muscle Biopsy",
                        "Procedure: Fat Biopsy",
                        "Diagnostic Test: Resting Energy Expenditure",
                        "Diagnostic Test: Magnetic resonance imaging"
                    ]
                },
                {
                    "label": "Obese Group",
                    "description": "Participants with body mass index 30-45 kg/m\\^2 will complete an outpatient study visit (body composition, blood draw, treadmill test, strength test) and an inpatient visit (muscle biopsies, fat biopsies, indirect calorimetry, exercise test).",
                    "interventionNames": [
                        "Diagnostic Test: Treadmill Test",
                        "Diagnostic Test: Strength Test",
                        "Diagnostic Test: Dual-Energy X-Ray Absorptiometry",
                        "Procedure: Muscle Biopsy",
                        "Procedure: Fat Biopsy",
                        "Diagnostic Test: Resting Energy Expenditure",
                        "Diagnostic Test: Magnetic resonance imaging"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Treadmill Test",
                    "description": "Measurement of whole-body fitness level. Walking or running on a treadmill and breathing into a machine that measures oxygen level during test. Heart rhythm is continuously monitored during exercise using an electrocardiogram (ECG).",
                    "armGroupLabels": [
                        "Lean Group",
                        "Obese Group"
                    ]
                },
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Strength Test",
                    "description": "Measure the strength of leg muscles by kicking against a machine with resistance.",
                    "armGroupLabels": [
                        "Lean Group",
                        "Obese Group"
                    ]
                },
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Dual-Energy X-Ray Absorptiometry",
                    "description": "Body composition measurement.",
                    "armGroupLabels": [
                        "Lean Group",
                        "Obese Group"
                    ],
                    "otherNames": [
                        "DEXA"
                    ]
                },
                {
                    "type": "PROCEDURE",
                    "name": "Muscle Biopsy",
                    "description": "Obtain a small piece of muscles from the upper part of the legs",
                    "armGroupLabels": [
                        "Lean Group",
                        "Obese Group"
                    ]
                },
                {
                    "type": "PROCEDURE",
                    "name": "Fat Biopsy",
                    "description": "Fat sample taken from abdomen",
                    "armGroupLabels": [
                        "Lean Group",
                        "Obese Group"
                    ]
                },
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Resting Energy Expenditure",
                    "description": "Measured by indirect calorimetry. Measures how much energy the body uses at rest by placing a special plastic dome over the head while resting quietly for 30 minutes. Measures oxygen breathed in and carbon dioxide breathed out.",
                    "armGroupLabels": [
                        "Lean Group",
                        "Obese Group"
                    ],
                    "otherNames": [
                        "REE"
                    ]
                },
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Magnetic resonance imaging",
                    "description": "MRI on upper leg to look at fat and muscle",
                    "armGroupLabels": [
                        "Lean Group",
                        "Obese Group"
                    ],
                    "otherNames": [
                        "MRI"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in muscle protein synthesis",
                    "description": "Skeletal muscle protein synthesis will be measured from the rate of incorporation of infused amino acids measured by mass spectrometry.",
                    "timeFrame": "Baseline, post-exercise approximately 8 hours"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in mRNA expression of exercise responsive genes",
                    "description": "Measure messenger ribonucleic acids (mRNAs) expression on exercise responsive genes",
                    "timeFrame": "Baseline, post-exercise approximately 8 hours"
                },
                {
                    "measure": "Change in expression and activation (phosphorylation) of signaling proteins",
                    "description": "Phosphorylation is a common mechanism for the regulation of receptor function",
                    "timeFrame": "Baseline, post-exercise approximately 8 hours"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n- Obese men and women \u2265 20 and \u2264 45 years of age.\n\nExclusion Criteria:\n\n* Diabetes or fasting plasma glucose \\>126 mg/dL\n* Anemia (female subjects hemoglobin of \\<11 g/dl and male subjects hemoglobin \\<12 g/dl)\n* Active coronary artery disease or history of unstable macrovascular disease (unstable angina, myocardial infarction, stroke, and revascularization of coronary, peripheral or carotid artery within 3 months of recruitment)\n* Renal failure (serum creatinine \\> 1.5mg/dl)\n* Chronic active liver disease defined as aspartate aminotransferase (AST)\\>144 IU/L or alanine transaminase (ALT) \\>165 IU/L)\n* Oral warfarin group medications or history of blood clotting disorders.\n* Smoking\n* Pregnancy or breastfeeding\n* Alcohol consumption greater than 2 glasses/day or other substance abuse\n* Untreated or uncontrolled hypothyroidism\n* Debilitating chronic disease (at the discretion of the investigators)",
            "healthyVolunteers": false,
            "sex": "ALL",
