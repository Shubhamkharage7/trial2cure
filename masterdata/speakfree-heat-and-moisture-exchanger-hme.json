            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "SpeakFree HME Hands Free Valve",
                    "description": "A voice prosthesis, SpeakFree HME Hands Free Valve, achieved with creation of a tracheoesophageal puncture will be placed over the stoma for 1 month.",
                    "armGroupLabels": [
                        "SpeakFree Hands Free Heat Moisture Exchanger (HEM) Valve Group"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Digital Occlusion",
                    "description": "Using participant's own fingers, they will be putting their fingers over the stoma.",
                    "armGroupLabels": [
                        "SpeakFree Hands Free Heat Moisture Exchanger (HEM) Valve Group"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "Digital Depression HME",
                    "description": "A digital handheld press-down HME will be placed over stoma.",
                    "armGroupLabels": [
