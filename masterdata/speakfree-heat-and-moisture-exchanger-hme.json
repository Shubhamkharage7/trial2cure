        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "SpeakFree Hands Free Heat Moisture Exchanger (HEM) Valve Group",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will be fitted to use a hand free HME device over their stoma in order to communicate with a voice prosthesis for one month. The participants will also serve as their own control performing voicing and speech tasks using digital occlusion and digital depression HME.",
                    "interventionNames": [
                        "Device: SpeakFree HME Hands Free Valve",
