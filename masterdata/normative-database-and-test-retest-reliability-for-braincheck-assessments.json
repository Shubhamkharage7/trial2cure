        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Normative",
                    "description": "BrainCheck assessment values from participants that have indications of healthy cognition at the time of testing ( a SLUMS result considered 'normal', more than 4 hours of sleep the night before, not having consumed alcohol or drugs within the last 6 hours, or not have participated in high capacity and strenuous physical activity within the last hour.)",
                    "interventionNames": [
                        "Device: BrainCheck Assessment"
                    ]
                },
                {
                    "label": "Test-Retest",
                    "description": "Comparing BrainCheck assessment values between an initial visit test and a follow-up visit test 7-14 days later within subjects.",
                    "interventionNames": [
                        "Device: BrainCheck Assessment"
                    ]
                },
                {
                    "label": "Per Protocol",
                    "description": "BrainCheck assessment values from participants that completed all procedures with no protocol deviations",
                    "interventionNames": [
                        "Device: BrainCheck Assessment"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "BrainCheck Assessment",
                    "description": "A computerized neurocognitive test administered via an electronic device, such as a phone, tablet or computer, that contains several assessments that will aid in establishing an individuals cognitive performance",
                    "armGroupLabels": [
                        "Normative",
                        "Per Protocol",
                        "Test-Retest"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Mean, standard deviation, and percentiles of values of BrainCheck Assessments",
                    "description": "BrainCheck Assessment values will be calculated by descriptive statistics. Mean, standard deviation, and percentiles (1, 5, 10, 25, 50, 75, 90, 95, 99) of the scores will be calculated for each individual assessment.",
                    "timeFrame": "5 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Test-retest reliability of BrainCheck Assessment values between two different time points",
                    "description": "Evaluate the test-retest reliability of BrainCheck assessments using Pearson Correlation coefficients on the descriptive statistics of each individual assessment.",
                    "timeFrame": "5 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Aged 50 or older\n* Willing and able to comply with study requirements and activities\n\nExclusion Criteria:\n\n* TBI within the last 6 months\n* Neurosurgery within the last month\n* Experienced a stroke\n* Self reported neurological condition (including mild neuropsychological impairment, dementia, Parkinson's disease, amyotrophic lateral sclerosis, Multiple sclerosis, or long-covid)",
