        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Standard of Care Group",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Subjects will receive standard of care pain medication Oxycodone for pain control following hip arthroscopy procedure",
                    "interventionNames": [
                        "Drug: Oxycodone"
                    ]
                },
                {
                    "label": "Non-Opiate Pain Control Group",
                    "type": "EXPERIMENTAL",
                    "description": "Subjects will receive a non-opiate pain control regime using Ibuprofen, Gabapentin, Acetaminophen, Methocarbamol for pain control following hip arthroscopy procedure.",
                    "interventionNames": [
                        "Drug: Ibuprofen",
                        "Drug: Gabapentin",
                        "Drug: Acetaminophen",
                        "Drug: Methocarbamol"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Oxycodone",
                    "description": "5 mg tablets every 4 hours postoperatively as needed for pain control",
                    "armGroupLabels": [
                        "Standard of Care Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Ibuprofen",
                    "description": "800 mg three times a day for 2 weeks postoperatively for pain control; not to exceed 3200 mg/day",
                    "armGroupLabels": [
                        "Non-Opiate Pain Control Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Gabapentin",
                    "description": "300 mg three times a day for 5 days then wean off by day 10 postoperatively",
                    "armGroupLabels": [
                        "Non-Opiate Pain Control Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Acetaminophen",
                    "description": "1000 mg three times a day for up to 4 weeks postoperatively for pain control; not to exceed 4 grams per day",
                    "armGroupLabels": [
                        "Non-Opiate Pain Control Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Methocarbamol",
                    "description": "500 mg three times a week for 2 weeks postoperatively",
                    "armGroupLabels": [
                        "Non-Opiate Pain Control Group"
                    ],
                    "otherNames": [
                        "Robaxin"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in pain score",
                    "description": "Measure using a visual analog scale (VAS) to rate pain on a scale 0-10; 0=no pain to 10=worst possible pain",
                    "timeFrame": "Baseline, 14 days post-operatively"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n\u2022 Scheduled for a primary Hip Arthroscopy at Mayo Clinic (Rochester, MN)\n\nExclusion Criteria:\n\n* medical history of known allergies or intolerance to allergies or intolerance to Motrin, Gabapentin, Tylenol, dexamethasone, tramadol, or Robaxin.\n* Substantial alcohol or drug abuse.\n* History of narcotics within 6 months of surgery.\n* Pregnancy.\n* Renal impairment.\n* Peptic ulcer disease.\n* GI bleeding.",
