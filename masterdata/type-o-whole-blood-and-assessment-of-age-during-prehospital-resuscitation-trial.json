        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Whole Blood",
                    "type": "EXPERIMENTAL",
                    "description": "Subjects will receive up to two units of whole blood as collected by local blood bank procedures and stored at 1-6 degrees Celsius initiated in the prehospital phase of care.",
                    "interventionNames": [
                        "Biological: low titer whole blood"
                    ]
                },
                {
                    "label": "Standard Care",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Subjects will receive prehospital crystalloid infusion or blood component transfusion resuscitation per site standard care for the respective Emergency Medical unit/service.",
                    "interventionNames": [
                        "Biological: Standard Care"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BIOLOGICAL",
                    "name": "low titer whole blood",
                    "description": "low titer whole blood, group O kept to either 21 days or 35 days based upon which preservation process is employed at each respective participating site",
                    "armGroupLabels": [
                        "Whole Blood"
                    ]
                },
                {
                    "type": "BIOLOGICAL",
                    "name": "Standard Care",
                    "description": "crystalloid infusion or blood component transfusion resuscitation",
                    "armGroupLabels": [
                        "Standard Care"
                    ]
                }
            ]
        },
