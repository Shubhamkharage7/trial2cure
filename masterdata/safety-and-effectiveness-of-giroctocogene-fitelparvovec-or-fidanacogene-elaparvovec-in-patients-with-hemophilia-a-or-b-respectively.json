        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Incidence of thromboembolic events",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "Incidence of factor inhibitor development",
                    "description": "FIX inhibitor development was defined as an inhibitor titer \\>= 0.6 Bethesda units per milliliter (BU/mL).",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "Incidence of hepatic malignancy",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "Incidence of liver abnormalities",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "Factor activity level",
                    "description": "Factor activity level will be reported. Factor levels may be measured using different assay methods including a one-stage assay or by chromogenic substrate assay and a second one-stage assay.",
                    "timeFrame": "Day 1 to 10 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Total ABR (treated or untreated; (excluding bleeds related to surgery)",
                    "description": "ABR (Annual Bleed Rate): number of bleeding episodes per year. This includes treated and untreated bleeds.\n\nThe ABR or the annualized number of bleeding episodes per year, will be derived for each participant for each observation period by using the following formula:\n\nABR = (Number of bleeds / Days in observation period) x 365.25 days/year.",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "Incidence of and time from vector infusion to resumption of prophylaxis",
                    "description": "Describe incidence of resumption of prophylaxis resumption and the time (in days) to resumption of prophylaxis after receiving vector infusion.",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "AIR of exogenous factor (excluding infusions related to surgery)",
                    "description": "The AIR or the annualized number of FIX infusions per year, will be derived for each participant for each observation period by using the following formula:\n\nAIR = (Number of FIX infusions / Days in observation period) x 365.25 days/year.",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "Consumption of exogenous factor (excluding infusions related to surgery)",
                    "description": "The annualized TFC in international units (IU) will be derived for each participant for each observation period using the following formula:\n\nAnnualized TFC = (Total units of FIX infused (IU)/ Days in observation period) x 365.25 days/year",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "Incidence of Non-hepatic malignancy",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "Incidence of Auto-immune disorders",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "Incidence of SAEs",
                    "description": "An SAE was an AE resulting in any of the following outcomes or deemed significant for any other reason: death; initial or prolonged inpatient hospitalization; life-threatening experience (immediate risk of dying); persistent or significant disability/incapacity; congenital anomaly; development of a clinical thrombotic event; development of factor inhibitor; development of a hepatic malignancy; development of drug-related elevated hepatic transaminases that fail to improve with immunosuppressive regimens; occurrence of a malignancy with reasonable possibility of being related to study drug.",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "All cause mortality",
                    "description": "All-cause mortality was defined as the death due to any cause during the course of study. Incidence rate was defined as the total number of participants with admissible events divided by the total (for all qualifying participants) time at risk for the cohort/treatment group of interest. Incidence rate of all-cause deaths was reported in this outcome measure.",
                    "timeFrame": "Day 1 to 10 years"
                },
                {
                    "measure": "EQ-5D-5L dimension and VAS scores",
                    "description": "The EQ-5D-5L comprises a 5-item health status measure and a visual analog rating scale/feeling thermometer. Using the 5-dimensional Health State Classification, participants are asked to respond to five questions on different aspects of their health status that assess the following:\n\n1. Mobility\n2. Self-care\n3. Usual activities\n4. Pain/Discomfort\n5. Anxiety/Depression",
                    "timeFrame": "Day 1 to 10 years"
                }
            ]
        },
