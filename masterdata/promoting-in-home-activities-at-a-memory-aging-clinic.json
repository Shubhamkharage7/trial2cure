        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Gentle Moves",
                    "type": "EXPERIMENTAL",
                    "description": "Three-month physical activity intervention.",
                    "interventionNames": [
                        "Behavioral: Gentle Moves"
                    ]
                },
                {
                    "label": "Usual Care",
                    "type": "NO_INTERVENTION",
                    "description": "Usual care provided by neuropsychologist."
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Gentle Moves",
                    "description": "A safe physical activity program which consists of a brief motivational enhancement and three movements to be performed in a seated position; and two movements in the standing position. The intervention will be delivered by a research coach.",
                    "armGroupLabels": [
                        "Gentle Moves"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Program Feasibility",
                    "description": "\u2022 Program feasibility measured by program adherence (e.g., self-reported program activities in the past 24 hours, yes/no).",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Program Acceptability",
                    "description": "\u2022 Program acceptability measured by patient's perception of the program difficulty. (Too hard, a little too hard, just right, a little too easy, much easy)",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Feasibility of NIH Toolbox Cognition Battery",
                    "description": "Feasibility of NIH Toolbox Cognition Battery measured by administration time.",
                    "timeFrame": "3 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "NIH Toolbox Cognition Battery scores",
                    "description": "NIH Toolbox Cognition Battery Fluid Cognition scores (range 59 - 140, higher score means better cognition)",
                    "timeFrame": "3 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Age 50+\n* Fluent in English\n* Diagnosed with MCI or mild dementia\n* Physically inactive (\\< 150 min/wk of planned PA)\n* Able to sit in a chair independently for 15+ minutes\n\nExclusion Criteria:\n\n* Have significant mental or physical disabilities that lead to inability to perform PA safely (e.g., major stroke)\n* Severe uncompensated hearing or visual loss\n* Having insufficient decisional capacity to consent to participate in research.\n* Lack of safety awareness\n* Unwilling to be assigned to Pro-Home MeC or control group\n* Currently participating in PA intervention research",
