        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Spiration Valve System (SVS)",
                    "description": "Spiration Valves are one-way endobronchial valves indicated for adult patients with shortness of breath and hyperinflation associated with severe emphysema in regions of the lung that have evidence of low collateral ventilation. The Spiration Valve is loaded in the deployment catheter, then passed through the channel of a flexible bronchoscope and deployed at the intended target location. The SVS device is designed to relieve the symptoms of shortness of breath and hyperinflation associated with severe emphysema without surgery."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Incidence of pre-specified thoracic adverse events of special interest (TAEsSI) through 12 months following the first implantation procedure",
                    "description": "The primary safety endpoint will be the incidence of device-related and procedure-related pre-specified thoracic adverse events of special interest (TAEsSI) through 12 months following the first implantation procedure.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Rate (per patient-year) of pre-specified thoracic adverse events of special interest (TAEsSI) through 36 months following the first implantation procedure",
                    "description": "An additional analysis will present the rate (per patient-year) of TAEsSI.",
                    "timeFrame": "36 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "45-day pneumothorax rate",
                    "description": "45-day pneumothorax rate, defined as pneumothorax requiring surgical intervention, or prolonged air leak \\> 7 days defined as the time from chest tube insertion to the time the air leak is not present.",
                    "timeFrame": "45 days"
                },
                {
                    "measure": "Survival rate over 24 months",
                    "description": "Survival rate over 24 months compared to the EMPROVE study control cohort",
                    "timeFrame": "24 months"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Targeted lobe volume reduction (TLVR)",
                    "description": "Target lobe volume reduction (TLVR) determined from quantitative High-Resolution Computed Tomography (HRCT) at 6 months",
                    "timeFrame": "6 months"
                },
                {
                    "measure": "Hyperinflation",
                    "description": "It will be measured by the ratio of residual volume to total lung capacity (RV/TLC) using plethysmography from baseline",
                    "timeFrame": "6 and 12 months"
                },
                {
                    "measure": "Forced expiratory volume in 1 second (FEV1)",
                    "description": "Relative (percentage) change from baseline at 6, 12, 24 and 36 months",
                    "timeFrame": "6, 12, 24 and 36 months"
                },
                {
                    "measure": "Modified Medical Research Council (mMRC) change",
                    "description": "Modified Medical Research Council (mMRC) change from baseline at 6, 12, 24 and 36 months",
                    "timeFrame": "6, 12, 24 and 36 months"
                },
                {
                    "measure": "St. George's Respiratory Questionnaire (SGRQ) change",
                    "description": "St. George's Respiratory Questionnaire (SGRQ) change from baseline at 6, 12, 24 and 36 months",
                    "timeFrame": "6, 12, 24 and 36 months"
                },
                {
                    "measure": "6 Minute Walk Test (6MWT) change",
                    "description": "6-minute walk test (6MWT) change from baseline at 6 and 12 months",
                    "timeFrame": "6 and 12 months"
                },
                {
                    "measure": "Body Mass, Airflow Obstruction, Dyspnea and Exercise Capacity (BODE) Index change",
                    "description": "Body mass, Airflow Obstruction, Dyspnea and Exercise Capacity (BODE) Index change from baseline at 6 and 12 months",
                    "timeFrame": "6 and 12 months"
                },
                {
                    "measure": "Responder Rates based on Minimum Clinically Important Difference (MCID)",
                    "description": "Responder rates based on Minimum Clinically Important Difference (MCID) for Effectiveness Observations",
                    "timeFrame": "3 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Adult patients with shortness of breath and hyperinflation associated with severe emphysema and evidence of low collateral ventilation.\n2. Subjects must understand and voluntarily sign an informed consent form.\n\nExclusion Criteria:\n\n1. Subjects who are not appropriate for SVS therapy based upon the US FDA-approved IFU requirements.\n2. Subjects who were withdrawn from this study for any reason will not be allowed to re-enroll.\n3. Subjects who have incomplete screening or baseline data.",
