        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Stage 1- Single-stage Fowler-Stephens orchidopexy (FSO)",
                    "type": "OTHER",
                    "description": "Stage 1 single surgery to clip the vessels to the testis, divide the spermatic vessels and bring the testis into the scrotum.",
                    "interventionNames": [
                        "Procedure: Single-stage Fowler-Stephens orchidopexy (FSO)"
                    ]
                },
                {
                    "label": "Stage 2- Two-stage Fowler-Stephens orchidopexy (FSO)",
                    "type": "OTHER",
                    "description": "Stage 2 2 surgeries: 1st surgery-Clip vessels to testis. 2nd surgery done 6-12 months later, divide the spermatic vessels and bring the testis into the scrotum.",
                    "interventionNames": [
                        "Procedure: Two-stage Fowler-Stephens orchidopexy (FSO)"
                    ]
                }
            ],
