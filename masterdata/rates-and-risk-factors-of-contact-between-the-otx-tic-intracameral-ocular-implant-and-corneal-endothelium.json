        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "OTHER",
                    "name": "OCT Imaging",
                    "description": "Each participant will receive imaging of the inferior sectors of the iridocorneal angle at 150\u00b0, and 180\u00b0, and 210\u00b0 with the swept-source ANTERION AS-OCT system under standardized light and dark lighting conditions by a trained research technician"
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Primary Outcome 1",
                    "description": "Predict contact between simulated OTX-TIC implant and corneal endothelium imaged using the ANTERION AS-OCT system.",
                    "timeFrame": "4-6 Months"
                },
                {
                    "measure": "Primary Outcome 2",
                    "description": "Characterize the variability in distance (mean and standard deviation) between the angle recess and Schwalbe's line",
                    "timeFrame": "4-6 Months"
                },
                {
                    "measure": "Primary Outcome 3",
                    "description": "Calculate the proportion of eyes with predicted contact between the simulated implant and corneal endothelium.",
                    "timeFrame": "4-6 Months"
                },
                {
                    "measure": "Primary Outcome 4",
                    "description": "Develop logistic regression models to identify demographic, clinical, and biometric risk factors for predicted contact.",
                    "timeFrame": "4-6 Months"
                }
            ]
        },
