        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in pulmonary function",
                    "description": "Pulmonary function will be measured using spirometry - the percent of predicted forced expiratory volume in the first second (ppFEV1)",
                    "timeFrame": "baseline, 3, 6, 9, 12 months"
                },
                {
                    "measure": "Change in days of inpatient hospitalization",
                    "description": "Days of inpatient hospitalization will be assessed",
                    "timeFrame": "baseline, 3, 6, 9, 12 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in quality of life",
                    "description": "Quality of life will be assessed using the 22-question SinoNasal Outcome test (range 0-110, with higher scores signifying worse quality of life)",
                    "timeFrame": "baseline, 3, 6, 9, 12 months"
                },
                {
                    "measure": "Change in quality of life",
                    "description": "Quality of life will be assessed using the Cystic Fibrosis Questionnaire-Revised instrument (range 0-100, with higher scores signifying worse quality of life)",
                    "timeFrame": "baseline, 3, 6, 9, 12 months"
                },
                {
                    "measure": "Change in olfaction",
                    "description": "Olfaction will be assessed using the Smell Identification Test (score range: 0-40, with higher scores demonstrated better olfactory ability)",
                    "timeFrame": "baseline, 3, 6, 9, 12 months"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Patient satisfaction regarding treatment for chronic rhinosinusitis",
                    "description": "Satisfaction will be assessed using 5-point Likert scales, ranging from 1 to 5 with 1 signifying \"not important\" and 5 signifying \"very important\"",
                    "timeFrame": "at baseline"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Subjects between the ages of 18 years old (inclusive) and 99 years old (inclusive, subjects over the age of 89 will be recorded to be 89 years old).\n2. Diagnosed with cystic fibrosis as established by genetic testing combined with clinical assessment and/or sweat chloride\n3. Diagnosed with chronic rhinosinusitis by multidisciplinary sinusitis guidelines\n4. chronic rhinosinusitis symptoms persisting beyond initial medical treatment\n5. Counseled for endoscopic sinus surgery and ongoing medical therapy with each patient electing their preferred treatment\n\nExclusion Criteria:\n\n1. Underwent endoscopic sinus surgery in past 12 months\n2. Will obtain follow up care at non-participating institutions\n3. Unable to complete follow-up surveys",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "maximumAge": "99 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Subjects will be recruited from cystic fibrosis and otolaryngology outpatient clinics.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
