        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Participants scheduled for non-gynecologic abdominopelvic surgery",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will be aged \u226545 years, have at least 1 fallopian tube, will not desire or plan to have children in the future, and will be scheduled to undergo non-gynecologic abdominopelvic surgery.",
