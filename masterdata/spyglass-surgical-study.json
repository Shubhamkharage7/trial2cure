        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Subjects with complex pancreaticobiliary disease",
                    "description": "All subjects will undergo the intraoperative endoscopy with either the SpyGlass Discover System or the SpyGlass DS Direct Visualization System.",
                    "interventionNames": [
                        "Device: SpyGlass Discover Digital System or SpyGlass DS Direct Visualization System"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "SpyGlass Discover Digital System or SpyGlass DS Direct Visualization System",
                    "description": "Observational, prospective study for the clinical utility of the SpyGlass Discover Digital Catheter and the SpyScope DS Catheter during an intraoperative endoscopy procedure.",
                    "armGroupLabels": [
                        "Subjects with complex pancreaticobiliary disease"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Technical success",
                    "description": "Perform intraoperative endoscopy as intended using a thin, single-use, flexible cholangiopancreatoscope as desired, including but not limited to:\n\n* Direct visualization of target lesion or stricture\n* Obtaining biopsies\n* Initiating and/or completing stone clearance",
                    "timeFrame": "During study procedure"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Number of adverse events",
                    "description": "Serious adverse events related to the study device, accessory devices used throughout the working channel of the SpyGlass\u2122 system, or the intraoperative endoscopy procedure",
                    "timeFrame": "1 month"
                },
                {
                    "measure": "Endoscopist rating of clinical impact of study procedure",
                    "description": "Characterization and rating of the clinical benefit and impact of intraoperative endoscopy using a thin, single-use, flexible cholangiopancreatoscope to the surgical procedure on a 1 (highly dissatisfied) to 10 (highly satisfied) scale",
                    "timeFrame": "During study procedure"
                },
                {
                    "measure": "Adequate diagnostic yield",
                    "description": "Adequate diagnostic yield of tissue biopsy for histopathology assessment",
                    "timeFrame": "1 month"
                },
                {
                    "measure": "Endoscopist rating of device",
                    "description": "Rating of the intraoperative performance of the SpyGlass\u2122 system and accessories during intraoperative endoscopy procedure compared to marketed reusable endoscopes on a 1 (highly dissatisfied) to 10 (highly satisfied) scale:\n\n1. Image quality\n2. Ability to irrigate field of view through the irrigation channels\n3. Ability to aspirate through the working channel\n4. Ability to retroflex\n5. Ability to obtain targeted biopsies using biopsy forceps\n6. Ability to grasp stones using wire basket\n7. Ability to perform lithotripsy\n8. Ability to advance accessories through catheter\n9. Ability to selectively advance into targeted ducts",
                    "timeFrame": "During study procedure"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Clinical indication for intraoperative endoscopy using a thin, single-use, flexible cholangiopancreatoscope\n* Written informed consent from patient or legally authorized representative of the patient\n\nExclusion Criteria:\n\n* Contraindication for intraoperative endoscopy using a thin, single-use, flexible cholangiopancreatoscope\n* \\<18 years of age\n* Potentially vulnerable subjects, including, but not limited to pregnant women",
