        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "patients diagnosed with NSCLC",
                    "description": "In phase I, the researchers will assess the ability of a rat-based ABP to detect the presence or absence of NSCLC-specific VOCs in urine samples from subjects with and without NSCLC. In phase II, the researchers will assess the ability of the ABP to detect the presence of NSCLC-specific VOCs in urine samples from subjects with suspected but undiagnosed NSCLC (clinical stage I to IIIA). Urine samples will be collected from 50 patients diagnosed with NSCLC and from 50 subjects without NSCLC for phase I and from 110 patients with suspected but undiagnosed NSCLC for phase II (total number of subjects = 210).",
                    "interventionNames": [
                        "Diagnostic Test: urine sample"
                    ]
                }
            ],
            "interventions": [
