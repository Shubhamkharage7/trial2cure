        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Pregnant Women & their fetuses/infants",
                    "description": "Information about pregnant women and their fetuses/infants, including the medical history, prenatal, perinatal, and postpartum periods (6 weeks after delivery) and throughout the treatment (e.g.surgery) for the child's medical condition (up to approx. child is @ 6 months of age), will be collected in this repository. The analysis of this information may help in understanding of the causes of fetal anomalies."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Data Collection",
                    "description": "Data collection from participant's medical records will be entered into a data base",
                    "timeFrame": "From the initial visit @ the High Risk Clinic through the infants approx. age of 6 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Pregnant females\n\n  * age 8 years to 50 years\n  * who have referred to the clinic for high risks pregnancies\n* Infants (male and female) born with anomalies\n\nExclusion Criteria:\n\n* Non-pregnant females.",
