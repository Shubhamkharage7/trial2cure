        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Direct Discrimination",
                    "type": "EXPERIMENTAL",
                    "description": "Simulated experience with direct incidents of everyday differential treatment that targets the participant's racial and ethnic background",
                    "interventionNames": [
                        "Behavioral: Direct Discrimination"
                    ]
                },
                {
                    "label": "Vicarious Discrimination",
                    "type": "EXPERIMENTAL",
                    "description": "Simulated experience with indirect incidents of differential treatment as recalled by another Black person",
                    "interventionNames": [
                        "Behavioral: Vicarious Discrimination"
                    ]
                },
                {
                    "label": "Non-Racism Daily Hassles",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "Simulated stress-inducing experience in day-to-day lives appropriate for participant's age and educational/vocational backgrounds",
                    "interventionNames": [
                        "Behavioral: Daily Hassles"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Direct Discrimination",
                    "description": "Simulated racial discrimination that is received directly from a perpetrator, commonly experienced in everyday lives by African American individuals",
                    "armGroupLabels": [
                        "Direct Discrimination"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Vicarious Discrimination",
                    "description": "Hearing or observing racial discrimination experienced by another person of the same racial background",
                    "armGroupLabels": [
                        "Vicarious Discrimination"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Daily Hassles",
                    "description": "Day-to-day stressors associated with school, work, finances",
                    "armGroupLabels": [
                        "Non-Racism Daily Hassles"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Momentary Alcohol and/or Cannabis Craving",
                    "description": "Momentary Alcohol and Cannabis Craving Scale will be administered (adapted from the Momentary Cannabis Craving Scale; Davis et al., 2024) immediately after the intervention. Mean scores will range from 1 to 7, with higher scores indicating greater momentary craving.",
                    "timeFrame": "Immediately after the intervention (i.e., within 1 minute)"
                },
                {
                    "measure": "Duration of Alcohol and Cannabis Intoxication",
                    "description": "Self-reported number of hours of intoxication within 48 hours after the intervention",
                    "timeFrame": "48 Hours Post-Intervention"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Identify as Black, African American, or Afro-Caribbean\n* Proficient in English\n* Alcohol and/or cannabis use at least 4 times in past month\n* Endorse having used alcohol and/or cannabis to cope\n\nExclusion Criteria:\n\n* Having participated in similar studies by the research team\n* Obtain cut-off scores on measures indicating possible alcohol and/or cannabis use disorder\n* In or seeking treatment for alcohol and/or cannabis use disorder",
