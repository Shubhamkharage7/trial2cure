        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Cohort 1 'IMPACT Cohort'",
                    "description": "Subjects with intermediate AMD in both eyes, and at least one eye with a drusen volume in the central 3 mm circle centered on the fovea of at least 0.02mm3 in the absence of GA or nGA as diagnosed with OCT en face imaging OR subjects with AMD (early or intermediate) diagnosed in one eye and exudative AMD diagnosed in the fellow eye will undergo SS-OCT imaging every 3 months for 2 years",
                    "interventionNames": [
                        "Device: SS-OCT imaging"
                    ]
                },
                {
                    "label": "Cohort 2 'SWAGGER Cohort'",
                    "description": "Subjects with GA or nGA secondary to AMD that is at least the size of a large druse (125 microns in diameter; 0.05 mm2) and no greater than 7 disc areas (17 mm2) in at least one eye will undergo SS-OCT imaging every 3 months for 2 years",
                    "interventionNames": [
                        "Device: SS-OCT imaging"
                    ]
                },
                {
                    "label": "Cohort 3",
                    "description": "Subjects with GA enrolled in another trial",
                    "interventionNames": [
                        "Device: SS-OCT imaging"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "SS-OCT imaging",
                    "description": "All subjects will undergo retinal imaging using the Zeiss PlexElite SS-OCT, a non-contact, non-invasive ocular imaging instrument",
                    "armGroupLabels": [
                        "Cohort 1 'IMPACT Cohort'",
                        "Cohort 2 'SWAGGER Cohort'",
                        "Cohort 3"
                    ],
                    "otherNames": [
                        "Zeiss PlexElite",
                        "PlexElite 9000",
                        "Swept Source OCT",
                        "Optical Coherence Tomography Angiography"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in the Percentage of Choroidal Perfusion Deficits at 1 year compared to Baseline",
                    "description": "Compare the percentage of choroidal perfusion deficits as measured using automated algorithms",
                    "timeFrame": "1-year and 2-year time points"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Pre-existing and new sub-clinical Macular Neovascularization (MNV)",
                    "description": "Identify the number of abnormal new vessels arising from the Choroid at Baseline, 1-year and 2-year time points",
                    "timeFrame": "1-year and 2-year time points"
                },
                {
                    "measure": "Automated Drusen Volume measurements",
                    "description": "Compare the automated measurements of drusen volume using the Zeiss algorithm with manual measurements by trained readers",
                    "timeFrame": "1-year and 2-year time points"
                },
                {
                    "measure": "Automated Geography Atrophy measurements",
                    "description": "Compare the automated measurements of Geography Atrophy area using the Zeiss algorithm with manual measurements by trained readers",
                    "timeFrame": "1-year and 2-year time points"
                },
                {
                    "measure": "Choroidal Thickness (millimeters)",
                    "description": "Correlate Choroidal Thickness measurements (millimeters) with Age related Macular Degeneration (AMD) progression/Geographic Atrophy growth rates",
                    "timeFrame": "1-year and 2-year time points"
                },
                {
                    "measure": "Choroidal Vascularity Index (percentage)",
                    "description": "Correlate Choroidal Vascularity Index (percentage) with Age related Macular Degeneration (AMD) progression/Geographic Atrophy growth rates",
                    "timeFrame": "1-year and 2-year time points"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Enrollment in and completion of the BIRC-01 study\n* Clinic diagnosis of non-exudative iAMD in at least one eye with a drusen volume in the central 3 mm circle centered on the fovea of at least 0.02 mm3 in the absence of GA or nGA as diagnosed with OCT en face imaging OR Clinical diagnosis of early or early/intermediate stage AMD in one eye in the absence of nGA or GA and exudative AMD in the other eye OR clinical diagnosis of GA or nGA secondary to AMD that is at least the size of a large druse (125 microns in diameter; 0.05 mm2) and no greater than 7 disc areas (17 mm2) in at least one eye which has never been treated with anti-VEGF agents\n* Willing and able to comply with clinic visits and study-related procedures\n* Provide signed informed consent\n\nExclusion Criteria:\n\n* Subjects with exudative AMD in both eyes\n* Eyes with evidence of non-proliferative and proliferative diabetic retinopathy.\n* Presence of confounding ocular diagnosis such as myopia \\>6D, or other ocular conditions that may cause retinal pigment epithelium atrophy or exudative MNV\n* Subjects currently or previously enrolled in other interventional clinical trials in which treatment was administered to the study eye.\n* Previous vitrectomy or intravitreal injections in the study eye.\n* Axial length measurement \u2265 26 mm.\n* Subjects unable to give informed consent.\n* Subjects who are unable to comply with imaging guidelines",
            "healthyVolunteers": false,
            "sex": "ALL",
            "stdAges": [
                "CHILD",
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Subjects with intermediate AMD, Geographic Atrophy and nascent Geographic Atrophy and who have completed the BIRC-01 study",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
