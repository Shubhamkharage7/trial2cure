        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Short-Term Primary Endpoint",
                    "description": "Survival to cardiac transplant or device explant for recovery free from disabling stroke (Modified Rankin Score \\> 3) or pre-defined severe Right Heart Failure at 6 months after implantation of the originally implanted device",
                    "timeFrame": "6 months"
                },
                {
                    "measure": "Long-Term Primary Endpoint",
                    "description": "Survival to cardiac transplant or device explant for recovery free from disabling stroke (Modified Rankin Score \\> 3) or pre-defined severe Right Heart Failure at 24 months after implantation of the originally implanted device",
                    "timeFrame": "24 months"
                }
            ],
            "secondaryOutcomes": [
                {
