        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Virtual mind-body group exercise classes (VMB)",
                    "type": "EXPERIMENTAL",
                    "description": "Will be provided with the schedule and links to the web-based mind-body exercises offered by MSK in real-time via the Zoom video conferencing platform. Each session is led by a licensed clinician (e.g., licensed dance therapist, certified yoga instructor, nurse specialist/physical trainer) with specific expertise in the oncology setting.",
                    "interventionNames": [
                        "Other: Virtual mind-body group exercise classes"
                    ]
                },
                {
                    "label": "Enhanced usual care (EUC)",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Pre-recorded self-care videos.",
                    "interventionNames": [
                        "Other: Enhanced usual care"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "OTHER",
                    "name": "Virtual mind-body group exercise classes",
                    "description": "Pre-operative period (baseline/intake day to day before surgery): mid- to high intensity classes focusing on aerobic physical activity, relaxation, and stress management (classes: Fitness for Everyone, Tai Chi, Mat Yoga, Dance Cardio, Meditation, Core Strength, Music Therapy)\n\n* Immediate post-operative period (post-operative hospitalization to 2 weeks after surgery): low- to mid-intensity classes focusing on relaxation, stress management and low-impact physical activity (classes: Chair Yoga, Dance Therapy, Meditation, Music Therapy)\n* Late post-operative period (3-12 weeks after surgery): mid- to high-intensity classes focusing on aerobic physical activity, relaxation, and stress management (classes: Fitness for Everyone, Tai Chi, Mat Yoga, Dance Cardio, Meditation, Core Strength, Music Therapy)",
                    "armGroupLabels": [
                        "Virtual mind-body group exercise classes (VMB)"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Enhanced usual care",
                    "description": "EUC arm will be referred to MSK's multimedia page on the Integrative Medicine website to access pre-recorded mind-body videos on their own time. These videos are 15-60 minutes in duration and consist of educational and self-care videos, including guided meditation and Tai Chi.",
                    "armGroupLabels": [
                        "Enhanced usual care (EUC)"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Feasibility of the Virtual Mind-Body Exercises program as measured by enrollment rate",
                    "description": "total number of patients invited, and the total number of study participants will be recorded",
                    "timeFrame": "1 year"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "improving patient-reported outcomes (PROs)",
                    "description": "Changes in HADS anxiety scores. The Hospital Anxiety and Depression Scale (HADS) will be used to explore the effect of treatments on psychological distress, specifically on anxiety. HADS is a 14-item scale with seven items measuring depression and seven items measuring anxiety. Each item is answered by the patient on a four-point (0-3) response category; possible scores range from 0-21 for anxiety and depression, with higher scores indicating higher symptomatology. Established cutoffs are: 0-7 not significant; 8-10 subclinical; and 11-21 clinically significant depression/anxiety.",
                    "timeFrame": "1 year"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Age \u2265 18 years or older\n* Any pancreas disease (benign, pre-malignant, or malignant) and will be undergoing pancreatectomy\n* Has at least one week to participate in the study prior to the anticipated surgery date\n* Eastern Cooperative Oncology Group (ECOG) Performance status 0 or 1\n* English-speaking only\n\nExclusion Criteria:\n\n* Unable to give informed consent due to cognitive impairment or impaired decisionmaking capacity\n* Cognitive impairment precluding response to study assessments\n* Received neoadjuvant or any chemoradiation in the past 60 days\n* Unwilling to accept random assignment\n* Unwilling to commit to the 12-week study time period\n* Unlikely to be compliant because of social factors that prevent patients from attending classes or doing home practice (e.g., unreliable internet, no access to a computer)",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
