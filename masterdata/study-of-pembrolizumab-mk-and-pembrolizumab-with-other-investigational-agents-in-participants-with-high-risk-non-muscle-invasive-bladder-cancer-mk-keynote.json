        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Pembrolizumab",
                    "type": "EXPERIMENTAL",
                    "description": "Participants with carcinoma-in-situ (CIS) with or without papillary tumors (Cohort A) and participants with papillary tumors only, without CIS (Cohort B) will receive pembrolizumab, 200 mg, intravenously, every 3 weeks (Q3W) for up to 24 months.",
                    "interventionNames": [
                        "Biological: Pembrolizumab"
                    ]
                },
                {
                    "label": "Pembrolizumab coformulation",
                    "type": "EXPERIMENTAL",
                    "description": "Participants with CIS with or without papillary tumors (Cohort C) will receive either pembrolizumab/vibostolimab or favezelimab/pembrolizumab coformulation intravenously Q3W for up to 24 months",
                    "interventionNames": [
                        "Biological: Pembrolizumab/vibostolimab coformulation",
                        "Biological: Favezelimab/pembrolizumab coformulation"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BIOLOGICAL",
                    "name": "Pembrolizumab",
                    "description": "Participants with CIS with or without papillary tumors (Cohort A) and participants with papillary tumors only, without CIS (Cohort B) receive pembrolizumab 200mg via IV infusion once every 3 weeks for up to 35 administrations",
                    "armGroupLabels": [
                        "Pembrolizumab"
                    ],
                    "otherNames": [
                        "MK-3475",
                        "KEYTRUDA"
                    ]
                },
                {
                    "type": "BIOLOGICAL",
                    "name": "Pembrolizumab/vibostolimab coformulation",
                    "description": "Participants with CIS with or without papillary tumors (Cohort C) receive pembrolizumab/vibostolimab (coformulation of 200 mg pembrolizumab and 200 mg vibostolimab) via IV infusion once every 3 weeks for up to 35 administrations",
                    "armGroupLabels": [
                        "Pembrolizumab coformulation"
                    ],
                    "otherNames": [
                        "MK-7684A"
                    ]
                },
                {
                    "type": "BIOLOGICAL",
                    "name": "Favezelimab/pembrolizumab coformulation",
                    "description": "Participants with CIS with or without papillary tumors (Cohort C) receive favezelimab/pembrolizumab (coformulation of 800mg favezelimab and 200 mg pembrolizumab) via IV infusion once every 3 weeks for up to 35 administrations",
                    "armGroupLabels": [
                        "Pembrolizumab coformulation"
                    ],
                    "otherNames": [
                        "MK-4280A"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Cohort A: Complete Response (CR) Rate of High-Risk Non-Muscle Invasive Bladder Cancer (NMIBC)",
                    "description": "The CR rate of high-risk NMIBC is defined as the proportion of participants in the analysis population whose tumors are in response to treatment and who are then free of high-risk NMIBC or worse per central pathology and radiology review.",
                    "timeFrame": "Up to approximately 6 months"
                },
                {
                    "measure": "Cohort B: 12-month Disease-Free Survival (DFS) Rate of High-Risk NMIBC",
                    "description": "DFS of high-risk NMIBC is defined as the time from the first dose to the first occurrence of high-risk NMIBC or worse per central pathology and radiology review. The 12-month DFS rate is the percentage of participants remaining free from high-risk NMIBC or worse at 12 months.",
                    "timeFrame": "Up to approximately 12 months"
                },
                {
                    "measure": "Cohort C: 12-month CR Rate of High-Risk NMIBC",
                    "description": "The 12-month CR rate of high-risk NMIBC is defined as the proportion of participants in the analysis population whose tumors are in response to treatment and who are then free of high-risk NMIBC or worse per central pathology and radiology review at 12 months.",
                    "timeFrame": "Up to approximately 12 months"
                },
                {
                    "measure": "All Cohorts: Number of Participants Who Experience an Adverse Event (AE)",
                    "description": "An AE is defined as any untoward medical occurrence in a participant or clinical investigation subject administered a pharmaceutical product and which does not necessarily have to have a causal relationship with this treatment.",
                    "timeFrame": "Up to approximately 27 months"
                },
                {
                    "measure": "All Cohorts: Number of Participants Who Discontinue Study Treatment Due to an AE",
                    "description": "An AE is defined as any untoward medical occurrence in a participant or clinical investigation subject administered a pharmaceutical product and which does not necessarily have to have a causal relationship with this treatment.",
                    "timeFrame": "Up to approximately 24 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Cohort A: CR Rate of Any Disease",
                    "description": "The CR rate of any disease is defined as the proportion of participants in the analysis population whose tumors are in response to treatment and who are then free of any disease or worse per central pathology and radiology review.",
                    "timeFrame": "Up to approximately 6 months"
                },
                {
                    "measure": "Cohort C: CR Rate of High-Risk NMIBC at 3 Months",
                    "description": "The CR rate of high-risk NMIBC at 3 months is defined as the proportion of participants in the analysis population whose tumors are in response to treatment and who are then free of high-risk NMIBC or worse per central pathology and radiology review at 3 months.",
                    "timeFrame": "Up to approximately 3 months"
                },
                {
                    "measure": "Cohort C: CR Rate of High-Risk NMIBC at 6 Months",
                    "description": "The CR rate of high-risk NMIBC at 6 months is defined as the proportion of participants in the analysis population whose tumors are in response to treatment and who are then free of high-risk NMIBC or worse per central pathology and radiology review at 6 months.",
                    "timeFrame": "Up to approximately 6 months"
                },
                {
                    "measure": "Cohort C: Overall CR Rate of High-Risk NMIBC",
                    "description": "The overall CR rate of high-risk NMIBC is defined as the proportion of participants in the analysis population whose tumors are in response to treatment and who are then free of high-risk NMIBC or worse per central pathology and radiology review at first evaluable efficacy assessment.",
                    "timeFrame": "Up to approximately 6 months"
                },
                {
                    "measure": "Cohort A: CR Rate of High-Risk NMIBC in Programmed Cell Death 1 (PDL-1) Positive Participants",
                    "description": "The CR of high-risk NMIBC is defined as the proportion of PDL-1 positive participants in the analysis population whose tumors are in response to treatment and who are then free of high-risk NMIBC or worse per central pathology and radiology review.",
                    "timeFrame": "Up to approximately 6 months"
                },
                {
                    "measure": "Cohort A and C: Duration of Response (DOR)",
                    "description": "The DOR is defined as the time from the first documented evidence of CR until the recurrence of disease or worse per clinical pathology and radiology review.",
                    "timeFrame": "Up to approximately 60 months"
                },
                {
                    "measure": "All Cohorts: Progression-Free Survival (PFS)",
                    "description": "PFS is defined as the time from the first dose to progression to worsening of grade or stage or death or to muscle invasive or metastatic disease or death per central urology, pathology, and radiology review.",
                    "timeFrame": "Up to approximately 60 months"
                },
                {
                    "measure": "All Cohorts: Overall Survival (OS)",
                    "description": "OS is defined as the time from first dose to death due to any cause.",
                    "timeFrame": "Up to approximately 60 months"
                },
                {
                    "measure": "Cohort B: DFS Rate of Any Disease",
                    "description": "DFS of any disease is defined as the time from the first dose to the first occurrence of any disease or worse per central pathology and radiology review. The DFS rate is the percentage of participants remaining free of any disease or worse.",
                    "timeFrame": "Up to approximately 60 months"
                },
                {
                    "measure": "Cohort B: 12-month DFS Rate of Any Disease",
                    "description": "DFS of any disease is defined as the time from the first dose to the first occurrence of any disease or worse per central pathology and radiology review. The 12-month DFS rate of any disease is the percentage of participants remaining free of any disease or worse at 12 months.",
                    "timeFrame": "Up to approximately 12 months"
                },
                {
                    "measure": "Cohort B: 12-month DFS Rate of High-Risk NMIBC in PDL-1 Positive Participants",
                    "description": "DFS of high-risk NMIBC is defined as the time from the first dose to the first occurrence of high-risk NMIBC or worse per central pathology and radiology review. The 12-month DFS rate is the percentage of participants remaining free from high-risk NMIBC or worse at 12 months. The 12-month DFS rate of high-risk NMIBC in PDL-1 positive participants will be reported.",
                    "timeFrame": "Up to approximately 12 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Histologically-confirmed diagnosis of high risk non-muscle-invasive (T1, high grade Ta and / or carcinoma in situ \\[CIS\\]) transitional cell carcinoma of the bladder (mixed histology tumors allowed if transitional cell histology is predominant histology).\n* Fully resected disease at study entry (residual CIS acceptable)\n* BCG-unresponsive high risk non-muscle-invasive bladder cancer after treatment with adequate BCG therapy\n* Ineligible for radical cystectomy or refusal of radical cystectomy\n* Available tissue from a newly obtained core biopsy of a tumor lesion not previously irradiated\n* Eastern Cooperative Oncology Group (ECOG) performance status of 0, 1, or 2\n* Adequate organ function\n* Female participants of childbearing potential have a negative urine or serum pregnancy test and must be willing to use an adequate method of contraception\n* Male participants must be willing to use an adequate method of contraception\n\nExclusion criteria:\n\n* Centrally assessed muscle-invasive, locally advanced nonresectable, or metastatic urothelial carcinoma (i.e., T2, T3, T4, and / or stage IV)\n* Centrally assessed concurrent extra-vesical (i.e., urethra, ureter, or renal pelvis) non-muscle invasive transitional cell carcinoma of the urothelium\n* Currently participating or has participated in a study of an investigational agent and received study therapy or received investigational device within 4 weeks prior to the first dose of study treatment\n* Received intervening intravesical chemotherapy or immunotherapy from the time of most recent cystoscopy / Transurethral Resection of Bladder Tumor (TURBT) to starting study treatment\n* Received prior chemotherapy, targeted small molecule therapy, or radiation therapy within 2 weeks prior to starting study treatment or not recovered from adverse events due to a previously administered agent\n* Known additional malignancy that is progressing or requires active treatment excepting basal cell carcinoma of the skin, squamous cell carcinoma of the skin that has undergone potentially curative therapy or in situ cervical cancer. A history of prostate cancer that was treated with definitive intent (surgically or through radiation therapy) is acceptable provided that the following criteria are met: Stage T2N0M0 or lower; Gleason score \u22647 and prostatic-specific antigen (PSA) undetectable for at least 1 year while off androgen deprivation therapy that was either treated with definitive intent or untreated in active surveillance that has been stable for the past year prior to study allocation\n* Active autoimmune disease that has required systemic treatment in the past 2 years\n* Evidence of interstitial lung disease or active non-infectious pneumonitis\n* Active infection requiring systemic therapy\n* Pregnant or breastfeeding, or expecting to conceive within the projected duration of the trial through 120 days after the last dose of study treatment\n* Prior therapy with an anti-programmed cell death 1 (PD-1), anti-PD-ligand 2 (L2) agent, or with an agent directed to another co-inhibitory T-cell receptor\n* Known human immunodeficiency virus (HIV)\n* Known active Hepatitis B or C infection\n* Received a live virus vaccine within 30 days of planned start of study treatment\n* Has had an allogeneic tissue/solid organ transplant",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
