        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "PeACE peer coaching arm",
                    "type": "EXPERIMENTAL",
                    "description": "Behavioral, PeACE, PeACE consists of 3 streamlined 30-minute psychosocial telephone counseling sessions delivered by a well-trained peer coach. Coaches are lay YARs from HBOC families demonstrating good knowledge, communication skills, and protocol mastery.",
                    "interventionNames": [
                        "Behavioral: PeACE"
                    ]
                },
                {
                    "label": "Community peer coaching arm",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Behavioral, usual care, Participants in the usual care arm will receive navigation to peer support with a range of community groups who provide these services.",
                    "interventionNames": [
                        "Behavioral: Community peer coaching"
                    ]
                }
            ],
