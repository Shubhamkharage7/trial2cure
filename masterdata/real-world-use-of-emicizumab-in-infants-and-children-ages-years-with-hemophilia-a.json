        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "The proportion of patients that are being treated with emicizumab",
                    "description": "The primary outcome will include the proportion of patients that are being treated with emicizumab for primary prophylaxis, as well as identifying breakthrough bleeding while on emicizumab.",
                    "timeFrame": "Within a year."
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Patients who receive additional doses of factor concentrate while on emicizumab",
                    "description": "Data collected will also include proportion of patients who receive additional doses of factor concentrate while on emicizumab for trauma or surgical interventions.",
