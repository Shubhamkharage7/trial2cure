            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Yaari Extractor",
                    "description": "The Yaari Extractor device is a single-use prescription device for neonatal extraction in deliveries that are complicated by shoulder dystocia. The Study Device consists of two curved, rigid plastic-coated metal Arms with Handles which are used for control of the device outside the body, and an elastic Engagement Element which conforms to the maternal and neonatal anatomy, and which is used for direct contact with the entrapped neonate within the birth canal.",
                    "armGroupLabels": [
                        "Yaari Extractor group"
                    ]
                }
            ]
