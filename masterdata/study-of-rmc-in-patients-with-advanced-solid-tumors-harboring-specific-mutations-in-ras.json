        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Experimental: RMC-6236",
                    "type": "EXPERIMENTAL",
                    "description": "Enrollment into dose exploration may be from any advanced solid tumor type with KRAS p.G12 mutations.\n\nEnrollment into dose expansion/optimization may be from groups consisting of patients with a single histotype/genotype (for example, KRAS G12-mutated NSCLC, PDAC, CRC, RAS mutant NSCLC, PDAC, CRC, Melanoma, gynecological cancer or other solid tumors not previously specified).\n\nRAS mutant is defined as any nonsynonymous mutation of KRAS, NRAS, or HRAS at codons 12, 13, or 61 (G12, G13, or Q61)",
                    "interventionNames": [
                        "Drug: RMC-6236"
                    ]
                }
            ],
