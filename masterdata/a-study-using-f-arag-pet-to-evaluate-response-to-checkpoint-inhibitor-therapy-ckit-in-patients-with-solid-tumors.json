        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Correlate change in [18F]F AraG signal following CkIT therapy with change in level of T cell infiltration in tumor biopsies",
                    "description": "Pre and post CkIT biopsies of the same tumor lesion will be performed an analyze T-cell tumor infiltration, this will be correlated to \\[18F\\]F-AraG PET signal.",
                    "timeFrame": "24 Months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "1. Correlate change in [18F]F AraG uptake signal in tumor lesions with clinical benefit rate (defined as Complete Response (CR) + Partial Response (PR) + stable disease (SD) > 4 months) using iRECIST criteria",
                    "timeFrame": "24 Months"
                },
                {
                    "measure": "2. Correlate change in [18F]F AraG signal in lung and GI tract with the occurrence of Grade 3 or 4 immune related adverse events.",
                    "timeFrame": "24 Months"
                }
            ]
        },
