        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Spontaneous coronary artery dissection (SCAD) group",
                    "type": "OTHER",
                    "description": "Subject with a history of SCAD will undergo a series of diagnostic studies to assess neurovascular function.",
                    "interventionNames": [
                        "Other: Arterial Tonometry",
                        "Diagnostic Test: Echocardiogram",
                        "Diagnostic Test: Carotid Ultrasounds",
                        "Diagnostic Test: Elastography",
                        "Other: Ankle-Brachial Index",
                        "Other: Microneurography with sympathoexcitatory maneuvers"
                    ]
                },
                {
                    "label": "Healthy Control Group",
                    "type": "OTHER",
                    "description": "Healthy subjects will undergo a series of diagnostic studies to assess neurovascular function.",
                    "interventionNames": [
                        "Other: Arterial Tonometry",
                        "Diagnostic Test: Echocardiogram",
                        "Diagnostic Test: Carotid Ultrasounds",
                        "Diagnostic Test: Elastography",
                        "Other: Ankle-Brachial Index",
                        "Other: Microneurography with sympathoexcitatory maneuvers"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "OTHER",
                    "name": "Arterial Tonometry",
                    "description": "We will measure arterial stiffness using arterial tonometry.",
                    "armGroupLabels": [
                        "Healthy Control Group",
                        "Spontaneous coronary artery dissection (SCAD) group"
                    ]
                },
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Echocardiogram",
                    "description": "We will collect limited ultrasound images of the heart.",
                    "armGroupLabels": [
                        "Healthy Control Group",
                        "Spontaneous coronary artery dissection (SCAD) group"
                    ]
                },
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Carotid Ultrasounds",
                    "description": "We will perform ultrasound imaging of the carotid artery.",
                    "armGroupLabels": [
                        "Healthy Control Group",
                        "Spontaneous coronary artery dissection (SCAD) group"
                    ]
                },
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Elastography",
                    "description": "We will measure the elastic properties of the arteries using elastography.",
                    "armGroupLabels": [
                        "Healthy Control Group",
                        "Spontaneous coronary artery dissection (SCAD) group"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Ankle-Brachial Index",
                    "description": "We will measure the ankle-brachial index to assess peripheral artery blood flow.",
                    "armGroupLabels": [
                        "Healthy Control Group",
                        "Spontaneous coronary artery dissection (SCAD) group"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Microneurography with sympathoexcitatory maneuvers",
                    "description": "We will measure sympathetic nerve activity at rest and in response to isometric handgrip, mental stress test, cold pressor test, Valsalva maneuvers.",
                    "armGroupLabels": [
                        "Healthy Control Group",
                        "Spontaneous coronary artery dissection (SCAD) group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Arterial thickness",
                    "description": "Measured by carotid ultrasound intimal media thickness reporting in mm",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Arterial stiffness",
                    "description": "Measured by carotid ultrasound pulse wave velocity reporting in m/s",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Baroreflex sensitivity",
                    "description": "The slope between heart rate and blood pressure at rest and during the Valsalva Maneuver (ms/mmHg)",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Sympathetic response to stress",
                    "description": "Blood pressure or muscle sympathetic nerve activity response to sympathoexcitatory stressors (mm/Hg or bursts/min).",
                    "timeFrame": "Baseline"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* For SCAD patients: history of SCAD as confirmed by coronary angiography or cardiac computed tomography angiography\n* For controls: no cardiac disease history\n\nExclusion Criteria:\n\n* Pregnancy\n* Known or suspected unstable cardiac condition within 4 weeks of the study\n* Active treatment with anticoagulation or dual antiplatelet therapy\n* Prisoner",
