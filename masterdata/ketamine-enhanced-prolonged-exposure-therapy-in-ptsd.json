        },
        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Ketamine and prolonged exposure (PE)",
                    "type": "EXPERIMENTAL",
                    "description": "Single IV ketamine 0.5 mg/kg 24-72 hrs prior to PE session at week 1,2, and 3 followed by 7 additional PE sessions.",
                    "interventionNames": [
                        "Drug: Ketamine"
                    ]
                },
                {
                    "label": "Midazolam and prolonged exposure (PE)",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "Single IV midazolam 0.045 mg/kg 24-72 hrs prior to PE session at week 1,2, and 3 followed by 7 additional PE sessions.",
                    "interventionNames": [
                        "Drug: Midazolam"
                    ]
                }
            ],
            "interventions": [
                {
