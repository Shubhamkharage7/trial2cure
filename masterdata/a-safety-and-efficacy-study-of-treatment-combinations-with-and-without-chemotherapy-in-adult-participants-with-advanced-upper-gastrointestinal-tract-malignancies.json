                "Zimberelimab",
                "Esophageal adenocarcinoma",
                "Gastric adenocarcinoma",
                "Gastric cancer",
                "Gastroesophageal junction cancer",
                "Anti-PD-1 antibody",
                "Anti-CD73",
                "anti-TIGIT antibody"
            ]
        },
