        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Rates of change in revised ALS functional rating scale (ALSFRS-R)",
                    "description": "Prepare motor outcome measures for clinical trials in sub-populations of patients with ALS or a related disorder who have identifiable genetic causes of disease",
                    "timeFrame": "48 months"
                },
                {
                    "measure": "Rates of change in Slow vital capacity (SVC)",
                    "description": "Prepare motor outcome measures for clinical trials in sub-populations of patients with ALS or a related disorder who have identifiable genetic causes of disease",
                    "timeFrame": "48 months"
                },
                {
                    "measure": "Rates of change in Spastic paraplegia rating scale (SPRS)",
                    "description": "Prepare cognitive and behavioral outcome measures for clinical trials in sub-populations of patients with ALS or a related disorder who have identifiable genetic causes of disease",
                    "timeFrame": "48 months"
                },
                {
                    "measure": "Rates of change in Edinburgh Cognitive and Behavioral ALS Screen (ECAS)",
                    "description": "Prepare cognitive and behavioral outcome measures for clinical trials in sub-populations of patients with ALS or a related disorder who have identifiable genetic causes of disease",
                    "timeFrame": "48 months"
                },
                {
                    "measure": "ALS Health Index (ALS-HI)",
                    "description": "Validate the ALS Health Index (ALS-HI), a novel patient reported outcome (PRO) measure",
                    "timeFrame": "48 months"
                },
                {
                    "measure": "Serum",
                    "description": "Determine the diagnostic utility of serum neurofilament concentrations",
                    "timeFrame": "48 months"
                },
                {
                    "measure": "Cerebrospinal Fluid (CSF)",
                    "description": "Determine the diagnostic utility of CSF neurofilament concentrations",
                    "timeFrame": "48 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria for affected individuals (primary participants) include:\n\n* Clinical diagnosis or suspicion of ALS or a related disorder, including, but not limited to, ALS-FTD, PLS, HSP, FTD, Multisystem Proteinopathy (MSP) and PMA.\n* Subject is able and willing to comply with study procedures\n\nExclusion Criteria for affected individuals (primary participants) include:\n\n* Subjects with a condition or who are in a situation which, in the PI's opinion, could confound the study finding or may interfere significantly with the individual's participation and compliance with the study protocol -- including but not limited to neurological, psychological and/or medical conditions\n\nInclusion criteria for biological family members (secondary participants) include:\n\n* Family member of an enrolled affected primary participant\n\nExclusion Criteria for biological family members (secondary participants) include:\n\n* Subjects with a condition or who are in a situation which, in the PI's opinion, could confound the study finding or may interfere significantly with the individual's participation and compliance with the study protocol -- including but not limited to neurological, psychological and/or medical conditions",
            "healthyVolunteers": true,
            "sex": "ALL",
            "minimumAge": "7 Years",
            "stdAges": [
                "CHILD",
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Primary participants - patients that have or are suspected to have ALS or a related disease.\n\nSecondary participants - family members of primary participants enrolled in the study",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
