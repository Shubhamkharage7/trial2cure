        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Correlation of hepatic fat estimation by the thermo-acoustic device with MRI-PDFF technique",
                    "description": "The study evaluates if the new thermo-acoustic device can accurately estimate the hepatic fat content among patients with non-alcoholic fatty liver disease to the currently approved method using MRI-PDFF scan.",
                    "timeFrame": "one year"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Evaluate the best and ideal number of the anatomical site for thermo-acoustic evaluation of liver",
                    "description": "The study evaluates the number of estimation required and the appropriate anatomical site to evaluate the liver to obtain best results",
                    "timeFrame": "One year"
                },
                {
                    "measure": "Evaluate the sensitivity of the thermo-acoustic device in the diagnosis of hepatic steatosis.",
                    "description": "Evaluate the sensitivity of the thermo-acoustic device in diagnosis presence of hepatic steatosis among patients with MRI-PDFF finding of more than 15% hepatic steatosis",
                    "timeFrame": "One year"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* patients diagnosed or suspected to have non-alcoholic fatty liver disease\n* Patients 18-70 years of age\n* be able to understand and sign on written informed consent\n* able to undergo ultrasound and MRI examinations\n\nExclusion Criteria:\n\n* any metal or electronic implants including but not limited to pacemakers, clips, hips\n* known history of pregnancy or becoming pregnant during study period\n* unable to understand and sign on written informed consent\n* intolerant to ultrasound and/or MRI examinations",
