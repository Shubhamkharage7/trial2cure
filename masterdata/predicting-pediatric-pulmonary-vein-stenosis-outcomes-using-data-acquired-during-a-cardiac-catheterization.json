            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Standardized catheterization assessment",
                    "description": "There are four components to the pulmonary vein assessment.\n\n1. Angiography: pulmonary artery wedge injection and/or selective pulmonary vein injection.\n2. Intravascular ultrasound: manual pullback of IVUS catheter from lobar segment to left atrium.\n3. Pressure assessment: pulmonary vein pressure (mmHg) measured before and after (if applicable) intervention.\n4. Compliance testing: the compliance of the vein will be measured using a compliant conventional balloon.",
                    "armGroupLabels": [
                        "Standardized catheterization assessment"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Pulmonary vein status",
                    "description": "Severity of pulmonary vein stenosis (score 0 to 3); Score 0: Unobstructed vein (best outcome), Score 1: Proximal vein disease, Score 2: Distal vein disease, Score 3: Vein atresia (worst outcome).",
                    "timeFrame": "12 months +/- 6 weeks or prior to study exit due to death or lung transplant"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Patient status",
                    "description": "Transplant free survival",
                    "timeFrame": "12 months"
                }
            ]
        },
