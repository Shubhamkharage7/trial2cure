        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Anti-MRSA therapy",
                    "description": "chart review to determine the clinical outcomes of patients with osteomyelitis who were placed on definitive anti-MRSA treatment, despite no identified MRSA cultures.",
                    "otherNames": [
                        "Anti-MRSA Antibiotics"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Comparison of treatment",
                    "description": "the comparison of treatment failure between anti-MRSA therapy and no anti-MRSA therapy defined as a composite of hospital readmission for osteomyelitis within 180 days of antibiotic initiation",
                    "timeFrame": "180 Days"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* \u2022 Admission to any MHS hospital between April 1, 2017 and April 1, 2023\n\n  * \\> 18 years of age\n  * Documented osteomyelitis location of the lower limb via ICD-10 code\n  * Documented imaging of lower limb osteomyelitis during index admission\n  * Planned intravenous (IV) antibiotics for at least 4 weeks\n\nExclusion Criteria:\n\n* \u2022 IV antibiotics for less than 24 hours inpatient\n\n  * Planned surgical intervention documented at admission\n  * Patients receiving monotherapy with an anti-MRSA agent\n  * Positive MRSA culture during index admission\n  * Current outpatient antibiotic use on index admission\n  * Repeated hospital admission during study period",
            "healthyVolunteers": false,
            "sex": "ALL",
