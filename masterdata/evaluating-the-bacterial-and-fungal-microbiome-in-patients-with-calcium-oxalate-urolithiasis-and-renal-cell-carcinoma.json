        "descriptionModule": {
            "briefSummary": "The purpose of the study is to identify bacterial and fungal microbiome associated with calcium oxalate (CO) urolithiasis and renal cell carcinoma (RCC)."
        },
        "conditionsModule": {
            "conditions": [
                "Calcium Oxalate Urolithiasis",
                "Renal Cell Carcinoma"
            ],
            "keywords": [
                "calcium oxalate stones",
                "renal cell carcinoma",
                "microbiome",
                "urinary microbiome"
            ]
        },
        "designModule": {
            "studyType": "OBSERVATIONAL",
            "patientRegistry": false,
            "designInfo": {
                "observationalModel": "CASE_ONLY",
                "timePerspective": "PROSPECTIVE"
            },
            "bioSpec": {
                "retention": "SAMPLES_WITH_DNA",
                "description": "the microbiome of voided urine, bladder urine, stone fragments, and tissue from renal papillary biopsies in patients with CO stones.\n\nThe microbiome of urine from the bladder, tissue from the renal tumor, and tissue from surrounding healthy renal tissue in patients with RCC\n\nVoided urine sample from healthy volunteers."
            },
            "enrollmentInfo": {
                "count": 90,
                "type": "ESTIMATED"
            }
        },
        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "calcium oxalate urolithiasis group",
                    "description": "Participants with CO urolithiasis scheduled for definitive endoscopic stone removal will be evaluated to identify bacterial and fungal microbiome in mid-stream voided urine sample, urine sample collected from the bladder, kidney stone fragment, and a renal papillae biopsy specimen."
                },
                {
                    "label": "renal cell carcinoma group",
                    "description": "Participants with RCC scheduled for definitive kidney removal surgery will be evaluated to identify bacterial and fungal microbiome in a catheterized urine specimen from the bladder, a biopsy of the tumor, and a biopsy of normal-appearing renal papillae specimen."
                },
                {
                    "label": "healthy control group",
                    "description": "The participants will provide a clean-catch voided midstream urine sample to identify bacterial and fungal microbiome."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Microbial community structure in patients with CO urolithiasis",
                    "description": "Alpha and beta diversity, principal coordinate analysis, and phylogenetic trees of the bacterial and fungal microbial community from mid-stream voided urine sample, urine sample collected from the bladder, kidney stone fragment, and a renal papillae biopsy specimen.",
                    "timeFrame": "Day 1"
                },
                {
                    "measure": "Microbial community structure in patients with RCC",
                    "description": "Alpha and beta diversity, principal coordinate analysis, and phylogenetic trees of the bacterial and fungal microbial community from catheterized urine specimen from the bladder, a biopsy of the tumor, and a biopsy of normal-appearing renal papillae specimen.",
                    "timeFrame": "Day 1"
                },
                {
                    "measure": "Microbial community structure in healthy participants",
                    "description": "Alpha and beta diversity, principal coordinate analysis, and phylogenetic trees of the bacterial and fungal microbial community from clean-catch urine specimen.",
                    "timeFrame": "Day 1"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Adults that are able to consent (\u2265 18 years of age)\n* No history of asymptomatic bacteriuria or Urinary Tract Infection within the past 1 month\n* No history of coagulopathy and not on anticoagulant or antiplatelet medication at the time of surgery.\n* For patients with urolithiasis, must not have a history of RCC\n* For patients with RCC, must not have a history of urolithiasis\n* Controls must have no history of stones, RCC, or urinary tract infection (within 1 month).\n* For all participants, no antimicrobial treatment within the past month\n\nExclusion Criteria:\n\n* Adults unable to consent\n* For patients with urolithiasis, technical difficulty in taking renal papillae biopsy.\n* History of urinary tract infection within the past 1 month\n* History of coagulopathy and on anticoagulants or antiplatelets that could not be stopped perioperatively.\n* Patients with nephrolithiasis and renal tumors with positive preoperative urine cultures- suggesting asymptomatic bacteriuria.\n* For patients with urolithiasis, a history of RCC\n* For patients with RCC, a history of urolithiasis\n* Controls who have a history of stones, RCC, or urinary tract infection.\n* For all participants, antimicrobial treatment within the past month.",
            "healthyVolunteers": true,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "1. Healthy participants\n2. Participants with CO urolithiasis scheduled for endoscopic kidney stone removal surgery.\n3. Participants with RCC who are scheduled for kidney removal surgery.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
