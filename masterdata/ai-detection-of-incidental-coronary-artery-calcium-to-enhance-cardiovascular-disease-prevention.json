        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Notification of Coronary Artery Calcification",
                    "type": "EXPERIMENTAL",
                    "description": "Notification to providers of the presence of coronary artery calcification automatically detected by AI based device (software) on chest CT.\n\nRecommendation of preventive therapy.",
                    "interventionNames": [
                        "Device: Electronic health record-based notification of Nanox.AI Coronary Artery Calcification Assessment"
                    ]
                },
                {
                    "label": "Non-notification of Coronary Artery Calcification",
                    "type": "NO_INTERVENTION",
                    "description": "No notification to providers of the presence of coronary artery calcification on chest CT automatically detected by AI based device (software) on chest CT.\n\nNo recommendation of preventive therapy."
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Electronic health record-based notification of Nanox.AI Coronary Artery Calcification Assessment",
                    "description": "The intervention involves electronic health record, email, or letter notification about the presence of coronary artery calcification on chest CT and recommendation of preventive therapy. This is based on results of the automatic detection of coronary artery calcification from AI device (software).",
                    "armGroupLabels": [
                        "Notification of Coronary Artery Calcification"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Proportion of participants with initiation or intensification of lipid-lowering therapy after intervention",
                    "description": "Initiation or intensification of therapy (yes/no) by 6 months after the intervention. Initiation: starting lipid-lowering therapy; intensification: increasing the dose of lipid-lowering therapy",
                    "timeFrame": "6 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Average change in LDL-C after intervention",
                    "description": "Average quantitative change in LDL-C (low-density lipoprotein cholesterol) between baseline and 12 months after intervention",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Proportion of participants with downstream invasive or noninvasive testing after intervention",
                    "description": "Composite outcome: Downstream testing (yes/no) including coronary artery calcium score, coronary CT angiography, invasive coronary angiography, or stress testing by 12 months after the intervention.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Proportion of participants with major adverse cardiovascular events after intervention",
                    "description": "Composite outcome: Occurrence of an adverse cardiovascular event (yes/no) including cardiovascular death, myocardial infarction, or stroke by 12 months after the intervention",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Proportion of participants with initiation or intensification of other preventive therapies after intervention",
                    "description": "Initiation or intensification of preventive therapy (yes/no) by 6 months after the intervention. Preventive therapy includes any of the following: aspirin, antihypertensive therapy, metabolic therapy. Initiation: starting preventive therapy; intensification: increasing the dose of preventive therapy",
                    "timeFrame": "6 months"
                }
            ]
        },
