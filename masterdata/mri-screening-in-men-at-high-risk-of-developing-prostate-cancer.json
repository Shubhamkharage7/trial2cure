        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "High Risk- Positive Germline Mutation (n=40):",
                    "type": "OTHER",
                    "description": "Men who harbor known germline mutations that have been associated with an increased risk of prostate cancer and aggressive disease (e.g. BRCA2, ATM, PALB2, etc.) with or without a known family history of prostate cancer.",
                    "interventionNames": [
                        "Other: Prostate MRI"
                    ]
                },
                {
                    "label": "High Risk- High GRS (n=40):",
                    "type": "OTHER",
                    "description": "Men who harbor significantly increased disease risk based upon genetic risk score (GRS) value \\>1.5 with or without a known family history of prostate cancer.",
                    "interventionNames": [
                        "Other: Prostate MRI"
                    ]
                },
                {
                    "label": "High Risk- Family History (n=45):",
                    "type": "OTHER",
                    "description": "Men with a family history of prostate cancer in at least one sibling, father, uncle, or grandfather but no known increased genetic risk of prostate cancer (has no pathogenic or likely pathogenic mutation along with a low genetic risk score (GRS\\<1.5).",
                    "interventionNames": [
                        "Other: Prostate MRI"
                    ]
                },
                {
                    "label": "Low Risk (n=125):",
                    "type": "OTHER",
                    "description": "No known germline mutation, low genetic risk score (GRS \\<1.5), and no known family history of prostate cancer.",
                    "interventionNames": [
                        "Other: Prostate MRI"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "OTHER",
                    "name": "Prostate MRI",
                    "description": "Group placement is determined by GRS score, Family history, and germline testing",
                    "armGroupLabels": [
                        "High Risk- Family History (n=45):",
                        "High Risk- High GRS (n=40):",
                        "High Risk- Positive Germline Mutation (n=40):",
                        "Low Risk (n=125):"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Proportion of patients with high grade prostate cancer detection.",
                    "description": "MRI results will be collected every 3 years for 15 years until high grade prostate cancer is detected as defined as Gleason greater than or equal to 7 (Grade Group 2).",
                    "timeFrame": "Every 3 years up to 15 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Male age 18 and older\n* No known history of prostate cancer\n* No previous prostate resection or ablation (e.g. TURP, photovaporization)\n\nExclusion Criteria:\n\n* Unable to tolerate MRI due to metal fragments or claustrophobia\n* Lack of a rectum\n* Hip arthroplasty",
