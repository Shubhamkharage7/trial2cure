            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Transplant preparative regimen of alemtuzumab, fludarabine, thiotepa, and melphalan",
                    "description": "Between days -23 and -15: alemtuzumab test dose, 3mg IV or SQ Day -14: alemtuzumab, 10mg IV or SQ Day -13: alemtuzumab, 15mg IV or SQ Day -12: alemtuzumab, 20mg IV or SQ Days -8 to -4: fludarabine, 30mg/m2 IV Day -4: thiotepa 4mg/kg IV q 12 hours Day -3: melphalan, 140mg/m2 IV Day 0: stem cell infusion Day +7: G-CSF",
                    "armGroupLabels": [
                        "Preparative"
                    ]
                }
            ]
        },
