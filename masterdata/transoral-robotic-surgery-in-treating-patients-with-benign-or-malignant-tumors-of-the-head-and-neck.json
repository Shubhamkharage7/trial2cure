        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Transoral robotic surgery (TORS)",
                    "type": "EXPERIMENTAL",
                    "description": "Patients will undergo TORS for oral and laryngopharyngeal benign and malignant lesions using the Da Vinci Robotic Surgical System. After surgery regular clinical assessments will be scheduled to see how the patient is doing. Patients will be asked to answer a quality of life assessment as part of the study. If patients are unable to come to the Ohio State University Medical Center for a physician appointment they will be contacted via phone or mailed a questionnaire to complete.",
                    "interventionNames": [
                        "Procedure: transoral robotic surgery",
                        "Procedure: quality of life assessment"
                    ]
                }
            ],
            "interventions": [
