        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "To measure the rate of change (growth rate slope) in geographic atrophy (GA) lesion size",
                    "timeFrame": "From baseline to Month 24]"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "To measure the change in best-corrected visual acuity (BCVA) as assessed using the Early Treatment Diabetic Retinopathy Study (ETDRS) scale",
                    "timeFrame": "From baseline to Month 24"
                },
                {
                    "measure": "To measure changes in the area and size of the inner/outer segment junction of photoreceptors by spectral domain optical coherence tomography (SD-OCT)",
                    "timeFrame": "From baseline to Month 24"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Subjects must have a confirmed diagnosis of GA with atrophic lesions in 1 or both eyes.\n* Minimum BCVA is required in the study eye\n\nExclusion Criteria:\n\n* The presence of diabetic macular edema or macular disease in either eye.\n* Diabetic retinopathy more advanced than mild nonproliferative diabetic retinopathy, or any other retinal vascular disease in either eye.\n* Uncontrolled diagnosed glaucoma in the study eye",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "60 Years",
