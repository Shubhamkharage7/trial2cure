        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Wiidookaage'win Facebook Group",
                    "type": "EXPERIMENTAL",
                    "description": "The Wiidookaage'win Facebook Group is an online, asynchronous group intervention that will run for 3-months with approximately 30 days of content developed. It is private and hidden, meaning only those in the study will be able to see or find the group. It will be moderated by two women from the study team (one AIAN and one biracial). A back-up moderator will also have access to the group. Participants will be able to comment, react, view, and post in the intervention group, and are encouraged to interact with one another.",
                    "interventionNames": [
                        "Behavioral: Wiidookaage'win Facebook Group"
                    ]
                },
                {
                    "label": "Online Resources Landing Page",
                    "type": "NO_INTERVENTION",
                    "description": "The Online Resources Landing Page was developed by the study team using https://carrd.co. It includes links to local programs in family services, substance use recovery, legal advocacy, etc. which already exist and may help support their recovery."
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Wiidookaage'win Facebook Group",
                    "description": "An online social media group program that incorporates social support, mindfulness-based, and cultural practices that may support OUD recovery.",
                    "armGroupLabels": [
                        "Wiidookaage'win Facebook Group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Feasibility of Facebook Intervention",
                    "description": "The study team will measure the feasibility from the combined reports of recruitment, study retention, level of Facebook intervention uptake/engagement, and treatment satisfaction.",
                    "timeFrame": "3 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Timeline Follow-back Interview and UDS to measure Opioid Abstinence",
                    "description": "The study team will explore the preliminary differential impact of the intervention compared with the control condition on opioid abstinence and MOUD continuation. Opioid and other substance use will be assessed through monthly Timeline Follow-Back (TLFB) interviews (Sobell \\& Sobell, 1992; Wray et al., 2016). At 1, 3, and 6-months, the study team will obtain a UDS that will be used along with the TLFB interviews to measure opioid abstinence.",
                    "timeFrame": "6 months"
                },
                {
                    "measure": "Reporting MOUD continuation",
                    "description": "The study team will summarize MOUD continuation as the proportion of participants reporting current use at 6-month follow-up.",
                    "timeFrame": "6 months"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Perceived Stress Scale and Communal Mastery Scale",
                    "description": "The study team will explore the differential impact of the intervention compared with the control condition on constructs targeted by our intervention, specifically perceived stress and coping with stressful life circumstances from a more collectivist orientation, respectively.",
                    "timeFrame": "3 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. AIAN person based on self-reported race/ethnicity\n2. Gender identity as a woman\n3. At least 18 years of age with no upper age limit\n4. Resides in Minnesota\n5. Meets criteria for OUD based on the DSM-5 Checklist (American Psychiatric Association, 2013)\n6. Self-reports at least one month of abstinence from opioid use based on TLFB interview and negative urine opiate screen\n7. Current use of MOUD\n8. Is comfortable speaking and reading English\n9. Has an existing Facebook account or willing to set one up\n10. Is willing and able to participate in the Facebook intervention for 3 months\n11. Has access to broadband internet on a mobile phone/computer/tablet at any location\n12. Is willing and able to travel to a community clinic in Minneapolis, Minnesota for the UDS or is willing to and able to complete a virtual Zoom UDS visit.\n13. Provides written informed consent\n\nExclusion Criteria:\n\n1. Self-reports current suicidality based on the Concise Heath Risk Tracking scale\n2. Participated in prior study phases (Aim 1 formative work) of NIDA CTN-0123",
            "healthyVolunteers": false,
            "sex": "FEMALE",
            "genderBased": true,
            "genderDescription": "Participants may be cisgender or transgender women. They may also identify as Two Spirit and include \"woman\" as part of their gender identity.",
            "minimumAge": "18 Years",
