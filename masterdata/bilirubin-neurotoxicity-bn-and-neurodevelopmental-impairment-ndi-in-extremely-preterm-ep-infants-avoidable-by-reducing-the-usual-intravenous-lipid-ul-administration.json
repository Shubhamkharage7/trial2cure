            "interventions": [
                {
                    "type": "DRUG",
                    "name": "usual prescribed intralipid (UL) regimen",
                    "description": "Intralipid will be started at 1 g/kg/day and routinely advanced by 1 g/kg/day each day to a max of 3 g/kg/day as per usual clinical practice",
                    "armGroupLabels": [
                        "usual prescribed intralipid (UL) regimen"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "restricted prescribed intralipid (RL) regimen",
                    "description": "Intralipid will be started at 0.5 g/kg/day and routinely advanced by 0.5 g/kg/day each day to a max of 1.5 g/kg/day (half rate of usual clinical practice).",
                    "armGroupLabels": [
                        "restricted prescribed intralipid (RL) regimen"
                    ]
                }
            ]
        },
