        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Electroencephalogram (EEG) Test",
                    "description": "EEG test and delirium assessments will be performed on subjects experiencing delirium in the ICU.",
                    "otherNames": [
                        "Delirium Assessments"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Perform Confusion Assessment Method for the ICU (CAM-ICU) assessment",
                    "description": "Research coordinator will perform and collect CAM-ICU assessment data on study subjects",
                    "timeFrame": "3 times per day, for a maximum of six (6) days"
                },
                {
                    "measure": "Perform Electroencephalogram (EEG) Test",
                    "description": "Record and collect EEG brain waves on study subjects using Ceribell Rapid Response EEG to assess multiple time-frequency EEG features to differentiate between delirium positive and delirium negative patients.",
                    "timeFrame": "6-8 hours per day, for a maximum of six (6) days"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria: All must be present\n\n* Age is 18 years or older\n* Admitted to the Intensive Care Unit (ICU)\n* Subject must be fluent in the language in which the delirium assessment is performed\n\nExclusion Criteria: All must be absent\n\n- Any condition that prevents use of the Ceribell EEG system on the subject for the entire anticipated EEG monitoring period. Examples may include: Craniectomy with missing bone flap in an area where Ceribell EEG electrodes are placed. Expected use of continuous clinical EEG for the entire anticipated monitoring period.",
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Subjects who experience at least one delirious episode during their ICU stay",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
