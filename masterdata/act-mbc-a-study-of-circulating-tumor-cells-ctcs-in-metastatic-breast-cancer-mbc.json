        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "To prospectively evaluate the impact of CellSearch CTCs on treatment decisions and response assessment in MBC patients",
                    "description": "Assessing proportion of providers that find CellSearch CTCs helpful in determining response to therapy in each unique patient (Questionnaire #2).",
                    "timeFrame": "3 to 4 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Assess barriers and receptiveness of providers in using CTCs in clinical practice",
                    "description": "Assessing provider perception of the general usefulness of CTC in pre and post-intervention surveys (Questionnaire #1 and #3)",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Evaluate if CTC results correlate with response assessment and disease progression defined by standard of care imaging",
                    "description": "Compare baseline and on treatment serial CTCs assessment to identify correlations with first restaging studies and with progression free survival",
                    "timeFrame": "18 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Invasive breast cancer with radiographic and/or clinical evidence of advanced metastatic or unresectable disease\n2. ER+/HER2- patients prior to starting 2nd line therapy or beyond OR ER/Progesterone Receptor (PR)/HER2-negative (Triple Negative) patients prior to starting any line of therapy\n3. Measurable and/or non-measurable disease is allowed\n4. Male or female breast cancer is allowed\n5. Age \\> 18 years\n6. Willingness to provide mandatory blood specimens\n7. Willing to return to enrolling institution for follow up imaging at least once\n\nExclusion Criteria:\n\n1. Life expectancy of \u2264 6 months\n2. Inability to provide blood samples based on the judgment of the treating provider",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Advanced/metastatic breast cancer of following subtypes/lines of systemic therapy in metastatic setting:\n\n* Estrogen Receptor (ER)+/Human Epidermal growth factor Receptor 2 (HER2) - patients prior to starting 2nd line or later therapy\n* Triple Negative (TN) patients prior to starting any line of systemic therapy",
            "samplingMethod": "PROBABILITY_SAMPLE"
        },
