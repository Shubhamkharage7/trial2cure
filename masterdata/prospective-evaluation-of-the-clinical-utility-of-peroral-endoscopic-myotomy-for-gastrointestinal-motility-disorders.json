        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Post procedural Reflux Rate",
                    "description": "Rate of Long-Term Clinical Success and Post-procedural Reflux Rate Among Patients with Esophageal Dysmotility Undergoing POEM",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Long-Term Clinical Efficacy of G-POEM for the management of medically refractory gastroparesis",
                    "description": "The main outcome of the study is to evaluate the proportion of patients who have clinical response to GPOEM. Clinical response is measured by the validated symptom assessment tool (gastroparesis cardinal symptom index - GCSI). Patients with a decrease in GCSI by one or more point will be defined as having a clinical response.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Long-Term Clinical Efficacy of Z-POEM for the management of Zenker's Diverticulum and Rate of Recurrence",
                    "description": "The main outcome of this study is to evaluate rate of recurrence of symptoms after Z-POEM for Zenker's diverticulum. Recurrence is defined as a return of patient's symptoms (dysphagia, odynophagia) at the time of clinic follow-up.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Evaluate the efficacy of POEM for the management of gastrointestinal disease for esophageal motility.",
                    "description": "The main outcome measure is to evaluate the long-term efficacy of POEM for esophageal motility disorders. Efficacy or clinical response is usually assessed using the Eckardt score. An Eckardt score of \\< 3 is traditionally defined as treatment response following POEM.",
                    "timeFrame": "12 months"
                }
            ]
        },
