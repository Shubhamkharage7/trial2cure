            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Transcutaneous Electrical Acustimulation (TEA)",
                    "description": "The severity and frequency participants pain will be measured during a run-in period of 2 weeks with to assess baseline pain severity and frequency. Eligible participants will have a 4-week treatment period at home. Stimulation with the TEA device will be performed for 30 minutes twice per day, in the morning and in the evening.\n\nIn addition to using the device, participants will have study visits, complete surveys, as well as provide medical information during the study.",
                    "armGroupLabels": [
                        "Transcutaneous Electrical Acustimulation (TEA)"
                    ]
                }
            ]
        },
