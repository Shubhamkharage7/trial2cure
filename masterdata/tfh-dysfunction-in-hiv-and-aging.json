        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Young HIV negative group",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "HIV uninfected participants that are 18-35 years of age will receive the standard dose flu vaccine. Participants who did not respond to the standard dose flu vaccination, as defined by less than a four-fold increase in flu antibody titer from baseline, will then receive the high dose flu vaccination 1 year after initial standard dose flu vaccination. Participants who respond to the standard dose flu vaccination will not receive the high dose flu vaccination.",
                    "interventionNames": [
                        "Biological: Standard dose influenza vaccination",
                        "Biological: High dose influenza vaccination"
                    ]
                },
                {
                    "label": "Young HIV positive group",
                    "type": "EXPERIMENTAL",
                    "description": "HIV infected viral suppressed participants, who are on anti-retroviral therapy (ART) aged 18-35 years of age, will receive the standard dose flu vaccine first followed by the high dose flu vaccine 1 year after initial standard dose.",
                    "interventionNames": [
                        "Biological: Standard dose influenza vaccination",
                        "Biological: High dose influenza vaccination"
                    ]
                },
                {
                    "label": "Old HIV negative group",
                    "type": "EXPERIMENTAL",
                    "description": "HIV uninfected participants that are 65 years and older will receive the standard dose flu vaccine first followed by the high dose flu vaccine 1 year after initial standard dose.",
                    "interventionNames": [
                        "Biological: Standard dose influenza vaccination",
                        "Biological: High dose influenza vaccination"
                    ]
                },
                {
                    "label": "Old HIV positive group",
                    "type": "EXPERIMENTAL",
                    "description": "HIV infected viral suppressed participants, who are on anti-retroviral therapy (ART) aged 65 years and older, will receive the standard dose flu vaccine first followed by the high dose flu vaccine 1 year after initial standard dose.",
                    "interventionNames": [
                        "Biological: Standard dose influenza vaccination",
                        "Biological: High dose influenza vaccination"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BIOLOGICAL",
                    "name": "Standard dose influenza vaccination",
                    "description": "Single 0.5 ml injection administered intramuscularly into participant's deltoid muscle",
                    "armGroupLabels": [
                        "Old HIV negative group",
                        "Old HIV positive group",
                        "Young HIV negative group",
                        "Young HIV positive group"
                    ],
                    "otherNames": [
                        "Fluzone quadrivalent"
                    ]
                },
                {
                    "type": "BIOLOGICAL",
                    "name": "High dose influenza vaccination",
                    "description": "4 times more concentrated single 0.7 ml injection administered intramuscularly into participant's deltoid muscle",
                    "armGroupLabels": [
                        "Old HIV negative group",
                        "Old HIV positive group",
                        "Young HIV negative group",
                        "Young HIV positive group"
                    ],
                    "otherNames": [
                        "Fluzone high dose quadrivalent"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in HAI antibody response",
                    "description": "Change in mean titers of Hemagglutination inhibition (HAI) antibody response will be measured using serum sample.",
                    "timeFrame": "Baseline to Month 6 (post standard dose flu vaccination), Baseline to Month 18 (post high-dose flu vaccination)"
                },
                {
                    "measure": "Change in neutralization antibody response",
                    "description": "Change in mean titers of neutralization antibody response will be measured using serum sample.",
                    "timeFrame": "Baseline to Month 6 (post standard dose flu vaccination), Baseline to Month 18 (post high-dose flu vaccination)"
                },
                {
                    "measure": "Percentage of Participants with Vaccine Response",
                    "description": "Percentage of participants that are vaccine responders and non-responders will be reported. Response is evaluated using seroconversion to the flu vaccine using serum sample.",
                    "timeFrame": "Month 13 (1 month post high-dose flu vaccination)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Percentage of T-follicular helper (Tfh) cells",
                    "description": "Percentage of Tfh cells as measured using peripheral blood samples",
                    "timeFrame": "Month 13 (1 month post high-dose flu vaccination)"
                },
                {
                    "measure": "Percentage of B cells",
                    "description": "Percentage of B cells as measured using peripheral blood samples",
                    "timeFrame": "Month 13 (1 month post high-dose flu vaccination)"
                },
                {
                    "measure": "Percentage of monocytes",
                    "description": "Percentage of monocytes as measured using peripheral blood samples",
                    "timeFrame": "Month 13 (1 month post high-dose flu vaccination)"
                },
                {
                    "measure": "Percentage of Tfh cells producing cytokines",
                    "description": "Percentage of Tfh cells producing cytokines as measured using peripheral blood samples",
                    "timeFrame": "Month 13 (1 month post high-dose flu vaccination)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. For HIV positive participants:\n\n   * HIV infection, as documented by any licensed ELISA test kit. Participants on ART as a result of prior HIV documented infection will not be required to provide proof of diagnosis of HIV infection.\n   * Additional criteria for HIV positive\n\n     1. on ART for at least 1 year. Occasional viral blips up to 1000 copies/ml also acceptable provided the patients are on continuing treatment,\n     2. Cluster of differentiation 4 (CD4) count available in the prior 6 months and \\>200/mm3\n     3. Undetectable viral load (\\< 40 copies/mL). Blips of \\<1000 copies/mL will be allowed.\n2. For HIV negative participants:\n\n   * Documented negative HIV test at the time of study entry, either by any licensed ELISA.\n3. For all participants:\n\n   1. Individuals age: \u226435 years and \u226565 years.\n   2. No history of other immunodeficiency disorders\n   3. Not on steroid or other immunosuppressive/immunomodulators medications.\n   4. No active malignancies.\n   5. Agreeable to receive both regular standard (STD-TIV) and high dose (HD-TIV) influenza vaccination.\n   6. Agreeable to participate in study for a complete course of study full visits including 2 consecutive flu seasons.\n   7. Able to provide informed consent.\n\nExclusion criteria\n\n1. Contraindication to receive influenza vaccination.\n2. Non-adherence to ART for HIV positive\n3. Unable to provide informed consent.\n4. Influenza vaccination already given during the current vaccination season.\n5. Known drug abuse including cocaine by history",
            "healthyVolunteers": true,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
