        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Regimen A: TBI/Thiotepa/Cyclophosphamide",
                    "type": "EXPERIMENTAL",
                    "description": "Patients in enrolled in Regimen A will receive the following:\n\n* Total Body Irradiation (TBI), hyper-fractionated to a dose of 1320 cGy depending on age, stage of disease and requirement of general anesthesia with lung shielding\n* Thiotepa, 5 mg/kg/day x 2 days via IV infusion over 4 hours daily or 10 mg/kg on one day\n* Cyclophosphamide, 60 mg/kg/day x 2 days via IV infusion over 2 hours daily (or if cyclophosphamide is contraindicated, fludarabine at 25 mg/m\\^2 x 5 days may be substituted)",
                    "interventionNames": [
                        "Radiation: Total Body Irradiation (TBI)",
                        "Drug: Thiotepa",
                        "Drug: Cyclophosphamide"
                    ]
                },
                {
                    "label": "Regimen B: Busulfan/Melphalan/Fludarabine",
                    "type": "EXPERIMENTAL",
                    "description": "Patients in enrolled in Regimen B will receive the following:\n\n* Busulfan, IV 0.8 mg/kg q6hours x 10 or 12 doses over 3 days, depending on the disease\n* Melphalan, 70 mg/m\\^2/day x 2 days via IV infusion over 30 minutes daily\n* Fludarabine, 25 mg/m\\^2/day x 5 days via IV infusion over 30 minutes daily",
                    "interventionNames": [
                        "Drug: Busulfan",
                        "Drug: Melphalan",
                        "Drug: Fludarabine"
                    ]
                }
            ],
