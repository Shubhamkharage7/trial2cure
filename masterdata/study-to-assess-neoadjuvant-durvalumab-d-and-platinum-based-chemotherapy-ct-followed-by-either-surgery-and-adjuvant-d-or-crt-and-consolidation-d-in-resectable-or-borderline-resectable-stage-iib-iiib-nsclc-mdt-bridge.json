                {
                    "measure": "Surgical safety: Intended surgical approach",
                    "description": "Intended surgical approach at baseline (minimally invasive vs open thoracotomy).",
                    "timeFrame": "At baseline"
                },
                {
                    "measure": "Surgical safety: Actual surgical approach",
                    "description": "Actual surgical approach (minimally invasive vs open thoracotomy).",
                    "timeFrame": "At surgery"
                },
