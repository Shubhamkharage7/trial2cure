        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Increlex\u00ae",
                    "description": "Increlex\u00ae (mecasermin \\[rDNA origin\\] injection), 10 mg/ml solution for injection, 40-120mcg/kg BID or 0,04 to 0,12 mg/kg BID, as prescribed by physician"
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Incidence of SAEs (including AESI of neoplasia) and all AEs, targeted AEs, deaths and withdrawals due to AEs.",
                    "description": "Targeted AE includes hypersensitivity; scoliosis; immunogenicity (presence of antibodies if available); slipped capital femoral epiphysis, headache, otitis media, papilloedema, hypoglycaemia (suspected or documented - documented means blood level glucose \\< 50 mg/dL or 2.78 mmol/L), acromegalic facial changes, gynaecomastia, hearing loss, intracranial hypertension, lipohypertrophy at injection sites, sleep apnoea, tonsillar hypertrophy, cardiomegaly, oedema and myalgia.",
                    "timeFrame": "During the treatment period up to 30 days after the last dose."
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Incidence of SAEs (including AESI of neoplasia), targeted AEs, all AEs, deaths, withdrawals due to AEs, special situations and concomitant medications",
                    "description": "In the overall population, and in the subset of children and adolescents exposed to Increlex\u00ae for at least 3 cumulative years excluding interruptions.",
                    "timeFrame": "Within 5 years post-treatment"
                },
                {
                    "measure": "Incidence of special situations and concomitant medications",
                    "timeFrame": "During the treatment period an average of 5 years and within 5 years post-treatment"
                },
