            "armGroups": [
                {
                    "label": "Intervention Group",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Will receive active medication semaglutide subcutaneously, once weekly, self-injection.\n\nMonth 1 - 0.24 mg SC once weekly x 4 weeks.(IE-1) Month 2- 0.5 mg SC once weekly x 4 weeks.(IE-2) Month 3 -1 mg SC once weekly x 4 weeks.(IE-3) Month 4 - 1.7 mg SC once weekly x 4 weeks.(IE-4) Month 5 - 2.4 mg SC once weekly x 4 weeks. (IE-5) Month 6 - 2.4 mg SC continue once weekly x 8 weeks.(IE-6) Month 7 - completion visit (IE-7)",
                    "interventionNames": [
                        "Drug: Semaglutide 2.4mg"
                    ]
                },
