        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Combination T-VEC/Pembrolizumab",
                    "type": "EXPERIMENTAL",
                    "description": "Pre-operative talimogene laherparepvec (T-VEC) with Pembrolizumab\n\nT-VEC - intra-lesional injection into palpable lymph nodes every 3 weeks for 6 months, or until complete response of target tumors.\n\nPembrolizumab - administered intravenously every 3 weeks for 6 months, then every 3 weeks for one year in the adjuvant setting following complete lymph node dissection.",
                    "interventionNames": [
                        "Drug: Pembrolizumab",
                        "Drug: Talimogene Laherparepvec"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
