        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "standard of care",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Standard of care arm will receive treatment at the time symptoms develop. Patients will be provided a list of recommended moisturizers by the female sexual medicine and women's health program (FSMWHP), but will purchase the moisturizers themselves at their local pharmacy.",
                    "interventionNames": [
                        "Other: questionnaires",
                        "Other: moisturizers"
                    ]
                },
                {
                    "label": "intervention",
                    "type": "EXPERIMENTAL",
                    "description": "Patients who are randomized to the upfront arm will receive sexual health counseling and be initiated on non-hormonal moisturizers 3-5 times per week (based on data regarding need in our patient population) at the time ovarian suppression. The upfront intervention group will also be educated about dilators and have a consultation with a pelvic floor physical therapist. Some potential vaginal moisturizers include Hyalogyn, Replens, Liquibeads, or Vitamin E capsules.",
                    "interventionNames": [
                        "Other: questionnaires",
                        "Other: moisturizers"
                    ]
                }
            ],
