        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "TruGraf\u00ae Liver gene expression panel (GEP)",
                    "description": "Test done on blood sample collected during for cause or routine liver biopsy"
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Correlation of the TruGraf\u00ae Liver GEP with rejection activity index (RAI) on surveillance and for-cause biopsy.",
                    "description": "To evaluate the performance of the blood-based TruGraf\u00ae Liver GEP to accurately diagnose rejection, confirmed by surveillance or for-cause liver biopsy.",
                    "timeFrame": "Week prior to or at time of biopsy"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Comparison of the GEP to non-rejection injury found on biopsy",
                    "description": "A comparison of the GEP to non-rejection injury (e.g., infection) found on biopsy",
                    "timeFrame": "Week prior to or at time of biopsy"
                },
                {
                    "measure": "Utility of the GEP for monitoring liver injury",
                    "description": "Utility of the GEP for monitoring liver injury (i.e., as treatment is weaned, does the GEP return to a non-rejection correlate).",
                    "timeFrame": "Week prior to or at time of biopsy"
                }
            ]
        },
        "eligibilityModule": {
