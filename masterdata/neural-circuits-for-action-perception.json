        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Selectivity for kinematics and movement type in neurophysiological responses",
                    "description": "Measure intracranial field potentials (in microvolts) in the gamma frequency band through neurophysiological responses comparing neural responses during action perception versus execution.\n\nWe will assess selectivity for kinematics for each movement type",
                    "timeFrame": "Through study completion, an average of 2 years."
                },
                {
                    "measure": "Evoked responses by action execution",
                    "description": "Measure intracranial field potentials (in microvolts) in the gamma frequency band through neurophysiological responses comparing neural responses during action perception versus execution.\n\nWe will quantify responses evoked by action execution",
                    "timeFrame": "Through study completion, an average of 2 years."
                },
                {
                    "measure": "Evoked responses by action observation",
                    "description": "Measure intracranial field potentials (in microvolts) in the gamma frequency band through neurophysiological responses comparing neural responses during action perception versus execution.\n\nWe will quantify responses evoked by action observation",
                    "timeFrame": "Through study completion, an average of 2 years."
                }
            ]
        },
