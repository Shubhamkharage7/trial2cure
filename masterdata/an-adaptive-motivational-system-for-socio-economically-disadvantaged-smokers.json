        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Self-Reported Participant Tobacco Cessation Rate (quit rate)",
                    "description": "Self-reported 6-month point prevalence cessation (yes/no) will be assessed at 6-month follow-up.",
                    "timeFrame": "6-months post-randomization"
                },
                {
                    "measure": "Biochemical Carbon Monoxide (CO) Verification of Tobacco Cessation",
                    "description": "Biochemical carbon monoxide (CO) verification testing will also be done for a small subset of participants, where 0-6 parts per million (PPM) measures a non-smoker and 7+ PPM indicates a current smoker.",
                    "timeFrame": "6-months post-randomization"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Time to First Quit Attempt",
                    "description": "Self-reported date of first quit attempt since starting the study will be assessed through text message response from participant during the 6-month intervention period.",
                    "timeFrame": "From randomization to 6-months post-randomization"
                }
            ]
