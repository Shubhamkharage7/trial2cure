        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Incidence of AE/SAE/DLT [Safety and Tolerability]",
                    "description": "Incidence, nature, and severity of adverse events and serious adverse events (SAEs) throughout the study in relation to UCART22 and/or lymphodepletion",
                    "timeFrame": "24 Months"
                },
