        "descriptionModule": {
            "briefSummary": "Study Design:\n\nThis is an international, multicenter, observational, non-interventional, prospective, blinded, single-arm, two-period study, to collect patient utterances that will be retrospectively analyzed to determine the sensitivity and UPNR of the HearO system.",
            "detailedDescription": "Two periods:\n\nRun-In period will be a period in which patients will submit daily recordings, baseline creation\n\nCore period will be a period in which patients will be followed up and will continually submit daily recordings for up-to 24 months per patient or until End-of-Study (EOS), whichever comes first."
        },
        "conditionsModule": {
            "conditions": [
                "Heart Failure"
            ]
        },
        "designModule": {
            "studyType": "OBSERVATIONAL",
            "patientRegistry": false,
            "designInfo": {
                "observationalModel": "CASE_ONLY",
                "timePerspective": "PROSPECTIVE"
            },
            "enrollmentInfo": {
                "count": 500,
                "type": "ESTIMATED"
            }
        },
        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Hearo App",
                    "description": "No intervention - only data collection"
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Heart Failure Events (HFEs)",
                    "description": "Until the end of the study, each participant will be assigned to one of the following event outcomes:\n\n1. Having at least one protocol-defined Heart Failure Event (HFE) in the CORE period\n2. Having no Heart Failure Event (HFE) during the entire CORE period",
                    "timeFrame": "EOS is defined as when a total of at least 78 first usable and CEC-adjudicated HFEs have occurred or up-to 24 months per patient"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Major Inclusion Criteria:\n\n1. Age 22 or greater\n2. Diagnosed with Symptomatic Chronic Heart Failure \\[NYHA II-IVa (ambulatory)\\]\n3. At least one of the following:\n\n   1. One ADHF hospitalization in the last 12 months\n   2. One unplanned IV/SC diuretic administration in the last 6 months\n   3. Two unplanned IV/SC diuretic administrations in the last 12 months\n   4. NTProBNP \\>500 pg/ml\n4. Clinically stable HF according to investigator discretion\n5. Willing to participate as evidenced by signing the written informed consent.\n\nMajor Exclusion Criteria:\n\n1. Unable to comply with daily use of the App,\n2. Has had a major cardiovascular event within 3 months prior to enrolment.\n3. Had a Cardiac Resynchronization Therapy Device (CRT) implanted or upgrading \u2264 1 month prior to screening visit.\n4. Has estimated Glomerular Filtration Rate (eGFR) \\< 30 ml/min/1.73.\n5. Is likely to undergo heart transplantation/ LVAD within 6 months of Screening Visit.\n6. Was treated for a significant COPD",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "22 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Outpatient clinics",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
