        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "VR-JIT",
                    "type": "EXPERIMENTAL",
                    "description": "In this arm, participants would participate in simulated interviews utilizing a software program and virtual interviewer, once a week for 90 minutes.",
                    "interventionNames": [
                        "Other: Virtual Reality Job Interview Training"
                    ]
                },
                {
                    "label": "Wonderworks",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "In this arm, participants would participate in a similar intervention, also once a week for 90 minutes. However, in this arm, the intervention would be in a virtual office environment completing office tasks.",
                    "interventionNames": [
                        "Other: Wonderworks"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "OTHER",
                    "name": "Virtual Reality Job Interview Training",
                    "description": "The treatment group (TX) will complete 7 sessions of VR-JIT (1 sessions per week for 7 weeks), designed to teach the skills \\& concepts of job interviews. Sessions are approximately 90 minutes long.",
                    "armGroupLabels": [
                        "VR-JIT"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Wonderworks",
                    "description": "The control group (CT) will complete 7 sessions of Wonderworks (1 session per week for 7 weeks) using skills in an office environment.",
                    "armGroupLabels": [
                        "Wonderworks"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in Mock Job Interview Score at 1 month",
                    "description": "Participants will complete a mock job interview at follow-up which will be compared to their initial interview and rated by blinded assessors on their job interview performance.",
                    "timeFrame": "1 month"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* I am between the ages of 18 and 65.\n* I have a diagnosis with moderate to severe TBI.\n* I am at least one year post injury.\n* I am able to speak and read English fluently.\n\nExclusion Criteria:\n\n* I have a history of multiple sclerosis, stroke, seizures or another neurological injury or disease in the past (like brain tumor or epilepsy).\n* I have a history of significant psychiatric illness (like bipolar disorder, schizophrenia or psychosis).\n* I am currently taking steroids and/or benzodiazepines as determined by study staff review of my medications.",
