        "descriptionModule": {
            "briefSummary": "This study will compare eating behaviors and measures of hunger and fullness between day and night workers in order to better understand why night workers are at increased risk for obesity and related diseases."
        },
        "conditionsModule": {
            "conditions": [
                "Shift Work Type Circadian Rhythm Sleep Disorder",
                "Eating Behavior"
            ]
        },
