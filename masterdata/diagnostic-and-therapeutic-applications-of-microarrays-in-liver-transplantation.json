        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Assign molecular scores (probability) of T cell mediated rejection, antibody mediated rejection in liver transplant biopsies, in a reference set of 100 biopsies",
                    "description": "Based on the reference set, create molecular classifier that predicts antibody mediated and T cell mediated rejection for next 200 biopsies",
                    "timeFrame": "two years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Assign in real time (two working days upon biopsy receipt) molecular scores (probability) of T cell mediated rejection and antibody mediated rejection.",
                    "description": "The molecular phenotype of a newly acquired sample predicts the histologic and clinical features of this sample when compared to the reference set.",
                    "timeFrame": "1 year"
                }
            ]
        },
