        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in visual ability",
                    "description": "Change in visual ability as measured by the Ultra Low Vision Visual Function Questionnaire. The scale measures both visual ability (Person Measure) and difficulty (Item Measure) in people with ultra low vision. 150 items on a likert scale ranging from 1 impossible, to 4 not difficult. Using a Rasch-Andrich rating model, Person Measures range from -6 to +2.8 logits, Item Measures range from -3.3 to +3.0 logits. Higher scores indicating better outcome.",
                    "timeFrame": "Baseline, 1 month, 3 months, 6 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in hand-eye coordination",
                    "description": "Change in hand eye coordination as assessed by a leap motion (Ultra Leap) sensor taped to a Virtual Reality headset to track hand eye coordination.",
                    "timeFrame": "Baseline, 1 month, 3 months, 6 months"
                },
                {
                    "measure": "Change in hearing ability",
                    "description": "Change in hearing ability as assessed by iHEARtest device (earphones, computer application). The participant presses the space bar on their computer keyboard every time they hear a sound. The iHEARtest provides a Hearing Number\u2122 in each ear on a scale from 1=profound impairment, to 5= no impairment.",
                    "timeFrame": "Baseline, 1 month, 3 months, 6 months"
                }
            ]
        },
