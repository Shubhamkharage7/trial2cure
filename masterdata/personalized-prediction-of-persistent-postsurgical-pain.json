        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Persistent Postsurgical Pain (PPSP)",
                    "description": "Clinically meaningful PPSP will be defined as pain in the surgical area present 3 months after surgery which fulfils one of the following criteria: 1) pain intensity on rest or movement \u22653 on 0-10 numerical rating scale (NRS), or 2) pain interference with any of the seven Brief Pain Inventory items (general activity, mood, walking ability, normal work \\[including housework\\], relations with other people, sleep, and enjoyment of life) \u22653 on 0-10 scale.",
                    "timeFrame": "up to 7 months following consent"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Adults age 18-75 who are candidates for major surgery (expected surgery duration \\>1 hour, and expected overnight admission to the hospital)\n* Access to a smartphone\n\nExclusion Criteria:\n\n* Participants who do not speak English",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "maximumAge": "75 Years",
            "stdAges": [
