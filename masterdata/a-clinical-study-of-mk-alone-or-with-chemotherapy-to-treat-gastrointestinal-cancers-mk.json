        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Sacituzumab tirumotecan + Chemotherapy",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will receive sacituzumab tirumotecan in one of two dose levels and chemotherapy by intravenous (IV) infusion, every 2 weeks. Participants will continue to receive the treatment until the cancer gets worse or they don't tolerate treatment.",
                    "interventionNames": [
                        "Biological: Sacituzumab tirumotecan",
                        "Drug: Fluorouracil (5-FU)",
                        "Drug: Leucovorin (LV) or levoleucovorin"
                    ]
                },
                {
                    "label": "Sacituzumab tirumotecan",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will receive sacituzumab tirumotecan every 2 weeks via IV infusion. Participants will continue to receive the treatment until the cancer gets worse or they don't tolerate the treatment.",
                    "interventionNames": [
                        "Biological: Sacituzumab tirumotecan"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BIOLOGICAL",
                    "name": "Sacituzumab tirumotecan",
