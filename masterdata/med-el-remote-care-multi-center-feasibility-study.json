        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Remote Care Completion",
                    "description": "Number and proportion of fully completed Remote Care visits.",
                    "timeFrame": "Up to 6 months post-training"
                },
                {
                    "measure": "Functional hearing",
                    "description": "Number and proportion of hearing changes detected using Remote Care.",
                    "timeFrame": "Up to 6 months post-training"
                },
                {
                    "measure": "Telemetry",
                    "description": "Number and proportion of telemetry changes detected using Remote Care.",
                    "timeFrame": "Up to 6 months post-training"
                },
                {
                    "measure": "Audio processor equipment",
                    "description": "Number and proportion of equipment issues detected using Remote Care.",
                    "timeFrame": "Up to 6 months post-training"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Patient time savings",
                    "description": "Average patient-reported time estimates associated with attending an in-clinic appointment compared to Remote Care completion time.",
                    "timeFrame": "Up to 6 months post-training"
                },
                {
                    "measure": "Patient estimates costs",
                    "description": "Descriptive summaries of patient-estimated costs associated with attending an in-clinic appointment.",
                    "timeFrame": "Up to 6 months post-training"
                },
                {
                    "measure": "Clinician time savings",
                    "description": "Average clinician-reported time estimates associated with in-clinic appointments compared to Remote Care completion time.",
                    "timeFrame": "Up to 6 months post-training"
                },
                {
                    "measure": "Patient Feedback Survey",
                    "description": "Subject satisfaction measured using a custom questionnaire administered after each use of Remote Care. Each item is scored using a 5-point Likert scale with 1 always indicating a negative response and 5 a positive response. A total higher score indicates higher satisfaction.",
                    "timeFrame": "Up to 6 months post-training"
                },
                {
                    "measure": "Clinician Feedback Survey",
                    "description": "Clinician satisfaction measured using a custom questionnaire administered at mid-enrollment and at study completion. Each item is scored using a 5-point Likert scale with 1 always indicating a negative response and 5 a positive response. A total higher score indicates higher satisfaction.",
                    "timeFrame": "Mid-enrollment to enrollment completion"
                },
                {
                    "measure": "Remote hearing assessments",
                    "description": "Average remote hearing assessment results compared to standard aided test results obtained at baseline.",
                    "timeFrame": "Up to 6 months post-training"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Implanted with at least one MED-EL cochlear implant\n* \u2265 2 weeks since activation of the cochlear implant\n* Availability of existing aided Pediatric/AZ Bio (quiet and/or noise) and CNC word scores, or the ability to complete these at baseline\n* Utilizing a compatible audio processor in the SONNET or RONDO product lines\n* Ability to complete app-based hearing assessments\n* Commitment to comply with all study procedures for the duration of the study\n* Access to the internet via a smartphone that meets the following requirements:\n* Android or iOS operating systems\n* A smartphone supporting Bluetooth\u00ae 4.2 or higher\n* A minimum of 200 MB free storage space\n\nExclusion Criteria:\n\n* Inability or unwillingness to perform the requirements of the clinical investigation\n* Unrealistic expectations regarding the possible benefits, risks, and limitations of remote cochlear implant care\n* Inability of the subject or caregiver to demonstrate basic skills for operating a smartphone, computer, or app-based tasks after training by the investigator",
            "healthyVolunteers": true,
            "sex": "ALL",
            "stdAges": [
                "CHILD",
                "ADULT",
                "OLDER_ADULT"
            ]
        },
