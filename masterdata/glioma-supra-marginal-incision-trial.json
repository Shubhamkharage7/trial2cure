            "armGroups": [
                {
                    "label": "Supramarginal resection (intervention arm)",
                    "type": "EXPERIMENTAL",
                    "description": "Planned resection beyond the Gadolinium (GAD)-enhancing region extending to either at least 1 cm into non-enhancing tissue, or the nearest non-enhancing sulcal boundary/ventricle wall if these structures are closer than 1 cm.",
                    "interventionNames": [
                        "Procedure: Supramarginal resection"
                    ]
                },
                {
                    "label": "Conventional (i.e. GTR) resection",
                    "type": "OTHER",
                    "description": "Planned resection of \u226595% of the GAD-enhancing regions of tumor without expanding the resection beyond this margin.",
                    "interventionNames": [
                        "Procedure: Conventional (i.e. GTR) resection"
                    ]
                }
            ],
