        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Pre-operative anxiety level in each group, as measured with the Surgical Anxiety Questionnaire.",
                    "description": "Surgical Anxiety Questionnaire is a validated, 17 question survey with scores ranging from 0 (least anxious) to 68 (most anxious)",
                    "timeFrame": "Immediately Pre-op"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "To assess a difference in anxiety levels, based on Surgical Anxiety Questionnaire score, in patients currently taking medication for anxiety",
                    "description": "Surgical Anxiety Questionnaire is a validated, 17 question survey with scores ranging from 0 (least anxious) to 68 (most anxious)",
                    "timeFrame": "Immediately Pre-op"
                },
                {
                    "measure": "To assess a difference in anxiety levels, based on Surgical Anxiety Questionnaire score, in patients that did or did not watch the video at home",
                    "description": "Surgical Anxiety Questionnaire is a validated, 17 question survey with scores ranging from 0 (least anxious) to 68 (most anxious)",
                    "timeFrame": "Immediately Pre-op"
                }
            ]
