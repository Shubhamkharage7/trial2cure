                "type": "ESTIMATED"
            }
        },
        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Group 1: Standard Treatment",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Participants will receive a connection to the Tobacco Free Florida Quitline and a 12 week supply of nicotine replacement therapy (the patch and lozenges)",
                    "interventionNames": [
                        "Behavioral: Smoking Cessation treatment delivered by Tobacco Free Florida Quitline",
                        "Drug: Nicotine patch",
                        "Drug: Nicotine Lozenge"
                    ]
                },
                {
                    "label": "Group 2: Motivation and Problem Solving (MAPS) counseling",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will receive a 12 week supply of nicotine replacement therapy (the patch and lozenges) along with the MAPS intervention, which consists of 6 MAPS counseling calls over 12 months, and individually tailored SMS text content driven by monthly smartphone delivered check-ins for 24 months",
                    "interventionNames": [
                        "Drug: Nicotine patch",
                        "Drug: Nicotine Lozenge",
                        "Behavioral: Motivation and Problem Solving (MAPS) intervention"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Smoking Cessation treatment delivered by Tobacco Free Florida Quitline",
                    "description": "Phone counseling with the state tobacco quitline",
                    "armGroupLabels": [
                        "Group 1: Standard Treatment"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Nicotine patch",
                    "description": "Participants will be provided with a 12 week supply of nicotine patches",
                    "armGroupLabels": [
                        "Group 1: Standard Treatment",
                        "Group 2: Motivation and Problem Solving (MAPS) counseling"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Nicotine Lozenge",
                    "description": "Participants will be provided with a 12 week supply of nicotine lozenges",
                    "armGroupLabels": [
                        "Group 1: Standard Treatment",
                        "Group 2: Motivation and Problem Solving (MAPS) counseling"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Motivation and Problem Solving (MAPS) intervention",
                    "description": "MAPS is a phone counseling approach to facilitating smoking cessation among cervical cancer survivors, which uses a combined motivational interviewing and social cognitive theory based approach to smoking cessation. Participants will receive 6 MAPS based counseling calls over 12 months, along with personally-tailored SMS-delivered text based messages delivered to the participants phone over 2 years.",
                    "armGroupLabels": [
                        "Group 2: Motivation and Problem Solving (MAPS) counseling"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "MAPS vs Standard Treatment in facilitating long term smoking abstinence",
                    "description": "The primary outcome of this study is self-reported, 7 day point prevalence abstinence over time through the 24 month assessment where participants who report no smoking in the previous 7 days at follow-up assessment are considered abstinent.\n\nEfficacy of MAPS vs Standard Treatment will be measured by comparing the number of participants in each group who were able to quit smoking and maintain smoking abstinence for 2 years.",
                    "timeFrame": "at 24 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* At least 100 cigarettes or more smoked in participants lifetime\n* Speaks English\n* Currently smoke 1 cigarette or more in the past 30 days\n* History of cervical cancer or high grade cervical dysplasia\n* Has a working smartphone\n* Has a valid home address\n* Reside in Florida at the time of study enrollment\n\nExclusion Criteria:\n\n* Receiving behavioral or pharmacological tobacco treatment\n* Household member enrolled in this study",
            "healthyVolunteers": false,
