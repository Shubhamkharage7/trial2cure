        "descriptionModule": {
            "briefSummary": "The goal of this research study is to compare the efficacy of a treatment approach that comprises both Motivation And Problem-Solving (MAPS)-based telephone counseling and a personally-tailored SMS-delivered text-based approach to quitline-delivered smoking cessation treatment to help participants with a history of cervical cancer or high-grade cervical dysplasia quit smoking."
        },
        "conditionsModule": {
            "conditions": [
                "Smoking Cessation",
                "Cervical Cancer",
                "High Grade Cervical Dysplasia"
            ]
        },
