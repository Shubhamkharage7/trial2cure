        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Dexamethasone (Group 2)",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will undergo an FNP-59 scan on day 0 in the am. Participants will then take\n\n1 mg dexamethasone 2x a day for 3 days to suppress cortisol production. Participants will then have a second FNP-59 scan on day 4 in the am.",
                    "interventionNames": [
                        "Drug: Dexamethasone (Group 2)",
                        "Combination Product: PET/CT Scan with FNP-59"
                    ]
                },
                {
                    "label": "Cosyntropin (Group 3)",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will undergo an FNP-59 scan on day 0 in the am. On day 4 the participant will arrive for imaging. Cosyntropin, 250 micro-gm will be administered IV. Five minutes following administration FNP-59 will be given. Following uptake of FNP-59 imaging will occur.",
                    "interventionNames": [
                        "Drug: Cosyntropin (Group 3)",
                        "Combination Product: PET/CT Scan with FNP-59"
                    ]
                },
                {
                    "label": "Adrenal pathology (Group 4)",
                    "type": "EXPERIMENTAL",
                    "description": "Whole-body PET/CT scans will be done on 4 patients at 1 hr and the other 4 patients at 6 hours. All the patients will have a whole-body PET/CT scan at 3 hours.",
                    "interventionNames": [
                        "Combination Product: PET/CT Scan with FNP-59"
                    ]
                }
            ],
