        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Song of Kin",
                    "type": "EXPERIMENTAL",
                    "description": "Voice (with or without guitar)- lullaby/ Holding meter",
                    "interventionNames": [
                        "Behavioral: Song of Kin"
                    ]
                },
                {
                    "label": "Gato box",
                    "type": "EXPERIMENTAL",
                    "description": "Simple rhythms using Remo Gato box will be played by the MT-BC using a 3rd to comfort, engage or sedate",
                    "interventionNames": [
                        "Other: Gato box"
                    ]
                },
                {
                    "label": "Ocean disc",
                    "type": "EXPERIMENTAL",
                    "description": "Creating a consistent womb-like sound soundscape to comfort, engage or sedate",
                    "interventionNames": [
                        "Other: Ocean disc"
                    ]
                },
                {
                    "label": "Contingent singing",
                    "type": "EXPERIMENTAL",
                    "description": "Provision of communicative vocalization-\"parantese\" to engage with infants, prosodic responses to infant cues (eye contact, body position)",
                    "interventionNames": [
                        "Other: Contingent singing"
                    ]
                },
                {
                    "label": "Tonal Vocal holding",
                    "type": "EXPERIMENTAL",
                    "description": "Providing a 'blanket of tone' to comfort, engage or sedate",
                    "interventionNames": [
                        "Other: Tonal Vocal holding"
                    ]
                },
                {
                    "label": "Muted shaker",
                    "type": "EXPERIMENTAL",
                    "description": "If infant is awake, the muted shaker will be used to entrain to the infant's vital signs-to comfort, soothe or sedate",
                    "interventionNames": [
                        "Other: Muted shaker"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Song of Kin",
                    "description": "Music therapists will assess meaningful song/theme (of choice/culture) and provide musical elements in a way that nurtures parents. They will then develop a simplified part of the theme into a soothing lullaby for their infants to be utilized directly by the parent/s and/or the music therapist to comfort, engage or sedate.",
                    "armGroupLabels": [
                        "Song of Kin"
                    ],
                    "otherNames": [
                        "Music psychotherapy with parent/s"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Gato box",
                    "description": "Depending on the time of intervention and need of the infant the music therapist will provide rhythmic entrainment that follows the heart rate or suck patterns of the infant",
                    "armGroupLabels": [
                        "Gato box"
                    ],
                    "otherNames": [
                        "Rhythmic entrainment"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Ocean disc",
                    "description": "Depending on the time of intervention and needs of the infant the music therapist will provide a whoosh sound using the Remo Ocean disc. It will be entrained to the infant's respiratory rate.",
                    "armGroupLabels": [
                        "Ocean disc"
                    ],
                    "otherNames": [
                        "Timbre entrainment"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Contingent singing",
                    "description": "Depending on the state of the infant, the music therapist will provide short segments of conversant prosody, to enhance quiet-alert, or stimulate relaxation/sleep",
                    "armGroupLabels": [
                        "Contingent singing"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Tonal Vocal holding",
                    "description": "Music therapist will provide a vowel, long held tone, sung without vibrato",
                    "armGroupLabels": [
                        "Tonal Vocal holding"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Muted shaker",
                    "description": "If infant is awake, the muted shaker will be used to entrain to the infant's vital signs-to comfort, soothe or sedate",
                    "armGroupLabels": [
                        "Muted shaker"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Heart Rate",
                    "timeFrame": "up to 2 weeks"
                },
                {
                    "measure": "Respiratory Rate",
                    "timeFrame": "up to 2 weeks"
                },
                {
                    "measure": "O2 sats",
                    "description": "Oxygen saturation levels (O2 sats)",
                    "timeFrame": "up to 2 weeks"
                },
                {
                    "measure": "Activity level",
                    "description": "The neonatologist or nurse will assess and mark as active/alert, active to stimulation, flaccid, hypertonic, hypotonic, irritable, jittery, lethargic, paralyzed\\&medicated, quiet, sedated, or tremors",
                    "timeFrame": "up to 2 weeks"
                },
                {
                    "measure": "Sleep category",
                    "description": "The neonatologist or nurse will assess and mark as quiet sleep, active sleep, drowsy state, alert inactivity, fussing or crying.",
                    "timeFrame": "up to 2 weeks"
                },
                {
                    "measure": "Feeding (sucking and absorption) category",
                    "description": "The neonatologist or nurse will rate feeding (sucking/absorption) from 1 (active/perpetual) to 12 (none/gavage)",
                    "timeFrame": "up to 2 weeks"
                },
                {
                    "measure": "Neonatal Abstinence Scoring (NAS)",
                    "description": "The NAS score lists 21 symptoms that are most frequently observed in opiate-exposed infants. Each symptom and its associated degree of severity are assigned a score and the total abstinence score is determined by totalling the score assigned to each symptom over the scoring period. Please provide the full scale info (example 0 (no symptoms) to 10 (severe symptoms)",
                    "timeFrame": "up to 2 weeks"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Postpartum Bonding Questionnaire (PBQ )",
                    "description": "Questionnaire to assess capacity for attachment in both mothers and fathers. 25 item scale with each item rated from always to never. Scoring is based on four categories: impaired bonding scores 0-60, rejection and pathological anger scores 0-35, infant-focused anxiety scores 0-20, and incipient abuse scores 0-10. Total range from 0 (good bonding) to 125 (poor bonding).",
                    "timeFrame": "Baseline"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\nInfants who meet the following inclusion criteria will be eligible to participate in the study irrespective of race, religion, ethnicity, or gender:\n\n* Admitted to the NICU immediately postpartum\n* Gestational age 28 weeks or older\n* No identified hearing disorder\n* Do not have a diagnosed developmental disability (i.e. Down Syndrome)\n* Do not have Fetal Alcohol Syndrome\n* Medically cleared to participate in the study by nurse or neonatologist\n* Parent or legal guardian able/willing to give consent \\& complete the PBQ (Postpartum Bonding Questionnaire)\n\nExclusion Criteria:\n\n* Admitted to the NICU at any time other than immediately postpartum\n* Gestational age \\<28 weeks old\n* Has an identified hearing disorder\n* Has diagnosed developmental disability (i.e. Down syndrome)\n* Has Fetal Alcohol Syndrome\n* Is not medically cleared to participate in the study by the nurse or neonatologist\n* Parent or legal guardian unable/unwilling to give consent",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "28 Weeks",
            "stdAges": [
                "CHILD",
                "ADULT",
                "OLDER_ADULT"
            ]
        },
