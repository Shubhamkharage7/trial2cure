        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Diplopia",
                    "description": "The degree of diplopia is quantified by measuring the field of single binocular vision, ranging from 0 to 3.\n\n0: no diplopia\n\n1. mild (diplopia appears more than 30 degrees from the primary position)\n2. moderate (diplopia appears between 10 and 30 degrees from the primary position)\n3. severe (diplopia appears within 10 degrees from the primary position)",
                    "timeFrame": "24 weeks post-treatment"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Globe position",
                    "description": "Exo/enophthalmos (in mm) using Naugle or Hertel exophthalmometer Hyper/hypophthalmos: vertical globe displacement is measured as the pupilar height difference (in mm) from the healthy side to the injured side",
                    "timeFrame": "6, 12, 24 weeks post-treatment"
                },
                {
                    "measure": "Visual acuity",
                    "description": "Measured using a visual test chart in healthy and injured sides with and without vision aids.",
                    "timeFrame": "6, 12, 24 weeks post-treatment"
                },
                {
                    "measure": "Extra-ocular mobility",
                    "description": "Follow-my finger test",
                    "timeFrame": "6, 12, 24 weeks post-treatment"
                },
                {
                    "measure": "Patient-reported outcomes",
                    "description": "AOCMF Injury symptom battery Self-reported diplopia Self-perception of visual acuity, strabismus and symmetry",
                    "timeFrame": "pre-treatment, 6, 12, 24 weeks post-treatment"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Age 18 years or older at the time of the injury\n* Patients with a dislocated fracture of the inferior and/or medial orbital wall, either\n\n  * Diagnosed at the study site via CT or CBCT within 2 weeks of the date of the injury, OR\n  * Who will undergo secondary reconstruction\n\nExclusion Criteria:\n\n* Bilateral orbital fracture\n* Concomitant displaced fracture(s) of the orbital roof or any other area of the orbit\n* Concomitant ruptured globe\n* Displaced fracture of the malar bone\n* Displaced midface fracture\n* Displaced nasoorbitoethmoidal (NOE) fracture or complex zygoma fractures\n* Presence at the time of enrollment of any disease with influence on eye motility (eg, strabismus, myasthenia gravis, Graves' ophthalmopathy)\n* Previous radiotherapy in the orbital region\n* Participation in any other medical device or medicinal product study within the previous month(s) that could influence the results of the present study",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Patients older than 18 years old with displaced fracture of the inferior and/or medial orbital wall (blow-out fractures) that require treatment (either surgically or non-surgically)",
            "samplingMethod": "PROBABILITY_SAMPLE"
        },
