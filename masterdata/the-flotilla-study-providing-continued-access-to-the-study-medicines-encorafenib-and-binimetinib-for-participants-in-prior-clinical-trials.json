            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Binimetinib only treatment",
                    "description": "Binimetinib will be administered at the same dose level (15 mg to 45 mg) as that received in the Parent Studies C4211001 and C4211003 with water irrespective of food, continuously, starting on Day 1.",
                    "armGroupLabels": [
                        "Binimetinib only treatment"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Encorafenib only Treatment",
                    "description": "Encorafenib will be self-administered at the same dose level as that received in the C4221010 Parent Study.",
                    "armGroupLabels": [
                        "Encorafenib only Treatment"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Encorafenib & Binimetinib Treatment",
                    "description": "Encorafenib will be administered on a 300 mg QD schedule and binimetinib will be administered on a 45 mg BID schedule, both orally as a flat fixed dose. Cetuximab will be administered at either 400 mg/m2 or 500 mg/m2 IV every 2 weeks on Days 1 and 15 of every cycle.",
                    "armGroupLabels": [
                        "Encorafenib & Binimetinib Treatment"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Treatment of Encorafenib & Binimetinib & Ribociclib",
                    "description": "Encorafenib capsule and ribociclib capsules or tablets will be co-administered orally QD with binimetinib tablets BID. The study drugs will be administered as a flat-fixed dose, and not by body weight or body surface area.",
                    "armGroupLabels": [
                        "Treatment of Encorafenib & Binimetinib & Ribociclib"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Treatment of Encorafenib & Binimetinib & Cetuximab",
                    "description": "Encorafenib will be administered on a 300 mg QD schedule and binimetinib will be administered on a 45 mg BID schedule, both orally as a flat fixed dose. Cetuximab will be administered at either 400 mg/m2 or 500 mg/m2 IV every 2 weeks on Days 1 and 15 of every cycle.",
                    "armGroupLabels": [
                        "Treatment of Encorafenib & Binimetinib & Cetuximab"
                    ]
                }
            ]
