            "designInfo": {
                "allocation": "RANDOMIZED",
                "interventionModel": "PARALLEL",
                "interventionModelDescription": "Randomized controlled trial. Group 1 - acupuncture Group 2 - no acupuncture 70 patients per group",
                "primaryPurpose": "TREATMENT",
                "maskingInfo": {
                    "masking": "DOUBLE",
                    "maskingDescription": "The patient will be under sedation during procedure, so will not be aware of whether or not they received acupuncture treatment.\n\nAnesthesia team in the operating room will be aware of treatment. PACU team and outcomes assessor will not be aware of group assignment",
                    "whoMasked": [
                        "PARTICIPANT",
                        "OUTCOMES_ASSESSOR"
                    ]
                }
            },
