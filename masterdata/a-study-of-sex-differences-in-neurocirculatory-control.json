        "descriptionModule": {
            "briefSummary": "The purpose of this study is to investigate potential sex differences in neurocirculatory control of blood pressure in patients with untreated obstructive sleep apnea (OSA)."
        },
        "conditionsModule": {
            "conditions": [
                "Obstructive Sleep Apnea"
            ]
        },
        "designModule": {
            "studyType": "OBSERVATIONAL",
            "patientRegistry": false,
            "designInfo": {
                "observationalModel": "COHORT",
                "timePerspective": "CROSS_SECTIONAL"
            },
            "enrollmentInfo": {
                "count": 60,
                "type": "ESTIMATED"
            }
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Blood pressure",
                    "description": "Measured in millimeters of mercury (mmHg).",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Arterial stiffness",
                    "description": "Measured by applanation tonometry reported as aortic pulse wave velocity (m/sec)",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Endothelial function",
                    "description": "Ultrasound assessment of flow-mediated vasodilation of the brachial artery. Brachial artery diameter and blood velocity will be measured after deflation to measure endothelial function.",
                    "timeFrame": "Baseline"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Sympathetic nerve burst frequency",
                    "description": "Measured by Microneurography reported as number of nerve bursts per minute (burst/min)",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Sympathetic nerve burst incidence",
                    "description": "Measured by Microneurography reported as number of nerve burst per 100 heart beats (burst/100 heart beats)",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Sympathetic nerve burst amplitude (AU)",
                    "description": "Measured by Microneurography reported in arbitrary units (AU)",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Total muscle sympathetic nerve activity (MSNA)",
                    "description": "Measured by Microneurography reported burst arbitrary units per minute (AU/min)",
                    "timeFrame": "Baseline"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Cerebral autoregulation",
                    "description": "Measured by transcranial doppler ultrasound (cm/sec).",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Sympathetic action potentials per burst",
                    "description": "Measured by Microneurography reported as action potentials per burst (reflecting firing rate).",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Sympathetic action potential clusters per burst",
                    "description": "Measured by Microneurography reported as clusters per burst (reflecting recruitment of latent subpopulations).",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Sympathetic action potential conduction latency of each cluster",
                    "description": "Measured by Microneurography reported as conduction latency of each cluster (reflecting synaptic delays and verifying larger amplitude clusters as new populations).",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Immune function",
                    "description": "Immune function will be measured as percent of parent cell population.",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Urinary Extracellular Vesicle Presence and Content",
                    "description": "Number of extracellular vesicles present in a 24 hour urine collection and the contents being carried by those vesicles.",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Plasma Extracellular Vesicle Presence and Content",
                    "description": "Number of extracellular vesicles present in blood plasma sample and the contents being carried by those vesicles.",
                    "timeFrame": "Baseline"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* 20-70 years of age\n\nExclusion Criteria:\n\n* Coronary artery disease\n* Heart failure\n* Pregnancy\n* COPD\n* Diabetes\n* CKD\n* Sleep disorders other than OSA\n* Shift workers\n* Individuals who typically go to sleep after midnight\n* Individuals who traveled across \u22652 time zones within one week of study visits\n* BMI \u226540.0kg/m2\n* Use of nicotine-containing products within the two years preceding study visits\n* Use of allopurinol, proton pump inhibitors, or other medications/supplements which interfere with outcome measures",
            "sex": "ALL",
            "minimumAge": "20 Years",
            "maximumAge": "70 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Subjects with or without untreated obstructive sleep apnea will be recruited from Mayo Clinic in Rochester, MN.",
