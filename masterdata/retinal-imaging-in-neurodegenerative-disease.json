        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in ganglion cell-inner plexiform layer (GCIPL) thickness",
                    "description": "Ganglion cell inner plexiform layer thickness as measured on optical coherence tomography scan of macula",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in retinal nerve fiber layer (RNFL) thickness",
                    "description": "Retinal nerve fiber layer thickness as measured on optical coherence tomography scan of macula",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in central subfield thickness (CST)",
                    "description": "Central subfield thickness as measured on optical coherence tomography scan of macula",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in choroidal vascularity index (CVI)",
                    "description": "Choroidal vascularity index as measured using the COIN software in 1500 um area centered on the fovea",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in foveal avascular zone (FAZ) area",
                    "description": "Foveal avascular zone area as measured in the superficial capillary plexus on 3mm optical coherence tomography angiography scan of the macula",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in average perfusion density (PD)",
                    "description": "Average perfusion density as measured in the ETDRS 3mm and 6mm circle and rings on optical coherence tomography angiography scan of the macula",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in average vessel density (VD)",
                    "description": "Average vessel density as measured in the ETDRS 3mm and 6mm circle and rings on optical coherence tomography angiography scan of the macula",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in average capillary perfusion density (CPD)",
                    "description": "Capillary perfusion density as measured on peripapillary 4.5mm optical coherence tomography angiography scan",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in average capillary flux index (CFI)",
                    "description": "Capillary flux index as measured on peripapillary 4.5mm optical coherence tomography angiography scan",
                    "timeFrame": "Baseline, 1 year"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in retinal vessel tortuosity",
                    "description": "Retinal vessel tortuosity measured on ultra-widefield scanning laser ophthalmoscopy image using VAMPIRE software",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in retinal vessel width gradient",
                    "description": "Retinal vessel width gradient measured on ultra-widefield scanning laser ophthalmoscopy image using VAMPIRE software",
                    "timeFrame": "Baseline, 1 year"
                },
                {
                    "measure": "Change in retinal vessel fractal dimension",
                    "description": "Retinal vessel fractal dimension measured on ultra-widefield scanning laser ophthalmoscopy image using VAMPIRE software",
                    "timeFrame": "Baseline, 1 year"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Adults with neurodegenerative disease ((MCI, PD, AD, FTD, DLB, ALS, MS, HD, TBI, concussion, PTSD and other neurodegenerations as well as Down Syndrome)\n* Adults without neurodegenerative disease\n\nExclusion Criteria:\n\n* Inability to cooperate with or complete testing or other neurologic or age- related ocular conditions that would impact image acquisition.\n* Eyes that have had intraocular surgery, other than cataract surgery.\n\nIf two eyes satisfy the inclusion criteria, both eyes will be included in the study. If one eye satisfies the inclusion criteria, the eye that qualifies will be included in the study.",
