        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Tailored Screen to Save (S2S) content",
                    "type": "EXPERIMENTAL",
                    "description": "Content to be delivered using a combination of short video and text-based information optimized for delivery via Quick Response (QR) codes for smartphones.",
                    "interventionNames": [
                        "Behavioral: mHealth Application"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "mHealth Application",
                    "description": "Self-directed application (app) that is downloaded to the participant's personal device. The app contains five informational videos, that provide an orientation to the content on the app, and discuss what colorectal cancer is, and the type of CRC screening tests that are available as well as a video with a personal history. There is a a National Cancer Institute (NCI) risk calculation tool, and a family history infographic. Once the videos have been viewed participants have the option to indicate if they wish to receive a call from the healthcare clinic to discuss scheduling an appointment for screening.",
                    "armGroupLabels": [
                        "Tailored Screen to Save (S2S) content"
                    ]
                }
            ]
        },
