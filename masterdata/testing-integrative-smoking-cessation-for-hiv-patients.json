        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "MT plus NRT Group",
                    "type": "EXPERIMENTAL",
                    "description": "Participants who receive the Mindfulness Training (MT) intervention for 4 weeks in addition to 6 weeks of Nicotine Replacement Therapy (NRT).These participants are responders (have quit smoking at the 1 month follow up) or non-responders (those who have not quit smoking at the 1 month follow up) and randomized to receive no additional intervention.",
                    "interventionNames": [
                        "Drug: Nicotine Replacement Therapy",
                        "Behavioral: Mindfulness Training Smoking Cessation Intervention"
                    ]
                },
                {
                    "label": "CM plus NRT Group",
                    "type": "EXPERIMENTAL",
                    "description": "Participants who receive the Contingency Management (CM) intervention for 4 weeks in addition to 6 weeks of NRT. These participants are responders (have quit smoking at the 1 month follow up) or non-responders (those who have not quit smoking at the 1 month follow up) and randomized to receive no additional intervention.",
                    "interventionNames": [
                        "Drug: Nicotine Replacement Therapy",
                        "Behavioral: Contingency Management Smoking Cessation Intervention"
                    ]
                },
                {
                    "label": "MT plus NRT with additional CM Group",
                    "type": "EXPERIMENTAL",
                    "description": "Participants who received the MT intervention for 4 weeks with 6 weeks of NRT and are non-responders (those who have not quit smoking at the 1 month follow up) and randomized to receive an additional CM intervention for another 4 weeks.",
                    "interventionNames": [
                        "Drug: Nicotine Replacement Therapy",
                        "Behavioral: Mindfulness Training Smoking Cessation Intervention",
                        "Behavioral: Contingency Management Smoking Cessation Intervention"
                    ]
                },
                {
                    "label": "CM plus NRT with additional MT Group",
                    "type": "EXPERIMENTAL",
                    "description": "Participants who received the CM intervention for 4 weeks with 6 weeks of NRT and are non-responders (those who have not quit smoking at the 1 month follow up) and randomized to receive an additional MT intervention for another 4 weeks.",
                    "interventionNames": [
                        "Drug: Nicotine Replacement Therapy",
                        "Behavioral: Mindfulness Training Smoking Cessation Intervention",
                        "Behavioral: Contingency Management Smoking Cessation Intervention"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Nicotine Replacement Therapy",
                    "description": "6 weeks of Nicotine replacement patches",
                    "armGroupLabels": [
                        "CM plus NRT Group",
                        "CM plus NRT with additional MT Group",
                        "MT plus NRT Group",
                        "MT plus NRT with additional CM Group"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Mindfulness Training Smoking Cessation Intervention",
                    "description": "Mindfulness Training Smoking Cessation Intervention administered online via Zoom. Each session lasts 60-90 minutes, twice weekly for 4 weeks for a total of 8 sessions.",
                    "armGroupLabels": [
                        "CM plus NRT with additional MT Group",
                        "MT plus NRT Group",
                        "MT plus NRT with additional CM Group"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Contingency Management Smoking Cessation Intervention",
                    "description": "Contingency Management Smoking Cessation Intervention includes one orientation session lasting about 60-90 minutes administered via Zoom. Participants will then be required to contact the research associate about their quitting progress three times a week for 4 weeks also by zoom.",
                    "armGroupLabels": [
                        "CM plus NRT Group",
                        "CM plus NRT with additional MT Group",
                        "MT plus NRT with additional CM Group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Number of participants reporting 7-day point-prevalence abstinence",
                    "description": "Defined as self-report of not smoking in the past 7-days, not even a puff and confirmed by expired carbon monoxide (CO) level of \\< 6 ppm.",
                    "timeFrame": "Up to 22 weeks"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Number of enrolled participants",
                    "description": "Study feasibility will be reported as the number of participants that were screened and enrolled per month",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Average enrollment time",
                    "description": "Study feasibility will be reported as the average time to enroll participants to form classes",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Retention rate",
                    "description": "Treatment specific retention rates will be reported as the number of participants completing all study procedures dived by the total of participants enrolled.",
                    "timeFrame": "Up to 22 weeks"
                },
                {
                    "measure": "Number of completed study measures",
                    "description": "Defined as number of in-person session attended, and number of homework assignments completed.",
                    "timeFrame": "Up to 22 weeks"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Diagnosed with HIV (based on self-report).\n* Be 18 years and older\n* Have smoked \u2265 5 cigarettes/day in the past year\n* Be interested in making a quit attempt in the next 30 days\n* Own a smartphone (apple/android), and plan to keep it active for the next 6 months\n* Able to consent\n* Have no plans to move in the next 6 months\n* Are not pregnant or planning to be pregnant in the following 6 months\n* Have any condition that, in the opinion of the investigator, would compromise the well-being of the patient or the study or prevent the patient from meeting or performing study requirements\n\nExclusion Criteria:\n\n* Have contraindication to NRT (past month myocardial infarction, history of serious arrhythmias/or unstable angina pectoris, dermatological disorder)\n* Are currently being treated for a psychiatric condition.\n* Are currently being treated for smoking cessation, alcoholism, or illicit drug use\n* Are adults unable to consent\n* Are individuals who are not yet adults\n* Are pregnant women\n* Are prisoners",
