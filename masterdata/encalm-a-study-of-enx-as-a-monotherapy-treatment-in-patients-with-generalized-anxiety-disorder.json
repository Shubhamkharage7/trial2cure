        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "ENX-102",
                    "type": "EXPERIMENTAL",
                    "description": "Four weeks of 2 mg of ENX-102 in capsule form followed by 3 weeks of tapered dose plus 2 weeks of placebo in capsule form (before and/or after the ENX-102 treatment period), taken orally once daily in the evening for a 9-week total treatment period.",
                    "interventionNames": [
                        "Drug: ENX-102"
                    ]
                },
                {
                    "label": "Placebo",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "Nine weeks of placebo in capsule form taken orally once daily in the evening for a 9-week total treatment period.",
                    "interventionNames": [
                        "Drug: Placebo"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "ENX-102",
