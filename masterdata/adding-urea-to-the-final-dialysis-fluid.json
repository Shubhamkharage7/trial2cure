            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Urea in the dialysate",
                    "description": "Adding urea to the dialysis fluid. Ure-Na 15 grams would be used. It would be added to the acid component of the dialysis fluid. The amount added would depend on the serum BUN concentration and is determined by a simple calculation. It would be available in powder form. Urea would be added just to the first 1-3 dialysis treatments as needed.",
                    "armGroupLabels": [
                        "Urea dialysate"
                    ],
                    "otherNames": [
                        "Ure-Na"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Disequilibrium",
                    "description": "Dialysis disequilibrium syndrome (DDS) refers to an array of neurological manifestations that are seen during or following dialysis. The symptoms can range from headache, nausea, blurred vision, restlessness and confusion to coma and seizures in rare cases. The physician will assess DDS.",
                    "timeFrame": "within 24 hours after starting dialysis"
                },
                {
                    "measure": "Serum potassium concentration",
                    "description": "Improvement in serum potassium concentration in mEq/L would be measured and documented with the study",
                    "timeFrame": "Potassium levels every 6 hours for 24 hours after end of dialysis"
                },
                {
                    "measure": "Serum CO2 concentration",
                    "description": "Improvement in metabolic acidosis would be monitored by checking serum CO2 concentration in mEq/L",
                    "timeFrame": "Serum CO2 levels every 6 hours for 24 hours after end of dialysis"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Serum BUN concentration",
                    "description": "The trend in serum BUN concentration in mg/dL would be followed",
                    "timeFrame": "Serum BUN concentration twice a day for 3 days"
                }
            ]
        },
