        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Single Distal Femur Implant Group",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Subjects will receive one of two types of implants for the distal femur fracture. The single implant will be either a plate and screws or a rod. The type of single implant used will be determined by the surgeon based on the characteristics of the fracture.",
                    "interventionNames": [
                        "Procedure: Single Distal Femur Implant"
                    ]
                },
                {
                    "label": "Dual Distal Femur Implant Group",
                    "type": "EXPERIMENTAL",
                    "description": "Subjects will receive two implants for fixation of the distal femur fracture. The dual implant will be either 2 plates with screws or a plate with screws and a rod. The type of dual implant used will be determined by the surgeon based on the characteristics of the fracture.",
                    "interventionNames": [
                        "Procedure: Dual Distal Femur Implant"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "PROCEDURE",
                    "name": "Single Distal Femur Implant",
                    "description": "Single implant constructs will be either a retrograde intramedullary nail with interlocking screws or a single plate and screw construct.",
                    "armGroupLabels": [
                        "Single Distal Femur Implant Group"
                    ]
                },
                {
                    "type": "PROCEDURE",
                    "name": "Dual Distal Femur Implant",
                    "description": "Dual implant constructs will either be an intramedullary nail with an additional plate and screw construct or dual (two plates in any orientation) plate and screw construct.",
                    "armGroupLabels": [
                        "Dual Distal Femur Implant Group"
                    ]
                }
            ]
        },
