        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Lung Transplant Recipients",
                    "type": "EXPERIMENTAL",
                    "description": "Lung transplant recipients with pre-transplant serological immunity to CMV. CMV-TCIP will be measured every 3 months post-transplant with antiviral prophylaxis discontinued when threshold is exceeded.",
                    "interventionNames": [
                        "Device: Cytomegalovirus T Cell Immunity Panel (CMV-TCIP)",
                        "Diagnostic Test: Donor-Derived Cell-Free DNA (dd-cfDNA) Assay",
                        "Drug: Valganciclovir"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Cytomegalovirus T Cell Immunity Panel (CMV-TCIP)",
                    "description": "Whole blood assay that evaluates T cell (CD4 and CD8) responses to cytomegalovirus (CMV) antigens via intracellular cytokine staining (ICS).",
                    "armGroupLabels": [
                        "Lung Transplant Recipients"
                    ],
                    "otherNames": [
                        "Viracor inSIGHT"
                    ]
                },
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Donor-Derived Cell-Free DNA (dd-cfDNA) Assay",
                    "description": "Determines the percentage of circulating cell-free DNA (cfDNA) in transplant recipients derived from donor grafts.",
                    "armGroupLabels": [
                        "Lung Transplant Recipients"
                    ],
                    "otherNames": [
                        "Viracor TRAC"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Valganciclovir",
                    "description": "Delivered as standard of care for post-lung transplant immunosuppression and infection prophylaxis. Started on the first day post-transplant at a standard dose of 900 mg daily and continued for 12 months. Dose adjustments may be necessary based on renal function and/or adverse effects (e.g. neutropenia). Dose adjustments are standard based on package insert recommendations.",
                    "armGroupLabels": [
                        "Lung Transplant Recipients"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "CMV Viremia-Free Survival",
                    "description": "Defined as the the time from lung transplant until the first detection of CMV DNA in patients' blood.",
                    "timeFrame": "Up to Month 12 Post-Transplant"
                }
            ],
