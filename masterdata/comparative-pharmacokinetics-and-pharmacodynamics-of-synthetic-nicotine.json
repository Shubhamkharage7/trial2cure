        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Mean Nicotine Exposure Over Time",
                    "description": "Plasma nicotine area under the concentrated time curve (AUC) (ng/ml\\*h)",
                    "timeFrame": "Up to 7 hours"
                }
