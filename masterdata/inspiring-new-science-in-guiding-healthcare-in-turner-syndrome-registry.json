            "conditions": [
                "Turner Syndrome"
            ]
        },
        "designModule": {
            "studyType": "OBSERVATIONAL",
            "patientRegistry": true,
            "targetDuration": "20 Years",
            "designInfo": {
                "observationalModel": "OTHER",
                "timePerspective": "PROSPECTIVE"
            },
            "bioSpec": {
                "retention": "SAMPLES_WITH_DNA",
                "description": "If resources are available and optional consent is provided, a 5-10mL blood sample will be obtained in 1 gold top and 1 purple top (EDTA) tube and processed for storage of plasma, serum, and buffy coat. These specimens will be stored in a biobank with a barcode to link the sample with the patient for future studies involving the assessment of hormones, proteins, metabolites, DNA, RNA, and other studies. Phlebotomy will be combined with clinical phlebotomy whenever possible."
            },
            "enrollmentInfo": {
                "count": 5000,
                "type": "ESTIMATED"
            }
        },
