        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Rate of Surgical Success",
                    "description": "Rate of surgical success defined as reduction to \\< 15 cc total volume of the supratentorial intracerebral hemorrhage on immediate Post-op CT Scan",
                    "timeFrame": "During surgical procedure to evacuate supratentorial intracerebral hemorrhage"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Subject Age is \\> 18\n* Subject with a Head CT that demonstrates an acute, spontaneous, primary, supratentorial ICH of volume \\> 20 mL, assessed via standard of care techniques\n* Subject Surgery can be initiated within 24 hours of the last known well time or, in patients with wake-up onset, within 24 hours of the time the patient awoke with symptoms.\n* Subject has a NIHSS score \\> 5\n* Subject has a baseline Modified Rankin Scale (mRS) Score \u2264 2\n* Subject with a CT Angiography demonstrating no vascular malformation\n\nExclusion Criteria:\n\n* Subject has an underlying vascular lesion defined as causative source of ICH\n* Subject has a profound neurological deficit defined as fixed/dilated pupils or bilateral extensor motor posturing\n* Subject has an Infratentorial or brainstem ICH\n* Subject has a known life expectancy \\< 6 months\n* Subject has an uncorrectable coagulopathy\n* Subject has a mechanical heart valve\n* Subject is pregnant\n* Subject participates in another concurrent interventional clinical trial\n* Subject who is unable to meet study follow-up requirements",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "general population with an acute, spontaneous, primary, supratentorial ICH",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
