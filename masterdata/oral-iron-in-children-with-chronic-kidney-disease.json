        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Oral Iron therapy",
                    "type": "EXPERIMENTAL",
                    "description": "Participant will receive oral iron therapy.",
                    "interventionNames": [
                        "Drug: Ferrous Sulfate"
                    ]
                },
                {
                    "label": "No oral iron therapy",
