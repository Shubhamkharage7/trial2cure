        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Incidence of human immunodeficiency virus (HIV)",
                    "description": "The total number of participants with incident HIV divided by the total follow-up will be used to calculate the incidence per 100 person years. HIV status is determined by HIV rapid test and a confirmatory Western blot test.",
                    "timeFrame": "Up to 12 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Successful respondent-driven sampling (RDS)",
                    "description": "Defined as half of the study sample enrolled and at least 50% of the responses to the questions of RDS acceptability are answered \"Not at all embarrassed or burdened by RDS\".",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Successful respondent-driven sampling (RDS)",
                    "description": "Defined as more than 80% of participants recruited through RDS.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Prevalence of penile human papillomavirus (HPV) infection, assessed by polymerase chain reaction (PCR)",
                    "description": "Summarized using proportions and exact 95% binomial confidence interval (CI). Will be estimated for specific types and for all combined.",
                    "timeFrame": "Up to 12 months"
                },
                {
                    "measure": "Prevalence of human papillomavirus (HPV)-associated lesions of the penis",
                    "description": "Summarized using proportions and exact 95% binomial CI. Will be estimated for specific types and for all combined.",
                    "timeFrame": "Up to 12 months"
                },
                {
                    "measure": "Prevalence of human papillomavirus (HPV)-associated lesions of the anus",
                    "description": "Summarized using proportions and exact 95% binomial CI. Will be estimated for specific types and for all combined.",
                    "timeFrame": "Up to 12 months"
                },
                {
                    "measure": "Prevalence of anal human papillomavirus (HPV) infection, assessed by polymerase chain reaction (PCR)",
                    "description": "Summarized using proportions and exact 95% binomial confidence interval (CI). Will be estimated for specific types and for all combined.",
                    "timeFrame": "Up to 12 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* HIV seronegative, as documented by any licensed HIV test according to National Acquired Immunodeficiency Syndrome (AIDS) Control Organization (NACO) guidelines\n* Participants report any sex with a man in the past 6 months\n* Participants must speak Hindi, Marathi, or English\n* Participant should not have any plans to move out of the area in the next 12 months and commit to attending two additional visits one at 6 months and one at 12 months\n\nExclusion Criteria:\n\n* Active drug or alcohol use or dependence, or other impairment that, in the opinion of the site investigator, would interfere with adherence to study requirements\n\n  * Participants with impairments that, in the opinion of the site Investigator, are temporary, will be asked to return another day for enrollment\n* Inability to provide informed consent\n* History of a sex change operation that would preclude collection of penile or scrotal specimens",
            "healthyVolunteers": false,
            "sex": "MALE",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "HIV-seronegative MSM in Mumbai, India",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
