        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Individual performance of non-technical skills assessed by Anaesthetist Non-Technical Skills (ANTS)",
                    "description": "The Anaesthetist Non-Technical Skills (ANTS) scoring system uses four categories assessing task management, teamworking, situation awareness and decision-making (1 to 4 points by categories). The minimum score is 4 and the maximum 16",
                    "timeFrame": "immediately after simulation"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Individual performance of non-technical skills assessed by Behaviorally Anchored Rating Scale (BARS)",
                    "description": "The BARS scoring system uses four categories assessing situation awareness, decision-making, communication and teamwork. The score ranges from 1 and 9 (1 = poor and 9 = excellent)",
                    "timeFrame": "immediately after simulation"
                },
                {
                    "measure": "System Usability Scale (SUS) Score",
                    "description": "The scale has 10 items. Scores ranges from 1-5 (1 = strongly disagree and 5 = strongly agree)",
                    "timeFrame": "immediately after simulation"
                },
                {
                    "measure": "The International Organization for Standardization (ISO) Ergonomic scale",
                    "description": "The scale has 6 items. Scores ranges from 1-5 (1 = strongly disagree and 5 = strongly agree)",
                    "timeFrame": "immediately after simulation"
                }
            ]
        },
