        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "endotoxin study group",
                    "description": "Patients scheduled for Percutaneous Nephrolithotomy (PCNL) as surgical treatment for their kidney stones will be evaluated for postoperative endotoxin levels as a risk marker for sepsis."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Number of participants at each endotoxin activity level after surgery.",
                    "description": "The endotoxin activity level assessed from blood samples will be categorized as low (\\<0.40), intermediate (0.40-0.59) or high (\u22650.60) endotoxin activity level. Analysis will be done via Endotoxin Activity Assay (EAA).",
                    "timeFrame": "Day 1 (within 30 minutes postoperative)"
                },
                {
                    "measure": "Corelation of endotoxin activity level with post-operative infectious complication",
                    "description": "infectious complications include fever, and signs and symptoms of systemic inflammatory response syndrome as evaluated by treating physician.",
                    "timeFrame": "up to 24 hours"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Adults that is able to consent (\u2265 18 years of age)\n* Scheduled to undergo PCNL at Uhealth tower and Jackson Memorial Hospitals\n* not currently immunosuppressed \\[White blood count (WBC) \\>= 2, not on immunosuppressive medications\\]\n\nExclusion Criteria:\n\n* unable to consent and \\< 18 years of age\n* currently immunosuppressed (WBC \\< 2, not on immunosuppressive medications)",
