        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Patterns of CAD and CAS disease characteristics",
                    "description": "CAD or CAS characteristics at diagnosis, enrollment, and during follow up (e.g., criteria used for diagnosis, symptoms, laboratory results) will be described.",
                    "timeFrame": "Up to 6 years"
                },
                {
                    "measure": "Number of participants with CAD or CAS complications",
                    "description": "Thromboembolic events (venous, arterial or cerebral), deaths related to CAD or CAS, other CAD/CAS complications (e.g., acute hemolytic crisis, hemolysis-related complications), and other comorbid conditions occurring before enrollment, at enrollment, and during follow up will be described.",
                    "timeFrame": "Up to 6 years"
                },
                {
                    "measure": "Patterns of use of CAD and CAS treatments",
                    "description": "CAD or CAS treatments received before enrollment, at enrollment, and during the follow up period will be summarized. CAD or CAS symptoms and complications, autoimmune disorders or lymphoproliferative disorders, physician assessment of treatment status, and changes in laboratory results will be described by CAD or CAS treatments.",
                    "timeFrame": "Up to 6 years"
                },
                {
                    "measure": "Number of participants with health-resource utilization",
                    "description": "Health resource utilization may include hospitalizations (including length of stay), outpatient visits, emergency room visits, specific transfusion visits, nursing home/rehabilitation center stays, and home healthcare visits during registry follow-up period",
                    "timeFrame": "Up to 6 years"
                },
                {
                    "measure": "Cold Agglutinin Disease Symptoms and Impact Questionnaire (CAD-SIQ)",
                    "timeFrame": "Up to 6 years"
                },
                {
                    "measure": "Functional Assessment of Chronic Illness Therapy-Fatigue (FACIT-Fatigue)",
                    "timeFrame": "Up to 6 years"
                },
                {
                    "measure": "36-Item Short Form Survey Instrument (SF-36) v2",
                    "timeFrame": "Up to 6 years"
                },
                {
                    "measure": "Sutimlimab cohort: Sutimlimab treatment patterns",
                    "description": "Sutimlimab dosing information, reasons for dose adjustments, reasons for discontinuation will be described.",
                    "timeFrame": "Up to 6 years"
                },
                {
                    "measure": "Sutimlimab cohort: Number of participants with adverse events, serious adverse events, adverse events of special interests (AESIs), other events of interests (e.g., Treatment emergent)",
                    "timeFrame": "Up to 6 years"
                },
                {
                    "measure": "Sutimlimab cohort: change from baseline in hemoglobin",
                    "timeFrame": "From baseline up to 6 years"
                },
                {
                    "measure": "Sutimlimab cohort: change from baseline in bilirubin",
                    "timeFrame": "From baseline up to 6 years"
                },
                {
                    "measure": "Sutimlimab cohort: change from baseline in Lactate dehydrogenase (LDH)",
                    "timeFrame": "From baseline up to 6 years"
                },
                {
                    "measure": "Sutimlimab cohort: number of blood cells transfusions",
                    "timeFrame": "Up to 6 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Patient aged \u226518 years\n2. Patient able to understand the purpose of the study and who (or whose legally authorized representative) provided signed and dated informed consent and authorization to use protected health information in accordance with national and local subject privacy regulations\n3. Patient with a diagnosis of CAD or CAS as per Investigator judgment based on the diagnosis criteria listed in study protocol\n\nExclusion Criteria:\n\n1. Patient with mixed warm and cold autoimmune hemolytic anemia, or warm autoimmune hemolytic anemia\n2. Patient actively participating in a CAD or CAS interventional clinical trial. After a patient completes participation in said trial, he/she may be eligible for enrollment in this registry.",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "All patients with CAD or CAS will be eligible, regardless of the type of CAD or CAS-specific therapy they are receiving. In addition, the Cadence registry will include a sutimlimab cohort study whose objective is to assess the safety and the effectiveness of sutimlimab in patients with CAD in a real-world setting.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
