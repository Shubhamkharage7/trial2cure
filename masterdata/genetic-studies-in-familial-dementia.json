        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Correlation between genetic factors and Alzheimer disease, dementia, and related phenotypes.",
                    "description": "Genetic factors will be measured through genome-wide genotyping arrays and/or whole-genome sequencing, and then correlated with Alzheimer disease and related phenotypes, such as cognitive impairment, functional impairment, and relevant biomarkers.\n\n.",
                    "timeFrame": "5 years"
                }
            ]
