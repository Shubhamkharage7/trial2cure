        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Vitamin D Replacement",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will receive prescribed oral vitamin D2 to achieve a serum vitamin D level between 30 ng/mL to 50 ng/mL.",
                    "interventionNames": [
                        "Drug: Ergocalciferol Capsules"
                    ]
                },
                {
                    "label": "Standard of Care",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Participants will receive standard-of-care advice to take over the counter vitamin D.",
                    "interventionNames": [
                        "Dietary Supplement: Cholecalciferol Capsules"
                    ]
                },
                {
                    "label": "Observation Arm",
                    "type": "NO_INTERVENTION",
                    "description": "Observation only"
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Ergocalciferol Capsules",
                    "description": "Vitamin D2 50,000IU by mouth once weekly for 8 weeks then once per month thereafter.",
                    "armGroupLabels": [
                        "Vitamin D Replacement"
                    ],
                    "otherNames": [
                        "Vitamin D2"
                    ]
                },
                {
                    "type": "DIETARY_SUPPLEMENT",
                    "name": "Cholecalciferol Capsules",
                    "description": "Vitamin D3 5,000IU by mouth daily",
                    "armGroupLabels": [
                        "Standard of Care"
                    ],
                    "otherNames": [
                        "Over the Counter Vitamin D3"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Self-reported neuropathic pain- sensory symptoms",
                    "description": "The European Organization for Research and Treatment of Cancer Quality of Life Questionnaire-CIPN (EORTC-QLQ-CIPN20) is a 20-item self-report questionnaire that includes a 9-item subscale assessing sensory symptoms of peripheral neuropathy. The questionnaire asks respondents to think about their past week and indicate to what extent they experienced symptoms of peripheral neuropathy: not at all; a little; quite a bit; or very much. The EORTC-QLQ-CIPN20 will be administered at baseline and at 12 weeks in order to assess change in scores over time.",
                    "timeFrame": "12 Weeks"
                },
                {
                    "measure": "Self-reported neuropathic pain- motor symptoms",
                    "description": "The European Organization for Research and Treatment of Cancer Quality of Life Questionnaire-CIPN (EORTC-QLQ-CIPN20) is a 20-item self-report questionnaire that includes a 8-item subscale assessing motor symptoms of peripheral neuropathy. The questionnaire asks respondents to think about their past week and indicate to what extent they experienced symptoms of peripheral neuropathy: not at all; a little; quite a bit; or very much. The EORTC-QLQ-CIPN20 will be administered at baseline and at 12 weeks in order to assess change in scores over time.",
                    "timeFrame": "12 weeks"
                },
                {
                    "measure": "Self-reported neuropathic pain- autonomic symptoms",
                    "description": "The European Organization for Research and Treatment of Cancer Quality of Life Questionnaire-CIPN (EORTC-QLQ-CIPN20) is a 20-item self-report questionnaire that includes a 3-item subscale assessing autonomic symptoms of peripheral neuropathy. The questionnaire asks respondents to think about their past week and indicate to what extent they experienced symptoms of peripheral neuropathy: not at all; a little; quite a bit; or very much. The EORTC-QLQ-CIPN20 will be administered at baseline and at 12 weeks in order to assess change in scores over time.",
                    "timeFrame": "12 weeks"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Self-reported neuropathic pain",
                    "description": "The number of participants with self reported neuropathic pain using the PROMIS Scale v. 2.0 - Neuropathic Pain Quality 5a. This 5-item questionnaire asks respondents to indicate the extent to which they experienced symptoms of peripheral neuropathy in the past week on a scale from 1 (\"not at all\") to 5 (\"very much\"). The PROMIS Scale v. 2.0 - Neuropathic Pain Quality 5a will be administered at baseline and at 12 weeks in order to assess change in scores over time.",
                    "timeFrame": "12 Weeks"
                },
                {
                    "measure": "Clinical diagnosis of peripheral neuropathy",
                    "description": "The number of participants with a clinical diagnosis of peripheral neuropathy. Chart review will be performed in order to determine whether an independent clinical diagnosis of peripheral neuropathy has been made for study participants.",
                    "timeFrame": "12 Weeks"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Diagnosis of stage I-III cancer of any type, OR diagnosis of stage IV cancer being treated with curative intent\n* Receiving or scheduled to begin taxane-based chemotherapy at Virginia Commonwealth University (VCU) Health Massey Comprehensive Cancer Center\n* Vitamin D serum level \\<20 ng/mL Note: study participants with Vitamin D serum level \u226520 ng/mL may be enrolled in the observational arm.\n\nExclusion Criteria:\n\n* Pre-existing diagnosis of neuropathy\n* Currently taking prescription Vitamin D (ergocalciferol)\n* Inability to converse in English\n* Pregnancy\n* Chronic kidney disease (stage IV or greater)\n* Known hyperparathyroidism\n* Hypercalcemia: Calcium levels \\>10.5 mg/dL Note: study participants with calcium levels \\<10.5 mg/dL may be enrolled in the observational arm.",
            "healthyVolunteers": false,
