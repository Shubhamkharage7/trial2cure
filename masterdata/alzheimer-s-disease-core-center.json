        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Neuropsychological Test Scores",
                    "description": "Cognitive function will be evaluated on a yearly basis using a battery of neuropsychological tests, which will then be scored and normed according to established guidelines.",
                    "timeFrame": "Up to 20 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Neurological Exam",
                    "description": "A thorough neurological exam will be performed once per year to evaluate neurological function.",
                    "timeFrame": "Up to 20 Years"
                },
                {
                    "measure": "Brain Tissue",
                    "description": "A brain autopsy will be performed upon the participant's death to establish histological diagnosis.",
                    "timeFrame": "From date of entry until date of death assessed up to 20 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\nFor patients with MCI, or dementia, the presence of cognitive deficits including memory, language, and executive function.\n\nExclusion Criteria:\n\nPresence of non-neurological medical conditions interfering with cognition.",
