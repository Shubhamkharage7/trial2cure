        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Epidural bupivacaine with hydromorphone patient-controlled anesthesia (EPCA)",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "* Standard of care at Washington University School of Medicine/Barnes-Jewish Hospital\n* Day of surgery: preoperative tylenol, gabapentin, celebrex, and epidural dosing per standard protocol.\n* Intraoperative: dexamethasone and epidural bupivacaine\n* Day of surgery postoperative: hydromorphone PCA, toradol, ibuprofen, tylenol, epidural bupivacaine\n* Postoperative: hydromorphone PCA, ibuprofen, oxycodone",
                    "interventionNames": [
                        "Drug: Bupivacaine"
                    ]
                },
                {
                    "label": "Intrathecal morphine with intraoperative lidocaine infusion",
                    "type": "EXPERIMENTAL",
                    "description": "* Day or surgery: tylenol, gabapentin, celebrex, and preoperative intrathecal morphine one time injection (150mcg)\n* Intraoperative: dexamethasone and lidocaine infusion 1 mg/kg ideal body weight\n* Day of surgery postoperative: toradol, ibuprofen, tylenol\n* Postoperative: ibuprofen, oxycodone, and hydromorphone prn",
                    "interventionNames": [
                        "Drug: Morphine",
                        "Drug: Lidocaine"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Bupivacaine",
                    "description": "-Epidural bupivacaine 0.1% 2-6ml/hr based on MAP within 10% of patient's baseline MAP",
                    "armGroupLabels": [
