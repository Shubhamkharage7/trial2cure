        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Collagenase Clostridium Histolyticum with Low Intensity Shockwave Therapy",
                    "type": "EXPERIMENTAL",
                    "description": "Subjects will receive Collagenase Clostridium Histolyticum (CCH) intralesional injection with low-intensity shockwave therapy (LiSWT).",
