        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Identification of unrecognized myocarditis",
                    "description": "Identification of unrecognized myocarditis in patients presenting PVCs with positive FDG-PET \\[fluorodeoxyglucose (FDG)-positron emission tomography (PET)\\] scanning.",
                    "timeFrame": "12 Months"
                },
                {
                    "measure": "Evaluation of efficacy of immunosuppressive therapy - PVC burden reduce",
                    "description": "Evaluation of efficacy of immunosuppressive therapy in patients with NICM with PVCs in reducing PVC burden as compared to their baseline PVC burden.",
                    "timeFrame": "12 Months"
                },
                {
                    "measure": "Evaluation of efficacy of immunosuppressive therapy - LVEF improvement",
                    "description": "Evaluation of efficacy of immunosuppressive therapy in patients with NICM by documenting improvement in LVEF compared to their baseline LVEF.",
                    "timeFrame": "12 Months"
                },
                {
                    "measure": "Associated atrial and ventricular arrhythmias",
                    "description": "Assessing incidence of associated atrial and ventricular arrhythmias as detected with ILR recordings in patients with apparently idiopathic PVCs.",
                    "timeFrame": "12 Months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Patients \\> 18 years of age\n* Have a Medtronic LINQ II ILR\n* Willing and able to give written informed consent\n\nExclusion Criteria:\n\n* History of myocardial infarction\n* Significant flow-limiting coronary artery disease (\u226550% stenosis) on invasive coronary angiography or Computed tomography angiography (CTA).\n* History of cardiac arrest\n* With existing implantable defibrillators\n* Currently pregnant",
            "sex": "ALL",
