        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "MR Guided Single Fraction Stereotactic Body Radiation Therapy (SBRT)",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will undergo simulation including Gd-EOB-DTPAenhanced MR and planning using minimal margins. Ten days later (+/- 3 days) participants will receive 40Gy single fraction treatment on Elekta Unity using Gd-EOB-DTPA-enhanced MR for image guidance and on-line adaptive replanning.",
                    "interventionNames": [
                        "Radiation: MR Guided Single Fraction Stereotactic Body Radiation Therapy (SBRT)"
