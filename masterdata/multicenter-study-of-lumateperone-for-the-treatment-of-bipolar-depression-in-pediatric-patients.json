        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Children's Depression Rating Scale-Revised (CDRS-R)",
                    "description": "The Children's Depression Rating Scale-Revised is an observer-rated, 17-item semi-structured scale for pediatric patients. The scale comprises cognitive, somatic, affective, and psychomotor symptoms of depression. Items are rated for severity on a 7-point scale (1 to 7) for 14 items and on a 5-point scale (1 to 5) for three items. Total scores range from 7 to 113.",
                    "timeFrame": "Week 6"
                }
            ],
            "secondaryOutcomes": [
