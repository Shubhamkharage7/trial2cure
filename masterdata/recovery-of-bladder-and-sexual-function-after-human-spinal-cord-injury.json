            }
        },
        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Measure symptomatic indices of autonomic dysreflexia",
                    "type": "NO_INTERVENTION",
                    "description": "The purpose of this arm is to systematically measure symptomatic indices of autonomic nervous system activation and corresponding cardiovascular changes in persons with spinal cord injuries during bladder filling and bowel stimulation."
                },
                {
                    "label": "Cardiovascular spinal cord epidural stimulation",
                    "type": "EXPERIMENTAL",
                    "description": "The purpose of this arm is to use spinal cord epidural stimulation for maintenance of blood pressure and heart rate in the lab during cystometry (bladder filling) and anorectal filling (bowel distension) and in the at-home setting for maintenance of normative blood pressure and heart rate that can be triggered from bladder filling and during bowel evacuation.",
                    "interventionNames": [
                        "Device: Spinal Cord Epidural Stimulation"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Spinal Cord Epidural Stimulation",
                    "description": "For arm 2, optimal stimulation parameters (cathode/anode configuration, stimulation frequency and voltage; placement of electrode from L1 to S1) will be identified for blood pressure and bladder pressure to achieve a bladder capacity in the target normative range of 400-450 mLs. Spinal cord epidural stimulation will then be used in a controlled lab setting for the regulation of blood pressure and heart rate to maintain normative values (target systolic pressure of 110-120 mmHg) and bladder pressure below 10 cmH2O during bladder filling up to the targeted capacity (fill volume of 400 mLs).",
                    "armGroupLabels": [
                        "Cardiovascular spinal cord epidural stimulation"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change from baseline in systolic blood pressure over a 24 hour period after 80 sessions (6 months)",
                    "description": "We will record systolic blood pressure every five minutes over a 24 hour period using a 24-hour blood pressure monitor",
                    "timeFrame": "Baseline, 80 sessions (6 months)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change from baseline in bladder capacity after 80 sessions (6 months)",
                    "description": "Using urodynamics we will measure bladder capacity in mL",
                    "timeFrame": "Baseline, 80 sessions (6 months)"
                },
                {
                    "measure": "Change from baseline in detrusor pressure after 80 sessions (6 months)",
                    "description": "Using urodynamics we will measure detrusor pressure in cmH2O.",
                    "timeFrame": "Baseline, 80 sessions (6 months)"
                },
                {
                    "measure": "Change from baseline in mean resting anal pressure after 80 sessions (6 months)",
                    "description": "Using anorectal manometry will will measure mean resting anal pressure in mmHg.",
                    "timeFrame": "Baseline, 80 sessions (6 months)"
                },
                {
                    "measure": "Change from baseline in mean squeeze pressure after 80 sessions (6 months)",
                    "description": "Using anorectal manometry we will measure mean squeeze pressure in mmHg.",
                    "timeFrame": "Baseline, 80 sessions (6 months)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Arm 1 Inclusion Criteria:\n\n* At least 18 years of age;\n* AIS A to D;\n* Neurogenic bladder and bowel dysfunction;\n* Stable medical condition\n\nArm 2 Inclusion Criteria:\n\n* At least 18 years of age;\n* AIS A to D;\n* Neurogenic bladder and bowel dysfunction;\n* Use of intermittent catheterization for bladder emptying;\n* Prior implantation of a Medtronic scES array\n\nArms 1 and 2 Exclusion Criteria:\n\n* Prior Botox injections of the bladder and/or bladder augmentation surgery;\n* Colostomy bag,\n* Ventilator dependent;\n* Any implanted pump (i.e. Baclofen pump, pain pump, etc.);\n* Pregnant at the time of enrollment or planning to become pregnant during the time course of the study",
            "healthyVolunteers": false,
            "sex": "ALL",
