        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "To assess the prevalence of obstructive sleep apnea",
                    "description": "Measure the apnea-hypopnea index (AHI) to determine burden of obstructive sleep apnea. Based on the AHI, sleep apnea will be diagnosed if the AHI is \u22655 events/hour.",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "To determine the severity of obstructive sleep apnea",
                    "description": "The severity of OSA is classified based on the AHI as follows: None/Minimal: AHI \\< 5 per hour, Mild: AHI \u2265 5, but \\< 15 per hour, Moderate: AHI \u2265 15, but \\< 30 per hour, Severe: AHI \u2265 30 per hour.",
                    "timeFrame": "10 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "24 hour mean arterial pressure",
                    "description": "Ambulatory measure of blood pressure in mmHg",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Vascular endothelial function",
                    "description": "Change in brachial artery diameter in response to hyperemia",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Insulin sensitivity",
                    "description": "Oral glucose tolerance testing",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Body composition",
                    "description": "Percentage body fat content",
                    "timeFrame": "10 years"
                }
            ]
        },
