        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Bottled pain formulations Group",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Participants in this group will use the standard-of-care medication treatment for ambulatory spinal surgery prescribed to them in different bottles for 7 days.",
                    "interventionNames": [
                        "Drug: Acetaminophen",
                        "Drug: Celebrex",
                        "Drug: Gabapentin",
                        "Drug: Famotidine",
                        "Drug: Decadron",
                        "Drug: Oxycodone"
                    ]
                },
                {
                    "label": "Multi-Modal regimen Group",
                    "type": "EXPERIMENTAL",
                    "description": "Participants in this group will receive the same drugs used during the standard-of-care treatment for ambulatory spinal surgery. However, the drugs will be prescribed using a simple multimodal medication pre-formulated package for 7 days.",
                    "interventionNames": [
                        "Drug: Acetaminophen",
                        "Drug: Celebrex",
                        "Drug: Gabapentin",
                        "Drug: Famotidine",
                        "Drug: Decadron",
                        "Drug: Oxycodone"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Acetaminophen",
                    "description": "Day 1 and Day 2: 1000 mg three times a day administered via tablet by mouth for a total cumulative daily dose of 3000 mg.\n\nDay 3 and Day 4: 825 mg three times a day administered via tablet by mouth for a total cumulative daily dose of 2475 mg.\n\nDay 5: 500 mg two times a day and 325 mg once a day administered via tablet by mouth for a total cumulative daily dose of 1325 mg.\n\nDay 6: 500 mg once a day and 325mg twice a day administered via tablet by mouth for a total cumulative daily dose of 1150 mg.\n\nDay 7: 500 mg twice a day administered via tablet by mouth for a total cumulative daily dose of 1000 mg.",
                    "armGroupLabels": [
                        "Bottled pain formulations Group",
                        "Multi-Modal regimen Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Celebrex",
                    "description": "Day 1: 200 mg twice a day administered via tablet by mouth for a total cumulative daily dose of 400 mg.\n\nDay 2 and Day 3: 100 mg three times a day administered via tablet by mouth for a total cumulative daily dose of 300 mg.\n\nDay 4: 100 mg twice a day administered via tablet by mouth for a total cumulative daily dose of 200 mg.\n\nDay 5: 50 mg twice a day administered via tablet by mouth for a total cumulative daily dose of 100 mg.\n\nNot prescribed on Days 6 and 7.",
                    "armGroupLabels": [
                        "Bottled pain formulations Group",
                        "Multi-Modal regimen Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Gabapentin",
                    "description": "Day 1 and Day 2: 300 mg three times a day administered via tablet by mouth for a total cumulative daily dose of 900 mg.\n\nDay 3: 200 mg three times a day administered via tablet by mouth for a total cumulative daily dose of 600 mg.\n\nDay 4: 200 mg two times a day and 100mg once a day administered via tablet by mouth for a total cumulative daily dose of 500 mg.\n\nDay 5: 100 mg three times a day administered via tablet by mouth for a total cumulative daily dose of 300 mg.\n\nDay 6: 100 mg two times a day administered via tablet by mouth for a total cumulative daily dose of 200 mg.\n\nDay 7: 100 mg once a day administered via tablet by mouth for a total cumulative daily dose of 100 mg.",
                    "armGroupLabels": [
                        "Bottled pain formulations Group",
                        "Multi-Modal regimen Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Famotidine",
                    "description": "Day 1- Day 7: 20 mg once a day administered via tablet by mouth for a total cumulative daily dose of 20 mg.",
                    "armGroupLabels": [
                        "Bottled pain formulations Group",
                        "Multi-Modal regimen Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Decadron",
                    "description": "Day 1: 4 mg three times a day administered via tablet by mouth for a total cumulative daily dose of 12 mg.\n\nDay 2: 4 mg two times a day administered via tablet by mouth for a total cumulative daily dose of 8 mg.\n\nDay 3: 2 mg two times a day administered via tablet by mouth for a total cumulative daily dose of 4 mg.\n\nNot prescribed on Days 4 through 7.",
                    "armGroupLabels": [
                        "Bottled pain formulations Group",
                        "Multi-Modal regimen Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Oxycodone",
                    "description": "A prescription of oxycodone as per standard of care will be provided to participants in case of breakthrough pain.",
                    "armGroupLabels": [
                        "Bottled pain formulations Group",
                        "Multi-Modal regimen Group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Adherence measured by the self-report medication nonadherence scale",
                    "description": "Adherence will be measured using a Likert scale ranging from \"never\" indicating that the participant followed all instructions and adhered to the medication to \"all the time. \"indicating that the participant never followed instructions and did not take the medication.",
                    "timeFrame": "up to 7 days"
                },
                {
                    "measure": "Satisfaction measured by Satisfaction Likert Scale",
                    "description": "Scores range from \"0\" not satisfied to \"10\" very satisfied",
                    "timeFrame": "up to 7 days"
                },
                {
                    "measure": "Ease of use as measured by Likert Scale",
                    "description": "Scores ranges from \"strongly disagree to Strongly agree\" regarding the ease of use of the medication package.",
                    "timeFrame": "up to 7 days"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Number of pain pills",
                    "description": "Count of pills taken by participants",
                    "timeFrame": "up to 7 days"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Adults: males or non-pregnant females.\n* Patients undergoing ambulatory microdiscectomies, foraminotomies, and spinal cord decompressions requiring pain control analgesics.\n\nExclusion Criteria:\n\n* Pregnancy\n* Active severe liver disease\n* Chronic kidney disease 3a-5 (moderate to severe)\n* Chronic obstructive pulmonary disease with impaired pulmonary function\n* Chronic steroid use\n* Chronic opioid use\n* Current use of narcotics\n* Allergy to sulfonamides\n* Allergy to NSAIDs\n* Allergy to Aspirin\n* Allergy to Tylenol\n* Allergy to Gabapentins\n* Allergy to H2 blockers (cimetidine, famotidine)\n* Allergy to steroid\n* Current use of gabapentins for any medical condition\n* Inability to be discharged home on the day of surgery\n* Inability to swallow pills\n* Myasthenia gravis\n* History of gastrointestinal ulcers or stomach bleeding\n* Have any condition that, in the investigator's opinion, will compromise the well-being of the patient or the study, or prevent the patient from meeting or performing study requirements.",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
