        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Nutrition Education Group",
                    "type": "EXPERIMENTAL",
                    "description": "Participants in this study arm will receive nutrition education once a month for 12 months.",
                    "interventionNames": [
                        "Behavioral: Nutrition Education"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Nutrition Education",
                    "description": "The intervention will consist of 12 monthly nutrition education sessions catered to people with spinal cord injuries. The curriculum is based on the 2020-2025 Dietary Guidelines for Americans such as consuming recommended amounts of fruits/vegetables, whole grains, etc. The sessions will be approximately 30-60 minutes and will be available in-person.",
                    "armGroupLabels": [
                        "Nutrition Education Group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in Nutrition Knowledge",
                    "description": "The General Nutrition Knowledge Questionnaire is an 88-item scale that has a scoring range of 0 to 88 with a high score indicating more knowledge of nutrition.",
                    "timeFrame": "Baseline, 12 months"
                }
            ],
            "secondaryOutcomes": [
