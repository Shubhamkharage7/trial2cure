        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in planned clinical management (Cohort A and B)",
                    "description": "The association of the test result on medical decision making",
                    "timeFrame": "Day 20"
                },
                {
                    "measure": "Change in planned clinical management (Cohort B)",
                    "description": "Evaluate the planned versus subsequent planned change in clinical management as a result of receiving the test result",
                    "timeFrame": "Day 0 vs Day 20"
                },
                {
                    "measure": "Difference between the actual age and symptomatology versus intended use criteria (Cohort A and B)",
                    "description": "Evaluate the intended use criteria",
                    "timeFrame": "Day 90"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Relationship between the test result and actual clinical management (Cohort B)",
                    "description": "Association of the test result on subsequent conducted changes in clinical management and test results when evaluating individuals with MCI or dementia in an ambulatory setting",
                    "timeFrame": "Day 90"
                },
                {
                    "measure": "Change in planned clinical management compared to conducted clinical management (Cohort B only)",
                    "description": "The evaluation of planned versus conducted change in management as a result of receiving the test result",
                    "timeFrame": "Day 20 vs Day 90"
                },
                {
                    "measure": "Change in probability of AD (Cohort A and B)",
                    "description": "Evaluate the probability of an AD Dx pre and post receipt of the test result",
                    "timeFrame": "Day 0 vs Day 20"
                },
                {
                    "measure": "Change in anti-AD medication use (Cohort A and B)",
                    "description": "Evaluate anti-AD medication use pre- and post-receipt of the test result",
                    "timeFrame": "Day 0 vs Day 20"
                },
                {
                    "measure": "Change in diagnosis (Cohort B)",
                    "description": "Evaluate any changes in diagnoses as a result of receiving the AD test result",
                    "timeFrame": "Day 0 vs Day 90"
                },
                {
                    "measure": "Relationship between the test result and planned testing (Cohort B)",
                    "description": "Evaluate the association between subsequent test ordering pre and post testing",
                    "timeFrame": "Day 0 vs Day 20 vs Day 90"
                },
                {
                    "measure": "Correlations between the net promoter score and ease of use by APS2 result (Cohort B)",
                    "description": "Focus group questions around net promoter score and ease of use, strengths, and limitations of testing",
                    "timeFrame": "Day 90"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Physician Inclusion Criteria:\n\n1. Memory care specialists actively practicing in the United States\n2. Practice serves individuals with MCI or dementia age \\> 55 years\n3. Average patient volume \\> 25 visits per week (all patients seen across practice)\n4. Memory care specialist with access to an online electronic survey\n\nPhysician Exclusion Criteria:\n\n1) Clinicians who practice in New York\n\nParticipant Inclusion Criteria:\n\n1. Individual with MCI or dementia\n2. Age \\>= 55 years\n\nParticipant Exclusion Criteria\n\n1. Individual requiring test related blood draw within the state of New York\n2. Participation does not seem to be in the best interest of the individual, per the ordering clinician",
            "sex": "ALL",
            "minimumAge": "55 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "The study population will include up to 40 memory care specialists who will use the PrecivityAD2 blood test and complete the respective surveys. Participating clinicians will be selected based on their ability to meet the acceptance measures from the inclusion and exclusion criteria. Cohort A will include up to 100 test samples collected for the PrecivityAD2 blood test. Cohort B will include up to 300 test samples collected for the PrecivityAD2 blood test.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
