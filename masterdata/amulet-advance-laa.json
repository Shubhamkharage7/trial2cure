        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Left atrial appendage occlusion (LAAO)",
                    "description": "Implantation of an Amplatzer Amulet left atrial appendage occluder"
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Composite endpoint: Death or complications",
                    "description": "Composite of all-cause death, ischemic stroke, systemic embolism, or device/procedure related complications requiring an invasive surgical or percutaneous intervention within 7 days of implant or hospital discharge, whichever is later",
                    "timeFrame": "Within 7 days of implant or hospital discharge, whichever is later"
                },
                {
                    "measure": "Device closure, defined as residual jet around the device \u2264 3mm",
                    "description": "Device closure (defined as residual jet around the device \u2264 3mm) at the first follow-up visit documented by trans-esophageal echocardiography (TEE), as assessed by an independent core laboratory",
                    "timeFrame": "Assessed at first trans-esophageal echocardiogram (TEE) occurring between 30-240 days post-implant."
                },
                {
                    "measure": "Composite endpoint: Ischemic stroke or systemic embolism",
                    "description": "Composite of ischemic stroke or systemic embolism through 24 months",
                    "timeFrame": "Through 24 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Intended for LAAO with the Amulet device\n* At least 18 years of age\n* Willing and capable of providing informed consent and participating in all testing associated with this clinical study\n\nExclusion Criteria:\n\n* Presence of other conditions that, in the investigator's opinion, could limit the subject's ability to participate in follow-up",
