        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Neutrophil populations",
                    "description": "compare neutrophil population between patients with SSI and without SSI by delineating transcriptomic profiles",
                    "timeFrame": "up to 3 days"
                },
                {
                    "measure": "Neutrophil functional impairment",
                    "description": "illustrate neutrophil subpopulation associated with neutrophil functional impairment and SSIs by probing phagocytosis and reactive oxygen species formation",
                    "timeFrame": "up to 3 days"
                }
            ]
        },
