        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "ctDNA collection from draining and peripheral veins",
                    "type": "EXPERIMENTAL",
                    "description": "Patient with suspected primary hepatobiliary or pancreatic cancer undergoing a diagnostic work-up with a percutaneous or trans-jugular biopsy (standard of care) and will undergo a sampling of the cancer draining vein during their biopsy procedure with a collection of an additional 10mL of blood.",
                    "interventionNames": [
                        "Diagnostic Test: ctDNA Blood Collection"
