        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Pain intensity and interference scores via NRS (Numeric Rating Scale)",
                    "description": "NRS score from 0(being none)-10(being the worst)",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Pain intensity and interference scores via BPI-sf (Brief Pain Inventory-short form)",
                    "description": "Pain intensity and interference scores via BPI-sf 0 to 10, where: 0 = no pain and 10 = pain as bad as you can imagine",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Opioid use via pre-and post-treatment analgesic medication use log",
                    "description": "Opioid use via pre-and post-treatment analgesic medication use log",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Functional status via KOOS-JR (Knee injury and Osteoarthritis Outcome Score for Joint Replacement)",
                    "description": "Functional status via KOOS-JR. 0(none)-4(extreme)",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Functional status via SANE (Single Assessment Numeric Evaluation)",
                    "description": "Functional status via SANE. Percentage of normal (0% to 100% scale with 100% being normal)",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Quality of Sleep via PROMIS-SD (PROMIS - Sleep Disturbance - Short Form 8b)",
                    "description": "Quality of Sleep via PROMIS-SD. 1(Best)-5(Worst)",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Adverse events related to pain therapies",
                    "description": "Adverse events related to pain therapies",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "General HRQOL via PROMIS-10 (PROMIS GLOBAL-10)",
                    "description": "General HRQOL via PROMIS-10. 1(Best)-5(Worst)",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Patient Satisfaction",
                    "description": "Patient satisfaction with pain management questionnaire (Patient Satisfaction Questionnaire). Two questions assessing satisfaction and benefit.",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Physical therapy utilization and work productivity via PT/Work Productivity",
                    "description": "Physical therapy (PT) utilization and work productivity via PT/Work Productivity (WP) questionnaire.",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Emergency Department (ED) visits",
                    "description": "Emergency Department (ED) visits",
                    "timeFrame": "18 months"
                },
