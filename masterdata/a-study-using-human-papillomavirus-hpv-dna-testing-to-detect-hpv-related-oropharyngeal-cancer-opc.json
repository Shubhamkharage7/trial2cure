            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "NavDx",
                    "description": "Evaluation of HPV presence via NavDx (v1) at two (or three) timepoints\n\no If one of the two blood samples is positive, the subject will have a third NavDx (v1) test approximately 1 month after the second timepoint. If the test is positive, the subject will move to surveillance phase.",
                    "armGroupLabels": [
                        "No previous history of diagnosed HPV-related head and neck cancer"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Proportion of participants found to have HPV-related OPC",
                    "description": ": The primary objective is to determine the feasibility of screening by estimating the yield (proportion of subjects found to have HPV-related OPC out of all subjects screened).",
                    "timeFrame": "Up to 5 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Ages 45 and older\n* Self-reported history of four or more lifetime sexual partners\n* Self-reported history of one or more lifetime oral sexual partner\n* No previous history of diagnosed HPV-related head and neck cancer\n\nExclusion Criteria:\n\n* History of diagnosed HPV-related OPC\n* Less than three sexual partners in lifetime\n* No history of oral sex performed\n* Younger than age 45",
            "healthyVolunteers": true,
            "sex": "ALL",
            "minimumAge": "45 Years",
