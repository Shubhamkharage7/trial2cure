        "descriptionModule": {
            "briefSummary": "This multinational, investigator-initiated, retrospective study aims to investigate outcomes of patients, who underwent transcatheter mitral valve implantation (TMVI), in comparison to those screened for TMVI but deemed ineligible, who subsequently underwent interventional mitral valve edge-to-edge repair, mitral valve surgery or medical/conservative therapy."
        },
        "conditionsModule": {
            "conditions": [
                "Mitral Regurgitation"
            ]
        },
        "designModule": {
            "studyType": "OBSERVATIONAL",
            "patientRegistry": false,
            "designInfo": {
                "observationalModel": "COHORT",
                "timePerspective": "RETROSPECTIVE"
            },
            "enrollmentInfo": {
                "count": 800,
                "type": "ESTIMATED"
            }
        },
        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Transcatheter Mitral Valve Implantation (TMVI)",
                    "description": "Patients with successful TMVI screening, who underwent Transcatheter Mitral Valve Implantation subsequently. All devices may be included."
                },
                {
                    "label": "Interventional mitral valve edge-to-edge repair (E2E)",
                    "description": "Patients with TMVI screening failure, who subsequently underwent interventional mitral valve edge-to-edge repair."
                },
                {
                    "label": "Mitral valve surgery (Surgery)",
                    "description": "Patients with TMVI screening failure, who subsequently underwent mitral valve surgery (i.e., mitral valve repair or replacement)."
                },
                {
                    "label": "Medical therapy (OMT)",
                    "description": "Patients with TMVI screening failure, who subsequently underwent conservative or optimal medical therapy (OMT)."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Mitral insufficiency grade 2+ or more",
                    "description": "Mitral insufficiency measured by transthoracic echocardiography. Assessment of MI severity according to current guidelines (ESC/EACTS and ACC/AHA) for valvular heart disease.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Freedom from device-related complications",
                    "timeFrame": "30 days"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Combined all-cause mortality or rehospitalization for congestive heart failure",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "All-cause mortality",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Cardiovascular mortality",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Rehospitalization for congestive heart failure",
                    "description": "Incidence of new-onset or worsening signs and symptoms of heart failure that required urgent therapy and resulted in hospitalization, e.g. as assessed by patient interviews.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Combined cardiovascular mortality or rehospitalization for congestive heart failure",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Unplanned mitral valve intervention",
                    "description": "Incidence of unplanned surgical or transcatheter re-intervention, heart transplantation or left ventricular assist device implantation, e.g. as assessed by patient interviews.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Combined all-cause mortality, rehospitalization for congestive heart failure or unplanned mitral valve intervention",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Combined cardiovascular mortality, rehospitalization for congestive heart failure or unplanned mitral valve intervention",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "New York Heart Association (NYHA) stage III or IV",
                    "description": "Assessment of New York Heart Association (NYHA) classification stage (in- or outpatient) after 12 months follow-up, as assessed by patient interviews.",
                    "timeFrame": "12 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* clinically significant mitral insufficiency\n* patient underwent screening for TMVI\n* echocardiography data at baseline (and after TMVI, E2E and surgery)\n* follow-up of at least 30 days\n\nExclusion Criteria:\n\n- age under 18 years",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "This study aimes to include all patients with significant mitral insufficiency, who underwent screening for possible transcatheter mitral valve implantation (TMVI), irrespective of the subsequent therapy.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
