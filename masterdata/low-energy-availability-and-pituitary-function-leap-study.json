        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Energy balanced metabolic diet (5-days)",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "45 kcals/kg of fat free mass \\[FFM\\]/day; 28% fat, 15% protein, 57% carbohydrate",
                    "interventionNames": [
                        "Behavioral: 5-day controlled metabolic dietary intake"
                    ]
                },
                {
                    "label": "Low energy availability metabolic diet (5-days)",
                    "type": "EXPERIMENTAL",
                    "description": "20 kcal/kg of FFM/day; 28% fat, 15% protein, 57% carbohydrate",
                    "interventionNames": [
                        "Behavioral: 5-day controlled metabolic dietary intake"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "5-day controlled metabolic dietary intake",
                    "description": "Over 5-days, all food will be provided from the metabolic kitchen and participants will be instructed to eat all of the food provided each day and only the food provided each day.",
                    "armGroupLabels": [
                        "Energy balanced metabolic diet (5-days)",
                        "Low energy availability metabolic diet (5-days)"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change from baseline in stimulated pituitary function, as measured by Luteinizing hormone (LH) area under the curve (AUC) over two hours following pituitary stimulation with a physiologic bolus intravenous dose of gonadorelin acetate",
                    "description": "Within-participant changes in response to both conditions will be measured, with lower LH AUC indicating a blunted pituitary response to stimulation.",
                    "timeFrame": "Baseline, ~4-weeks, ~8-weeks (visits timed to to days 1-5 of the menstrual cycle)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Female\n* Age 18-30\n* Body mass index between 18.5-24.9 OR 30-45\n* Have regular menstrual cycles between 18-40 days over the past 6 months\n* Weight stable (not gained or lost 5kg) in the past 3 months\n\nExclusion Criteria:\n\n* Using hormonal birth control currently or within the past three months\n* Polycystic ovary syndrome (current or past diagnosis)\n* Pregnant, lactating, planning to become pregnant in next 6 months\n* Engaging in regular intentional aerobic exercise\n* Significant medical issues (e.g.,cardiovascular disease or diabetes)",
