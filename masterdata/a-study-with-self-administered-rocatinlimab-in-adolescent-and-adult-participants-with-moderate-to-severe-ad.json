        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Rocatinlimab Dose 1",
                    "type": "EXPERIMENTAL",
                    "description": "Rocatinlimab will be self-administered subcutaneously using a prefilled syringe (PFS). Participants will receive rocatinlimab every 4 weeks (Q4W) for 52 weeks with a loading dose at week 2.",
                    "interventionNames": [
                        "Combination Product: Rocatinlimab Prefilled Syringe"
                    ]
                },
                {
                    "label": "Rocatinlimab Dose 2",
                    "type": "EXPERIMENTAL",
                    "description": "Rocatinlimab will be self-administered subcutaneously using a PFS. Participants will receive rocatinlimab Q4W for 52 weeks with a loading dose at week 2.",
                    "interventionNames": [
                        "Combination Product: Rocatinlimab Prefilled Syringe"
                    ]
                }
            ],
