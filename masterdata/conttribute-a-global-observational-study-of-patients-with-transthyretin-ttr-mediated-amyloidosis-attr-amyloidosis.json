        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Patients with ATTR amyloidosis",
                    "description": "Patients with a diagnosis of ATTR amyloidosis, hereditary or wild type, will be eligible for the study and will follow routine clinical care."
                },
                {
                    "label": "Pre-symptomatic Carriers",
                    "description": "Pre-symptomatic carriers with a known disease-causing TTR variant will be eligible for the study and will follow routine clinical care."
                }
