        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "high interactive virtual human administered nutrition module",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "The virtual health assistant will interactively collect nutrition information (alcohol, red meat, and processed meat intake) and report risk information back to users in visual and audio format",
                    "interventionNames": [
                        "Other: virtual technology - high interactive"
                    ]
                },
                {
                    "label": "low interactive virtual human module",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Complete the current intervention module that includes items assessing alcohol and meat intake.",
