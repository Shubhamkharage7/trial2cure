        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Arm 1 - Opioid Post-Op Pain Regimen",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Patients randomized to Arm 1 will receive the current most commonly prescribed pain control regimen after a minimally invasive tubal sterilization procedure at discharge.\n\nThese medications include:\n\nTylenol 500 mg orally every 6 hours scheduled x 30 tablets Ibuprofen 600 mg orally every 6 hours scheduled x 30 tablets and Oxycodone 5 mg orally every 4 hours as needed x 12 tablets\n\nAll patients will be instructed to use Acetaminophen and Ibuprofen around the clock for the first 72 hours and as needed thereafter",
                    "interventionNames": [
                        "Drug: Acetaminophen",
                        "Drug: Ibuprofen",
                        "Drug: Oxycodone"
                    ]
                },
                {
                    "label": "Arm 2 - Non-Opioid Post-Op Pain Regimen",
                    "type": "EXPERIMENTAL",
                    "description": "Patients randomized to Arm 2 will not receive an opioid prescription after minimally invasive tubal sterilization procedures at discharge. They will receive only Tylenol and Ibuprofen as follows:\n\nTylenol 500 mg orally every 6 hours scheduled x 30 tablets Ibuprofen 600 mg orally every 6 hours scheduled x 30 tablets and\n\nAll patients will be instructed to use Acetaminophen and Ibuprofen around the clock for the first 72 hours and as needed thereafter.\n\nParticipants will be informed that if they need additional pain medications, these will not be withheld. Participants in the second arm who require additional pain medications will receive the same amount of oxycodone as in arm 1.",
                    "interventionNames": [
                        "Drug: Acetaminophen",
                        "Drug: Ibuprofen"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Acetaminophen",
                    "description": "All patients will receive Acetaminophen 500 mg orally every 6 hours scheduled x 30 tablets at post-operative discharge.\n\nAll patients will be instructed to take Acetaminophen around the clock for the first 72 hours and as needed thereafter.",
                    "armGroupLabels": [
                        "Arm 1 - Opioid Post-Op Pain Regimen",
                        "Arm 2 - Non-Opioid Post-Op Pain Regimen"
                    ],
                    "otherNames": [
                        "Tylenol"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Ibuprofen",
                    "description": "All patients will receive Ibuprofen 600 mg orally every 6 hours scheduled x 30 tablets at post-operative discharge.\n\nAll patients will be instructed to take Ibuprofen around the clock for the first 72 hours and as needed thereafter.",
                    "armGroupLabels": [
                        "Arm 1 - Opioid Post-Op Pain Regimen",
                        "Arm 2 - Non-Opioid Post-Op Pain Regimen"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Oxycodone",
                    "description": "Patients randomized to Arm 1 will receive Oxycodone 5 mg orally every 4 hours as needed x 12 tablets at post-operative discharge\n\nPatients randomized to Arm 2 will not receive an Oxycodone prescription at post-operative discharge.\n\nHowever, participants will be informed that if they need additional pain medications, these will not be withheld. Participants in the second arm who require additional pain medications will receive the same amount of Oxycodone as in arm 1",
                    "armGroupLabels": [
                        "Arm 1 - Opioid Post-Op Pain Regimen"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Numeric post-operative pain score",
                    "description": "Patient reported pain on \"Post-Operative Day 1 Survey\" and \"Post-Operative Day 7 Survey\" using the Likert pain scale from 0 to 10, where 0 is no pain and 10 is severe pain. A higher score indicates a worse outcome.",
                    "timeFrame": "post-operative day 1 and post-operative day 7"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Satisfaction with post-operative pain relief",
                    "description": "As indicated on \"Post-Operative Day 1 Survey\" and \"Post-Operative Day 7 Survey\" by a binary \"yes\" or \"no\" response. \"Yes\" indicates a favorable outcome and \"no\" indicates a worse outcome.",
                    "timeFrame": "post-operative day 1 and post-operative day 7"
                },
                {
                    "measure": "Satisfaction with post-operative mobility",
                    "description": "As indicated on \"Post-Operative Day 1 Survey\" and \"Post-Operative Day 7 Survey\" by a binary \"yes\" or \"no\" response. \"Yes\" indicates a favorable outcome and \"no\" indicates a worse outcome.",
                    "timeFrame": "post-operative day 1 and post-operative day 7"
                },
                {
                    "measure": "Total narcotic consumption at one week post-operative",
                    "description": "Number of Oxycodone pills used by each patient by the end of post-operative day 7",
                    "timeFrame": "post-operative day 7"
                },
                {
                    "measure": "Occurrence of defined opioid related side effects (N/V, constipation, dizziness, itchiness)",
                    "description": "As indicated on \"Post-Operative Day 1 Survey\" and \"Post-Operative Day 7 Survey\" by a binary \"yes\" or \"no\" response. \"No\" indicates a favorable outcome and \"yes\" indicates a worse outcome.",
                    "timeFrame": "post-operative day 1 and post-operative day 7"
                },
                {
                    "measure": "Suboptimal pain control as assessed by the number of patients requesting additional medication or seeking unplanned medical care for a post-surgical pain-related concern",
                    "description": "Number of patients who request additional pain medications at the time of post-operative surveys, call provider phone line for additional pain medications, or return to an Emergency Department, clinic/office, etc due to a pain related issue.",
                    "timeFrame": "Within 30 days of surgery"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Individuals with a fallopian tube (unilateral and/or bilateral)\n* Age 18 years old and above\n* Undergoing minimally invasive (laparoscopic or robotic) unilateral or bilateral salpingectomy or other tubal sterilization procedure as the primary procedure\n* Benign indications for salpingectomy/tubal sterilization\n* Agreeing to participate\n\nExclusion Criteria:\n\n* Chronic pain syndromes patients including fibromyalgia\n* Patients currently on long-term (i.e. for more than three months) opioid use\n* Conversion to laparotomy\n* Allergy or other contraindication to the prescribed medications such as acetaminophen or oxycodone\n* Salpingectomy that occurs in conjunction with a major Gyn surgery (i.e. hysterectomy, etc)\n* Salpingectomy performed for treatment of ectopic pregnancy\n* Patients with a history of gastritis and/or GI bleeding",
            "healthyVolunteers": true,
            "sex": "FEMALE",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
