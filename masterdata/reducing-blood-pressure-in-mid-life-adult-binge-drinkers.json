                },
                {
                    "measure": "Changes in urine catecholamine levels",
                    "description": "Urine samples will be collected for 24 hours to measure norepinephrine levels (ug/g)",
                    "timeFrame": "Baseline and after an 8-week intervention"
                },
                {
                    "measure": "Changes in blood pressure",
                    "description": "Systolic and Diastolic blood pressure will be measured at rest and for 24 hours.",
                    "timeFrame": "Baseline and after an 8-week intervention"
                },
