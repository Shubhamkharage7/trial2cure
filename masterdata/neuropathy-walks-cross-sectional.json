        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Gait - Walking velocity",
                    "description": "Temporal characteristic of gait measured in m/sec as captured with a ZENO instrumented walkway",
                    "timeFrame": "Week 0 (one-time)"
                },
                {
                    "measure": "Gait - Step Length",
                    "description": "Spatial characteristic of gait measured in meters as captured with a ZENO instrumented walkway",
                    "timeFrame": "Week 0 (one-time)"
                },
                {
                    "measure": "Gait - Step Time",
                    "description": "Temporal characteristic of gait measure in seconds as captured with a ZENO instrumented walkway",
                    "timeFrame": "Week 0 (one-time)"
                },
                {
                    "measure": "Gait - Stride Width",
                    "description": "Spatial characteristic of gait measured in meters as captured with a ZENO instrumented walkway",
                    "timeFrame": "Week 0 (one-time)"
                },
                {
                    "measure": "Gait - Cadence",
                    "description": "Temporal characteristic of gait measured in steps per minute as captured with a ZENO instrumented walkway",
                    "timeFrame": "Week 0 (one-time)"
                },
                {
                    "measure": "Gait - Foot Fall Area",
                    "description": "Spatial characteristic of gait measured in square meters as captured with a ZENO instrumented walkway",
                    "timeFrame": "Week 0 (one-time)"
                },
                {
                    "measure": "Physical Performance",
                    "description": "Physical performance score generated via the Short Physical Performance Battery. (SPPB). SPPB procedures generate a total score reflective of physical performance. Minimum score=0; maximum score=12; lower score=worse outcome",
                    "timeFrame": "Week 0 (one-time)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Walking Endurance",
                    "description": "Walking endurance as measured with the 6-Minute Walk test (distance walked over span of 6 minutes).",
                    "timeFrame": "Week 0 (one-time)"
                },
                {
                    "measure": "Whole Body Strength",
                    "description": "Mid-thigh pull test will be used to generate a score expressed as force in pounds or kilograms that is reflective of whole body strength.",
                    "timeFrame": "Week 0 (one-time)"
                },
                {
                    "measure": "Pain pressure threshold",
                    "description": "Pain pressure threshold (expressed in pounds or kilograms of force) at lower extremity landmarks via use of a digital algometer",
                    "timeFrame": "Week 0 (one-time)"
                },
                {
                    "measure": "Physical Activity",
                    "description": "Physical activity characteristics: (1) over a span of 5 days captured via a wrist-band style Actigraph activity monitor; (2) self-reported physical activity over past 7 days using International Physical Activity Questionnaire - Short Form",
                    "timeFrame": "5 days from date of all other outcomes"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* diagnosis of HIV infection currently treated with antiretroviral therapy (ART)\n* cluster of differentiation 4 cell (CD4 cell) count of at least 200 cells/mm3\n* 18-64 years of age\n* able to read and write in English\n* means to travel to a study site\n* Additional inclusion for cohort with painful neuropathy in the feet:\n* presence of peripheral neuropathy symptoms in feet\n* average daily self-reported pain in the feet of at least 3/10 on a 0-10 numerical pain scale\n* pain in the feet present for at least the past 3 months\n* no changes in medications used to manage pain in the past 4 weeks\n\nExclusion Criteria:\n\n* current opportunistic infection(s)\n* cluster of differentiation 4 cell (CD4 cell) count \\<200 cells/mm3\n* dementia\n* uncontrolled psychiatric disorder\n* wounds or sores on the feet\n* musculoskeletal or neurological conditions (other than distal sensory neuropathy) that may affect gait\n* pregnancy",
