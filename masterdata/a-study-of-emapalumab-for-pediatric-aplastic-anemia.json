        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Emapalumab, then Standard IST",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will first receive Emapalumab for 6 weeks. After treatment with emapalumab, participants will receive standard IST with drugs called equine anti-thymocyte globulin (hATG) and cyclosporin (CsA) in addition to a lower dose of emapalumab",
                    "interventionNames": [
                        "Biological: Emapalumab"
                    ]
                },
                {
                    "label": "Emapalumab, then HCT",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will first receive Emapalumab for 6 weeks. After treatment with emapalumab, participants will have a standard hematopoietic stem cell transplant (HCT).",
                    "interventionNames": [
                        "Biological: Emapalumab"
                    ]
                }
            ],
