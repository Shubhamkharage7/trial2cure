            "interventions": [
                {
                    "type": "DIETARY_SUPPLEMENT",
                    "name": "Beet-it Juice",
                    "description": "Patients will ingest 1 bottle (70ml) of beet-it juice (98% concentrated beet juice, 2% lemon juice containing 400 mg/6.5 mmol nitrate).",
                    "armGroupLabels": [
                        "Control Group",
                        "PCD Group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in nasal NO as measured by a chemiluminescence analyzer",
                    "timeFrame": "Baseline,after beet juice consumption(1 hour after consumption)"
                },
                {
                    "measure": "Change in nasal NO as measured by a chemiluminescence analyzer",
                    "timeFrame": "Baseline,after beet juice consumption( 2 hours after consumption)"
                },
                {
                    "measure": "Change in nasal FeNO as measured by using the NIOX MINO\u00ae Airway Inflammation Monitor",
                    "timeFrame": "Baseline,after beet juice consumption(1 hour after consumption)"
                },
                {
                    "measure": "Change in nasal FeNO as measured by using the NIOX MINO\u00ae Airway Inflammation Monitor",
                    "timeFrame": "Baseline,after beet juice consumption(2 hours after consumption)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in ciliary beat frequency as measured using high-speed video-microscopy (HSVM)",
                    "timeFrame": "after beet juice consumption(2 hours after consumption)"
                },
                {
                    "measure": "Change in lung function as measured by spirometry",
                    "description": "spirometry is used to assess how well your lungs work by measuring how much air you inhale,how much you exhale and how quickly you exhale",
                    "timeFrame": "Baseline,after beet juice consumption(2 hours after consumption)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\nExperimental group:\n\n* PCD patients\n\nControl Group:\n\n* patients with no known chronic lung disease\n* 18 years to 99 years\n\nExclusion Criteria(both PCD patients and control group):\n\n* any other pulmonary co-morbidities and diseases entities like cystic fibrosis, nasal sinus surgery, nasal sinus hypoplasia/aplasia, deviated nasal septum, nasal polyps and with upper respiratory tract infection\n* patients allergic to beet\n* a known prolonged bleeding disorder.",
