        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "E-Nose Technology",
                    "type": "EXPERIMENTAL",
                    "description": "Patients will undergo E-Nose testing at baseline (before any treatment is administered) and again approximately when the patient is scheduled for a CT of the Chest and/or PET/CT after initiation of any treatment, as medically permissible and at the PI's discretion. Patients will undergo E-Nose testing in a presurgical or interventional radiology suite.",
                    "interventionNames": [
                        "Diagnostic Test: E-Nose testing",
                        "Diagnostic Test: Research blood"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "E-Nose testing",
                    "description": "At baseline (before any treatment is administered) approximately when the patient is scheduled for a CT of the Chest and/or PET/CT after initiation of any treatment, as medically permissible and at the PI's discretion. A breath sample from each patient will be captured in a cartridge, which will form the basis for the E-Nose analysis, to be performed by study investigators at UCB.",
                    "armGroupLabels": [
                        "E-Nose Technology"
                    ]
                },
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Research blood",
                    "description": "Research blood tests will be obtained at baseline and the first three standard of care follow up visits, scheduled by their treating physician along with a CT of the Chest and/or PET/CT1 to determine whether serum biomarkers of MPM.",
                    "armGroupLabels": [
                        "E-Nose Technology"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change of MPMspecific VOCs",
                    "description": "compared with imaging (using RECIST 1.1 and mRECIST).",
                    "timeFrame": "up to 1 year"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Aged 30-85 years\n* No history of thoracic cancer or extrathoracic cancer potentially involving pleural metastases (i.e., breast, gastric, colon, or pancreas cancer)\n* Recent diagnosis of MPM\n* Documented, signed, and dated informed consent, obtained before any procedures, for the proposed research study, and signed standard surgical consent for surgical resection\n\nExclusion Criteria:\n\n* Aged \\<30 or \\>85 years at the first outpatient visit\n* History of thoracic or extrathoracic cancer that puts the subject at risk for pleural or pulmonary metastases",
