        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Study Drug",
                    "type": "EXPERIMENTAL",
                    "description": "Riociguat (Adempas) 0.5mg to 2.5 mg three time daily - oral medication",
                    "interventionNames": [
                        "Drug: Adempas"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Adempas",
                    "description": "After obtaining baseline data, patients will be placed on oral riociguat at a dose of 0.5 mg TID which will be up titrated over two months to a maximum dose of 2.5 mg TID or maximum tolerated dose. After 3 months from onset of riociguat initiation, the University of California San Diego (UCSD) shortness of breath score, 6 Minute Walk Test (6MWD), and resting/exercise Right Heart Catheterization (RHC) will be repeated.",
                    "armGroupLabels": [
                        "Study Drug"
                    ],
                    "otherNames": [
                        "Riociguat"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in mean pulmonary arterial pressure",
                    "description": "Measurements include mPAP (mean pulmonary arterial pressure) measured by right heart catheterization (RHC)",
                    "timeFrame": "Baseline and 3 months"
                },
                {
                    "measure": "Change in cardiac output",
                    "description": "Measurements include CO (cardiac output) mPAP/CO slope measured by right heart catheterization (RHC)",
                    "timeFrame": "Baseline and 3 months"
                },
                {
                    "measure": "Change in Pulmonary artery compliance",
                    "description": "Measurements include Pulmonary artery compliance measured by right heart catheterization (RHC)",
                    "timeFrame": "Baseline and 3 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in 6 minute walk test",
                    "description": "Measurements include change in 6 minute walk test (6 MWT) following 3 months of study drug",
                    "timeFrame": "Baseline and 3 months"
                },
                {
                    "measure": "Change in New York Heart Association (NYHA) functional class",
                    "description": "Measurements include Change in New York Heart Association (NYHA) functional class following 3 months of study drug",
                    "timeFrame": "Baseline and 3 months"
                },
                {
                    "measure": "Change in the University of California San Diego (UCSD) shortness of Breath Score",
                    "description": "Measurements include: Change in the University of California San Diego (UCSD) shortness of Breath Score following 3 months of study drug",
                    "timeFrame": "Baseline and 3 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Adult patients who underwent PTE or BPA for Chronic thromboembolic pulmonary hypertension (CTEPH) at least six months prior to screening and report ongoing subjective exercise limitation,\n* able to give consent\n* able to perform a exercise protocol\n\nExclusion Criteria:\n\n* Patients with known Residual Pulmonary Hypertension (RPH) by RHC following PTE for CTEPH\n* ongoing PAH-specific vasodilator therapy\n* known contraindication to riociguat\n* a physical limitation to completing an exercise protocol",
