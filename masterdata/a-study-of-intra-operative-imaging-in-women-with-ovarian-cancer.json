        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Arm A",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Randomized to standard technique and assessment of anastomosis without the use of NIR angiography",
                    "interventionNames": [
                        "Diagnostic Test: Endoscopy"
                    ]
                },
                {
                    "label": "Arm B",
                    "type": "EXPERIMENTAL",
                    "description": "Randomized to additional assessment of proximal colonic stump and anastomotic perfusion using NIR angiography",
                    "interventionNames": [
                        "Diagnostic Test: Intravenous Indocyanine Green/ICG injection",
