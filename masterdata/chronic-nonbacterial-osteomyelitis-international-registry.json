        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "The change of CNO disease activity score",
                    "description": "Disease activity score is calculated as the sum of number of clinical lesion count, patient global assessment (0-10), physician global assessment (0-10)",
                    "timeFrame": "3-6 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Total number of CNO lesions on MRI",
                    "description": "The total number of CNO lesions from MRI as previously described (Zhao, et al. J Rheum 2019) will be used to monitor response in a subset",
                    "timeFrame": "3-6 months"
                },
                {
                    "measure": "Safety monitoring",
                    "description": "Serious adverse events including infections that require IV antibiotics, malignancy, hematological, hepatic, dermatological side effects will be reported",
                    "timeFrame": "5 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* - Age at enrollment is equal to or younger than 21 years of age\n* Presence of bone edema on STIR or T2 fat saturation sequence on MRI within 12 weeks of enrollment\n* Whole body imaging evaluation (either WB MRI or bone scintigraphy)\n* Bone biopsy to exclude infection or malignancy unless bone lesions follow typical distribution or there is IBD, psoriasis, or palmar plantar pustulosis\n\nExclusion Criteria:\n\n* - History of or current malignancy\n* Current infectious osteomyelitis\n* Contraindication to the selected treatment agent",
            "healthyVolunteers": false,
            "sex": "ALL",
            "maximumAge": "21 Years",
            "stdAges": [
                "CHILD",
                "ADULT"
            ],
            "studyPopulation": "Children with a diagnosis of CNO/CRMO who have failed NSAIDs and/or with active spinal lesion",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
