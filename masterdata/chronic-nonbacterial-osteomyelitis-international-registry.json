        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "disease modifying anti-rheumatic drug, DMARD",
                    "description": "1. Methotrexate 1 mg/kg (max 25 mg) PO or SQ weekly\n2. Sulfasalazine 30 mg/kg (max 1000 mg) PO twice daily\n3. Leflunomide 10-20 mg PO daily",
                    "interventionNames": [
                        "Drug: Methotrexate",
                        "Drug: Sulfasalazine",
                        "Drug: Leflunomide"
                    ]
                },
                {
                    "label": "tumor necrosis factor inhibitor, TNFi",
                    "description": "1. Adalimumab (subcutaneous) 10-40 mg SQ every other week\n2. Etanercept (subcutaneous) 12.5-50 mg SQ every week\n3. Infliximab (intravenous) 10 mg/kg (max 1000 mg) i.v. at week 0,2, 6 then every 4 weeks\n4. Golimumab (subcutaneous or intravenous) 2 mg/kg (max 200 mg) at every 4 weeks",
                    "interventionNames": [
                        "Drug: Etanercept",
                        "Drug: Adalimumab",
                        "Drug: Certolizumab",
                        "Drug: Infliximab",
                        "Drug: Golimumab"
                    ]
                },
                {
                    "label": "bisphosphonate",
                    "description": "1. Pamidronate 1 mg/kg (max 90 mg) (intravenous)\\*:\n\n   Option 1: every month Option 2: 3 consecutive days every 3 months\n2. Zoledronic acid 0.0125-0.05 mg/kg (max 4mg) (intravenous): every 3-6 months. \\* Both options may use lower dose of 0.5 mg/kg at the initiation of the treatment. All options allow concurrent use of NSAIDs.",
                    "interventionNames": [
                        "Drug: Pamidronate",
                        "Drug: Zoledronic acid"
                    ]
                },
                {
                    "label": "non-steroidal anti-inflammatory drugs",
                    "description": "1. Naproxen 10 mg/kg (max 500 mg) PO twice daily\n2. Indomethacin 1 mg/kg (max daily dose 150 mg) PO twice or three times daily\n3. Meloxicam 0.1-0.3 mg/kg (max 15 mg) PO daily\n4. Piroxicam 10-20 mg PO daily\n5. Ibuprofen 10 mg/kg (max 800 mg) PO 3-4 times daily",
                    "interventionNames": [
                        "Drug: NSAID"
                    ]
                }
            ],
