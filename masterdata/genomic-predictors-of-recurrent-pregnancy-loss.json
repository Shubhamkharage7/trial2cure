        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Determine genetic etiology for Recurrent Pregnancy Loss",
                    "description": "The WGS tool will be used to analyze the POC sample and parental blood samples to determine the clinical reportable genetic cause for RPL",
                    "timeFrame": "5 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "1. Women with loss of a current singleton pregnancy at \\< 20 0/7 weeks gestation (documented by ultrasonography or histopathological examination) and one or more prior pregnancy losses.\n2. Euploid current pregnancy by karyotype or microarray (a limited number of aneuploid losses will be included as part of the pilot)\n3. No history of parental karyotype abnormalities\n4. No history of antiphospholipid antibody syndrome\n5. No evidence of uncontrolled diabetes\n6. No evidence of uncontrolled thyroid disease\n7. No history of autoimmune disease (SLE, RA)\n8. No history of uterine anomalies\n9. No history of cervical insufficiency",
            "healthyVolunteers": true,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "maximumAge": "50 Years",
            "stdAges": [
                "ADULT"
            ],
            "studyPopulation": "Couples who have had two or more previous pregnancy losses and have undiagnosed cause of recurrent pregnancy loss.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
