        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "SCFI",
                    "description": "SCFI participants will complete a survey once per week for three consecutive weeks. They will participate in training for the FoodImage app and deploy it the last two weeks of their study period. During these last two weeks, these participants will also have waste collected on designated days by a waste pickup contractor."
                }
            ]
