        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "QRS duration as measured by 12-lead ECG",
                    "description": "The normal duration (interval) of the QRS complex is 0.08 and 0.10 seconds (80 and 100 ms). When the duration is between 0.10 and 0.12 seconds it is intermediate or slightly prolonged. A QRS duration of greater than 0.12 seconds is considered abnormal.",
                    "timeFrame": "Baseline to 6 months"
                },
                {
                    "measure": "QRS morphology as measured by 12-lead ECG",
                    "description": "Normal QRS width is 70-100 ms (a duration of 110 ms is sometimes observed in healthy subjects). The QRS width is useful in determining the origin of each QRS complex (e.g. sinus, atrial, junctional or ventricular).",
                    "timeFrame": "Baseline to 6 months"
                }
            ]
        },
        "eligibilityModule": {
