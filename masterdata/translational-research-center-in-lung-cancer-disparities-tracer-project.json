            "bioSpec": {
                "retention": "SAMPLES_WITHOUT_DNA",
                "description": "Saliva will be conducted for analyzing biomarkers of stress (i.e. cortisol, alpha-amylase), and biomarkers related to smoking (i.e. cotinine)\n\nUrine will be collected to determine the presence of illicit substances as an indicator of exclusion/inclusion into the study.\n\nPatients that provide consent will have additional saliva and urine collected and stored in a centralized biorepository for future research"
            },
            "enrollmentInfo": {
                "count": 100,
                "type": "ESTIMATED"
            }
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Examining racial differences in acute stress responses",
                    "description": "Examine racial differences in acute stress responses using a validated psychological stress challenge called the Trier Social Stress Test (TSST)",
                    "timeFrame": "Within 1 month following baseline"
                },
                {
                    "measure": "Examining changes in daily cortisol patterns according to race",
                    "description": "Examine racial differences in daily diurnal cortisol slopes to evaluate hypothalamic-pituitary-adrenal-axis dysregulation and acute stress",
                    "timeFrame": "Baseline, 4- and 8 weeks post Trier Social Stress Test laboratory visit"
                },
                {
                    "measure": "Examine changes in stress responses and smoking behaviors as it relates to social determinant factors",
                    "description": "Patients will be administered a survey using validated measures to examine socioeconomic, structural, and psychosocial stressors that impact daily stress and smoking behaviors",
                    "timeFrame": "Baseline, 4- and 8-weeks post Trier Social Stress Test laboratory visit"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* African American and white males\n* Between the ages of 18-75 years old\n* Smoke at least 5 to 10 cigarettes per day\n\nExclusion Criteria:\n\n* Smokers who have a serious cognitive disorder\n* Have a personal history of lung cancer\n* Personal history of usage of illicit drug and alcohol abuse\n* Enrollment in a smoking cessation treatment program during the past 6 months\n* Current use of a nicotine replacement therapy\n* Have any positive responses on the Mini International Neuropsychiatric Interview (MINI) screener",
            "healthyVolunteers": true,
