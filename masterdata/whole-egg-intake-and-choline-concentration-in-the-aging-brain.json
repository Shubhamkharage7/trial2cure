        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Low Egg Consumers",
                    "description": "Participants who consume fewer than 1 whole egg per week"
                },
                {
                    "label": "Moderate Egg Consumers",
                    "description": "Participants who consume 5-9 whole eggs per week"
                },
                {
                    "label": "High Egg Consumers",
                    "description": "Participants who consumes great than or equal to 14 whole eggs per week"
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Association Between Whole Egg Intake and Cerebral Choline",
                    "description": "The investigators will recruit a variety of whole egg consumers. Egg consumption will be validated with a 7-day food record 1 week before the MRI. Cerebral choline levels are measured by magnetic resonance spectroscopy (MRS)",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Association Between Cerebral Choline and Cognition",
                    "description": "The investigators will test the association between cerebral choline as measured by MRS and cognition. Cognition will be measured by the NIH ToolBox cognitive battery.",
                    "timeFrame": "Baseline"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Aged between 60 - 85 years of age\n* Good general health with no concomitant diseases\n* Cognitively normal as defined as Mini-mental state examination (MMSE) score \u2265 25\n* BMI range: 20 - 40 kg/m2\n\nExclusion Criteria:\n\n* Presence of central neurological disease (e.g., brain tumor, stroke, epilepsy, motor neuron disease, multiple sclerosis, Alzheimer's, Parkinson's)\n* Prior major head trauma with loss of consciousness\n* Presence of active unstable and life-threatening illness (e.g., cancer)\n* Presence of major psychiatric disorder within the past 3 years including depression, anxiety, and alcohol (over 3 drinks per day or total of 18 per week) or drug abuse (DSM-IV criteria; Geriatric Depression Score \\> 6)\n* Presence of diabetes mellitus or use of anti-diabetic agents\n* Use of psychoactive and investigational medications\n* Use of hormone replacement therapy\n* Tobacco smokers\n* Subjects with MRI contraindications",
            "healthyVolunteers": true,
            "sex": "ALL",
            "minimumAge": "60 Years",
            "maximumAge": "85 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Participants will be recruited from the greater Kansas City. Recruitment for this research opportunity by email, community flyers, and through the social media platform, Nextdoor. Researchers will also use recruitment databases including the Recruitment and Eligibility Database hosted by the KU Alzheimer's Disease Center (P30 AG035982) and the Pioneers Participant Registry hosted by KU Medical Center and the University of Kansas Hospital. Both of these registries consist of highly characterized, community-dwelling individuals that have given prior consent to be directly contacted for research opportunities.",
