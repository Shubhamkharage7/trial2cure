        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "DermAI and Patch Allergen Testing",
                    "type": "EXPERIMENTAL",
                    "description": "Subjects will have two allergen patches on their forearms for 48 hours and a photograph image of where the patches were placed afterwards for human review and AI predictions of reactions.",
                    "interventionNames": [
                        "Device: AI algorithm to evaluate photographs of skin test patch regions",
                        "Device: Allergen Patch"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "AI algorithm to evaluate photographs of skin test patch regions",
                    "description": "Interpretation of skin test patch regions via artificial intelligence (AI)",
                    "armGroupLabels": [
                        "DermAI and Patch Allergen Testing"
                    ],
                    "otherNames": [
                        "DermAI"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "Allergen Patch",
                    "description": "Contains dime-sized disks which have different substance to which a person may be allergic.",
                    "armGroupLabels": [
                        "DermAI and Patch Allergen Testing"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Classification of test site region reaction",
                    "description": "Number of skin patch interpretations using novel AI algorithms analysis to be in concordant with human review interpretation for the classification of regions within the test panel that either were a reaction (reaction grade 1+) or not (grade 0).",
                    "timeFrame": "1 week"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Classification of test site region using a 5-point scale",
                    "description": "Number of skin patch interpretation using novel AI algorithms analysis to be in concordant with human review interpretation for the classification of the test site region using a 5-point scale: Grades 0 - 4.",
                    "timeFrame": "1 week"
                },
                {
                    "measure": "False Positive Rate",
                    "description": "Number of false positive AI interpretation compared with human review interpretation",
                    "timeFrame": "1 week"
                },
                {
                    "measure": "False Negative Rate",
                    "description": "Number of false negative AI interpretation compared with human review interpretation",
                    "timeFrame": "1 week"
                },
                {
                    "measure": "Early termination of testing",
                    "description": "Number of early termination of testing",
                    "timeFrame": "1 week"
                },
                {
                    "measure": "Participants that removed patches early",
                    "description": "Number of participants that removed patches early",
                    "timeFrame": "1 week"
                },
                {
                    "measure": "Adverse events related to allergen exposure",
                    "description": "Number of adverse events related to allergen exposure",
                    "timeFrame": "1 week"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n- Willing and able to provide informed consent.\n\nExclusion Criteria:\n\n* Has used topical or oral steroids two weeks prior to patch testing.\n* Currently taking immunosuppression agents or is immunocompromised due to medical condition.\n* No sunburn or rash at site of testing.\n* Women who are breastfeeding or pregnant.\n* Treatment with ultraviolet (UV) light (including tanning) during the two weeks prior to visit.\n* Subjects unable to comply with patch test study requirements including multiple return visits and activity restrictions (e.g., protecting patch test area from excess moisture due to showering).",
