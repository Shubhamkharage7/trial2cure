        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Autism Parent Navigators Intervention",
                    "type": "EXPERIMENTAL",
                    "description": "Autism Parent Navigators Intervention is a manualized, in-home, peer support model for parents with a young child recently diagnosed with ASD.",
                    "interventionNames": [
                        "Behavioral: Autism Parent Navigators"
                    ]
                },
                {
                    "label": "Peer mentoring",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Peer mentors will offer an equivalent number of in-home or telephone visits for families, providing non-manualized peer support.",
                    "interventionNames": [
                        "Behavioral: Peer mentoring"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Autism Parent Navigators",
                    "description": "The Autism Parent Navigator program consists of six sessions delivered to parents in the home, supporting adjustment to ASD, accessing services, and coparenting support.",
                    "armGroupLabels": [
                        "Autism Parent Navigators Intervention"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Peer mentoring",
                    "description": "Six in-home or telephone sessions, depending on family uptake, providing non-manualized general support as usual from parent peer mentors regarding adjustment to ASD.",
                    "armGroupLabels": [
                        "Peer mentoring"
                    ]
                }
            ]
        },
