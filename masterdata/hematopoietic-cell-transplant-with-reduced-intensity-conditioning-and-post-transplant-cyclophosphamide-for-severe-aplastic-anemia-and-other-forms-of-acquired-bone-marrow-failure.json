        "descriptionModule": {
            "briefSummary": "A phase II trial of a reduced intensity conditioned (RIC) allogeneic hematopoietic cell transplant (HCT) with post-transplant cyclophosphamide (PTCy) for idiopathic severe aplastic anemia (SAA), paroxysmal nocturnal hemoglobinuria (PNH), acquired pure red cell aplasia (aPRCA), or acquired amegakaryocytic thrombocytopenia (aAT) utilizing population pharmacokinetic (popPK)-guided individual dosing of pre-transplant conditioning and differential dosing of low dose total body irradiation based on age, presence of myelodysplasia and/or clonal hematopoiesis."
        },
        "conditionsModule": {
            "conditions": [
                "Severe Aplastic Anemia",
                "Acquired Amegakaryocytic Thrombocytopenia",
                "Acquired Pure Red Cell Aplasia",
                "Paroxysmal Nocturnal Hemoglobinuria"
            ],
            "keywords": [
                "HCT",
                "RIC",
                "SAA",
                "PTCy",
                "aAT",
                "aPRCA"
            ]
        },
