        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "HIV Disease Progression",
                    "description": "Factors of interest for this outcome include virologic suppression, immune impairment, immune activation, changes in ART, cumulative exposure to specific ART, viral resistance, co-infections, and host genetic polymorphisms. Data will be collected through chart abstraction.",
                    "timeFrame": "Annually for 6 years"
                },
                {
                    "measure": "Metabolic Anomalies",
                    "description": "Data will be collected by chart review, physical assessments, and laboratory evaluations.",
                    "timeFrame": "Annually for 6 years"
                },
                {
