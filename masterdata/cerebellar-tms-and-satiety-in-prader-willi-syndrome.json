        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Active cerebellum rTMS",
                    "type": "EXPERIMENTAL",
                    "description": "Cerebellar targeted iTBS, once daily, one week",
                    "interventionNames": [
                        "Device: Repetitive Transcranial Magnetic Stimulation (rTMS)"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Repetitive Transcranial Magnetic Stimulation (rTMS)",
                    "description": "rTMS is a technique of TMS that allows for selective external manipulation of neural activity in a non-invasive manner. During rTMS a rapidly changing current is passed through an insulated coil placed against the scalp. This generates a temporary magnetic field, which in turn induces an electrical current in neurons and allows for modulation of neural circuitry.\n\nOther Name: iTBS",
                    "armGroupLabels": [
                        "Active cerebellum rTMS"
                    ]
                }
            ]
        },
