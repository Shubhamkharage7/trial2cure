            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Edwards Alterra Adaptive Prestent with SAPIEN 3 Transcatheter Pulmonary Valve System",
                    "description": "The Edwards Alterra Adaptive Prestent is designed to reduce the diameter of large irregular right ventricular outflow tracts (RVOTs) and provide a circular, semi-rigid landing zone to place an Edwards SAPIEN 3 transcatheter heart valve (THV).",
                    "armGroupLabels": [
                        "TPVR"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Acute Device Success",
                    "description": "Acute Device Success, defined as a non-hierarchical composite of:\n\n* Single Alterra deployed in the desired location\n* Single THV implanted in the desired location within Alterra\n* Right ventricle-pulmonary artery peak-to-peak gradient \\< 35 mmHg post-THV implantation\n* Less than moderate total pulmonary regurgitation (PR) by discharge transthoracic echocardiogram\n* Free of Alterra/SAPIEN 3 explant at 24 hours post-implantation",
                    "timeFrame": "24 hours post-procedure"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Native or surgically-repaired RVOT with severe PR\n2. Clinically indicated for pulmonary valve replacement\n3. Planned for treatment with the Alterra prestent and SAPIEN 3 THV\n\nExclusion Criteria:\n\n1. Inability to tolerate an anticoagulation/antiplatelet regimen\n2. Active bacterial endocarditis or other active infections",
            "healthyVolunteers": false,
            "sex": "ALL",
            "stdAges": [
                "CHILD",
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Subjects with severe PR who have a native or surgically-repaired RVOT, are clinically indicated for pulmonary valve replacement, and are planned for treatment with the Alterra prestent and SAPIEN 3 THV",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
