        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Survival to birth",
                    "description": "Number of fetuses diagnosed with alpha thalassemia who survive to birth, compared to number of fetuses diagnosed with alpha thalassemia who have fetal demise or are terminated in utero. This is measured in number of fetuses alive at birth divided by number of all fetuses.",
                    "timeFrame": "6 months"
                },
                {
                    "measure": "Vineland-3 Adaptive Behavior Scale",
                    "description": "Results of neurodevelopmental testing using the Vineland Adaptive Behavior Scale version 3. The Vineland-3 scoring system is based on scores for three specific adaptive behavior domains: Communication, Daily Living Skills, and Socialization. The domain scores are expressed as standard scores with a mean of 100 and standard deviation of 15.",
                    "timeFrame": "10-15 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Gestational age at birth",
                    "description": "Gestational age of the child at birth. This is measured in weeks.",
                    "timeFrame": "6 months"
                },
                {
                    "measure": "Mechanical ventilation",
                    "description": "Duration (if any) of requiring mechanical ventilation after birth. This is measured in days.",
                    "timeFrame": "1 year"
                },
                {
                    "measure": "Length of hospitalization",
                    "description": "Duration of the child's hospitalization after birth. This is measured in days.",
                    "timeFrame": "6 months-1 year"
                },
                {
                    "measure": "Resolution of hydrops",
                    "description": "Evaluate whether receiving fetal therapy leads hydrops fetalis to resolve. This is measured by ultrasound findings.",
                    "timeFrame": "6 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* diagnosis of alpha thalassemia (prenatal or postnatal) with genotype consistent with ATM or BHFS phenotype\n* referred to the University of California, San Francisco Fetal Treatment Center for fetal diagnosis, management and/or evaluation for the ongoing in utero stem cell transplantation clinical trial\n\nExclusion Criteria:\n\n- none",
