                {
                    "type": "COMBINATION_PRODUCT",
                    "name": "Ketamine Infusion plus RO DBT",
                    "description": "The participant will receive 4 weeks of 0.5mg/kg intravenous ketamine given over 40 minutes, as is routinely done in ketamine treatment and four months of RO DBT treatment.",
                    "armGroupLabels": [
                        "Ketamine Infusion",
                        "Radically Open Dialectical Behavior Therapy (RO DBT)"
                    ]
                }
            ]
