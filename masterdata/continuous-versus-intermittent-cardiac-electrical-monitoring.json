        "descriptionModule": {
            "briefSummary": "The purpose of this study is to validate the continuous patch monitoring system to evaluate cardiac arrhythmias in patients receiving drugs that can cause cardiac complications and compare the continuous patch system with standard electrocardiograms (ECGs)."
        },
        "conditionsModule": {
            "conditions": [
                "Torsades de Pointe Caused by Drug",
                "Long QT Syndrome"
            ]
        },
