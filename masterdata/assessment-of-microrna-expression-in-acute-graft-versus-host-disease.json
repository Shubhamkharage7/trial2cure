        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Allogeneic SCT recipients",
                    "description": "Patients who are receiving allogeneic stem cell transplantation at the Ohio State University are eligible and will be consented for the study.",
                    "interventionNames": [
                        "Procedure: Blood samples"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "PROCEDURE",
                    "name": "Blood samples",
                    "description": "2-3 tablespoons of blood weekly for the first 100 days (about 14 weeks) following allogeneic transplant. In addition a blood sample will be obtained before starting the chemotherapy or radiation regimen for the transplant, in the day of the transplant and if your doctor suspects that you have acute graft-versus-host disease.",
                    "armGroupLabels": [
                        "Allogeneic SCT recipients"
                    ],
                    "otherNames": [
                        "peripheral blood",
                        "whole blood"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Measure miR-155 expression levels over time in the serum from patients receiving allogeneic stem cell transplant, and evaluate if miR-155 expression can predict development of aGVHD (Acute Graft-versus-Host Disease)",
                    "timeFrame": "up to 14 weeks"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Compare miR-155 expression at the time of clinical suspicion of aGVHD in allogeneic recipients with aGVHD diagnosis with matched allogeneic recipients without aGVHD.",
                    "timeFrame": "up to 14 weeks"
                },
                {
                    "measure": "Correlate miR-155 expression at the time of clinical suspicion of aGVHD in allogeneic recipients with the severity of aGVHD.",
                    "timeFrame": "up to 15 weeks"
                },
                {
                    "measure": "Perform serum global microRNA analysis in allogeneic recipients at the time of clinical suspicion of aGVHD to identify other miRNAs that are associated with aGVHD diagnosis and severity.",
                    "timeFrame": "up to 15 weeks"
                },
                {
                    "measure": "Collect and store serum/plasma and mononuclear cells from patients receiving allogeneic blood and marrow stem cell transplantation.",
                    "description": "Perform the following: a) validation of the miRNA expression profiling signatures; b) correlation of miRNA expression among serum, plasma and mononuclear cells; and c) correlation of relevant miRNA targets or regulators expression (at the RNA or protein level) with miR-155 and other miRNAs expression identified in the aGVHD miRNA signatures.",
                    "timeFrame": "up to 14 weeks"
                }
            ]
        },
