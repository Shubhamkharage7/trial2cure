        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "IPG Activated",
                    "type": "EXPERIMENTAL",
                    "description": "The group of participants who have had a successful trial (\\>50% pain relief) during the trial phase",
                    "interventionNames": [
                        "Device: Spinal Cord Stimulator (SCS)"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Spinal Cord Stimulator (SCS)",
                    "description": "Spinal cord stimulation system (SCS) is an approved system to manage chronic intractable pain of the limbs and/or leg by delivering electrical stimulation using implantable leads and rechargeable implantable pulse generator (IPG) with 16 output channels. The IPG is implanted in a subcutaneous pocket and is capable of stimulating the spinal cord nerves when used with one or two 8-contact percutaneous or paddle leads.",
                    "armGroupLabels": [
                        "IPG Activated"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Trial success rate/Responder rate",
                    "description": "Percentage of patients with a successful trial phase (responder rate) i.e. at least 50% reduction in patient-reported overall pain relief.",
                    "timeFrame": "2 weeks"
                },
                {
                    "measure": "Patient-reported overall pain relief",
                    "description": "The average percentage of patient-reported pain relief at 3,6 and 12 months.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Leg pain",
                    "description": "Leg pain will be assessed using a 10 cm Visual Analog Scale (VAS) with 0 indicating no pain and 10 indicating the worst pain. Following outcome measures will be derived from the VAS score on leg pain.\n\n* Responder rate for leg pain i.e. percentage of patients who experience at least 50% reduction in leg pain compared to Baseline at 3, 6, and 12 months.\n* Change from Baseline in mean leg pain at 3, 6, and 12 months\n* Percentage change from Baseline in mean leg pain at 3, 6, and 12 months",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Quality of life measure",
                    "description": "Change from baseline in quality of life at 3, 6 and 12 months will be assessed using the EQ-5D-5L questionnaire.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Pain Inventory",
                    "description": "Change from baseline in pain symptoms at 3, 6 and 12 months assessed by 11-point BPI-DPN (Brief Pain Inventory for Diabetic Peripheral Neuropathy) pain scale, with 0 indicating no pain and 10 indicating worst pain.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Global impression of change in health status",
                    "description": "The patient's general health status at 3, 6 and 12 months will be assessed both by the patients themselves as well as the study investigators using the 7-point PGIC (Patient Global Impression of Change) and the CGIC (Clinician Global Impression of Change) instrument respectively. Responses for this questionnaire range from \"no change (or condition has got worse)\" to \"a great deal better\".",
                    "timeFrame": "12 months"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Patients Work Status",
                    "description": "Patients' current work status (working for pay, working without pay, self-employed, out of work for more than 1 year, out of work for less than 1 year, homemaker/taking care of the house and/or family, student, retired, unable to work) will be collected at baseline and repeated at the 12 months follow up visit.",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Device Safety",
                    "description": "Subjects will be assessed for the procedure and device-related adverse events starting at enrollment and continuing through study completion. An adverse event may include but is not limited to infection, lead revisions, IPG revisions, device malfunction, and explantation of devices.",
                    "timeFrame": "12 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\nTo participate in the study, patients must meet all the following inclusion criteria:\n\n1. Have been clinically diagnosed with diabetes, according to the local country diabetes association guidelines, as well as painful diabetic neuropathy (PDN) of the lower limbs refractory to conventional medical management.\n2. Average pain intensity (over the last 7 days) of \u22655 out of 10 cm on the Visual Analog Scale (VAS) in the lower limbs at enrollment/baseline.\n3. The clinical decision has been made to provide treatment using the Nevro Spinal Cord Stimulation that includes 10 kHz therapy prior to enrollment in the study.\n4. Be willing and capable of giving written informed consent.\n5. Be willing and able to comply with study-related requirements and procedures and attend all scheduled visits.\n\nExclusion Criteria:\n\nTo participate in the study, patients must not meet any of the following exclusion criteria:\n\n1. Have a diagnosis of a lower limb mononeuropathy (e.g., causalgia and tibial or peroneal neuropathies), have had a lower limb amputation other than toes, or have large (\u22653 cm) and/or gangrenous ulcers of the lower limbs\n2. Have a medical condition or diagnosis that is inconsistent with Nevro's SCS System guidelines in the Physician's Manual for the relevant country, or as per standard clinical practice.\n3. Have a medical condition or pain in other areas, not intended to be treated in this study, that could interfere with study procedures, accurate pain reporting, and/or confound the evaluation of study endpoints, as determined by the Investigator (such as primary headache, fibromyalgia, post-herpetic neuralgia, osteoarthritis, peripheral vascular disease, or small vessel disease).",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
