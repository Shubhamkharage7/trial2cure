        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Sham Stimulation",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "With the coil directed away from the patient, 600 pulses of TcMS will be delivered at minimal energy output.",
                    "interventionNames": [
                        "Device: Magnetic Stimulation"
                    ]
                },
                {
                    "label": "Low Frequency TcMS",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "With the coil directed at the stellate ganglion, 60 minutes of 1Hz stimulation will be delivered targeting the stellate ganglion.",
                    "interventionNames": [
                        "Device: Magnetic Stimulation"
                    ]
                },
                {
                    "label": "Theta Burst Stimulation TcMS",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "With the coil directed at the stellate ganglion, 600 pulses of continuous theta burst stimulation will be delivered targeting the stellate ganglion.",
                    "interventionNames": [
                        "Device: Magnetic Stimulation"
                    ]
                }
            ],
