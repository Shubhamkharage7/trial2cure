            "studyType": "OBSERVATIONAL",
            "patientRegistry": false,
            "designInfo": {
                "observationalModel": "COHORT",
                "timePerspective": "PROSPECTIVE"
            },
            "bioSpec": {
                "retention": "SAMPLES_WITH_DNA",
                "description": "Blood will be collected at pre-specified baseline, on-treatment and post-treatment timepoints. Left-over tissue may be collected from standard of care procedures depending on the indication."
            },
            "enrollmentInfo": {
                "count": 500,
                "type": "ESTIMATED"
            }
        },
        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Cohort 001: KRAS mutated CRC",
                    "description": "No intervention."
                },
                {
                    "label": "Cohort 002: KRAS mutated PDAC",
                    "description": "No intervention."
                },
                {
                    "label": "Cohort 003: KRAS mutated NSCLC",
