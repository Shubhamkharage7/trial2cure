        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Event rate of all cause death",
                    "timeFrame": "1 year"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Event rate of cardiovascular death",
                    "description": "A. Death due to proximate cardiac cause B. Death caused by non-coronary vascular condition C. All procedure-related death D. All valve-related death E. Sudden or unwitnessed death F. Death of unknown cause",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Event rate of myocardial infarction",
                    "description": "according to Valve Academic Research Consortium (VARC) criteria",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Event rate of cerebrovascular accident",
                    "description": "stroke and TIA (Transient Ischemic Attack)",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Event rate of bleeding",
                    "description": "according to Valve Academic Research Consortium (VARC) criteria",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Event rate of Vascular access site and access-related complication",
                    "description": "according to Valve Academic Research Consortium (VARC) criteria",
                    "timeFrame": "30 days"
                },
                {
                    "measure": "Event rate of Acute kidney injury",
                    "timeFrame": "30 days"
                },
                {
                    "measure": "Event rate of Permanent pacemaker insertion",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Event rate of Other TAVR-related complication",
                    "description": "A. Conversion to open surgery B. Coronary obstruction C. Mitral valve apparatus damage or dysfunction D. Cardiac tamponade E. Endocarditis F. Valve thrombosis G. Valve mal-positioning H. TAV-in-TAV deployment",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Event rate of Prosthetic valve dysfunction",
                    "description": "according to Valve Academic Research Consortium (VARC) criteria\n\nA. Prosthetic aortic valve stenosis B. Prosthesis-patient mismatch C. Prosthetic aortic valve regurgitation",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Event rate of Composite endpoint",
                    "description": "A. Device success\n\nB. Early safety\n\n: death, cerebrovascular event, fatal bleeding, acute kidney injury, coronary obstruction requiring intervention, major vascular complication, valve dysfunction requiring intervention\n\nC. Clinical efficacy : death, cerebrovascular event, valve-related symptom requiring hospitalization or devastating heart failure, NYHA class\\* III, IV dyspnea, valve dysfunction\n\n\\*the New York Heart Association (NYHA) Functional Classification",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "Event rate of Structural valve deterioration",
                    "timeFrame": "10 years"
                },
                {
                    "measure": "NYHA class",
                    "timeFrame": "30 days"
                },
                {
                    "measure": "NYHA class",
                    "timeFrame": "1 year"
                },
                {
                    "measure": "Valve area",
                    "timeFrame": "1 year"
                },
                {
                    "measure": "Event rate of free from atrial fibrillation",
                    "timeFrame": "10 years"
                }
            ]
        },
