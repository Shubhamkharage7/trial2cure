        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Cohort 1",
                    "type": "EXPERIMENTAL",
                    "description": "Participants in this cohort will receive glipizide for up to 4 months and participate in continuous glucose monitoring for as long as they are receiving the drug.",
                    "interventionNames": [
                        "Drug: Glipizide"
                    ]
                },
                {
                    "label": "Cohort 2a",
                    "type": "NO_INTERVENTION",
                    "description": "Participants in this cohort will include people who have received various types of treatment for their hyperglycemia and pancreatic cancer. People's medical records will be reviewed to compare the effects of glipizide with the effects of other standard medications used to treat hyperglycemia."
                },
                {
                    "label": "Cohort 2b",
                    "type": "NO_INTERVENTION",
                    "description": "Participants in this cohort will complete a questionnaire about their use of hyperglycemia medications. Participants from Cohort 2a will take part in this group."
                }
            ],
