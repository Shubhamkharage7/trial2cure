        },
        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Moderate Renal Dysfunction",
                    "type": "EXPERIMENTAL",
                    "description": "Moderate renal dysfunction will receive a 20% dose reduction of fludarabine and no dose reduction for cyclophosphamide.",
                    "interventionNames": [
                        "Drug: Fludarabine",
                        "Drug: Cyclophosphamide"
                    ]
                },
                {
                    "label": "Severe Renal Dysfunction",
                    "type": "EXPERIMENTAL",
                    "description": "Several renal dysfunction will receive a 40% dose reduction of fludarabine and no dose reduction for cyclophosphamide.",
                    "interventionNames": [
                        "Drug: Fludarabine",
                        "Drug: Cyclophosphamide"
                    ]
                },
                {
                    "label": "Dialysis Participants",
                    "type": "EXPERIMENTAL",
                    "description": "Participants on dialysis will receive a 50% dose reduction of fludarabine and a 25% dose reduction of cyclophosphamide.",
                    "interventionNames": [
                        "Drug: Fludarabine",
                        "Drug: Cyclophosphamide"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Fludarabine",
                    "description": "Received IV on Days -5 to -3 before CAR T cell therapy",
                    "armGroupLabels": [
                        "Dialysis Participants",
                        "Moderate Renal Dysfunction",
                        "Severe Renal Dysfunction"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Cyclophosphamide",
                    "description": "Received IV on Days -5 to -3 before CAR T cell therapy",
                    "armGroupLabels": [
                        "Dialysis Participants",
                        "Moderate Renal Dysfunction",
                        "Severe Renal Dysfunction"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Occurrence of CRS",
                    "description": "Collection of maximum grade of CRS (based on CTCAE v5) and when this event occurs post CAR T cell therapy infusion",
                    "timeFrame": "90 days"
                },
                {
                    "measure": "Occurrence of ICANS",
                    "description": "Collection of maximum grade of ICANs (based on CTCAE v5) and when this event occurs after CAR T cell therapy infusion",
                    "timeFrame": "90 days"
                },
                {
                    "measure": "Occurrence of Cytopenias",
                    "description": "Collection of grade 3 or higher (based on CTCAE v5) cytopenias not resolved by Day 30 post CAR T cell infusion",
                    "timeFrame": "90 days"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Receiving lymphodepleting chemotherapy prior to commercial CAR-T administration for multiple myeloma, leukemia, or lymphoma\n* Adequate bone marrow function to receive lymphodepleting chemotherapy\n* Renal function \\</= 60mL/min/1.73m2\n* ECOG 0-2\n\nExclusion Criteria:\n\n* Relative CNS disorders\n* Active uncontrolled infection or any other concurrent disease or medical condition that was deemed to interfere with the conduct of the study as judged by the investigator\n* Use of therapeutic dose systemic corticosteroids (defined as \\>20mg/day prednisone or equivalent) within 72 hours of CAR-T administration",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
