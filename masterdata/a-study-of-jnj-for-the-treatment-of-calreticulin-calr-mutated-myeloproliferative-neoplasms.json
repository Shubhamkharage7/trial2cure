        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Dose Escalation (Part 1) and Dose Expansion (Part 2)",
                    "type": "EXPERIMENTAL",
                    "description": "In dose escalation (Part 1), participants will receive JNJ-88549968. The dose will be escalated sequentially to determine the recommended phase 2 dose (RP2D) and optimal dosing schedule(s) based on safety, pharmacokinetic, pharmacodynamic, and preliminary assessment of efficacy across several dose regimens. In dose expansion (Part 2), participants will receive JNJ-88549968 at the RP2D regimen(s) determined in dose escalation (Part 1).",
                    "interventionNames": [
                        "Drug: JNJ-88549968"
                    ]
                }
            ],
