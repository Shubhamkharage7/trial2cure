        "descriptionModule": {
            "briefSummary": "The primary aims of this mixed-methods trial are to test the feasibility and acceptability of the novel Cognitive Strategies, Mindfulness, and Rehabilitation Therapy (C-SMART) delivered via telehealth to patients with primary brain tumors and mild neurocognitive disorder (mNCD)."
        },
        "conditionsModule": {
            "conditions": [
                "Glioma, Mixed",
                "Mild Neurocognitive Disorder",
                "Brain Tumor"
            ],
            "keywords": [
                "Mindfulness",
                "Cognitive rehabilitation"
            ]
        },
