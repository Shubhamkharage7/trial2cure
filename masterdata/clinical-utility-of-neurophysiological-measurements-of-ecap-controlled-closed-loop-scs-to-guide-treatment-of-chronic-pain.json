            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Evoke Spinal Cord Stimulation (SCS) System",
                    "description": "A spinal cord stimulation (SCS) system that measures and records evoked compound action potentials (ECAPs) and automatically adjusts the stimulation current to maintain a consistent ECAP amplitude.",
