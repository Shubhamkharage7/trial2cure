            "interventions": [
                {
                    "type": "PROCEDURE",
                    "name": "Diagnostic MRI",
                    "description": "Participants will be scanned using a standard clinical prostate MRI protocol.",
                    "armGroupLabels": [
                        "Interventional Arm"
                    ]
                },
                {
                    "type": "PROCEDURE",
                    "name": "Diagnostic Prostate Biopsy",
                    "description": "Participants will undergo a prostate biopsy as their standard of care treatment. The provider performing biopsy will select areas to be biopsies as part of normal clinical judgment.",
                    "armGroupLabels": [
                        "Interventional Arm"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "Risk Map DSS tool",
                    "description": "The Risk Map DSS tool will select up to 2 additional areas to be biopsied based on analysis of MRI images. The clinical provider will make final decision on areas to be biopsied.",
                    "armGroupLabels": [
                        "Interventional Arm"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Rate of biopsy samples positive for prostate cancer versus those that are negative as assessed by receiver operating characteristic curve",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Rate of biopsy samples positive for prostate cancer versus those that are negative as assessed by area under the curve",
                    "timeFrame": "12 months"
                }
            ]
