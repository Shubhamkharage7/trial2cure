        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change of satiety",
                    "description": "Change of hunger feeling (appetite) will be examined using visual analog scale (0-10): 0 least and 10 greatest",
                    "timeFrame": "0, 20, 40, 60, 90 and 120 minutes post snack consumption"
                },
                {
                    "measure": "Change of glucose level",
                    "description": "Change of blood glucose response will be determined to examine effects of mango consumption vs low fat cookie consumption",
                    "timeFrame": "0, 30, 60, 90 and 120 minutes post snack consumption; baseline and week 12"
                },
                {
                    "measure": "Change of insulin level",
                    "description": "Change of blood insulin level will be determined to examine effects of mango consumption vs low fat cookie consumption",
                    "timeFrame": "0 and 60 minutes post snack consumption, baseline and week 12"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change of microbiome",
                    "description": "Change of microbiome diversity will be determined to examine effects of mango consumption vs low fat cookie consumption",
                    "timeFrame": "Baseline, week 4 and week12"
                }
            ]
        },
