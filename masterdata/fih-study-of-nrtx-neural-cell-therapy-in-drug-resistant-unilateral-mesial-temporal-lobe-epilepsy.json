        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Frequency of serious or severe AEs",
                    "description": "The primary endpoint of the study is the frequency of serious or severe AEs over the period of 1 year after administration, comparing all subjects treated with NRTX-1001 (Stage 1 + Stage 2) with all subjects who receive sham treatment.",
                    "timeFrame": "1 year"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in seizure frequency",
                    "description": "The difference in the median percentage change from baseline in seizure frequency (seizures per 28 days) between subjects that receive NRTX-1001 and those who receive sham treatment, assessed in Stage 2.",
                    "timeFrame": "1 year"
                },
                {
                    "measure": "Responder rate",
                    "description": "The difference in the 75% responder rate, e.g., the difference between the fraction of subjects who receive NRTX-1001 and those that receive sham treatment who experience at least a 75% reduction from baseline in seizure frequency, assessed in Stage 2.",
                    "timeFrame": "1 year"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Key Inclusion Criteria:\n\n1. Male or Female, age \u226518 to 65\n2. Focal seizures, clinically defined as TLE\n3. Has failed to achieve seizure control despite adequate trials of at least 2 ASDs at appropriate doses\n4. Currently on stable doses (at least 1 month) of approved ASDs\n5. Single seizure focus confirmed as within one temporal lobe\n6. For subjects entering Stage 1, the seizure focus is either a) in the non-dominant hemisphere, or b) in the dominant hemisphere and the subject has a Rey Auditory Verbal Learning Test (RAVLT) assessed within one year of screening, or at the screening visit, that is at least 1.5 standard deviations below the population mean.\n7. Seizure frequency averages \u22652 per 28-day period over the 6 months prior to screening.\n\nKey Exclusion Criteria:\n\n1. Epilepsy due to other and/or progressive neurologic disease\n2. Significant other medical condition which would impair safe participation\n3. Primary or secondary immunodeficiency\n4. Suicide attempt in the past year\n5. Severe psychiatric disorders\n6. Chronic indwelling intracranial device\n7. MRI indicating potential malignant lesion\n8. Pregnancy, or currently breastfeeding",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "maximumAge": "65 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
