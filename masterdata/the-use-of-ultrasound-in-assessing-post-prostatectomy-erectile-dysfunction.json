        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Ultrasound viscoelastography (UVE) in radical proctectomy recovery",
                    "type": "EXPERIMENTAL",
                    "description": "Subjects undergoing radical prostatectomy for prostate cancer disease as standard of care will have a ultrasound vibroelastography performed before surgery, 3 months, 6 months and 9 months post-prostatectomy.",
                    "interventionNames": [
                        "Diagnostic Test: Ulrasound Vibroelastography (UVE)"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Ulrasound Vibroelastography (UVE)",
                    "description": "Ultrasound technique to quantitatively assess tissue stiffness (elasticity and viscosity) by applying vibration through a specialized probe (indenter).",
                    "armGroupLabels": [
                        "Ultrasound viscoelastography (UVE) in radical proctectomy recovery"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in Viscoelasticity",
                    "description": "As measured by Ultrasound Vibroelastography (UVE)",
                    "timeFrame": "Baseline, 3 months, 6 months and 9 months post-prostatectomy"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion criteria include:\n\n* Age \\> 40 years\n* Clinically localized prostate cancer (American Urological Association Grade Groups 1-2; cT1c or cT2a-b; PSA \\< 10) without clinic or imaging evidence of localized extra-prostatic or metastatic disease (i.e. AUA low and favorable intermediate risk prostate cancer)\n* International Index of Erectile Function (IIEF) of \u2265 21 points at baseline (no or mild erectile dysfunction)\n* Patient-expressed interest in consultation for sexual function (erectile function) preservation/ optimization\n* Planned bilateral nerves-paring prostatectomy\n\nExclusion criteria include:\n\n* Moderate or severe ED based in IIEF criteria (score \\< 21)\n* History of prior pelvic or penile surgery\n* Current or prior androgen deprivation therapy\n* Planned non-nerve sparing prostatectomy",
