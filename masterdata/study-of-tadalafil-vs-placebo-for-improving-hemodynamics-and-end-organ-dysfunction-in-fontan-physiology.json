        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Tadalafil Group",
                    "type": "EXPERIMENTAL",
                    "description": "Study participants will receive 10 mg of Tadalafil daily for 1 week, then 20 mg daily for 1 week, and finally 40 mg daily for 50 weeks for a total therapy time of 52 weeks. Tadalafil will be taken orally in capsule form once daily.",
                    "interventionNames": [
                        "Drug: Tadalafil"
                    ]
                },
                {
                    "label": "Placebo Group",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "Study participants will receive a placebo capsule that looks identical to the Tadalafil capsule. The placebo will be taken orally once daily for 52 weeks.",
                    "interventionNames": [
                        "Drug: Placebo"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Tadalafil",
                    "description": "10 mg once daily orally week 1, 20 mg once daily orally during week 2, and 40 mg once daily orally during weeks 3 through 52",
                    "armGroupLabels": [
                        "Tadalafil Group"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Placebo",
                    "description": "Looks exactly like the study drug, but it contains no active ingredient",
                    "armGroupLabels": [
                        "Placebo Group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Microvascular endothelial function",
                    "description": "Measured by the reactive hyperemic index using peripheral artery tonometry (EndoPAT)",
                    "timeFrame": "52 weeks"
                },
                {
                    "measure": "Pulmonary vascular reserve",
                    "description": "The slope of mean pulmonary artery pressure / cardiac output (mPAP/CO)",
                    "timeFrame": "52 weeks"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Liver stiffness",
                    "description": "Liver magnetic resonance elastography (MRE) will be used to assess liver stiffness expressed in Pascals (Pa)",
                    "timeFrame": "52 weeks"
                },
                {
                    "measure": "Renal function",
                    "description": "Measured by the glomerular filtration rate",
                    "timeFrame": "52 weeks"
                },
                {
                    "measure": "Exercise capacity",
                    "description": "Assessed using peak VO2 (oxygen consumption) obtained from an upright treadmill or supine bike cardiopulmonary exercise test with echocardiogram",
                    "timeFrame": "52 weeks"
                },
                {
                    "measure": "Patient reported quality of life",
                    "description": "Measured by the Minnesota Living with Heart Failure Questionnaire that asks subjects to rate how much their heart failure (heart condition) has affected their life during the past month (4 weeks) on a scale of 0=no and 5=very much.",
                    "timeFrame": "52 weeks"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Patients \u2265 18 years old.\n* Have previously undergone a Fontan Palliation.\n* Able to exercise using a supine bike.\n* Ability and willingness to provide written consent.\n* Undergoing a clinically indicated Cardiac Catheterization\n\nExclusion Criteria:\n\n* Patients \\< 18 years old.\n* Current intravenous inotropic drugs.\n* Current use of alpha-blockers, pulmonary vasodilators, or nitrates.\n* Unable to exercise.\n* Pregnancy or lactating.\n* Unable or unwilling to consent.",
