        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Changes from baseline in Vh:Cd as assessed by esophagogastroduodenoscopy with biopsy after 2-week gluten challenge (GC)",
                    "description": "Vh:Cd is the ratio of villous height to crypt depth, a histological assessment",
                    "timeFrame": "29 days"
                }
            ],
            "secondaryOutcomes": [
                {
