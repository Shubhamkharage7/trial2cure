            "armGroups": [
                {
                    "label": "INSTACARE arm",
                    "type": "EXPERIMENTAL",
                    "description": "Participants in this arm will receive the INSTACARE intervention for three months.",
                    "interventionNames": [
                        "Behavioral: INSTACARE"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "INSTACARE",
                    "description": "CHWs will provide participants with support via phone calls, home-visits, and accompaniment to clinic visits to improve medication management and HIV viral suppression. CHWs will contact participants at least once per week for three months.",
                    "armGroupLabels": [
                        "INSTACARE arm"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Communication Preferences",
                    "description": "Communication preferences will be reported as the number of participants who prefer any of the following intervention modes of administration: text messaging, phone calls, home visits, accompaniment to clinic visits, utilization of existing health portals and phone applications. Data will be collected via survey administered to participants by CHW.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Number of CHW contacts",
                    "description": "CHW contacts with participants will be extracted from CHW tracking logs.",
                    "timeFrame": "3 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Ages 18 years or older\n* patient from adult HIV outpatient clinic at University of Miami/Jackson Memorial Health System\n* self-identify as Black\n* uncontrolled viral suppression as determined by treating clinician\n* English speaking\n\nExclusion Criteria:\n\n* Any other condition(s) that the treating clinician determines would limit participant from meaningful study participation.",
