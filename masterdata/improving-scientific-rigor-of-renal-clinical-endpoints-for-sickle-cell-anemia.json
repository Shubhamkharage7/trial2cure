        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "To develop a novel SCA-specific eGFR equation and compare with existing eGFR equations validated for the general population",
                    "description": "The investigators will use biologically relevant variables to develop a new equation for eGFR in pediatric and adult sickle cell patients",
                    "timeFrame": "5 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "To determine the longitudinal concordance between pediatric and adult novel and standard eGFR equations with mGFR and variables that may alter longitudinal inferences.",
                    "description": "To compare the concordance between eGFR and mGFR from baseline to one year",
                    "timeFrame": "5 years"
                },
                {
                    "measure": "To test the correlation of the novel mGFR method using VFI FAST and iohexol mGFR.",
                    "description": "To perform a 3 hour mGFR and compare this mGFR to the 6 hour mGFR (iohexol)",
                    "timeFrame": "5 years"
                }
            ]
        },
