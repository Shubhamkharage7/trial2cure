        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Pulse CO-Oximetry Technology",
                    "description": "In addition to the standard pulse oximeter used for clinical care, up to six SpHb finger sensors will be applied to the patient. Additional sensors may also be applied to the forehead, neck, chest and ears.\n\nMasimo FDA cleared pulse CO-Oximeter devices: Radical-7, Rad-87, Rad-57, Pronto, Pronto-7, for the noninvasive measurement of SpO2, PR, PI, PVI, SpCO, SpMet, and SpHb, total hemoglobin and respiratory rate.\n\nNo treatment decisions will be made based on the new technology and no extra medical procedures are required for the study."
                },
                {
                    "type": "OTHER",
                    "name": "Blood draw",
                    "description": "4mL arterial blood samples (up to 1 per hour), outside standard of care, are drawn throughout the surgical procedure and sent to UCDMC lab/Masimo lab for analysis."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Comparison of Masimo Rainbow technology to arterial blood samples taken during surgery",
                    "description": "Correlation between standard, lab-based, hemoglobin measurement and concurrent SpHb.",
                    "timeFrame": "Five year"
                }
            ]
        },
