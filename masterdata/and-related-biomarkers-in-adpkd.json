        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "NOX4 levels",
                    "description": "Determination of NOX4 levels from urine and plasma samples",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "mtDNA copy number",
                    "description": "Determination of mtDNA copy number from urine and plasma samples",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Tricarboxylic Acid (TCA) cycle metabolites",
                    "description": "Determination of TCA cycle metabolite concentration from urine and plasma samples",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "REDOX status",
                    "description": "Determination of REDOX from urine and plasma samples",
                    "timeFrame": "Baseline"
                },
                {
                    "measure": "Total kidney volume (TKV)",
                    "description": "Determined by MRI",
                    "timeFrame": "Baseline"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Estimated Glomerular Filtration Rate (eGFR)",
                    "description": "Estimated by CKD-epi equation",
                    "timeFrame": "Baseline"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* ADPKD (based on Ravine et al. criteria)\n* Class 1 A-E according to imaging classification\n* Male and female subjects 15 - 40 years of age\n* Estimated GFR\\> 70 mL/min/1.73 m2 (CKD-EPI)\n* Ability to provide written, informed consent\n\nExclusion Criteria:\n\n* Class 2 according to imaging classification\n* Concomitant systemic disease affecting the kidney\n* Diabetes mellitus\n* Predicted urine protein excretion in \\>1 g/24 hrs\n* Use of antioxidants i.e. vitamins, Nrf2 activators\n* Abnormal urinalysis",
            "sex": "ALL",
            "minimumAge": "15 Years",
            "maximumAge": "40 Years",
            "stdAges": [
                "CHILD",
                "ADULT"
            ],
            "studyPopulation": "Male and female patients with a previous diagnosis of ADPKD",
