        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Personality-based treatment 2 week baseline",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will receive treatment tailored to 1 of 3 personality dimensions after a 2 week baseline assessment",
