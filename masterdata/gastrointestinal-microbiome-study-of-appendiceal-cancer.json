        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Microbiome cohort",
                    "type": "OTHER",
                    "description": "1. Pre-operative (baseline) bio-specimen collection (fecal sample) and completion of two food and lifestyle questionnaires.\n2. Six to twelve weeks following cytoreductive surgery with hyperthermic intraperitoneal chemotherapy (CRS/HIPEC), post-operative bio-specimen collection (fecal sample) and completion of two food and lifestyle questionnaires\n3. Six to twelve months following cytoreductive surgery with hyperthermic intraperitoneal chemotherapy (CRS/HIPEC), post-operative bio-specimen collection (fecal sample) and completion of two food and lifestyle questionnaires",
                    "interventionNames": [
                        "Other: Bio-specimen collection",
                        "Other: Questionnaire"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "OTHER",
                    "name": "Bio-specimen collection",
                    "description": "Collection of bio-specimens (fecal sample) at 3 different time points One pre-operative collection, Two post-operative collections",
                    "armGroupLabels": [
                        "Microbiome cohort"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Questionnaire",
                    "description": "Completion of lifestyle and food questionnaires (2) at each bio-specimen collection timepoint",
                    "armGroupLabels": [
                        "Microbiome cohort"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Determine whether the intestinal microbiome of patients with appendiceal tumors with peritoneal carcinomatosis differs from that of a healthy, age-matched cohort",
                    "description": "Appendiceal cohort to healthy age-matched cohort",
                    "timeFrame": "36 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Compare the gut microbiome of appendiceal tumors by histopathology",
                    "description": "Low-grade and high-grade tumors",
                    "timeFrame": "36 months"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Identify potential biomarkers associated with appendiceal tumors",
                    "description": "Molecular profile",
                    "timeFrame": "36 months"
                },
                {
                    "measure": "Identify alterations in the gut microbiome post-CRS/HIPEC",
                    "description": "Compare pre-HIPEC and post-HIPEC samples",
                    "timeFrame": "36 months"
                }
            ]
        },
        "eligibilityModule": {
