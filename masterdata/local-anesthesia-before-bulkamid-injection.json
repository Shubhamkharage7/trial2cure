            "armGroups": [
                {
                    "label": "Topical anesthesia alone",
                    "type": "EXPERIMENTAL",
                    "description": "2.5% EMLA cream + intraurethral 2% lidocaine gel for 15 min",
                    "interventionNames": [
                        "Procedure: Topical lidocaine",
                        "Procedure: EMLA cream"
                    ]
                },
                {
                    "label": "Topical anesthesia with periurethral block",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "2.5% EMLA cream + intraurethral 2% lidocaine gel for 15 min\n\n+ periurethral block (5 cc 1% lidocaine or 0.25% bupivacaine at 3 and 9 o'clock to depth of 1.5 cm using 25G needle)\n\nBlock sits for 5 min if lidocaine, 10 min if bupivacaine",
                    "interventionNames": [
                        "Procedure: Topical lidocaine",
                        "Procedure: EMLA cream",
                        "Procedure: Periurethral block"
                    ]
                }
