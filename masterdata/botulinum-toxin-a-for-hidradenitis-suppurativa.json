        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Experimental",
                    "type": "EXPERIMENTAL",
                    "description": "During their treatment visit, subjects will receive Botulinum Toxin-A in one axillae and normal saline in the other, in a double-blinded fashion.",
                    "interventionNames": [
                        "Drug: Botulinum toxin type A"
                    ]
                },
                {
                    "label": "Placebo Comparator",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "At three months, Group 1 subjects will receive 50-units BTX-A and NS in the same axillae as before while Group 2 will receive NS in both axillae.",
                    "interventionNames": [
                        "Drug: Normal saline"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Normal saline",
                    "description": "Thirty subjects with active bilateral axillary hidradenitis suppurativa will be consecutively enrolled and randomly stratified into two groups (Group 1 and Group 2). Both groups will contain five subjects each with mild, moderate, and severe disease. All participants will have 50-units of Botulinum Toxin-A (BTX-A) injected intradermally in one axilla and normal saline (NS) placebo administered in the other in a randomized, double-blinded fashion. At three months, Group 1 will receive 50-units BTX-A and NS in the same axillae as before while Group 2 will receive NS in both axillae.",
                    "armGroupLabels": [
                        "Placebo Comparator"
                    ],
                    "otherNames": [
                        "Placebo"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Botulinum toxin type A",
                    "description": "Thirty subjects with active bilateral axillary hidradenitis suppurativa will be consecutively enrolled and randomly stratified into two groups (Group 1 and Group 2). Both groups will contain five subjects each with mild, moderate, and severe disease. All participants will have 50-units of Botulinum Toxin-A (BTX-A) injected intradermally in one axilla and normal saline (NS) placebo administered in the other in a randomized, double-blinded fashion. At three months, Group 1 will receive 50-units BTX-A and NS in the same axillae as before while Group 2 will receive NS in both axillae.",
                    "armGroupLabels": [
                        "Experimental"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in Number of lesions",
                    "description": "Change in number of lesions (nodules, abscesses, and fistulous tracts). Any changes in lesion count or size will be documented and subjective symptoms such as pain will be noted.",
                    "timeFrame": "baseline and 3 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in Hidradenitis Suppurativa Quality of Life (HiSQOL) score",
                    "description": "HiSQOL is a patient-reported outcome measure developed for clinical trials to address disease-specific changes in HR quality of life (HRQOL). The item scores are summed to create a total ranging from 0 to 68, with higher scores indicating more severe impact on HRQOL.",
                    "timeFrame": "baseline, 3 months and 6 months"
                },
                {
                    "measure": "Change in Duration of Effect of BTX-A in Hidradenitis Suppurativa",
                    "description": "Subjects will be clinically evaluated at 3, 6, and 12 months to monitor qualitative and quantitative change in lesions (nodules, abscesses, fistulous tracts) and symptoms (pain, drainage, swelling, and impairment) and duration of change, if any.",
                    "timeFrame": "3 months, 6 months, 12 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Subjects with active axillary hidradenitis suppurativa of the same severity bilaterally. regimen\n* For females of reproductive potential: use of highly effective contraception for at least 1 month prior to screening and agreement to use such a method during study participation\n* Agreement to forego other treatments for hidradenitis for the duration of the study\n\nExclusion Criteria:\n\n* Current use of antibiotics, hormonal therapies, steroids, and/or biologics\n* Presence of cardiac pacemaker\n* Pregnancy or lactation\n* Known neuromuscular disorder\n* Known hyperhidrosis\n* Febrile illness within one month\n* Treatment with another investigational drug or other intervention within three months\n* Patients without active axillary hidradenitis suppurativa",
            "healthyVolunteers": false,
            "sex": "ALL",
