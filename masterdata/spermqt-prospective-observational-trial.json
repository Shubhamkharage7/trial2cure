        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Number of patients that have and have not achieved pregnancy during rounds 1 and 2 of intrauterine insemination (IUI)",
                    "description": "Number of patients that have and have not achieved pregnancy during rounds 1 and 2 of IUI in each bracket of SpermQT score (Poor, Normal, and Excellent)",
                    "timeFrame": "6 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Number of patients that have and have not achieved pregnancy during subsequent rounds of intrauterine insemination (IUI) and in vitro fertilization (IVF)",
                    "description": "Number of patients that have and have not achieved pregnancy during subsequent rounds of IUI and with IVF in each bracket of SpermQT score (Poor, Normal, and Excellent)",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Number of patients that have and have not achieved a live birth",
                    "description": "Number of patients that have and have not achieved a live birth in each bracket of SpermQT score (Poor, Normal, and Excellent)",
                    "timeFrame": "18 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* The subjects have been recommended to pursue IUI, or they have recently completed an IUI (regardless of outcome)\n* Male partner is under 45 years of age\n* Female partner is under 38 years of age\n* The total motile sperm count from the raw semen analysis must be greater than or equal to 10 million\n\nExclusion Criteria:\n\n* The female or male partner have a BMI equal to or greater than 40\n* The male partner has undergone male testosterone replacement therapy in the last 6 months\n* Either the female or male partner have a history of recurrent pregnancy loss, defined as 2 or more consecutive clinical/ultrasound pregnancy losses\n* There are any known factors contributing to female factor infertility, such as but not limited to:\n* Severe Endometriosis (stage 3 or higher, endometrioma on ovaries)\n* Multiple uterine fibroids 5cm or larger\n* Severe Asherman's Syndrome\n* Severe Mullerian anomaly\n* Lack of tubal patency in at least one fallopian tubes",
            "sex": "ALL",
            "minimumAge": "18 Years",
            "maximumAge": "45 Years",
            "stdAges": [
                "ADULT"
            ],
            "studyPopulation": "Couples that are eligible for an IUI procedure and/or have been recommended to pursue IUI, and/or couples have recently completed an IUI cycle (no matter the outcome)",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
