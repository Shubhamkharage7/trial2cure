        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Digital Health Coaching for Pancreatic Cancer Patients",
                    "type": "OTHER",
                    "description": "Initial in-person and/or virtual supportive care, social work, and dietitian consultation within 4 weeks of diagnosis and/or 3 weeks of study enrollment.",
                    "interventionNames": [
                        "Behavioral: Coaching Sessions"
                    ]
                },
                {
                    "label": "Digital Health Coaching for Caregivers",
                    "type": "OTHER",
                    "description": "Initial in-person and/or virtual supportive care, social work, and dietitian consultation within 4 weeks of diagnosis and/or 3 weeks of study enrollment.",
                    "interventionNames": [
                        "Behavioral: Coaching Sessions"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Coaching Sessions",
                    "description": "Enrolled participants will set up an account through a secure and encrypted web portal managed by Osara Health.\n\nCoaches will follow-up with their designated patients and/or caregivers to schedule individual coaching sessions via phone, text, or email depending on the participant's communication preference. Participants will have an initial coaching consultation by phone to go over the purpose and structure of coaching sessions.\n\nParticipants will then have an additional 3-4 coaching sessions (depending on time availability and need) to discuss symptom management, nutrition and exercise, sleep, and mindfulness.\n\nParticipants will receive content for each of the modules via email.",
                    "armGroupLabels": [
                        "Digital Health Coaching for Caregivers",
                        "Digital Health Coaching for Pancreatic Cancer Patients"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Recruitment Rate",
                    "description": "Measured by \u2265 50% of eligible participants recruited.",
                    "timeFrame": "Up to 12 Months"
                },
                {
                    "measure": "Participant Acceptance of the Digital Health Coaching Program",
                    "description": "Measured by \u2265 70% of participant rating of the intervention as satisfactory. 4 item scale (score range 0-20) will be utilized.",
                    "timeFrame": "Up to 12 Weeks"
                },
                {
                    "measure": "Participant Ease of Usability of the Digital Health Coaching Program",
                    "description": "Measured by \u2265 70% of participant rating the mobile application as easy-to-use for scheduling coaching visits.\n\n4 item scale (score range 0-20) will be utilized.",
                    "timeFrame": "Up to 12 Weeks"
                },
                {
                    "measure": "Feasibility of Utilizing the Digital Health Coaching Program",
                    "description": "Measured by \u2265 70% of participant rating of being likely to utilize the intervention.\n\n4 item scale (score range 0-20) will be utilized.",
                    "timeFrame": "Up to 12 Weeks"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* 18 years of age or older.\n* Participant must have a pancreatic cancer diagnosis or be a caregiver such as a friend or spouse.\n* Participant must be planning to receive treatment and/or care management at Moffitt Cancer Center.\n* Participant must be able to speak and read English.\n* Participant must be able to provide informed consent.\n\nExclusion Criteria:\n\n* Documented or observable psychiatric or neurological disorder that would interfere with study participation such as severe dementia\n* Any participant that does not meet all inclusion criteria will be excluded.",
