        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "LINQ/LINQ II",
                    "description": "Algorithm developed and validated to continuously measure QT intervals for every beat using LINQ/LINQ II ECG. This study is aimed at determining if QT interval changes can be detected during and after antiarrhythmic drug loading by the designed QT detection algorithm from LINQ/LINQ II ECG."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
