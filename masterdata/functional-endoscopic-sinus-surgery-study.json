            "primaryOutcomes": [
                {
                    "measure": "Change in Endoscopic Grading of Nasal Bleeding",
                    "description": "Endoscopic Grading of Nasal Bleeding will be calculated using the 0-5 point Boezaart surgical field grading scale; 0 being no bleeding and 5 being severe bleeding.",
                    "timeFrame": "Every 10 minutes up to 36 hours"
                }
