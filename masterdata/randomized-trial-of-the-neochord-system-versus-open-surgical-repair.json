                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "NeoChord DS1000",
                    "description": "Trans-apical, beating heart repair of degenerative mitral valve insufficiency with artificial chordae implanted with the NeoChord DS1000",
                    "armGroupLabels": [
                        "Treatment Group"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "Surgical Mitral Valve Repair",
                    "description": "Surgical repair of degenerative mitral valve insufficiency with cardiopulmonary bypass, and annuloplasty ring and some form of leaflet repair or artificial chordae.",
                    "armGroupLabels": [
                        "Control Group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Proportion of subjects free of Major Adverse Events (MAEs) in the treatment group when compared to subjects in the control group.",
                    "timeFrame": "Post-operative Day (POD) 30"
                },
                {
                    "measure": "Proportion of subjects free of Grade II, III or IV mitral regurgitation, mitral valve replacement or mitral valve reintervention in the treatment group when compared to subjects in the control group.",
                    "timeFrame": "1 year"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Is a candidate for mitral valve repair with cardiopulmonary bypass\n* Has Grade III moderate or Grade IV severe degenerative mitral valve regurgitation\n* Primary segmental prolapse of the A2 or P2 segment or prolapse extending to an adjacent segment (P1, P3, A1, A3) who have a single eccentric regurgitant jet on echocardiogram\n* Anterior leaflet covers at least 65% of anterior-posterior annular distance or an anterior leaflet that would provide sufficient coaptation after chord placement\n* Anatomic and general suitability\n\nExclusion Criteria:\n\n* Prior mitral valve surgery\n* Concomitant cardiac procedures\n* Other cardiac procedures within 3 months",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "21 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
