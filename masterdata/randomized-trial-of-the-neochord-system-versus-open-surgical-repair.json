        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Treatment Group",
                    "type": "EXPERIMENTAL",
                    "description": "Subjects with degenerative mitral valve insufficiency treated with artificial chordae implanted using the NeoChord DS1000",
                    "interventionNames": [
                        "Device: NeoChord DS1000"
