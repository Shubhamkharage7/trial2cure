        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Arm A - Tislelizumab and SX-682",
                    "type": "EXPERIMENTAL",
                    "interventionNames": [
                        "Drug: Tislelizumab",
                        "Drug: SX-682"
                    ]
                }
