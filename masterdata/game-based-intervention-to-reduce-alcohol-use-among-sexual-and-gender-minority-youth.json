            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Singularities Game",
                    "description": "Singularities is a theory-based, community-informed, web-accessible, roleplaying game-based intervention incorporating 4 primary components: fostering healthy identity development in a safe environment; encouraging help-seeking behaviors; encouraging use of productive coping; alcohol-harm reduction; and encouraging healthy internet and social media use.",
                    "armGroupLabels": [
                        "Singularities Game"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Psychosocial Education",
                    "description": "Psychoeducational core components of the Singularities intervention delivered in an instructional text format to participants through a Qualtrics-based training program.",
                    "armGroupLabels": [
                        "Psychoeducation Readings"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Binge drinking frequency",
                    "description": "Primary outcome is past-month frequency of binge drinking, defined as consuming 5+ drinks in a two-hour period (regardless of assigned/biological sex at birth to be inclusive of gender minorities) assessed via NIAAA-recommended quantity-frequency alcohol survey items.",
                    "timeFrame": "3 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Drinks Per Week",
                    "description": "Sum of the # of drinks consumed each day of a typical week in the past three months via the Daily Drinking Questionnaire. Higher scores indicate worse outcome (greater drinker per week). The lower limit of the range is zero and there is no upper limit.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Drinking Frequency",
                    "description": "Number of days on which one or more drinks were consumed in past month via Quantity-Frequency items.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Maximum # of Drinks",
                    "description": "Largest # of drinks consumed on days when drinking occurred in past month via Quantity-Frequency items.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "High-Intensity Binge Drinking Frequency",
                    "description": "Number of days in past month in which 8+ and 10+ drinks (regardless of assigned/biological sex at birth to be inclusive of gender minorities) were consumed, assessed via Quantity-Frequency items.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Total Volume Consumed",
                    "description": "Quantity-Frequency items multiplying drinking frequency (past-month # of drinking days) by quantity (average number of drinks consumed per drinking day in past month)",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Alcohol Use Initiation",
                    "description": "Survey item from the Youth Risk Behavior Survey assessing lifetime alcohol use.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Binge Drinking Initiation",
                    "description": "Survey item from the Youth Risk Behavior Survey assessing lifetime binge drinking.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Alcohol-Related Consequences:",
                    "description": "Rutgers Alcohol Problems Index (RAPI) assessing frequency of negative alcohol-related problems",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Cigarette Smoking",
                    "description": "Frequency \u00d7 Quantity using 2 items assessing past-month frequency of cigarette smoking (in days) and average number of cigarettes smoked per day.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Electronic Nicotine Delivery System Use",
                    "description": "1 item assessing the frequency of electronic nicotine delivery system use in the past 30 days.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Marijuana Use",
                    "description": "1 item about frequency of marijuana use in the past month.",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Stress",
                    "description": "Summative score of 10 items assessing past-month stress (Perceived Stress Scale). Possible range is 0-40, wherein higher scores indicate worse outcomes (i.e., higher perceived stress).",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Anxiety Symptoms",
                    "description": "Average of 10 items assessing past-week symptoms of anxiety (Severity Measure for Generalized Anxiety Disorder Child Age 11-17 years).",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Depressive Symptoms",
                    "description": "Summative score of 9 items assessing past-week symptoms of depression (Patient Health Questionnaire-9 for Adolescents). Possible range is 0-27, wherein higher scores indicate worse outcomes (i.e., greater depression severity).",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Loneliness",
                    "description": "Average of 8 items assessing symptoms of loneliness (University of California, Los Angeles Loneliness Scale). Possible range is 0-3, wherein higher scores indicate worse outcomes (i.e., greater loneliness).",
                    "timeFrame": "3 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Live in the U.S.,\n* 14-18 years old\n* Have a sexual minority identity (i.e., gay, lesbian, bisexual, or queer) or a gender minority identity (i.e., consider themselves to be transgender or nonbinary),\n* Have an internet-accessible computer, smart phone, or tablet,\n* Provide an email address.\n\nExclusion Criteria:\n\n* Cisgender heterosexuals.",
