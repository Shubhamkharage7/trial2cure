        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Flexible Ureteroscopy with Dusting",
                    "type": "OTHER",
                    "description": "Flexible ureteroscopy with the dusting lasing technique. MOSES parameters will be set to an energy of 0.3-0.4 Jules and a frequency of 50-80 hertz",
                    "interventionNames": [
                        "Device: MOSES laser system"
                    ]
                },
                {
                    "label": "Flexible Ureteroscopy with Fragmentation",
                    "type": "OTHER",
                    "description": "Flexible ureteroscopy utilizing the Fragmentation lasing technique. MOSES parameters will be set to 0.8-1 JULES and a frequency of 8-10 hertz",
                    "interventionNames": [
                        "Device: MOSES laser system"
                    ]
                },
                {
                    "label": "Flexible Ureteroscopy with Hybrid",
                    "type": "OTHER",
                    "description": "Flexible ureteroscopy utilizing the Hybrid lasing technique which will use both sets of parameters.",
