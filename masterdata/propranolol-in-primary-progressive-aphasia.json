        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Propanolol and MRI",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will receive propranolol via oral capsule. The drug dosage will be titrated slowly to ensure the drug is tolerated well.",
                    "interventionNames": [
                        "Drug: Propranolol",
                        "Device: Magnetic Resonance Imaging (MRI)"
                    ]
                },
                {
                    "label": "Placebo and MRI",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "Participants will receive placebo via oral capsule.",
                    "interventionNames": [
                        "Device: Magnetic Resonance Imaging (MRI)",
                        "Drug: Placebo"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Propranolol",
                    "description": "Propranolol will be given on a titration schedule in which participants will begin with small doses of the drug and increase to a larger dosage over the course of three weeks. Propranolol will be taken for a total of 9 weeks.",
                    "armGroupLabels": [
                        "Propanolol and MRI"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "Magnetic Resonance Imaging (MRI)",
                    "description": "Magnetic Resonance Imaging (MRI) will be performed at 3 Tesla and 7 Tesla, for both propranolol and placebo arms.",
                    "armGroupLabels": [
                        "Placebo and MRI",
                        "Propanolol and MRI"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Placebo",
                    "description": "Placebo will be given on the same schedule as the propranolol regime.",
                    "armGroupLabels": [
                        "Placebo and MRI"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in Neuropsychological Assessment Battery Naming Test",
                    "timeFrame": "Day 1, 4 Weeks, 8 Weeks, 10 Weeks, 14 Weeks,18 Weeks"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in State-Trait Anxiety Inventory for Adults",
                    "description": "The State-Trait Anxiety scores range from 20-40. A higher score indicates more anxiety.",
                    "timeFrame": "Day 1, 4 Weeks, 8 Weeks, 10 Weeks,14 Weeks,18 Weeks"
                },
                {
                    "measure": "Change in Semantic Word Fluency Tasks",
                    "description": "The Semantic Word Fluency task measures the subject's ability to name as many items in a minute in a given category. The total number of correct and non-repeated responses are totaled for each category. There is no minimum or maximum score. Higher scores indicate better word fluency.",
                    "timeFrame": "Day 1, 8 Weeks,18 Weeks"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* 1. Age: 50 and older\n* 2. Primary Progressive Aphasia diagnosis\n* 3. Native English speaker\n\nExclusion Criteria:\n\n* 1. Unable to provide consent\n* 2. Taking alpha 2 agonists (clonidine and guanfacine)\n* 3. Other major psychological or neurological diagnosis\n* 4. Major head trauma that contributed to their condition\n* 5. Allergic reaction to adhesives\n* 6. Uncorrected vision/hearing impairments\n* 7. Diabetes\n* 8. Reactive airway disease\n* 9. Untreated hypothyroidism\n* 10. Bradyarrhythmia\n* 11. Unexplained syncope\n* 12. Pregnancy (assessed verbally on the days of MR imaging)\n* 13. Drugs that interact with propranolol, such as alpha 2 agonists\n* 14. Claustrophobia, inner ear implants, aneurysm or other surgical clips, metal foreign bodies in eye, pacemaker or other contraindication to MR scanning. Subjects with any implanted device that cannot be verified as MRI compliant will be excluded.",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "50 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
