        },
        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "PROCEDURE",
                    "name": "Standard of Care Biopsy",
                    "description": "Patients will be evaluated with multiparametric MRI prostate followed by ExactVu micro-ultrasound to identify target lesions all of which will be biopsied by the examiner"
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Detection Rate",
                    "description": "To compare the detection rates of clinically significant prostate cancer lesions by ExactVu micro-ultrasound and by MRI-fusion",
                    "timeFrame": "2 years"
                },
                {
                    "measure": "Feasibility of ExactVu",
                    "description": "To determine the feasibility of using ExactVu micro-ultrasounds to screen for prostate cancer in socioeconomically disadvantaged populations with limited access to care. Feasibility is measured by the percentage of patients that successfully undergo the study procedures.",
                    "timeFrame": "2 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Predictive Performance Measures",
                    "description": "To evaluate the sensitivity of ExactVu micro-ultrasound method and MRI-fusion method in detection of prostate cancer",
                    "timeFrame": "2 years"
                },
                {
                    "measure": "Predictive Performance Measures",
                    "description": "To evaluate the negative predictive value of ExactVu micro-ultrasound method and MRI-fusion method in detection of prostate cancer",
                    "timeFrame": "2 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Patients scheduled to undergo biopsy and with one of the following:\n\n   * Men with suspicion of prostate cancer\n   * Men on active surveillance\n2. Age \u226518 \\[30\\]\n3. Have available multiparametric prostate MRI\n4. Able to provide written, informed consent\n5. No significant medical illness which in the opinion of the Investigator would preclude entry to study procedures\n6. Be willing and able to comply with scheduled visits\n\nExclusion Criteria:\n\n1. Previously confirmed prostate cancer diagnosis with Grade Group \\>= 2\n2. Unable to undergo prostate biopsy\n3. Prostate MRI unable to be evaluated using the PI-RADS v2 criteria\n4. Men with contraindication for MRI or a prostate biopsy\n5. Prostate biopsy within 8 weeks prior to mpMRI.\n6. Any history of prostate treatment",
            "sex": "MALE",
            "minimumAge": "18 Years",
            "maximumAge": "99 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "The study population will cover all races, including socioeconomically disadvantaged minorities, that have been diagnosed with prostate cancer.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
