        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Managing Cancer and Living Meaningfully (CALM)",
                    "type": "EXPERIMENTAL",
                    "description": "CALM optimally consists of six individual sessions of 45 to 60 minutes, delivered over a three- to six-month period. CALM sessions address four broad and interrelated domains found to be important and relevant in this population: (1) symptom management and communication with healthcare providers, (2) changes in personal relationships, (3) sense of meaning and purpose, and (4) the future, hope and mortality.",
                    "interventionNames": [
                        "Behavioral: CALM"
                    ]
                },
                {
                    "label": "Treatment as Usual (TAU)",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "Treatment as usual (TAU) for managing distress in brain cancer involves being provided a list of local / national resources (e.g., psychologist, social worker, or other mental health providers) if an individual chooses to seek treatment for the distress they are experiencing.",
                    "interventionNames": [
                        "Behavioral: Treatment as usual"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "CALM",
                    "description": "The CALM intervention is a supportive-expressive therapy that is administered by CALM trained clinicians in a particular style.",
                    "armGroupLabels": [
                        "Managing Cancer and Living Meaningfully (CALM)"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Treatment as usual",
                    "description": "Treatment as usual includes a list of local / national resources (e.g., psychologist, social worker, or other mental health providers) if an individual chooses to seek treatment for the distress they are experiencing.",
                    "armGroupLabels": [
                        "Treatment as Usual (TAU)"
                    ]
                }
            ]
        },
