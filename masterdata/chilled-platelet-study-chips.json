        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Hemostatic efficacy",
                    "description": "Assessed by a bleeding score, a modified Peri-Operative bleeding score: the score ranges from 1-5. A lower score signifies a better outcome.",
                    "timeFrame": "24 hours after first study platelet transfusion"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Chest tube output",
                    "description": "Assessed by volume",
                    "timeFrame": "24 hours from first study platelet transfusion or ICU admit (determined from which occurs last)"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Blood product administration",
                    "description": "The quantity of RBCs, Plasma, Platelets, Cryoprecipitate, and Whole blood",
                    "timeFrame": "24 and 72 hours from start time of first study platelet transfusion"
                },
                {
                    "measure": "Total Dose of individual hemostatic adjuncts",
                    "description": "Antifibrinolytic and coagulation factor concentrates",
                    "timeFrame": "24 and 72 hours from start time of first study platelet transfusion"
                },
                {
                    "measure": "Mechanical ventilation",
                    "description": "Duration of mechanical ventilation days",
                    "timeFrame": "up to 28 days after first study platelet transfusion"
                },
                {
                    "measure": "Length of stay",
                    "description": "ICU and hospital length of stay",
                    "timeFrame": "up to 28 days"
                },
                {
                    "measure": "Relative change in hemostatic parameters",
                    "description": "CBC (platelet count and hemoglobin), PT/INR, PTT, fibrinogen, and TEG",
                    "timeFrame": "6 and 24 hours after first study platelet transfusion"
                },
                {
                    "measure": "Unplanned sternal closure delay",
                    "description": "Sternum left open after cardiac surgery",
                    "timeFrame": "prior to ICU admission"
                },
                {
                    "measure": "Re-exploration for bleeding",
                    "description": "Return to OR after ICU admission",
                    "timeFrame": "24 hours first study platelet transfusion"
                },
                {
                    "measure": "Unplanned extracorporeal support post operatively",
                    "description": "Placed on ECMO",
                    "timeFrame": "within 48 hours of first study platelet transfusion"
                },
                {
                    "measure": "Morbidities after ICU admission",
                    "description": "Acute respiratory distress syndrome, need for and duration of renal support, renal failure, septic shock",
                    "timeFrame": "within 7 days of first study platelet transfusion"
                },
                {
                    "measure": "Measures of end organ injury",
                    "description": "If BUN, creatinine, lactate, troponin, alanine aminotransferase (ALT) are collected clinically.",
                    "timeFrame": "within 48 hours of first study platelet transfusion"
                },
                {
                    "measure": "Transfusion associated adverse events",
                    "description": "As per CDC guidelines",
                    "timeFrame": "within 7 days of first study platelet transfusion"
                },
                {
                    "measure": "Arterial thrombotic event",
                    "description": "Stroke, MI",
                    "timeFrame": "within 7 days of first study platelet transfusion"
                },
                {
                    "measure": "Venous thrombotic event",
                    "description": "Deep vein thrombosis or pulmonary embolism confirmed by ultrasound, venography, perfusion scan, spiral CT, MRI, or pulmonary angiogram",
                    "timeFrame": "within 7 days of first study platelet transfusion"
                },
                {
                    "measure": "All cause mortality",
                    "description": "Death for any reason",
                    "timeFrame": "up to 28 days"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Viable neonates \u2265 3 kg at time of enrollment (as defined in Section 4.1) OR age greater than 28 days and less than 85 years of age at time of consent; AND\n* Planned complex cardiac surgery with planned use of cardiopulmonary bypass, with an expectation of bleeding requiring platelet transfusion.\n\nExclusion Criteria:\n\n* Expected order for washed or volume reduced platelets\n* Patient with known anti-platelet antibodies\n* Platelet transfusion refractoriness due to anti-HLA antibodies\n* Known or suspected pregnancy\n* Previously randomized in this study\n* Conscious objection or unwillingness to receive blood products\n* Known IgA deficiency\n* Known congenital platelet disorder\n* Known congenital bleeding disorder\n* Planned post-operative extracorporeal membrane oxygenation (ECMO), ventricular assist device (VAD), and/or continuous renal replacement therapy (CRRT)/ hemodialysis\n* Patients intended to receive whole blood either intra-operative or post-operative for bleeding\n* Platelet transfusion (of any type) within 24 hours prior to the date of surgery\n* Pre-operative thrombocytopenia, defined as platelet count \\<75x10(9)/L, based on the most recent labs completed within 72 hours prior to the date of surgery.",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "0 Days",
            "maximumAge": "84 Years",
            "stdAges": [
                "CHILD",
                "ADULT",
                "OLDER_ADULT"
            ]
        },
