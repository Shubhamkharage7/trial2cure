        "descriptionModule": {
            "briefSummary": "The purpose of this study is to better understand the natural history of oropharyngeal carcinoma (OPC), with or without an association with the human papilloma virus (HPV). For this study, the investigators plan to collect blood from OPC patients prior to treatment and at six subsequent time points."
        },
        "conditionsModule": {
            "conditions": [
                "Oropharyngeal Carcinoma",
                "Human Papilloma Virus"
            ]
        },
        "designModule": {
            "studyType": "OBSERVATIONAL",
