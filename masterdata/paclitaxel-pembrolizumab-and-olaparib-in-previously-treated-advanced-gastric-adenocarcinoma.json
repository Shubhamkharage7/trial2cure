                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Olaparib",
                    "description": "1. Olaparib will be taken by mouth daily for 3 weeks (every 21 days).\n2. Drug - 100 mg or 300mg",
                    "armGroupLabels": [
                        "Arm 2: Cohort 2-Unmeasurable Disease",
                        "Cohort 1 - Measurable Disease"
                    ],
                    "otherNames": [
