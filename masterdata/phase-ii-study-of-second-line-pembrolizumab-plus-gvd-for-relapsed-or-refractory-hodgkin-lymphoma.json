        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "pembrolizumab plus GVD",
                    "type": "EXPERIMENTAL",
                    "description": "Part 1: Patients will receive 2-4 cycles of pembrolizumab plus GVD\n\nPart 2: up to 40 patients will be enrolled onto an expansion cohort. On the expansion, patients who achieve CR to 4 cycles of pembro-GVD will receive 13 cycles of pembrolizumab maintenance (instead of HDT/ASCT).",
                    "interventionNames": [
                        "Drug: pembrolizumab",
                        "Drug: gemcitabine",
                        "Drug: vinorelbine",
                        "Drug: liposomal doxorubicin",
                        "Procedure: Stem cell mobilization and collection"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "pembrolizumab",
                    "description": "Age 10-17: 2mg/kg (cap at 200mg), Age \u2265 18: 200 mg (flat) ,Day 1 of each 3 week cycle IV infusion 2-4 cycles (pre-ASCT)",
                    "armGroupLabels": [
                        "pembrolizumab plus GVD"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "gemcitabine",
                    "description": "1000mg/m\\^2 Days 1 and 8 of each 3 week cycle IV infusion 2-4 cycles",
                    "armGroupLabels": [
                        "pembrolizumab plus GVD"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "vinorelbine",
                    "description": "20mg/m\\^2 Days 1 and 8 of each 3 week cycle IV infusion 2-4 cycles",
                    "armGroupLabels": [
                        "pembrolizumab plus GVD"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "liposomal doxorubicin",
                    "description": "15mg/m\\^2 Days 1 and 8 of each 3 week cycle IV infusion 2-4 cycles",
                    "armGroupLabels": [
                        "pembrolizumab plus GVD"
                    ]
                },
                {
                    "type": "PROCEDURE",
                    "name": "Stem cell mobilization and collection",
                    "description": "Stem cell mobilization and collection will be performed as per standard MSKCC guidelines after 2-4 cycles of pembrolizumab-GVD.",
                    "armGroupLabels": [
                        "pembrolizumab plus GVD"
                    ]
                }
            ]
        },
