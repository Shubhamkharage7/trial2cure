        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Dose Escalation Phase: Blinatumomab Subcutaneous Formulation 1 (SC1)",
                    "type": "EXPERIMENTAL",
                    "description": "Cohorts of at least 3 participants each will be treated with escalating doses of bilinatumomab to determine the maximum tolerated dose (MTD). The MTD will be defined as the dose for which the estimate of the toxicity rate from an isotonic regression (Yan et al, 2017) is closest to the target toxicity rate. Safety, pharmacokinetics (PK), pharmacodynamics (PD) and efficacy will be assessed.",
                    "interventionNames": [
                        "Drug: Blinatumomab"
                    ]
                },
                {
                    "label": "Dose Expansion Phase: Blinatumomab SC1",
                    "type": "EXPERIMENTAL",
                    "description": "Up to 4 cohorts of participants with R/R B-ALL will be enrolled to the preliminary recommended phase 2 dose (RP2D) and schedule determined from dose escalation phase. Each cohort will aim to further assess safety, pharmacokinetics (PK), pharmacodynamics (PD), and efficacy.",
                    "interventionNames": [
                        "Drug: Blinatumomab"
                    ]
                },
                {
                    "label": "Ph-IIC: Clinical PK Evaluation of SC Blinatumomab Formulations",
                    "type": "EXPERIMENTAL",
                    "description": "1 cohort of participants will be enrolled into the Ph-IIC arm. The clinical PK evaluation cohort (Ph-IIC) will be conducted to compare the PK of SC1 and SC2 formulations at the RP2D determined from the dose expansion phase, in participants with R/R B-ALL.",
                    "interventionNames": [
                        "Drug: Blinatumomab"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Blinatumomab",
                    "description": "Blinatumomab will be administered as a subcutaneous (SC) injection.",
