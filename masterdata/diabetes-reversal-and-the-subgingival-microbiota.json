        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Bacterial composition",
                    "description": "as determined by 16s rRNA community profiling",
                    "timeFrame": "18 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Bacterial mRNA profiling",
                    "description": "RNA will be isolated then amplified by the MessageAmp II bacterial RNA amplification kit (Ambion/Life Technologies). Ribosomal RNA will be depleted with the RiboZero human/mouse and meta bacterial kits (Epicentre/Illumina) and human mRNA will be depleted with oligo(dT). Illumina sequencing libraries will be constructed with the Illumina TruSeq RNA kit and assessed with an Illumina HiSeq instrument.",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "Gingival inflammation",
                    "description": "samples will be assessed for inflammatory cytokines (e.g. IL-1\u03b2, IL-17 or TNF) by multiplex assay or ELISA assay",
                    "timeFrame": "18 mos."
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Men and women undergoing bariatric surgery who are 25-65 years old.\n* BMI \\> 35 kg/m2.\n* Diabetic subjects: HbA1c\\> 6.0% or fasting plasma glucose \\>126 mg/dl).\n* Normoglycemic subjects: HbA1c\\<5.75% or fasting plasma glucose \\<100 mg/dl).\n* Dental criteria: Minimum of 4 posterior teeth.\n* Signed and dated informed consent form.\n* Willing to comply with all study procedures and be available for the duration of the study.\n\nExclusion Criteria:\n\n* Subjects diagnosed with type 1 diabetes, maturity onset diabetes of the young (MODY), or latent autoimmune diabetes in adults (LADA).\n* Women who are considering pregnancy or are currently breastfeeding.\n* Individuals with a history of chronic inflammatory or autoimmune diseases or taking medications that affect immune function or affect body weight such as chronic systemic steroids.\n* Currently smoke more than 10 cigarettes per day.\n* Periodontal treatment within 3 months of bacterial sampling.\n* Acute periodontal infection (abscess) within 1 month of bacterial sampling or abscess in teeth or adjacent teeth within 1 month of sampling.\n* Treatment with antibiotics within one month of sample collection (except as part of the surgical regimen of prophylactic antibiotic immediately prior to surgery).\n* A treatment regimen of steroids within 1 month of sample collection.",
            "sex": "ALL",
            "minimumAge": "25 Years",
            "maximumAge": "65 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "The study participants will be men and women, 25-65 years old, with a Body Mass Index (BMI) of \u226535 kg/m2 who are scheduled to undergo bariatric surgery at the University of Pennsylvania or Temple University for treatment of obesity or obesity and type 2 diabetes. The total target enrollment is 130 diabetic and 104 normoglycemic subjects.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
