        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Simple decompression",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "In-situ decompression releases only the compressive ligamentous structures overlying the ulnar nerve.",
                    "interventionNames": [
                        "Procedure: Simple decompression"
                    ]
                },
                {
                    "label": "Subcutaneous anterior transposition",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Anterior transposition repositions the ulnar nerve, providing decompression and lengthening by moving the nerve anterior to the axis of elbow rotation",
                    "interventionNames": [
                        "Procedure: Subcutaneous anterior transposition"
                    ]
                }
            ],
            "interventions": [
