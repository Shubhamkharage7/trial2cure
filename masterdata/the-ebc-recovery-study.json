        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Traumatic Brain Injury Patients",
                    "description": "Traumatic brain injury (TBI) patients will be recruited and studied using electroencephalogram (EEG) to monitor their brain response to auditory and visual stimuli."
                },
                {
                    "label": "Healthy Volunteers",
                    "description": "Healthy volunteers will serve as the control group and will undergo electroencephalogram (EEG) to monitor their brain response to auditory and visual stimuli."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Glasgow Outcome Scale-Extended (GOSE) Scale",
                    "description": "The GOSE is an outcome scale, range 1-8 evaluating the patients' functional outcomes - higher score indicates better recovery",
                    "timeFrame": "Up to 5 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Coma Recovery Scale - Revised (CRS-R)",
                    "description": "The CRS-R will evaluate the patients' conscious state, range 0-23 -higher score indicates a better conscious state",
                    "timeFrame": "Up to 5 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\nFor TBI Participants\n\n* TBI adult (18 years and older) patients who are admitted to the intensive care unit\n* Unconscious with Glasgow Coma Scale (GCS) \\<8 on presentation and unable to follow bedside commands.\n* A negative COVID-19 test on arrival\n\nFor Healthy Volunteers:\n\n* 18 years and older\n\nExclusion Criteria:\n\nFor All Participants:\n\n* \\<18 years of age\n* Pregnant woman\n* Patients with cardiac arrest on presentation",
