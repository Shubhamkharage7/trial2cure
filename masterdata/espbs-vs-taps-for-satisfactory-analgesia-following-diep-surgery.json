            "interventions": [
                {
                    "type": "PROCEDURE",
                    "name": "Pre-operative Erector Spinae Plane block prior to DIEP surgery",
                    "description": "After obtaining informed consent to participate in the study, patients will be randomized by a computer 50/50 to either the Erector Spinae Plane group or the Transversus Abdominus Plane group at the time of their surgical clinic visit prior to their scheduled surgery. Then on the morning of surgery, the Erector Spinae Plane group will receive a bilateral ultrasound-guided Erector Spinae Plane peripheral nerve block. During this intervention, 0.25% bupivacaine with dexamethasone will be injected below the fascia of the erector spinae muscle group.",
                    "armGroupLabels": [
                        "Group 1: Erector Spinae Plane blocks"
                    ],
                    "otherNames": [
                        "ESPBs"
                    ]
                },
                {
                    "type": "PROCEDURE",
                    "name": "Pre-operative Transversus Abdominus Plane blocks prior to DIEP surgery",
                    "description": "On the morning of surgery, the patients in the TAPs group will receive a bilateral ultrasound-guided Transversus Abdominus plane peripheral nerve block. During this intervention, 0.25% bupivacaine with dexamethasone will be injected into the transversus abdominus plane above the transversus abdominus muscle.",
                    "armGroupLabels": [
                        "Group 2: Transversus Abdominis Plane blocks"
                    ],
                    "otherNames": [
                        "TAPs"
                    ]
                }
            ]
        },
