        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Columbia-Suicide Severity Rating Scale (C-SSRS)",
                    "description": "The C-SSRS is a semi-structured interview designed to assess the severity and intensity of suicidal ideation, suicidal behavior, and non-suicidal self injurious behavior over a specified time period. The measurement of suicidal ideation is based on five \"yes\" or \"no\" questions with accompanying descriptions arranged in order of increasing severity.",
                    "timeFrame": "All Visits - Baseline (Day -1 [V2]) to Week 12 (V10)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Montgomery-\u00c5sberg Depression Rating Scale (MADRS)",
                    "description": "The MADRS is a clinician rated scale measuring depression severity, consisting of 10 items, each scored from 0 (normal) to 6 (severe), for a total possible score of 60; higher scores denote greater severity.",
                    "timeFrame": "Baseline (V2) to Day 1 (V4), Week 1 (V5), Week 2 (V6), Week 3 (V7), Week 6 (V8), Week 9 (V9), and Week 12 (V10)"
                },
                {
                    "measure": "Clinical Global Impression - Modified for Depression (CGI-D)",
                    "description": "The CGI is a 3-item observer-rated scale that measures illness severity, global improvement or change and therapeutic response.",
                    "timeFrame": "Baseline (V2) to Week 3 (V7) and Week 12 (V10)"
                },
                {
                    "measure": "Clinical Global Impressions - Modified for Suicidal Ideation (CGI-SI)",
                    "description": "The CGI is a 3-item observer-rated scale that measures illness severity, global improvement or change and therapeutic response.",
                    "timeFrame": "Baseline (V2) to Week 3 (V7), and Week 12 (V10)"
                },
                {
                    "measure": "Concise Health Risk Tracking Self Report 12 Items (CHRT-SR)",
                    "description": "The CHRT-SR is a 12-item self-report measure that systematically assesses both suicidal thinking and associated thoughts that might indicate the propensity for suicidal acts.",
                    "timeFrame": "Baseline (V2) to Day 1 (V4), Week 1 (V5), Week 2 (V6), Week 3 (V7), Week 6 (V8), Week 9 (V9) and Week 12 (V10)"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Modified Scale for Suicidal Ideation (MSSI)",
                    "description": "The MSSI is an 18-item clinician rated scale to assess the presence or absence of suicide ideation and the degree of severity of suicidal ideas. Each of the 18 items is rated on a scale of 0 to 3 where a higher score indicates greater suicidal severity.",
                    "timeFrame": "Baseline (V2) to Day 1 (V4), Week 1 (V5), Week 2 (V6), Week 3 (V7), Week 6 (V8), Week 9 (V9) and Week 12 (V10)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Between 18 and 65 years of age at Screening\n* Diagnosis of Major Depressive Disorder (MDD)\n* Significant level of suicidal thoughts with active ideation and without immediate intent\n* Failure to respond to 2 medications in the current episode\n\nExclusion Criteria:\n\n* Current or past history of schizophrenia, psychotic disorder, bipolar disorder, borderline personality disorder, etc.\n* Current alcohol or substance use disorder",
