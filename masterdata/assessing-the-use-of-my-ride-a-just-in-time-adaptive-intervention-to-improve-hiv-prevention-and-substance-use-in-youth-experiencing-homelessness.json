        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "MY-RIDE group",
                    "type": "EXPERIMENTAL",
                    "interventionNames": [
                        "Behavioral: MY-RIDE group"
                    ]
                },
                {
                    "label": "Attention-control group",
                    "type": "ACTIVE_COMPARATOR",
                    "interventionNames": [
                        "Behavioral: Attention-control group"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Attention-control group",
                    "description": "Participants will receive a one-hour general health promotion session from a nurse and be issued a phone that will deploy the EMA on the same schedule as the intervention arm. They will also receive usual care from the clinics, drop-in center, and shelter recruitment sites. Usual care includes accessing social workers and case managers for immediate housing, food, and clothing needs and assistance in navigating the local Continuum of Care system to access social services.",
                    "armGroupLabels": [
                        "Attention-control group"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "MY-RIDE group",
                    "description": "MY-RIDE has three main components: 1) One nurse-led face-to-face session; 2) three months of Ecological Momentary Assessments (EMA) with personalized messaging delivered by phone in real-time in response to one's current risk; and 3) access to on-demand healthcare/PrEP navigation and referrals.",
                    "armGroupLabels": [
                        "MY-RIDE group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in substance use",
                    "timeFrame": "baseline, 3, 6, and 12 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in condom use",
                    "timeFrame": "baseline, 3, 6, and 12 months"
                },
                {
                    "measure": "Change in PrEP uptake as assessed by the self reported tool",
                    "description": "PrEP uptake is defined as the self initiation of HIV prevention medication",
                    "timeFrame": "baseline, 3, 6, and 12 months"
                },
                {
                    "measure": "Change in sexually transmitted infections (STIs)",
                    "description": "This will be assessed by a blood test to detect biomarkers for HIV and Syphilis, and urine test for Gonorrhea and Chlamydia",
                    "timeFrame": "baseline, 3, 6, and 12 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* have engaged in substance use (alcohol or illicit drugs)\n* speak English\n* are experiencing homelessness\n* engaged in sexual activity in the last 6 months or plan to in the next month\n* are not planning to move out of the metro area during the 12-month study period.\n\nExclusion Criteria:\n\n* Youth Experiencing Homelessness (YEH) who have low literacy based on the Rapid Estimate of Adult Literacy in Medicine-Short Form (scores \\< 4)",
