            "interventions": [
                {
                    "type": "DIETARY_SUPPLEMENT",
                    "name": "Beet-root juice",
                    "description": "Two days prior to the forearm endurance protocol, participants will be provided and instructed to consume a 400mg nitrate once per day. On the day of the forearm endurance protocol, the participants will be provided and instructed to consume a third 400mg dose of nitrates at least 150 minutes before forearm endurance testing.",
                    "armGroupLabels": [
                        "Beet-root juice plus red-light therapy",
                        "Beet-root juice plus sham light therapy"
                    ],
                    "otherNames": [
                        "Beet it Sport"
                    ]
                },
                {
                    "type": "RADIATION",
                    "name": "red-light therapy",
                    "description": "Participants will place their dominant forearm 15 centimeters away from the light source and have their dominant forearm exposed to 660nm wave length light for 10-minutes.",
                    "armGroupLabels": [
                        "Beet-root juice plus red-light therapy",
                        "Placebo plus red-light therapy"
                    ],
                    "otherNames": [
                        "MitoMIN"
                    ]
                },
                {
                    "type": "DIETARY_SUPPLEMENT",
                    "name": "Placebo",
                    "description": "Two days prior to the forearm endurance protocol, participants will be provided and instructed to consume a nitrate depleted placebo once per day. On the day of the forearm endurance protocol, the participants will be provided and instructed to consume a third dose of the nitrate depleted placebo at least 150 minutes before forearm endurance testing.",
                    "armGroupLabels": [
                        "Placebo plus red-light therapy",
                        "Placebo plus sham light therapy"
                    ]
                },
                {
                    "type": "RADIATION",
                    "name": "sham light",
                    "description": "Participants will place their dominant forearm 15 centimeters away from the light source and have their dominant forearm exposed to sham light for 10-minutes.",
                    "armGroupLabels": [
                        "Beet-root juice plus sham light therapy",
                        "Placebo plus sham light therapy"
                    ]
                }
            ]
