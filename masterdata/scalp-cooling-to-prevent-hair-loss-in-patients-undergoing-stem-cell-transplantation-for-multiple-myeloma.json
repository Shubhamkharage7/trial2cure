        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Penguin Cold Cap",
                    "type": "EXPERIMENTAL",
                    "description": "Patients will receive Penguin cold cap: 60 minutes prior to melphalan infusion and continue for 5 hours after melphalan infusion start time, for a total of 6 hours, on Days -2 and -1.",
                    "interventionNames": [
                        "Device: Penguin Cold Cap"
                    ]
                }
            ],
