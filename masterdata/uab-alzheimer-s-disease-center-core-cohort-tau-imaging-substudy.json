        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "[F-18]AV-1451-PET/MRI",
                    "type": "EXPERIMENTAL",
                    "description": "All participants in this study will undergo a tau-PET imaging using the tracer \\[F-18\\]AV-1451 with a simultaneous PET/MRI system. The \\[F-18\\]AV-1451 dosage is 740MBq (10 mCi) given intravenously, and the PET/MRI imaging will occur 75-105 min after tracer injection.",
                    "interventionNames": [
                        "Drug: [F-18]AV-1451-PET"
                    ]
