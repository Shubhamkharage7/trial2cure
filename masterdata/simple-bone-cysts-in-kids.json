        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Cyst healing",
                    "description": "Healing will be graded according to a 4-point modified Neer's classification by radiologists",
                    "timeFrame": "2 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Clinical measures (Cyst features)",
                    "description": "Cyst features (ie. appearance, size) will be described or measured from X-rays taken annually after treatment",
                    "timeFrame": "1 and 2 years"
                },
                {
                    "measure": "Functional measures (Questionnaire scores)",
                    "description": "Function in participants will be evaluated using self-reported questionnaires for activity level, perception of illness, and pain annually after treatment",
                    "timeFrame": "1 and 2 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Patients with a confirmed simple bone cyst in a long bone (ie. arm or leg) within last 3 months\n* At least 3 weeks must have elapsed since last fracture\n* At least 3 months must have elapsed since last cyst treatment\n* Patients and/or their legal representatives willing to provide written informed consent (and assent, when appropriate)\n* Patients with stabilizing implants in the bone where the cyst is located\n\nExclusion Criteria:\n\n* Patients with bone disease (ie. osteogenesis imperfecta, cancer, osteoporosis, Paget's disease)\n* Pregnant or breastfeeding female\n* Patients with cysts crossing the growth plate (area where bone grows)",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "2 Years",
            "maximumAge": "21 Years",
            "stdAges": [
                "CHILD",
                "ADULT"
            ]
        },
