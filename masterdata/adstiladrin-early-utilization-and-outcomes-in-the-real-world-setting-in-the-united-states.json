        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Whether or not a patient with CIS (with or without concomitant high-grade Ta or T1 papillary disease) achieves Complete Response (CR) at first evaluation from first ADSTILADRIN instillation",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Whether or not a patient with CIS (with or without concomitant high-grade Ta or T1 papillary disease) achieves CR at any time within 1 year from first ADSTILADRIN instillation",
                    "timeFrame": "1 year"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Duration of CR in patients with CIS (with or without concomitant high-grade Ta or T1 papillary disease) who achieve CR",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "High-grade recurrence-free survival",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Incidence of being HGRF after one ADSTILADRIN instillation and incidence of being HGRF at 1 year from first ADSTILADRIN instillation",
                    "timeFrame": "1 year"
                },
                {
                    "measure": "Incidence of and time to cystectomy",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Progression-free survival (PFS)",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Overall Survival (OS)",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Mortality due to bladder cancer",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Prior treatments and outcomes before starting ADSTILADRIN treatment",
                    "timeFrame": "Before starting ADSTILADRIN treatment"
                },
                {
                    "measure": "Number of ADSTILADRIN instillations received and time intervals between instillations",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Reasons for discontinuation of ADSTILADRIN treatment",
                    "timeFrame": "End of trial (up to 2 years)"
                },
                {
                    "measure": "Concomitant therapies for bladder cancer and major comorbidities",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Re-treatment of non-responders with ADSTILADRIN at 3 months or when deemed appropriate by the treating physician",
                    "timeFrame": "3 months"
                },
                {
                    "measure": "Subsequent line of therapy following ADSTILADRIN discontinuation",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Customized patient experience survey (before first ADSTILADRIN instillation and before each instillation thereafter)",
                    "timeFrame": "Before first ADSTILADRIN instillation and before each instillation thereafter"
                },
                {
                    "measure": "Patients' caregiver experience as measured by WPAI:CG (administered at the same timepoints as the patients)",
                    "description": "WPAI:CG is Work Productivity and Activity Impairment, as adapted for caregiving.",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Customized physician surveys (at 1 month following first patient first instillation of ADSTILADRIN, or as soon as possible upon site activation, and at 12 and 24 months post first patient first instillation at each respective site)",
                    "description": "Domains captured will include product considerations and treatment satisfaction",
                    "timeFrame": "Up to 2 years"
                },
                {
                    "measure": "Type, incidence, relatedness to ADSTILADRIN, severity grading (National Cancer Institute (NCI) Common Terminology Criteria for Adverse Events (CTCAE) Version 5.0), and seriousness of adverse events collected",
                    "timeFrame": "End of trial (up to 2 years)"
                },
                {
                    "measure": "Proportion of patients with pre-specified biomarker tested in the routine care setting, along with timing, type of sampling and laboratory results",
                    "description": "Routine care setting e.g., Cxbladder Detec, UroVysion FISH, nuclear matrix protein 22 (NMP22) and bladder tumor antigen (BTA) tests\\]",
                    "timeFrame": "End of trial (up to 2 years)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Prescribed and scheduled treatment with ADSTILADRIN per physician discretion or received the first instillation of ADSTILADRIN per physician discretion after 5 September 2023 but prior to site activation\n* Signed and dated ICF.\n* Age 18 years or older at day ICF is signed.\n\nExclusion Criteria:\n\n* Currently enrolled in a clinical trial.\n* Participants who have previously been treated with ADSTILADRIN in the context of a clinical trial\n* Participant is pregnant or breastfeeding.",
