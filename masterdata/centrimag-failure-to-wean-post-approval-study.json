            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "CentriMag Circulatory Support System",
                    "description": "The CentriMag Circulatory Suppose System is indicated for providing support for up to 30 days for one or both ventricles of the heart to treat post-cardiotomy patients who fail-to-wean from cardiopulmonary bypass, providing a bridge to decision when it is unclear whether the patient's heart will recover or whether the patient will need alternative, longer-term therapy.",
                    "armGroupLabels": [
                        "Treatment"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Survival to 30 days Post CentriMag support or to hospital discharge",
                    "description": "The proportion of subjects who survive to 30 days post CentriMag support or to hospital discharge (whichever is longer).",
                    "timeFrame": "30 days"
                },
                {
                    "measure": "Survival to induction of anesthesia for surgery for transplant or a long-term system",
                    "description": "The proportion of subjects who do not recover and are bridged to a transplant or long-term system",
                    "timeFrame": "Approximately 30 days"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Subject \\>18 years of age\n* Subject or legal representative has signed Informed Consent Form (ICF)\n* Subject has CentriMag Circulatory Support System implanted due to Failure To Wean from Cardiopulmonary Bypass\n\nExclusion Criteria:\n\n\u2022 Concomitant use of Extracorporeal Membrane Oxygenation (ECMO) during CentriMag Circulatory Support System implantation due to FTW from CPB",
            "healthyVolunteers": false,
