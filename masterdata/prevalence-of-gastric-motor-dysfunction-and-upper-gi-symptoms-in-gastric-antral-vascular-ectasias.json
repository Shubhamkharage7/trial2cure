        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Cirrhosis With Gastric Antral Vascular Ectasia",
                    "description": "Patients with Gastric Antral Vascular Ectasia with cirrhosis will undergo gastric motor function testing. This will be measured by radioscintigraphy after overnight fast using a radiolabeled meal.",
                    "interventionNames": [
                        "Diagnostic Test: Gastric Emptying"
                    ]
                },
                {
                    "label": "Cirrhosis Without Gastric Antral Vascular Ectasia",
                    "description": "Patients without Gastric Antral Vascular Ectasia with cirrhosis will undergo gastric motor function testing. This will be measured by radioscintigraphy after overnight fast using a radiolabeled meal.",
                    "interventionNames": [
                        "Diagnostic Test: Gastric Emptying"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Gastric Emptying",
                    "description": "Measured by radioscintigraphy after overnight fast using a radiolabeled meal consisting of two 99mTc - labeled scrambled eggs, one slice of whole wheat bread and one glass of 111In DTPA skim milk (320kcal, 30% fat). A variable region of interest program will be used to quantitate counts in the stomach. Data will be summarized as GE T1/2 and GE % emptied at 1, 2 and 4 hours.\n\nTo assess gastric emptying, the protocol meal as described above will be given. Anterior and posterior scans of the abdomen, each of two minutes duration, will be obtained right after the meal is ingested to ensure adequate acquisition of counts. Scans will then be done at time 0 subsequently in 15-minute intervals to 90 minutes, then in 30-minute intervals thereafter to 240 minutes supine.",
                    "armGroupLabels": [
                        "Cirrhosis With Gastric Antral Vascular Ectasia",
                        "Cirrhosis Without Gastric Antral Vascular Ectasia"
                    ],
                    "otherNames": [
                        "Gastric motor function testing"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Abridged Bowel Disease Questionnaire",
                    "description": "The Abridged Bowel Disease Questionnaire is a 16-item questionnaire that measures Bowel Disease symptoms. Each item is scored as yes or no. Scores answering yes indicate more bowel disease, and scores answering no indicate less or no bowel disease.",
                    "timeFrame": "baseline"
                },
                {
                    "measure": "Nepean Dyspepsia Index",
                    "description": "The Nepean Dyspepsia Index is a 10-item questionnaire that measures dyspepsia problems. Each item is scored from 1-5. 1 = Not at all, 5 = Extremely. Total scores range from 10 - 50, lower scores indicate less dyspepsia problems, higher scores indicate more dyspepsia problems.",
                    "timeFrame": "baseline"
                },
                {
                    "measure": "Leuven Postprandial Distress Scale",
                    "description": "The Leuven Postprandial Distress Scale is an 8-item questionnaire that measures symptoms of dyspepsia. Each item is scored from 1-5. 1 = Absent, 5 = very severe. Total scores range from 0-40 Lower scores indicate less or no symptoms of dyspepsia, higher scores indicate more symptoms of dyspepsia.",
                    "timeFrame": "baseline"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Gastric Emptying Half Time (T 1/2) of Solids",
                    "description": "Subjects will ingest a radio labeled meal consisting of two 99mTc. Anterior and posterior scans of the abdomen, each of two minutes duration, will be obtained right after the meal is ingested to ensure adequate acquisition of counts.",
                    "timeFrame": "1 hour, 2 hours, 4 hours"
                },
                {
                    "measure": "Gastric Emptying Half Time (T 1/2) of liquids",
                    "description": "Subjects will ingest one glass of 111In DTPA skim milk. Anterior and posterior scans of the abdomen, each of two minutes duration, will be obtained right after the meal is ingested to ensure adequate acquisition of counts.",
                    "timeFrame": "1 hour, 2 hours, 4 hours"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Patients \\>18 years of age with endoscopic evidence of Gastric Antral Vascular Ectasia.\n* Willingness to participate in the study\n* Capacity to provide informed consent.\n* No evidence of organic, systemic, or metabolic disease (e.g. diabetes mellitus) to explain the symptoms on routine investigations\n\nExclusion Criteria:\n\n* Patients with active bleeding.\n* Patients with severe decompensated liver disease.\n* Patients with chronic kidney disease.\n* Presence of other diseases (structural or metabolic) which could interfere with interpretation of the study results.\n* Pregnant or lactating women due to radiation exposure.",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "19 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Patients who have undergone an endoscopy and found to have GAVE will be included. Patients with underlying cirrhosis and GAVE in comparison to control patients with cirrhosis and no GAVE.",
            "samplingMethod": "PROBABILITY_SAMPLE"
        },
