        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "High risk OSA",
                    "description": "Cohort of individuals found to sleep have scores categorized as \"High risk,\" on the Berlin questionnaire."
                },
                {
                    "label": "Low risk OSA",
                    "description": "Cohort of individuals found to sleep have scores categorized as \"Low risk,\" on the Berlin questionnaire."
                },
                {
                    "label": "Short sleepers",
                    "description": "Cohort of individuals found to have average sleep duration of \\< 7 hours per night. Individuals who sleep on average less than 6 hours per night will further be sub-categorized as very short sleeprs."
                },
                {
                    "label": "Long sleepers",
                    "description": "Cohort of individuals found to have average sleep duration of \\>9 hours per night."
                },
                {
                    "label": "Average lengths leepers",
                    "description": "Cohort of individuals found to have an average sleep duration between 7 and 9 hours per night"
                },
                {
                    "label": "Regular sleepers",
                    "description": "Cohort of individuals whose standard deviation of sleep duration falls within 1 standard deviation of their mean sleep duration."
                },
                {
                    "label": "Irregular sleepers",
                    "description": "Cohort of individuals whose standard deviation of sleep duration falls greater than 1 standard deviation of their mean sleep duration"
                },
                {
                    "label": "Severe insomnia risk",
                    "description": "Cohort of individuals whose scores on the Insomnia Severity Index would be categorized as severe"
                },
                {
                    "label": "Moderate insomnia risk",
                    "description": "Cohort of individuals whose scores on the Insomnia Severity Index would be categorized as moderate"
                },
                {
                    "label": "No insomnia risk",
                    "description": "Cohort of individuals whose scores on the Insomnia Severity Index would be categorized as low"
                },
                {
                    "label": "Increased social jet lag",
                    "description": "Individuals who are more than 1 hour less or more of sleep on weekdays compared to average weekend sleep."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Patient Health Questionnaire 9",
                    "description": "Baseline depression scores on a scale from 0 to 27, which higher scores indicated more depression",
                    "timeFrame": "Administered one time at enrollment"
                },
                {
                    "measure": "Bi-weekly mood scores",
                    "description": "Mood scores based on an 11 point rating scale, with higher scores indicated a better mood",
                    "timeFrame": "Averaged bi-weekly over a two month period"
                },
                {
                    "measure": "Subjective sleep quality",
                    "description": "Biweekly assessment of refreshing sleep quality on an 11 point rating scale, with higher scores indicated better subjective sleep from the participant",
                    "timeFrame": "Averaged bi-weekly over a two month period"
                },
                {
                    "measure": "Change in BMI",
                    "description": "Based on standard BMI categories; calculated based on self-reported data of height and weight",
                    "timeFrame": "Measured as the difference between BMI reported at the beginning of the study and at the two-month mark."
                }
            ]
        },
