        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Oral Nifedipine",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Oral nifedipine: Once diagnosis of hypertensive emergency is confirmed on repeat blood pressure, patients randomized to oral nifedipine will receive 10mg initially, with repeated doses of 20mg every 20min, for up to a maximum of 5 doses or until the therapeutic blood pressure goal of \\<160 systolic and \\<105 diastolic is achieved. If the therapeutic goal is not achieved after 5 doses, crossover to the alternative study medication will occur.",
                    "interventionNames": [
                        "Drug: Nifedipine"
                    ]
                },
                {
                    "label": "Intravenous labetalol",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Intravenous Labetalol: Patients randomized to IV labetalol will receive 20mg initially, followed by escalating doses of 40mg, 80mg, 80mg, then 80mg every 20 minutes until the therapeutic goal is achieved, for a maximum of 5 doses. If the therapeutic goal is not achieved after 5 doses, crossover to the alternative study medication will occur.",
                    "interventionNames": [
                        "Drug: Labetalol"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Labetalol",
                    "description": "Intravenous labetalol, a short acting ant-hypertensive",
                    "armGroupLabels": [
                        "Intravenous labetalol"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Nifedipine",
                    "description": "Oral nifedipine, a short acting ant-hypertensive",
                    "armGroupLabels": [
                        "Oral Nifedipine"
                    ],
