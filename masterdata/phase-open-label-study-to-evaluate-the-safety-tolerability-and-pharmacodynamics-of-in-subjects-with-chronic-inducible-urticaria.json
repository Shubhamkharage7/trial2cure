        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Safety and tolerability of EP262",
                    "description": "Assessed by the incidence of treatment-emergent adverse events",
                    "timeFrame": "Measured from Day 1 to End of Study or Early Termination (up to 12 weeks)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in Critical Friction Threshold (CFT)",
                    "description": "Assessed by the number of prongs that elicit a wheal response using a FricTest (in subjects with symptomatic dermographism)",
                    "timeFrame": "Measured from Baseline to Week 4"
                },
                {
                    "measure": "Change in Critical Temperature Threshold (CTT)",
                    "description": "Assessed as the highest temperature that elicits a wheal response using a TempTest (in subjects with cold urticaria)",
                    "timeFrame": "Measured from Baseline to Week 4"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Diagnosis of CIndU (symptomatic dermographism or cold urticaria) for greater than 3 months and positive response to applicable skin provocation testing\n* Willing to discontinue chronic treatment with antihistamines during the study\n\nExclusion Criteria:\n\n* Urticaria with a clear underlying etiology other than symptomatic dermographism or cold urticaria\n* Other active skin diseases that might confound the study evaluations (eg, atopic dermatitis, bullous pemphigoid, prurigo nodularis, dermatitis herpetiformis)\n* Regularly experience wheals covering the area of the body where skin testing will be conducted (eg, more than one third of the volar surface of the forearms)",
            "healthyVolunteers": false,
