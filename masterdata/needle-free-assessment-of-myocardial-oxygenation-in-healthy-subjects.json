        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Physiological Parameters Monitoring",
                    "description": "1. Saturation of peripheral oxygen as percentage\n2. Heart rate as beats per minute\n3. Noninvasive blood pressure as mmHg\n4. Partial pressure of oxygen as mmHg\n5. Partial pressure of carbon dioxide as mmHg",
                    "timeFrame": "3 hours"
                },
                {
                    "measure": "Assessment of Symptom Severity and Incidence",
                    "description": "1. Chest pain\n2. Headache\n3. Dizziness\n4. Confusion\n5. Fatigue\n6. Upset stomach\n7. Nausea\n8. Vomiting",
                    "timeFrame": "3 hours"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Participant Completion",
                    "description": "Procedure tolerability will be evaluated by the number of participants completing day 1 and day 2 of study participation.",
                    "timeFrame": "Day 1 and Day 2"
                },
                {
                    "measure": "Patient comfort level score",
                    "description": "Participants will be asked to rate their comfort level for breathing from the mask and the gas changes as Painful, Not Comfortable, Tolerable for short time, tolerable for entire study, or comfortable.",
                    "timeFrame": "3 hours"
                },
                {
                    "measure": "Quality of Cardiac MRI",
                    "description": "The cardiac MRI images will be assessed for quality.",
                    "timeFrame": "3 hours"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Adults \u2265 45 years of age\n2. No known cardiac disease\n\nExclusion Criteria:\n\n1. Low blood pressure (systolic blood pressure \\< 100 mmHg)\n2. Inability to voluntarily increase their breathing rate if prompted to do so\n3. Persons with the inability to lie supine for 30-40 minutes\n4. Persons who are pregnant, nursing, or implanted with intrauterine devices (IUD's)\n5. Persons with significant neurological, pulmonary, renal or hepatic disease\n6. Persons who have an abnormal cardiac rhythm or resting heart rate \\>100/min\n7. Inability or unwillingness to provide informed consent",
            "healthyVolunteers": true,
