                    "measure": "Tmax: Time to Reach the Maximum Plasma Concentration for CNTY-101",
                    "timeFrame": "Day 1 up to 2 years"
                },
                {
                    "measure": "t1/2: Terminal Disposition Phase Half-life for CNTY-101",
                    "timeFrame": "Day 1 up to 2 years"
                },
                {
                    "measure": "AUC: Area under the Concentration-time Curve for CNTY-101",
                    "timeFrame": "Day 1 up to 2 years"
                },
                {
                    "measure": "Percentage of Participants With at Least one Treatment Emergent Adverse Event (TEAE)",
                    "timeFrame": "Day 1 up to 2 years"
                },
                {
                    "measure": "Percentage of Participants With Clinically Significant Laboratory Abnormalities",
                    "timeFrame": "Day 1 up to 2 years"
                },
