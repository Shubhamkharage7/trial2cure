        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Experimental: FitMi AD Exercise Program",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will perform exercises by interacting with FitMi pucks, as described and monitored on a tablet computer. Participants will be asked to exercise at least 30 minutes per day for 3 months.",
                    "interventionNames": [
                        "Device: FitMi AD"
                    ]
                },
                {
                    "label": "Active Comparator: Conventional booklet of exercise program",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Participants will perform exercises described in a booklet similar to what is typically provided to individuals. Participants will be asked to exercise at least 30 minutes per day for 3 months.",
                    "interventionNames": [
                        "Other: Conventional exercise program"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "FitMi AD",
                    "description": "Exercise using the motion sensing devices and a tablet computer",
                    "armGroupLabels": [
                        "Experimental: FitMi AD Exercise Program"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Conventional exercise program",
                    "description": "Exercise following printed sheets or booklet",
                    "armGroupLabels": [
                        "Active Comparator: Conventional booklet of exercise program"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in Functional Reach Test",
                    "description": "How far can someone reach while standing in a fixed position",
                    "timeFrame": "Baseline, Immediately Post-Treatment (an average of 3 months)"
                },
                {
                    "measure": "Change in Timed Up and Go test",
                    "description": "Time it takes to stand up from a chair and walk a short distance",
                    "timeFrame": "Baseline, Immediately Post-Treatment (an average of 3 months)"
                },
                {
                    "measure": "Change in 30 second chair stand test",
                    "description": "How many times someone stand up from a chair in 30 seconds",
                    "timeFrame": "Baseline, Immediately Post-Treatment (an average of 3 months)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Visual Analog Pain Scale",
                    "description": "Indicates perceived pain intensity. The scale consists of a line, often 10 cm long, with verbal anchors at either end (i.e. \"no pain\" on the far left and \"the most intense pain imaginable\" on the far right). The patient places a mark at a point on the line corresponding to the patient's rating of pain intensity.",
                    "timeFrame": "Baseline, Immediately Post-Treatment (an average of 3 months)"
                },
                {
                    "measure": "Global Physical Activity Questionnaire",
                    "description": "Assesses physical activity habits",
                    "timeFrame": "Baseline, Immediately Post-Treatment (an average of 3 months)"
                },
                {
                    "measure": "Geriatric Depression Scale",
                    "description": "Evaluates depression in elderly individuals. Scores of 0-4 are considered normal, depending on age, education, and complaints; 5-8 indicate mild depression; 9-11 indicate moderate depression; and 12-15 indicate severe depression.",
                    "timeFrame": "Baseline, Immediately Post-Treatment (an average of 3 months)"
                },
                {
                    "measure": "Pittsburgh Sleep Quality Index",
                    "description": "Measures the quality and patterns of sleep in adults. Each item is weighted on a 0-3 interval scale. The global PSQI score is then calculated by totaling the seven component scores, providing an overall score ranging from 0 to 21, where lower scores denote a healthier sleep quality.",
                    "timeFrame": "Baseline, Immediately Post-Treatment (an average of 3 months)"
                },
                {
                    "measure": "Duration of exercise performed",
                    "description": "Duration of exercise performed over intervention period.",
                    "timeFrame": "Immediately Post-Treatment (an average of 3 months)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* have MCI or mild dementia due to AD (i.e. Clinical Dementia Rating (CDR) score 1 for mild dementia or 0.5 for MCI)\n* ability to clearly see the screen and hear the audio instructions from the tablet\n* have a family member or friend who can answer questions about the participant's daily living skills\n* willing to participate in a research study.\n\nExclusion Criteria:\n\n* age \\< 50 years old\n* use of a wheelchair as a primary mobility device (use of a cane or walker is permitted)\n* presence of other neurologic conditions such as movement disorders or history of stroke\n* other severe concurrent medical conditions that may prevent the participants from completing the 3-month study",
