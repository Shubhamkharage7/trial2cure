        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "AXIOS stent related or AXIOS endoscopic and/or study procedure related serious adverse events through 30 days post stent placement.",
                    "timeFrame": "Through study completion, an average of 1 year"
                },
                {
                    "measure": "Successful Stent Placement",
                    "description": "Transmural placement of the AXIOS stent with confirmed stent patency by visualization of the contralateral wall of the small bowel through the AXIOS stent lumen. Visualization of blue dye in the gastric lumen or injection of contrast to confirm continuity of the gastroenteric lumen, may be used to provide supplemental confirmation of intended stent placement.",
                    "timeFrame": "During the procedure"
                },
                {
                    "measure": "Clinical Success",
                    "description": "Improvement of GOOS by 1 point from baseline to any point during 14 days after AXIOS stent placement and GOOS remaining at 1 or more without the need for reintervention due to loss of stent function at 30 days post-index procedure or death, whichever comes first.",
                    "timeFrame": "through 14 days after procedure"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Clinical success over time defined as improvement of GOOS by 1 point from baseline at any time during 14 days after AXIOS stent placement and GOOS remaining at 1 or more. This endpoint will be evaluated at 3 months, 6 months, and 12 months",
                    "description": "A patient undergoing reintervention for GOO is considered a failure of clinical success",
                    "timeFrame": "Through study completion, an average of 1 year"
                },
                {
                    "measure": "Time to resumption of oral intake after stent placement.",
                    "timeFrame": "Immediately after the procedure"
                },
                {
                    "measure": "Time to start or resume chemotherapy after AXIOSTM stent placement (where applicable)",
                    "timeFrame": "Immediately after the procedure"
                },
                {
                    "measure": "Change in Quality-of-Life score (SF-12 questionnaire) from baseline to 30 days, 3 months, 6 months, and 12 months",
                    "timeFrame": "Through study completion, an average of 1 year"
                },
                {
                    "measure": "Incidence of Device Deficiencies",
                    "description": "Including but not limited to stent migration/misplacement, stent occlusion, leakage at site of stent placement.",
                    "timeFrame": "Immediately after the procedure"
                },
                {
                    "measure": "Incidence of Reintervention for GOO",
                    "description": "need for repeat treatment for persistent or recurrent GOO symptoms",
                    "timeFrame": "Immediately after the procedure"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Gastric outlet obstruction from unresectable malignant neoplasm\n2. Eligible for endoscopic intervention\n3. GOOS of 0 or 1 0 - no oral intake 1 - liquids only 2 - soft solids only 3 - low-residue or full diet\n4. 18 years of age or older\n5. Willing and able to comply with the study procedures or legally authorized representative (LAR) must provide written informed consent form (ICF) to participate in the study\n6. The distance between the gastric lumen and jejunal lumen must be no more than 1.0 cm\n\nExclusion Criteria:\n\n1. Patients with baseline ECOG \\> 2 and/or Karnofsky Performance score \\< 30\n2. Gastric cancer or any malignant infiltration precluding a cancer free puncture site of the AXIOSTM stent\n3. Abnormal coagulation INR \\> 1.5 and not correctable (per the discretion of the physician) or who require continuous complete anticoagulation, and/or any underlying condition associated with high risk of bleeding\n4. Altered anatomy of the upper gastrointestinal tract due to surgery of esophagus, stomach and duodenum that might preclude endoscopic gastroenterostomy\n5. Multiple-level bowel obstruction downstream from the intended location of the EUS- guided bypass confirmed by radiography such as small bowel series or computed tomography\n6. Presence of intraperitoneal fat between the gastric lumen and jejunal lumen estimated to be larger than than 1.0 at the proposed site of AXIOSTM stent insertion\n7. Gastric varices located within a 3.5-4 cm of the device insertion location on the gastric wall\n8. Ascites Grade \u2265 2 confirmed by cross-sectional imaging\n9. Vessels located within 3.5-4 cm of the pathway from the device insertion location on the gastric wall to the device insertion location of the small bowel wall\n10. History of multiple abdominal surgeries and/or evidence of small bowel adhesions\n11. Allergic to any of the device materials\n12. Contraindications to use of electrical devices\n13. Pregnancy\n14. Prisoners and other vulnerable populations",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
