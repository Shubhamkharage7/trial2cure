        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Behavioral Intentions for continued use",
                    "description": "The self-report measure on a 7-point scale will be completed following e-cigarette self-administration.",
                    "timeFrame": "1 hour"
                },
                {
                    "measure": "Intensity of sensory attributes",
                    "description": "The 5-item self-report measure will be completed following e-cigarette self-administration, using the general Labeled Magnitude Scale.",
                    "timeFrame": "1 hour"
                },
                {
                    "measure": "Degree of liking or disliking of sensations",
                    "description": "The self-report measure will be completed following e-cigarette self-administration, using the Labeled Hedonic Scale.",
                    "timeFrame": "1 hour"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Subjective Appeal",
                    "description": "The 11-item modified Cigarette Evaluation Questionnaire (mCEQ) will be completed following e-cigarette self-administration.",
                    "timeFrame": "1 hour"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion criteria:\n\n1. Young adult e-cigarette user or older adult smoker\n2. Willing to abstain from all nicotine product use for 12 hours prior to the study visit\n3. Read and speak English\n\nExclusion criteria:\n\n1. Currently attempting to quit nicotine products\n2. Currently pregnant, planning to become pregnant, or breastfeeding\n3. Current or past use of tobacco products other than e-cigarettes or cigarettes\n4. Self-reported diagnosis of lung disease\n5. Self-reported new or unstable cardiovascular disease",
