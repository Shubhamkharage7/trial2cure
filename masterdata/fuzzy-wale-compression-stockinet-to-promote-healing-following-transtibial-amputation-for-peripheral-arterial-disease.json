        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Fuzzy Wale Compression Stockinet",
                    "type": "EXPERIMENTAL",
                    "description": "Subjects with peripheral arterial disease requiring transtibial amputation will have Fuzzy Wale Compression Stockinet applied 3-7 days pre-operatively and utilized for 4-8 weeks post operatively or until prosthetic fitting and wound has achieved full durable healing.",
                    "interventionNames": [
                        "Device: Fuzzy Wale Compression Stockinet"
                    ]
                },
                {
                    "label": "Standard of care",
                    "type": "NO_INTERVENTION",
                    "description": "Subjects with peripheral arterial disease requiring transtibial amputation will receive standard of care treatment compression stockinet."
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Fuzzy Wale Compression Stockinet",
                    "description": "EdemaWear is a tubular, circular knit elastic stockinet fabricated with elastic, vertically oriented fuzzy wales, with the compression provided by the elastic, horizontal oriented Lycra\u00ae spandex yarns knitted in between. The stockinet is applied from the foot to the popliteal crease, over the affected area. EdemaWear can be applied in direct contact with the skin or over a wound care dressing. Sizing is based on leg circumference. EdemaWear is indicated for edema management of various etiologies (lymphedema, chronic venous insufficiency, congestive heart failure), and optimizing wound healing. The stockinette is often combined with other boxed compression sets. The manufacturer's stated dosage of EdemaWear is 8-12mmHg when used alone.",
                    "armGroupLabels": [
                        "Fuzzy Wale Compression Stockinet"
                    ],
                    "otherNames": [
                        "EdemaWear"
                    ]
                }
            ]
        },
