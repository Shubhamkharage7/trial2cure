        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Adopt the NIH PROMIS system (The Assessment Center Instrument Library, which includes all Fixed Length Short Form and Computer Adaptive Test/Item Bank instruments from PROMIS) to construct a modified comprehensive PRO instrument.",
                    "description": "Using a modified NIH PRO measurement system. This system will involve different domains and instruments. Patients will answer PRO questions at the time of recruitment (baseline), then followed up 3 months and 6 months after the first survey (follow-up).\n\nThe modified comprehensive PRO instrument will specifically ask patients to report symptoms based on their health status within a fixed time window, and at the time when the survey questions are given.",
                    "timeFrame": "18-month study length, with a pilot reliability study."
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Selected comorbidities, all-cause hospitalization and ER visits.",
                    "description": "Extracted from patients' electronic health records.",
                    "timeFrame": "3 months and 6 months follow-up."
                },
                {
                    "measure": "Patients' COVID vaccination and mAb therapy history will be verified based on self report and EHR.",
                    "timeFrame": "18-month study length"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Have been tested for COVID at one of the MultiCare facilities,\n* AND 18 years old or older at the viral test,\n* AND have no problem understanding English or Spanish.\n\nExclusion Criteria:\n\n* Pediatric patients younger than 18 years old at the time of testing,\n* OR patients who are pregnant,\n* OR patients who plan to get pregnant in 3 months,\n* OR patients who are hospitalized at the time of recruitment,\n* OR patients who have cognitive impairment(s) at the recruitment.",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "A small representative sample of up to 50 patients (25 hospitalized and alive at the time of the extraction) will be randomly selected and invited to test for the PRO instrument",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
