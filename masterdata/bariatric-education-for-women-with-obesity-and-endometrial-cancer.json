        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Control Group",
                    "description": "If the patient is in group 1, she will get standard education for weight loss, which will be provided by her Gynecologic Oncologist. This will take about 15 minutes.",
                    "interventionNames": [
                        "Behavioral: Standard weight loss education"
                    ]
                },
                {
                    "label": "Video Group",
                    "description": "If the patient is in group 2, she will get standard education for weight loss provided by her Gynecologic Oncologist. Then she will be asked to view a 15-minute video to get information regarding obesity, the relationship of obesity and endometrial cancer, and the safety and benefits of bariatric surgery. These will take about 30 minutes.",
                    "interventionNames": [
                        "Behavioral: Standard weight loss education + Educational Video"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Standard weight loss education",
                    "description": "Participants will be randomized and given the standard weight loss education by the Gynecologic Oncology provider.",
