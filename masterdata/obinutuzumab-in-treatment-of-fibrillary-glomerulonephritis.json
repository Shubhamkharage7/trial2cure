        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Obinutuzumab in Treatment of Fibrillary Glomerulonephritis",
                    "type": "EXPERIMENTAL",
                    "description": "Patients with biopsy proven fibrillary GN who have \\>1 gram/24 hour of proteinuria and eGFR \u2265 20 ml/min/BSA will be treated with Obinutuzumab.",
