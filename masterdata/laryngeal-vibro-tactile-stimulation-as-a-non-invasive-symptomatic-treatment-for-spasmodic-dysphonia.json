        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Low dose / continuous VTS",
                    "type": "EXPERIMENTAL",
                    "description": "Low dose refers to receiving VTS 5 times/week for 20 minutes each; non-task-specific, constant stimulation during non-speech",
                    "interventionNames": [
                        "Device: vibro-tactile stimulation (VTS)"
                    ]
                },
                {
                    "label": "Low dose / speech activated VTS",
                    "type": "EXPERIMENTAL",
                    "description": "Low dose refers to receiving VTS 5 times/week for 20 minutes each; VTS during connected speech",
                    "interventionNames": [
                        "Device: vibro-tactile stimulation (VTS)"
                    ]
                },
                {
                    "label": "High dose / continuous VTS",
                    "type": "EXPERIMENTAL",
                    "description": "High dose refers to receiving VTS 7 times/week for 20 minutes each; non-task-specific, constant stimulation during non-speech",
                    "interventionNames": [
                        "Device: vibro-tactile stimulation (VTS)"
                    ]
                },
                {
                    "label": "High dose / speech activated VTS",
                    "type": "EXPERIMENTAL",
                    "description": "High dose refers to receiving VTS 7 times/week for 20 minutes each; VTS during connected speech",
                    "interventionNames": [
                        "Device: vibro-tactile stimulation (VTS)"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "vibro-tactile stimulation (VTS)",
                    "description": "To apply vibro-tactile stimulation, participants will use a wearable device. It contains two vibratory motors (Precision MicrodrivesTM, Model 307 - 100) that are low-voltage (\\~1V), non-invasive and will be located approximately above the lateral portions of the thyroid cartilage. For this protocol, they vibrate at frequencies between 40 - 100 Hz (i.e., 100 times per second",
                    "armGroupLabels": [
                        "High dose / continuous VTS",
                        "High dose / speech activated VTS",
                        "Low dose / continuous VTS",
                        "Low dose / speech activated VTS"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in perceived speech effort (PSE)",
                    "description": "Speaker will assess effort themselves and assign an score (range: 0-10 with a score of '10' indicating most severe effort)",
                    "timeFrame": "24 months"
                },
                {
                    "measure": "Change in smoothed cepstral peak prominence (CPPS)",
                    "description": "CPPS provides a measure of the strength of the fundamental frequency within background aperiodicity (physical unit is dB)",
                    "timeFrame": "24 months"
                },
                {
                    "measure": "Change in speech quality vector (SQV) (%)",
                    "description": "A derived measure designed to understand if a participant's voice symptom improvement occurs across objective (CPPS) and subjective measures of speech (PSE). It is based on the relative change between two time points",
                    "timeFrame": "24 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* 18 years of age or older\n* Confirmed diagnosis of adductor or abductor LD for a minimum of 6 months.\n\nExclusion Criteria:\n\n* Regular intake of benzodiazepines\n* Cognitive impairment: score \\< 27 on the Mini-mental State Examination (MMSE).\n* Identify with a neurological or musculoskeletal impairment affecting speech motor function. These impairments may include a form of: Dyskinesia, Dystonia other than LD, Essential Tremor, Huntington's Disease, Multiple System Atrophy, Muscle Tension Dysphonia, Parkinsonism, Progressive Supranuclear Palsy, Spasticity, Intracranial Neoplasm (brain tumor), Spinal Neoplasm, Cerebrovascular Accident (Stroke), Mild Traumatic Brain Injury, Intracranial Hemorrhage, Multiple Sclerosis.\n* Non-English speaker. We will not enroll people who cannot speak English, because the voice assessment procedures (Test sentences, CAPE_V clinical rating) have only been validated for English speakers at this point.\n* Pregnant people. We will not enroll pregnant people as the device used in this study is investigational.",
            "healthyVolunteers": false,
