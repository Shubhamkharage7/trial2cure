            "armGroups": [
                {
                    "label": "Interpersonal Psychotherapy adapted for Type 2 Diabetes",
                    "type": "EXPERIMENTAL",
                    "description": "Participants in the IPT-T2D will receive 1 hour weekly IPT from a licensed psychologist in a group setting for 6 weeks.",
                    "interventionNames": [
                        "Behavioral: Group Program"
                    ]
                },
                {
                    "label": "Health Education adapted for Type 2 Diabetes",
                    "type": "EXPERIMENTAL",
                    "description": "Participants in the Health Education-T2D will receive 1 hour weekly Health Education lessons from a licensed psychologist in a group setting for 6 weeks.",
                    "interventionNames": [
                        "Behavioral: Group Program"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
