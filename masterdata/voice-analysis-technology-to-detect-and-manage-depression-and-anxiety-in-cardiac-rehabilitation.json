        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Cardiac rehabilitation plus voice analysis",
                    "type": "EXPERIMENTAL",
                    "description": "Subjects will complete 12 weeks of cardiac rehabilitation as per clinical care and utilize a voice analysis smartphone app.",
                    "interventionNames": [
                        "Device: Ellipsis Health Voice Application"
                    ]
                },
                {
                    "label": "Cardiac rehabilitation only",
                    "type": "NO_INTERVENTION",
                    "description": "Subjects will complete 12 weeks of cardiac rehabilitation as per clinical care."
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Ellipsis Health Voice Application",
                    "description": "Smartphone application that utilizes voice biomarkers to assess for potential presence and severity of depression and anxiety",
                    "armGroupLabels": [
                        "Cardiac rehabilitation plus voice analysis"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in depressive symptoms",
                    "description": "Measured using the self-reported Patient Health Questionnaire-9 (PHQ-9) to assess for major depressive disorder. Possible score range from 0 to 27, with higher scores indicating a worse outcome/greater severity of depressive symptoms.",
                    "timeFrame": "Baseline, 12 weeks"
                },
                {
                    "measure": "Change in anxiety symptoms",
                    "description": "Measured using the self-reported Generalized Anxiety Disorder 7-Item Scale (GAD-7) that assesses anxiety symptoms. Possible score range from 0 to 21, with higher scores indicating a worse outcome/greater severity of anxiety symptoms.",
                    "timeFrame": "Baseline, 12 weeks"
                },
                {
                    "measure": "Change in perceived stress",
                    "description": "Measured using the self-reported Perceived Stress Scale-14 (PSS-14), a 14 item questionnaire which assesses the degree to which situations in life are stressful within the last month. Each item is scored on a 5 point Likert scale from 0 (never) to 4 (very often), higher total scores indicate a worse outcome/greater severity of perceived stress.",
                    "timeFrame": "Baseline, 12 weeks"
                },
                {
                    "measure": "Change in quality of life",
                    "description": "Measured using the self-reported abbreviated generic Quality of Life Scale developed through the World Health Organization (WHOQOL-BREF) questionnaire that assesses an individual's perceptions of their health and well-being within the last two weeks. Each item is scored on a 5 point Likert scale, higher scores indicate greater perceived quality of life.",
                    "timeFrame": "Baseline, 12 weeks"
                },
                {
                    "measure": "Change in health behaviors",
                    "description": "Measured using the self-reported Current Health Behaviors (HB) questionnaire, a 13 item questionnaire which assesses behaviors and life circumstances during the last month. Each item is scored on a 10 point Likert scale, higher scores indicate better outcome.",
                    "timeFrame": "Baseline, 12 weeks"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Enrolled in cardiac rehabilitation program to start within 3 months from hospital discharge.\n* Owns a smartphone.\n* Willing to download and use a smartphone app.\n* Able to read, write, and speak English.\n\nExclusion Criteria:\n\n* Actively treated for an anxiety or depressive disorder through psychotherapy or pharmacologic treatments.\n* Cardiac transplant.\n* Active substance use.\n* Neurocognitive disorder.\n* Active psychosis.\n* Mania diagnosis.\n* Active suicidality",
