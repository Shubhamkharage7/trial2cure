        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Knowledge about TECW",
                    "description": "Percent correct on 12-item knowledge test based on Protect our Planet: The Earth is Not Disposable (California Youth Advocacy Network) and Environmental Impact of Cigarette Butts (Tobacco Free CA) videos. Higher scores reflect more accurate knowledge about TECW",
                    "timeFrame": "6 weeks"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Perceived harmfulness of TECW",
                    "description": "Questions adapted from Epperson et al. (2021) and Hoek et al. (2020); score ranges from 1-5, higher scores reflect greater perceived harm",
                    "timeFrame": "6 weeks"
                },
                {
                    "measure": "Belief that TEC manufacturers should be accountable for waste mitigation",
                    "description": "Questions adapted from Hoek et al. (2020); score ranges from 1-4, higher scores reflect stronger regulatory belief",
                    "timeFrame": "6 weeks"
                },
                {
                    "measure": "Use of TECW Tracker",
                    "description": "Number of reports received in ArcGIS-based TECW Tracker program",
                    "timeFrame": "6 weeks"
                },
                {
                    "measure": "Readiness to engage in regulatory behavior",
                    "description": "Questions adapted from Pulvers (2022); scores range from 1-5; higher scores reflect greater regulatory readiness",
                    "timeFrame": "6 weeks"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* student at University of California, Davis or California State University San Marcs\n* between the ages of 18 and 25\n* taking at least one class on the main campus\n* at least six months until graduation\n* have a smart phone with location services enabled for apps\n* regular access to a device with sufficient WiFi\n* private space for three remote visits on over the next six weeks and one visit in six months\n* willing to have camera on during remote visits\n\nExclusion Criteria:\n\n* None",
