        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Intervention",
                    "type": "EXPERIMENTAL",
                    "description": "Clinicians in this arm will be trained to conduct the adherence intervention in general rheumatology clinic with all follow up patients with a chronic rheumatic disease.\n\nPatients seen by these clinicians will contribute to the outcome data collection.",
                    "interventionNames": [
                        "Behavioral: Clinician-led adherence intervention"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Clinician-led adherence intervention",
                    "description": "The clinician will review real time pharmacy refill data during clinic and use effective communication techniques to discuss adherence with the patient",
                    "armGroupLabels": [
                        "Intervention"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Intervention Feasibility",
                    "description": "Percent of providers agreeing to participate",
                    "timeFrame": "4 weeks"
                },
                {
                    "measure": "Intervention Feasibility",
                    "description": "Provider survey based on the Feasibility of Intervention Measure, scores range from 1-5, 5 is most feasible",
                    "timeFrame": "4 weeks"
                },
                {
                    "measure": "Intervention Feasibility",
                    "description": "Percent of visits with EMR documentation of providers reviewing refills",
                    "timeFrame": "4 weeks"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Acceptability",
                    "description": "Provider survey based on the Acceptability of Intervention Measure, scores range from 1-5, 5 is most acceptable",
                    "timeFrame": "4 weeks"
                },
                {
                    "measure": "Acceptability, as measured by the percentage of patients who report a positive feeling after having an adherence conversation with their providers",
                    "description": "Patient survey will be administered after their provider visit",
                    "timeFrame": "4 weeks"
                },
                {
                    "measure": "Fidelity",
                    "description": "Provider survey about the frequency of conducting the intervention components of reviewing pharmacy refills and using effective communication strategies",
                    "timeFrame": "4 weeks"
                },
                {
                    "measure": "Fidelity",
                    "description": "Frequency of EMR documentation of completing the intervention components of reviewing pharmacy refills and using effective communication strategies",
                    "timeFrame": "4 weeks"
                },
                {
                    "measure": "Change in medication adherence",
                    "description": "90-day medication possession ratio at 3 months after the intervention visit compared to baseline",
                    "timeFrame": "baseline, 3 months"
                },
                {
                    "measure": "Change in medication adherence",
                    "description": "90-day medication possession ratio at 6 months after the intervention visit compared to baseline",
                    "timeFrame": "baseline, 6 months"
                }
            ]
        },
