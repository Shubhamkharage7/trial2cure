            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "High volume evacuation (HVE)",
                    "description": "Intra oral suction will be done and during the procedure,bacterial colony forming units (cfus) will be captured during dental care by placing large format petri dishes containing bacterial culture media in the dental operatory with open lids. Once aerosol capture is complete, petri dishes will be incubated and quantified under BSL-2 safety conditions in the lab.Patient will be given hydrogen peroxide rinse.",
                    "armGroupLabels": [
                        "HVE suction and external evacuation device (EED) and hydrogen peroxide",
                        "HVE suction and extraoral vacuum aspirator (EVA) and hydrogen peroxide",
                        "High volume evacuation (HVE) suction only and hydrogen peroxide"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "Extraoral vacuum aspirator (EVA)",
                    "description": "Extraoral suction connected to dental suction will be used and bacterial colony forming units (cfus) will be captured during dental care by placing large format petri dishes containing bacterial culture media in the dental operatory with open lids. Once aerosol capture is complete, petri dishes will be incubated and quantified under BSL-2 safety conditions in the lab.Patient will be given hydrogen peroxide rinse.",
                    "armGroupLabels": [
                        "HVE suction and extraoral vacuum aspirator (EVA) and hydrogen peroxide"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "External evacuation device (EED)",
                    "description": "Extraoral suction unit will be used and bacterial colony forming units (cfus) will be captured during dental care by placing large format petri dishes containing bacterial culture media in the dental operatory with open lids. Once aerosol capture is complete, petri dishes will be incubated and quantified under BSL-2 safety conditions in the lab.Patient will be given hydrogen peroxide rinse.",
                    "armGroupLabels": [
                        "HVE suction and external evacuation device (EED) and hydrogen peroxide"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Hydrogen Peroxide",
                    "description": "Subjects will be given 1.5 % Oral Solution of hydrogen peroxide mouth rinse",
                    "armGroupLabels": [
                        "HVE suction and external evacuation device (EED) and hydrogen peroxide",
                        "HVE suction and extraoral vacuum aspirator (EVA) and hydrogen peroxide",
                        "High volume evacuation (HVE) suction only and hydrogen peroxide"
                    ]
                }
            ]
        },
