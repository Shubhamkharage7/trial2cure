        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Sensitivity and Specificity",
                    "description": "Standard measures of accuracy calculated using cross-tabulation of predicted risk category and confirmed cancer progression over a 5-year period from biopsy",
                    "timeFrame": "5 years"
                },
                {
                    "measure": "Survival analysis",
                    "description": "Kaplan-Meier analyses to compare time to a positive diagnosis of oral cancer between patients/biopsies with a STRATICYTE\u2122 classification of Low- and Elevated-Risk.",
                    "timeFrame": "5 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "AUC",
                    "description": "Area under the receiver operator curve",
                    "timeFrame": "5 years"
                },
                {
                    "measure": "C-index (Harrell's)",
                    "description": "From all possible pairs of patients, compute the number of concordant and discordant pairs. Compute C-index as the proportion of all possible pairs that are concordant.",
                    "timeFrame": "5 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* From the archive, any patient who presents with clinically evident oral lesions and biopsy-proven dysplasia (punch or scalpel biopsies; any grade or classification system)\n* Patients with initial oral lesions with epithelial atypia suspicious for neoplasia, with:\n* No histological evidence of cancer with clinical follow-up data for a period of at least five years; or\n* OSCC development (histologic or documented evidence of invasive cancer).\n* Patients who had archived biopsy tissue blocks of a previous oral lesion(s) meeting the above criteria and retained at the same clinical center.\n\nExclusion Criteria:\n\n* Patients with oral lesions or dysplasia with no indication of OSCC development or follow-up data of less than five years in non-OSCC patients.\n* Patients diagnosed with oral epithelial dysplasia concomitant with OSCC at the time of the biopsy's original pathology report or a subsequent clinical note of cancer progression within three months post-biopsy.",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "The target population are patients presenting with an OPMD who have received a biopsy of their lesion. The study sample will be composed of patients with previously biopsied OPMDs from contracted clinics.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
