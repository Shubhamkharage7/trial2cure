            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Subjects undergoing planned aseptic revision surgery to replace part or all of their hip or knee implant or subjects undergoing planned revision for infection",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
