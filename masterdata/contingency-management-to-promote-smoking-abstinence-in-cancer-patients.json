        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Abstinence Rates as assessed by self-report for past 7 days, Carbon Monoxide Breath Monitoring and/or Anabasine testing",
                    "description": "Abstinence will be defined by 7 days by self-report of no tobacco products, confirmed by CO breath test less than or equal to 4ppm and/or urine anabasine levels less than or equal to 2 ng/ml",
                    "timeFrame": "Changes between the study intake appointment to day of surgery, up to five weeks apart"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Abstinence Rates as assessed by self-report for past 7 days, Carbon Monoxide Breath Monitoring and/or Anabasine testing",
                    "description": "Abstinence will be defined by 7 days by self-report of no tobacco products, confirmed by CO breath test less than or equal to 4ppm and/or urine anabasine levels less than or equal to 2 ng/ml",
                    "timeFrame": "Three and Six months after surgery date"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Age 18 or older\n2. smoking 1 or more combustible tobacco products per day\n3. diagnosed with or suspicion of any type of operable cancer\n\nExclusion Criteria:\n\n1. unstable psychiatric/medical conditions such as suicidal ideation, acute psychosis, or dementia\n2. non-English speaking\n3. use of alternative nicotine delivery systems (e.g., e-cigarettes, snus, etc)\n4. pregnant women",
