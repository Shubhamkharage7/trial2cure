        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Educational videos arm",
                    "type": "EXPERIMENTAL",
                    "interventionNames": [
                        "Behavioral: Educational videos"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Educational videos",
                    "description": "Participants will be delivered a link to complete demographic information and the initial assessment. Upon completing the (initial) pre-test knowledge assessment, participants will be sent links to the three educational videos staggered across 7 days. Educational videos will determine gaps in knowledge related to opioid use and overdose based on questionnaire responses. Upon the completion of the 7 days, participants will be sent a link to the (final) post-test knowledge assessment to complete.",
                    "armGroupLabels": [
                        "Educational videos arm"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Acceptability of the educational materials presented via the educational videos, including cultural congruence as assessed by a questionnaire",
                    "description": "This questionnaire has 10 questions that is measured on a Likert scale from 1-4, higher number indicating more acceptability.",
                    "timeFrame": "end of study (Day 7)"
                },
                {
                    "measure": "Changes in opioid related knowledge as assessed by a questionnaire",
                    "description": "This has 3 subscales\n\n1. Brief Opioid Overdose Knowledge (BOOK)\n\n   This has 12 questions and each is scored from 0(true), 1(false) for a maximum score of 12, higher score indicating more knowledge\n2. Opioid Overdose Knowledge Scale (OOKS)\n\n   This has 9 questions, and participants have to identify all correct answers. Score ranges from 0(no correct answers) -33 (identified all correct answers) higher score indicates more knowledge\n3. Opioid Overdose Attitude Scale (OOAS)\n\nThis has 28 questions scored on a Likert scale from 1(completely disagree)-5(completely agree), higher number indicating more positive attitude",
                    "timeFrame": "Baseline, end of study (Day 7)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Must identify as Hispanic/Latino/a descent\n* Have an adolescent between the ages of 15 and 17 years old living in their home a majority of the time\n* Have English or Spanish proficiency\n\nExclusion Criteria:\n\n* Adolescent or parent currently in opioid treatment\n* Parent self-reports a substance use (including opioid use) disorder for themselves or adolescent",
