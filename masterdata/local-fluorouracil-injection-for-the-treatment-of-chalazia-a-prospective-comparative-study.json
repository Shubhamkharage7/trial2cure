        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Incision and Curettage",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "A vertical incision over the area of chalazion will be done. Inflammatory material will be removed and the chalazion capsule will be excised.",
                    "interventionNames": [
                        "Procedure: Incision and Curettage"
                    ]
                },
                {
                    "label": "Injection of Triamcinolone Acetonide",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "0.1 ml of triamcinolone is injected directly in the chalazion.",
                    "interventionNames": [
                        "Drug: Triamcinolone Acetonide",
                        "Procedure: Incision and Curettage"
                    ]
                },
                {
                    "label": "Injection of 5-fluorouracil",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "0.1 ml of 5-fluorouracil is injected directly in the lesion transconjunctivally.",
                    "interventionNames": [
                        "Drug: 5-fluorouracil Injection",
                        "Procedure: Incision and Curettage"
                    ]
                },
                {
                    "label": "Injection of triamcinolone/5FU mixture",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "0.1 ml of a 4:1 mixture of 4 parts 5-FU and 1 part triamcinolone is injected in the lesion.",
                    "interventionNames": [
                        "Procedure: Incision and Curettage",
                        "Drug: Triamcinolone Acetonide/5-fluorouracil mixture"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "5-fluorouracil Injection",
                    "description": "If the chalazion is present at follow-up visit, another injection of 5-FU will be given.",
                    "armGroupLabels": [
                        "Injection of 5-fluorouracil"
                    ],
                    "otherNames": [
                        "5-FU"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Triamcinolone Acetonide",
                    "description": "If chalazion present at follow-up, another Triamcinolone injection will be done.",
                    "armGroupLabels": [
                        "Injection of Triamcinolone Acetonide"
                    ],
                    "otherNames": [
                        "triamcinolone"
                    ]
                },
                {
                    "type": "PROCEDURE",
                    "name": "Incision and Curettage",
                    "description": "If chalazion present at follow-up visit, patient will be randomized to receive local injection of either triamcinolone, 5-FU, or triamcinolone/5-FU mixture.",
                    "armGroupLabels": [
                        "Incision and Curettage",
                        "Injection of 5-fluorouracil",
                        "Injection of Triamcinolone Acetonide",
                        "Injection of triamcinolone/5FU mixture"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Triamcinolone Acetonide/5-fluorouracil mixture",
                    "description": "If chalazion persists at follow-up visit, another injection of triamcinolone/5-FU mixture will be done.",
                    "armGroupLabels": [
                        "Injection of triamcinolone/5FU mixture"
                    ]
                }
            ]
        },
