        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Treatment",
                    "type": "EXPERIMENTAL",
                    "description": "Atezolizumab (Tecentriq) intravenous (IV) 1200mg flat dose day 1 then every 3 weeks.\n\nPirfenidone (Esbriet) orally (PO) with food according to this schedule:\n\nDays 1-14: 267 milligrams (mg) orally three times per day (PO TID) Days 15-29: 534 mg PO TID Days 30 onward until progression: 801 mg PO TID",
                    "interventionNames": [
                        "Drug: Atezolizumab"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Atezolizumab",
                    "description": "Atezolizumab is given as an intravenous infusion at 1200 mg every 3 weeks. Pirfenidone is taken by mouth 3 times a day with the dose increasing every 2 week until day 30",
                    "armGroupLabels": [
                        "Treatment"
                    ],
                    "otherNames": [
                        "Pirfenidone"
                    ]
                }
            ]
