            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Superion\u2122 IDS device",
                    "description": "The Superion\u2122 IDS is a minimally-invasive spinal implant that treats LSS symptoms by limiting extension at the symptomatic level that compresses the neural elements and is designed for percutaneous surgical placement.",
                    "armGroupLabels": [
                        "Superion\u2122 IDS device"
                    ]
                }
            ]
