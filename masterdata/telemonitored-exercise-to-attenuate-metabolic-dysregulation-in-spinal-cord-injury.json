        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Spinal Cord Injury Exercise Group",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will exercise at home 3-5 times per week under supervision via teleconference. Participants will be part of this group for 18-20 weeks.",
                    "interventionNames": [
                        "Other: Spinal Cord Injury Exercise Group"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "OTHER",
                    "name": "Spinal Cord Injury Exercise Group",
                    "description": "Participants will have an initial accommodation period of 4 weeks, during which four 10-minute exercise bouts/day will be interspersed with 5 minutes of rest. The bouts will gradually increase until at least continuous 40-minute sessions. The remaining 12-week intervention will be set at 70% max heart rate for a total of 16 weeks.",
                    "armGroupLabels": [
                        "Spinal Cord Injury Exercise Group"
                    ]
                }
            ]
        },
