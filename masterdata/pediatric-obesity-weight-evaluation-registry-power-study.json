        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Assess Patient Response to Weight Management Program",
                    "description": "This is a composite outcome measure that will be measured with changes in patient weight and BMI, fasting lipid profile, liver enzymes, fasting glucose and insulin, hemoglobin A1C and blood pressure.",
                    "timeFrame": "2 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Measure Program Retention",
                    "description": "This will be measured by the number of visits and days that occur after the initial medical evaluation.",
                    "timeFrame": "2 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* age 18 years or younger\n* overweight or obese patient\n* initial medical evaluation in a pediatric weight management program between March 1, 2014-April 30, 2020.\n\nExclusion Criteria:\n\n* no exclusion criteria",
