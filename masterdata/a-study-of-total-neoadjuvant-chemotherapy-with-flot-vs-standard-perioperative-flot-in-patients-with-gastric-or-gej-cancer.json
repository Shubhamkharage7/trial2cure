        "descriptionModule": {
            "briefSummary": "This is a randomized pilot study to evaluate and to compare the completion rates of Total Neoadjuvant chemotherapy with FLOT ( FLOT-TNT) and perioperative chemotherapy with FLOT ( FLOT-POP).",
            "detailedDescription": "Participants will be randomized 1:1 to either total Neoadjuvant chemotherapy with with FLOT-TNT ( all 4 cycles of chemotherapy before surgery) or standard of care FLOT-POP ( 2 cycles of chemotherapy before surgery and 2 cycles of chemotherapy after surgery). Each cycle of chemo is 28 days long with chemo administered on Days 1 and Day 15 of the cycle."
        },
        "conditionsModule": {
            "conditions": [
                "Gastric Adenocarcinoma",
                "Gastroesophageal Junction Adenocarcinoma"
            ],
            "keywords": [
                "Gastric cancer",
                "GEJ cancer"
            ]
        },
        "designModule": {
            "studyType": "INTERVENTIONAL",
            "phases": [
