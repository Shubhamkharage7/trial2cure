            "designInfo": {
                "allocation": "RANDOMIZED",
                "interventionModel": "PARALLEL",
                "interventionModelDescription": "This study is a two arm phase II pilot design, with eligible subjects randomized 1:1 to either Arm A ( Investigational arm) or Arm B ( Standard arm). The chemotherapy dose modification will follow routine clinical care. Both arms will receive chemotherapy and curative surgery.",
                "primaryPurpose": "TREATMENT",
                "maskingInfo": {
                    "masking": "NONE"
                }
            },
