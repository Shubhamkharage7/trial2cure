        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Deactivated device",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Wear deactivated pregnancy coach device for the remainder of pregnancy.",
                    "interventionNames": [
                        "Device: Deactivated Pregnancy Coach"
                    ]
                },
                {
                    "label": "Active device",
                    "type": "EXPERIMENTAL",
                    "description": "Wear active device for remainder of pregnancy (including 3 day run-in period)",
                    "interventionNames": [
                        "Device: Active Pregnancy Coach"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Deactivated Pregnancy Coach",
                    "description": "Naturalistic monitoring of their typical sleep patterns and behaviors",
                    "armGroupLabels": [
                        "Deactivated device"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "Active Pregnancy Coach",
                    "description": "The Pregnancy Coach will begin to deliver alerts when supine sleep is detected. Sleep metrics will be visible on the smartphone app.",
                    "armGroupLabels": [
                        "Active device"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Proportion of sleep time spent in the supine position as measured by the Pregnancy Coach",
                    "description": "Will be conducted using the naturalistic arm (deactivated device). Simple descriptive statistics to report the average number of minutes and the average proportion of supine sleep per week.",
                    "timeFrame": "Until delivery, approximately 12 weeks"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Proportion of sleep time spent with objective snoring as indicated by the Pregnancy Coach",
                    "description": "Will be conducted using the naturalistic arm (deactivated device). Proportion of time will be determined during each gestational week.",
                    "timeFrame": "Until delivery, approximately 12 weeks"
                },
                {
                    "measure": "Proportion of participants where their 'going to sleep' position was the same as their dominant sleep position",
                    "description": "Will be conducted using the naturalistic arm (deactivated device). We will determine whether the \"going to sleep\" position during each gestational week is predictive of the most dominant sleep position.",
                    "timeFrame": "Until delivery, approximately 12 weeks"
                },
                {
                    "measure": "Percentage of sleep position changes following alert from Pregnancy Coach",
                    "description": "Will be conducted using the intervention arm (active device). Percentage of sleep position changes in response to an alert from the Pregnancy Coach will be reported for each gestational week.",
                    "timeFrame": "Until delivery, approximately 12 weeks"
                }
            ]
        },
