            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Propranolol three times a day",
                    "description": "Patients with larger (\\>2cm) or multiple (\\>1) hemangiomas in this arm will get propranolol in the following regimen:\n\nWeek 1: 0.5 mg/kg/day divided TID Week 2: 1 mg/kg/day divided TID Week 3: 2 mg/kg/day divided TID. Patients will stay on this dosage until their 3 month follow up visit.",
                    "armGroupLabels": [
                        "ter in die (TID)three times a day"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Propranolol twice a day",
                    "description": "Patients with larger (\\>2cm) or multiple (\\>1) hemangiomas in this arm will get propranolol in the following regimen:\n\nWeek 1: 0.5 mg/kg/day divided BID Week 2: 1 mg/kg/day divided BID Week 3: 2 mg/kg/day divided BID. Patients will stay on this dosage until their 3 month follow up visit.",
                    "armGroupLabels": [
                        "bis in die (BID)twice a day"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Timolol",
                    "description": "Patients with a small (\\<2cm) isolated infantile hemangioma are prescribed timolol 0.5% ophthalmic drops, a topical beta-blocker(1 drop BID directly onto the hemangioma).",
                    "armGroupLabels": [
                        "Control"
                    ]
                }
            ]
        },
