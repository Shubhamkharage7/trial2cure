        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Non-Particulate Steroid",
                    "type": "EXPERIMENTAL",
                    "description": "Patients in this arm will receive an injection of 1 milliliter of 2% lidocaine with 10 milligrams of dexamethasone to one or both sacroiliac joints. If participants initially achieve relief from the injection but then have a return of pain they may be offered a second injection with the same drug at the same dose.",
                    "interventionNames": [
                        "Drug: Dexamethasone",
                        "Drug: 2% Lidocaine HCl Injection"
                    ]
                },
                {
                    "label": "Particulate Steroid",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Patients in this arm will receive an injection of 1 milliliter of 2% lidocaine with 40 milligrams of methylprednisolone to one or both sacroiliac joints. If participants initially achieve relief from the injection but then have a return of pain they may be offered a second injection with the same drug at the same dose.",
                    "interventionNames": [
                        "Drug: Methylprednisolone",
                        "Drug: 2% Lidocaine HCl Injection"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Dexamethasone",
                    "description": "This is a non-particulate steroid commonly used to treat SI joint pain.",
                    "armGroupLabels": [
                        "Non-Particulate Steroid"
                    ],
                    "otherNames": [
                        "Ozurdex",
                        "Maxidex",
                        "DexPak"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Methylprednisolone",
                    "description": "This is a particulate steroid commonly used to treat SI joint pain.",
                    "armGroupLabels": [
                        "Particulate Steroid"
                    ],
                    "otherNames": [
                        "Solu-medrol",
                        "Depo-Medrol",
