        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "lifestyle intervention plus phentermine-topiramate",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will be enrolled in a weight loss program based on lifestyle changes and the use of phentermine-topiramate to aid with weight loss",
