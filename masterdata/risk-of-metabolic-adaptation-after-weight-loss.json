                    ]
                },
                {
                    "label": "lifestyle intervention",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will be enrolled in a weight loss program based on lifestyle changes. They will be given a 6-month subscription to a calorie-reduced meal replacement program that also offers lifestyle coaching.",
                    "interventionNames": [
                        "Other: lifestyle intervention with meal replacement program"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "OTHER",
                    "name": "phentermine-topiramate combined with lifestyle intervention",
                    "description": "participants will be enrolled in a medical weight loss program and will be offered a 6 month prescription for phentermine-topiramate (note: The goal of the study is NOT to assess efficacy or safety of this drug) in addition to lifestyle advice.",
                    "armGroupLabels": [
                        "lifestyle intervention plus phentermine-topiramate"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "lifestyle intervention with meal replacement program",
                    "description": "participants will be enrolled in a medical weight loss program and will be given a 6 month subscription to a calorie-reduced meal replacement program, in addition to lifestyle advice.",
                    "armGroupLabels": [
                        "lifestyle intervention"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "change in measured and predicted resting metabolic rate during and after weight loss",
                    "description": "resting metabolic rate will be measured by indirect calorimetry every 2 months for 6 months, then every 6 months for an additional 12 month. Predicted resting metabolic rate will be calculated with a mathematical formula at the same time points",
                    "timeFrame": "18 months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "change in circulating leptin during and after weight loss",
                    "description": "blood will be assayed for this hormone every 2 months for 6 months, then every 6 months for an additional 12 months",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "change in circulating thyroid hormones during and after weight loss",
                    "description": "blood will be assayed for this hormone every 2 months for 6 months, then every 6 months for an additional 12 months",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "change in circulating GLP-1 during and after weight loss",
                    "description": "blood will be assayed for this hormone every 2 months for 6 months, then every 6 months for an additional 12 months",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "change in circulating PYY during and after weight loss",
                    "description": "blood will be assayed for this hormone every 2 months for 6 months, then every 6 months for an additional 12 months",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "change in circulating AgRP during and after weight loss",
                    "description": "blood will be assayed for this hormone every 2 months for 6 months, then every 6 months for an additional 12 months",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "change in 24h urine catecholamines during and after weight loss",
                    "description": "urine will be assayed for these hormones every 2 months for 6 months, then every 6 months for an additional 12 months",
                    "timeFrame": "18 months"
                },
                {
                    "measure": "change in heart rate variability during and after weight loss",
                    "description": "blood will be assayed for this hormone every 2 months for 6 months, then every 6 months for an additional 12 months",
                    "timeFrame": "18 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\nBMI \\> or = 30 any ethnicity\n\nExclusion Criteria:\n\nweight \\> or = 400 lbs.\n\ndiabetes (type 2 or type 1)\n\nhypertension\n\nheart disease\n\nkidney disease\n\nliver disease\n\nactive cancer\n\npost solid organ or bone marrow transplant\n\nHIV/AIDS\n\npregnancy\n\nbreastfeeding\n\ncurrent smoker\n\ncurrent recreational drug use",
