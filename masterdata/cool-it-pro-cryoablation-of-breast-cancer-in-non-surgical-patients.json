        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Cryoablation",
                    "description": "Following pre-procedural imaging evaluation, patients will receive ultrasound-guided percutaneous cryoablation as part of their routine treatment of their breast cancer. Follow up imaging (if tolerated) will be performed at 6 months and 12 months and annually thereafter for until 3 years post-procedure. Data will be collected on patient demographics, disease characteristics, treatment, treatment complications, follow-up imaging, response (for 3 years post-procedure), and quality of life.",
                    "interventionNames": [
                        "Device: Cryoablation"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Cryoablation",
