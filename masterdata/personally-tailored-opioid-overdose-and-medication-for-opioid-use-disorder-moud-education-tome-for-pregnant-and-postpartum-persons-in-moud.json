        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Personally-Tailored Opioid-overdose and Medication for opioid use disorder (MOUD) Education (TOME)",
                    "type": "EXPERIMENTAL",
                    "description": "Drug: Naloxone kit\n\nNaloxone nasal spray (NARCAN) is a potentially life-saving medication that can stop or reverse the effects of an opioid overdose. This nasal spray is approved by the FDA for reversing OODs and has a favorable side-effects profile. A take-home kit will be provided to participants, which may be useful in the event of a future opioid overdose.\n\nBehavioral: Personally-Tailored Opioid-overdose and Medication for opioid use disorder (MOUD) Education (TOME)\n\nTOME entails a trained RA: 1) administering a REDCap survey to assess an individual's opioid-overdose/MOUD knowledge; and 2) reviewing the personal feedback reports with the recipient.",
                    "interventionNames": [
                        "Behavioral: Personally-Tailored Opioid-overdose and Medication for opioid use disorder (MOUD) Education (TOME)"
                    ]
                },
                {
                    "label": "Control",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Drug: Naloxone kit\n\nNaloxone nasal spray (NARCAN) is a potentially life-saving medication that can stop or reverse the effects of an opioid overdose. This nasal spray is approved by the FDA for reversing OODs and has a favorable side-effects profile. A take-home kit will be provided to participants, which may be useful in the event of a future opioid overdose.\n\nBehavioral: SAMHSA handouts\n\nSAMHSA handouts: 1) \"Opioid Overdose Prevention Toolkit: Safety Advice for Patients and Family Members\"; 2) \"Opioid Overdose Prevention Toolkit: Recovering from Opioid Overdose\"; and 3) \"Medication-Assisted Treatment for Opioid Addiction: Facts for Families and Friends\". These handouts can be offered as physical copies or electronically.",
                    "interventionNames": [
                        "Behavioral: Control"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Personally-Tailored Opioid-overdose and Medication for opioid use disorder (MOUD) Education (TOME)",
                    "description": "The TOME intervention is a modified version of the personally-tailored opioid overdose prevention education and naloxone distribution (PTOEND) intervention (Winhusen et al. Evaluation of a personally-tailored opioid overdose prevention education and naloxone distribution intervention to promote harm reduction and treatment readiness in individuals actively using illicit opioids. Drug Alcohol Depend. Nov 1 2020;216:108265). Modifications include updating the knowledge assessment/education to reflect changes in the drug supply (i.e., the current high prevalence of fentanyl and increasing prevalence of xylazine) and the addition of items specific to pregnancy. Like PTOEND, TOME is a computer-guided intervention which utilizes REDCap to complete assessments and automatically generate personally-tailored feedback reports.",
                    "armGroupLabels": [
                        "Personally-Tailored Opioid-overdose and Medication for opioid use disorder (MOUD) Education (TOME)"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Control",
                    "description": "Participants randomized to the control condition will be offered three SAMHSA handouts.",
                    "armGroupLabels": [
                        "Control"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Medication for Opioid Use Disorder (MOUD) knowledge score",
                    "description": "This outcome is measured by the Opioid Overdose and Treatment Awareness Survey (OOTAS) knowledge evaluation:\n\nMOUD knowledge, potential score of 0-10 - higher score indicates more knowledge",
                    "timeFrame": "Screening through Week 3"
                },
                {
                    "measure": "Opioid Overdose knowledge score",
                    "description": "This outcome is measured by the first three sections of the Opioid Overdose and Treatment Awareness Survey (OOTAS).\n\nOpioid-overdose knowledge, potential score of 0-31 - higher score indicates more knowledge",
                    "timeFrame": "Screening through Week 3"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Medication for Opioid Use Disorder (MOUD) Internalized Stigma",
                    "description": "This will be assessed with the The Methadone Maintenance Treatment Stigma Mechanisms Scale (MMT-SMS) questionnaire.\n\nScore range: 25 - 125 - higher score indicates greater MOUD stigma",
                    "timeFrame": "Screening through Week 3"
                },
                {
                    "measure": "Drug Self-efficacy",
                    "description": "This will be assessed with the Thoughts about abstinence (TAA) instrument.\n\nDrug Self-efficacy Score range: 0 - 9 - higher score indicates greater expected success in avoiding drug use",
                    "timeFrame": "Screening through Week 3"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\nPotential participants must be:\n\n1. 18 years of age or older;\n2. Pregnant or be within 12 months postpartum;\n3. Enrolled in MOUD (either buprenorphine or methadone) at the study site or affiliated clinic where enrollment can be confirmed;\n4. Able to understand the study, and having understood, provide written informed consent in English\n\nExclusion Criteria:\n\nPotential participants must not:\n\n1. have suicidal or homicidal ideation requiring immediate attention.",
            "healthyVolunteers": false,
            "sex": "FEMALE",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ]
        },
