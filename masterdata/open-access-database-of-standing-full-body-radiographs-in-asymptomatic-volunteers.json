            "interventions": [
                {
                    "type": "RADIATION",
                    "name": "EOS imaging",
                    "description": "The EOS imaging system will be utilized to obtain full-body radiographs in frontal and sagittal projections obtained in the upright standing position. Measurements will include a coronal plumb-line, sagittal vertebral axis (SVA), external auditory meatus plumb-line, cervical lordosis, thoracic kyphosis, lumbar lordosis, hip flexion/extension, knee flexion/extension, ankle flexion/extension, T1 tilt, T1 spino-pelvic instance, acetabular index, pelvic tilt (PT), pelvic incidence (PI), and sacral slope (SS).",
                    "armGroupLabels": [
                        "asymptomatic EOS Imaging"
                    ]
                }
            ]
        },
