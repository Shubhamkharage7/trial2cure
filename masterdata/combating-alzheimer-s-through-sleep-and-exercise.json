            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "DREEM 2 Headband",
                    "description": "Participants will wear the DREEM 2 headband for 3 months for a minimum of 4 out of 7 nights per week and will participate in its digital therapeutic Cognitive Behavioral Therapy - Insomnia (CBT-I) intervention (entails brain stimulation to improve slow wave sleep and sleep hygiene counseling tips to improve sleep behaviors).",
                    "armGroupLabels": [
                        "Exercise and Sleep Group",
                        "Sleep Only Group"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Exercise Routine",
                    "description": "Participants will partake in a guided exercise routine. Each routine consists of 30-minute mild circuit training session twice per week for 3 months.",
                    "armGroupLabels": [
                        "Exercise Only Group",
                        "Exercise and Sleep Group"
                    ]
                }
            ]
