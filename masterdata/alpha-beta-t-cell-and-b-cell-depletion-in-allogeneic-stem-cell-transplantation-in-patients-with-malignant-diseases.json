            "primaryOutcomes": [
                {
                    "measure": "Acute graft versus host disease (aGVHD) incidence",
                    "description": "Compare the incidence of grade II to IV aGVHD following allogeneic stem cell trasplantation utilizing \u03b1/\u03b2 CD3+ T-cell and CD19+ B-cell depletion compared to historical controls by day +100",
                    "timeFrame": "100 days"
                }
            ],
            "secondaryOutcomes": [
