                    "timeFrame": "1 year"
                },
                {
                    "measure": "Number of urinary tract infection episodes",
                    "description": "Diagnose of UTI will be measured for this outcome",
                    "timeFrame": "up to 2 years"
                },
                {
                    "measure": "Severity of UTI symptoms as measured by the UTI Symptom Assessment questionnaire",
                    "description": "UTI Symptom Assessment questionnaire measures severity of UTI symptoms. Possible score range from 0 to 3 (0= Did not have 1= Mild 2=Moderate, and 3=Severe), higher score indicates higher severity of UTI symptoms",
                    "timeFrame": "1 year"
                },
                {
                    "measure": "Duration of intervals between UTI episodes",
                    "description": "Duration of intervals will be measured between one UTI episode to the next UTI episode",
                    "timeFrame": "1 year"
                }
            ]
