            "primaryOutcomes": [
                {
                    "measure": "Number of Urinary Tract Infections",
                    "description": "It would be measured by counting the positive urine cultures, or UTI-like episodes requiring antibiotic treatment during the 1 year of the study period.",
                    "timeFrame": "1 year"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Number of oral anti-biotic usage",
