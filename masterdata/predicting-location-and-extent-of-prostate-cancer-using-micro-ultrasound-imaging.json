        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "ExactVu Imaging",
                    "type": "EXPERIMENTAL",
                    "description": "EV29L transducer will be inserted in the subject's rectum. Cine sweeps will be performed to save images of:\n\n1. The entire prostate from posterior to anterior (may require 2 sweeps to cover base and apex)\n2. The peripheral zone using the highest zoom setting (30mm depth) on the system (may require 2 sweeps to cover base and apex). Analysis of these images will be performed after surgery but before prostatectomy",
                    "interventionNames": [
                        "Device: ExactVu"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "ExactVu",
                    "description": "clinical micro-ultrasound, imaging Device, Manufacturer; Exact Imaging",
                    "armGroupLabels": [
                        "ExactVu Imaging"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Micro-Ultrasound Detection of High Grade Prostate Cancer (Gleason Score \u22657) vs. Surgical Pathology",
                    "description": "Evaluate the ability of micro-ultrasound to identify high grade prostate cancer foci (Gleason score \u22657) in comparison to final surgical pathology result (prostatectomy or biopsy)",
                    "timeFrame": "From date of micro ultrasound exam until the date of Prostate biopsy data are obtained (average of 7 days"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Prostate Cancer Border Identification Post-Prostatectomy",
                    "description": "Evaluate the ability of micro-ultrasound to identify the borders of prostate cancer foci in comparison to final surgical pathology result in men after prostatectomy.",
                    "timeFrame": "3 years"
                },
                {
                    "measure": "Micro-Ultrasound Detection of Low Grade Prostate Cancer (Gleason Score 6)",
                    "description": "Evaluate the ability of micro-ultrasound to detect low grade prostate cancer foci (Gleason score 6)",
                    "timeFrame": "3 years"
                },
                {
                    "measure": "Prostate Cancer Detection: Micro-Ultrasound vs. mpMRI Comparison",
                    "description": "Comparison of prostate cancer detection between micro-ultrasound and multiparametric magnetic resonance imaging (mpMRI).",
                    "timeFrame": "3 years"
                },
                {
                    "measure": "Measuring correlation between Prostatectomy pathology and imaging correlation",
                    "description": "Perform a detailed pathologic and imaging correlation between findings on micro-ultrasound, MRI and final surgical histopathology in men who undergo prostatectomy",
                    "timeFrame": "3 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Scheduled for MR-US fusion prostate biopsy or radical prostatectomy\n* Agree to consent to the study\n\nExclusion Criteria:\n\n* Does not agree to consent.",
