        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Polatuzumab vedotin",
                    "type": "EXPERIMENTAL",
                    "description": "Evaluable patients for safety Patients receiving 1 dose of Polatuzumab Vedotin will be evaluable for safety.\n\nEvaluable patients for response Only in patients who are in PR or SD prior to PV and received a minimum of 3 doses will be evaluable.\n\nEvaluable patients for EFS, PFS, OS All patients who have completed conditioning and autoSCT will be evaluable for EFS, PFS, and OS.",
                    "interventionNames": [
