        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Primary Safety Endpoint: Characterization of device-related adverse events",
                    "timeFrame": "Surgery through 12 months post-op"
                },
                {
                    "measure": "Primary Effectiveness Endpoint: Change in back and leg pain Visual Analog Score (VAS) scores",
                    "description": "VAS scores range from 0 (no pain) to 100 (pain is as bad as it could possibly be). Decreases in scores indicate reductions in pain.",
                    "timeFrame": "Baseline to 3 months post-op"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in neurological status",
                    "description": "Change in neurological status measured by the neurological component of the standard physical exam.",
                    "timeFrame": "Baseline to 3, 6, and 12 months post-op"
                },
                {
                    "measure": "Change in disability measured by the Oswestry Disability Index (ODI).",
                    "description": "Each question is scored from 0-5. The scores from each question are summed, then divided by the maximum number possible, then multiplied by 100 to represent a percentage. The percentages are broken down into 5 category ranges: 0% to 20%: minimal disability, 21%-40%: moderate disability; 41%-60%: severe disability; 61%-80%: crippled; 80%-100%: bed-bound or exagerating.",
                    "timeFrame": "Baseline to 3, 6, and 12 months post-op"
                },
                {
                    "measure": "Change in quality of life as measured by PROMIS Scale v1.2 - Global Health.",
                    "description": "The is a 10-item patient-reported questionnaire in which the response options are presented as a 5-point rating scale. The results of the scores are are used to calculate two summary scores: a Global Physical Health score and a Global Mental Health score. These scores are then standardized to the general population, using the \"T-Score.\" The average \"T-Score\" for the United States population is 50 points, with a standard deviation of 10 points. Higher scores indicate a healthier patient.",
                    "timeFrame": "Baseline to 3, 6, and 12 months post-op"
                },
                {
                    "measure": "Change in quality of life as measured by EQ-5D-5L.",
                    "description": "The EQ-5D-5L survey consists of 5 dimensions with the score for each ranging from 1 (no problems) to 5 (unable to do). For individual dimensions, negative changes (e.g. from 5 to 3) indicate improvement. The scores on these five dimensions will be presented as a health profile and converted to a single summary index number (utility). Positive changes in the total score indicate improvement.",
                    "timeFrame": "Baseline to 3, 6, and 12 months post-op"
                },
                {
                    "measure": "Change in use of analgesics over time",
                    "description": "Change in use of medications at all follow up visits.",
                    "timeFrame": "Baseline through 12 months post-op"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Amount of device migration",
                    "description": "Amount of device migration as measured by anteroposterior (AP) and lateral x-rays.",
                    "timeFrame": "Surgery to 3, 6, and 12 months post-op"
                },
                {
                    "measure": "Costs and cost-effectiveness of the intervention",
                    "description": "The cost effectiveness analysis will report total healthcare-related expenditures reported by patients from baseline to 12 months post-op and 12 month quality-adjusted life-years (QALYs). Incremental cost per QALY will be estimated and compared to previously published studies.",
                    "timeFrame": "Baseline through 12 months post-op"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Indicated for a single level or two level adjacent spinal fusion having back pain and radiculopathy\n* Have been diagnosed with chronic, intractable pain of the lower back and/or leg refractory to conservative therapy for at least 3 of months\n* At least 6 months since last surgical procedure on the spine\n* Be 21 years of age or older at the time of enrollment\n* Be willing and capable of giving informed consent\n* Be willing and able to comply with study-related requirements\n\nExclusion Criteria:\n\n* Any prior spinal fusion at index or adjacent level\n* Pregnant\n* Have a life expectancy of less than 1 year\n* Be concomitantly participating in another clinical study\n* Be involved in an injury claim under current litigation\n* Baseline narcotic use of \u2265 100 MME per day\n* Significant untreated addiction to dependency producing medications\n* Current active implantable medical device\n* Cancer\n* Have osteoporosis\n* Active infection\n* Allergies to system components\n* AGE \\> 80\n* Expected need to undergo MRI imaging in the future\n* Other significant comorbidities",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "21 Years",
            "maximumAge": "80 Years",
