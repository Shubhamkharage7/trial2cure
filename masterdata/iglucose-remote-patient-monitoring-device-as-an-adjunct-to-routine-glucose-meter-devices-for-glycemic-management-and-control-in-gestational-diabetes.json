            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "iGlucose Smart Meter System",
                    "description": "Subjects will have routine care enhanced by use of theThe iGlucose Smart meter system. This is an example of integration of a SMBG system with internet-based technology; including cell phone notifications for reminders, feedback, and communication with clinicians",
                    "armGroupLabels": [
                        "iGlucose Smart Meter"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "Routine Care-RX meter",
                    "description": "Subjects will receive routine care with the insurance prescribed glucose meter. They will self manage with traditional diabetes diary.",
                    "armGroupLabels": [
                        "Rx glucose meter"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Neonatal morbidity",
                    "timeFrame": "at the time of discharge (about 2-7 days after birth)"
                },
                {
                    "measure": "Neonatal gestational age at delivery",
                    "timeFrame": "at delivery"
                },
                {
                    "measure": "# of neonates who where Small for gestational age (SGA)",
                    "timeFrame": "at delivery"
                },
                {
                    "measure": "Neonatal intensive care unit (NICU) length of stay",
                    "timeFrame": "delivery to discharge (2-7 days)"
                },
                {
                    "measure": "# of Neonates with hyperbilirubinemia-requiring therapy.",
                    "timeFrame": "delivery to discharge (2-7 days)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Average Neonatal Weight",
                    "description": "To evaluate the relationship of change in average neonatal weight to two skill sets of Self-monitoring of blood glucose (SMBG), operational and interpretive, as demonstrated by patient-perceived self-efficacy and adherence to self-care behaviors",
                    "timeFrame": "at the time of discharge (about 2-7 days after birth)"
                },
                {
                    "measure": "Maternal Mean fasting glucose",
                    "timeFrame": "perinatal to postpartum (32 weeks)"
                },
                {
                    "measure": "Maternal Mean post prandial glucose",
                    "timeFrame": "perinatal to postpartum (32 weeks)"
                },
                {
                    "measure": "Maternal # of Hypoglycemia events (<60 mg/dl)",
                    "timeFrame": "perinatal to postpartum (32 weeks)"
                },
                {
                    "measure": "Maternal weight gain during pregnancy",
                    "timeFrame": "perinatal to postpartum (32 weeks)"
                },
                {
                    "measure": "Maternal Gestational hypertension",
                    "timeFrame": "perinatal to postpartum (32 weeks)"
                },
                {
                    "measure": "Maternal Preeclampsia",
                    "timeFrame": "perinatal to postpartum (32 weeks)"
                },
                {
                    "measure": "Maternal Cesarean delivery",
                    "timeFrame": "at delivery"
                },
                {
                    "measure": "Maternal Operative vaginal delivery",
                    "timeFrame": "at delivery"
                },
                {
                    "measure": "Effect of SMBG checks on pregnancy",
                    "description": "To evaluate relationship between percent of total achieved SMBG checks, diabetes distress, and maternal and neonatal outcomes, for women living with GDM and T2DM during pregnancy.",
                    "timeFrame": "perinatal to postpartum (32 weeks)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria\n\n* All patients \\>/= women 18 years of age with gestational diabetes or type 2 diabetes during pregnancy and identified by the research team and consented before 30.6 weeks.\n* Non-anomalous singletons\n* Ability to provide informed consent\n\nExclusion Criteria\n\n* Patients with Type 1 diabetes\n* Patients with fetal major congenital abnormalities\n* Patients identified/referred after 30.6 weeks",
