        "descriptionModule": {
            "briefSummary": "The overall aim of the study is to develop and validate a Rheumatoid Arthritis-Interstitial Lung Disease (RA-ILD) clinical prediction model (screening tool) based on risk factors to guide screening for ILD in patients with RA using High Resolution Computed Tomography (HRCT)."
        },
        "conditionsModule": {
            "conditions": [
                "Arthritis, Rheumatoid",
                "Lung Diseases, Interstitial"
            ]
        },
        "designModule": {
