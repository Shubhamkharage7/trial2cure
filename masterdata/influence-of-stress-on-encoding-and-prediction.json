        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Acute Stress",
                    "type": "EXPERIMENTAL",
                    "description": "Participants complete the socially evaluated cold pressor test (SECPT), a validated laboratory-based stress induction procedure involving submerging an arm in an ice bath",
                    "interventionNames": [
                        "Behavioral: Socially Evaluated Cold Pressor Test"
                    ]
                },
                {
                    "label": "No Stress",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Participants complete a matched condition with no stress-related exposure involving submerging an arm in warm water",
                    "interventionNames": [
                        "Behavioral: Control"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Socially Evaluated Cold Pressor Test",
                    "description": "The SECPT is a brief validated laboratory-based stress induction containing both physical and social elements",
                    "armGroupLabels": [
                        "Acute Stress"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Control",
                    "description": "This is matched to the SECPT but with warm instead of cold water",
                    "armGroupLabels": [
                        "No Stress"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Changes in fMRI signal",
                    "description": "fMRI signal during learning will be assessed using connectivity, multivariate, and univariate approaches over 1-2 hour long learning period",
                    "timeFrame": "1-2 hours"
                },
                {
                    "measure": "Statistical learning",
                    "description": "Online measures of statistical learning (reaction time) based on item predictability during task",
                    "timeFrame": "1-2 hours"
                },
                {
                    "measure": "Retention of statistical learning",
                    "description": "Offline measure of statistical learning assessed through familiarity tests the next day (comparison of accurate recognition of previously-studied pairs to chance, 50%)",
                    "timeFrame": "1 hour"
                },
                {
                    "measure": "Episodic memory",
                    "description": "Memory for individual items will be assessed through performance on old/new recognition tests the next day, quantified as hit rates and A' (non-parametric normalized ratio of hits to false alarms)",
                    "timeFrame": "1 hour"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Cortisol reactivity",
                    "description": "Change in salivary cortisol levels following the SECPT/control procedure",
                    "timeFrame": "1-2 hours"
                },
                {
                    "measure": "Alpha-amylase reactivity",
                    "description": "Change in salivary alpha-amylase levels following the SECPT/control procedure",
                    "timeFrame": "1-2 hours"
                }
            ]
        },
