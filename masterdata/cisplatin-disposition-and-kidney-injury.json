        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Kidney Injury with Cisplatin and 5-HT3 Antagonist Antiemetic Regimen as Assessed by a 1.5 fold increase in a Biomarker Panel",
                    "description": "The effects of 5-HT3 antagonist antiemetic drugs on cisplatin kidney injury as indicated by a 1.5 fold increase in the urinary biomarker panel values at 3 days after treatment",
                    "timeFrame": "3 days"
                },
                {
                    "measure": "The Effects of 5-HT3 Antagonist Antiemetic Drugs on Cisplatin Secretion",
                    "description": "The changes to cisplatin secretion in the urine (as an early biomarker for the detection of kidney injury) as indicated by a 6 mg difference between 5-HT3 Antagonist Antiemetic Drugs at 3 days after treatment",
                    "timeFrame": "3 days"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Targeted Genetic Polymorphisms are Associated with Risk of Kidney Injury Due to Cisplatin and 5-HT3 Antagonist Antiemetic Regimen as Assessed by a 1.5 fold increase in a Biomarker Panel",
                    "description": "The influence of targeted genetic polymorphisms on risk of kidney injury due to cisplatin and 5-HT3 Antagonist Antiemetic Drugs as indicated by a 1.5 fold increase in a urinary biomarker panel values at 3 days after treatment",
                    "timeFrame": "3 days"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Male or female patient prescribed cisplatin at a dose of \\>25 mg/m\\^2\n* Age 18-80 years\n* Hemoglobin \\>/=10 g/dl\n* No consumption of grapefruit juice or alcohol within 7 days\n* No history of alcohol consumption of \\>14 drinks/week\n* No history of organ transplantation or kidney dialysis\n* Willingness to comply with study\n* Not pregnant or lactating\n* No changes in chronic medications within 2 weeks\n* Estimated glomerular filtration rate (eGFR) \\> 60 ml/min\\^2\n* Normal liver function (ALT and AST \\<2x ULN)\n\nExclusion Criteria:\n\n* Diagnosis of kidney cancer\n* Previous exposure to platinum-based chemotherapy\n* Herbal supplement use beyond marijuana\n* Exposure to other known nephrotoxins (including contrast agents) within the previous 2 weeks\n* Concurrent use of competitive inhibitors of transport proteins (metformin, cimetidine, ranitidine, antiviral drugs, cephalosporins, topotecan, methotrexate, vinblastine)\n* Severe gastrointestinal disease with fluid losses\n* Diagnosis of a rapidly progressive glomerulonephritis\n* Allergy or contraindication to 5-HT3 Antagonists",
            "healthyVolunteers": false,
