        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Study Participant",
                    "description": "Study participants can be individuals with either a clinical diagnosis or genetic confirmation of Friedreich ataxia. In addition, this study enrolls Friedreich ataxia carriers and unaffected controls."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Friedreich Ataxia Rating Scale",
                    "description": "rating scale based on clinical neurologic examination",
                    "timeFrame": "once every 1 year"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "9-hole peg test",
                    "description": "timed test of fine motor skills performed as a set of four trials (two trials per hand), for patients with FA who are able to complete this testing",
                    "timeFrame": "once every 1 year"
                },
                {
                    "measure": "timed 25 foot walk",
                    "description": "timed 25 foot walk is performed twice for patients with FA who are able to complete this testing. Assistive devices such as canes, service dogs, walkers, or crutches are permitted.",
                    "timeFrame": "once every 1 year"
                },
                {
                    "measure": "Vision assessment",
                    "description": "High and low contrast visual acuity tested on patients with FA who are able to perform this test. Glasses or contact lenses are permitted.",
                    "timeFrame": "once every 1 year"
                },
                {
                    "measure": "Quality of Life Questionnaires",
                    "description": "a set of quality of life questionnaires is administered for study participants with Friedreich ataxia. Questionnaires include items such as activities of daily living, overall opinion on health and function, and fatigue-related questions.",
                    "timeFrame": "once every 1 year"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Optional sample collection",
                    "description": "study participants will be asked to provide a cheek swab and/or blood sample for a variety of different measures including frataxin protein level assessments or other biomarker tests",
                    "timeFrame": "once every 1 year"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Males or females age 4 to 80 years.\n2. Genetically confirmed diagnosis of FA (for carrier/control cheek swab and blood samples this is not required).\n3. Clinically confirmed diagnosis of FA, pending confirmatory genetic testing through a commercial or research laboratory (for carrier/control cheek swab and blood samples this is not required).\n4. Parental/guardian permission (informed consent) and if appropriate, child assent.\n\nExclusion Criteria:\n\n1) Signs or symptoms of severe cardiomyopathy (such as congestive heart failure)",
            "healthyVolunteers": true,
            "sex": "ALL",
            "minimumAge": "4 Years",
            "maximumAge": "80 Years",
            "stdAges": [
                "CHILD",
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Participants with either a clinical diagnosis or genetic test confirmation of Friedreich ataxia. Study also accepts carriers of Friedreich ataxia along with healthy volunteers.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
