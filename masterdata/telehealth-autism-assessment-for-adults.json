            "primaryOutcomes": [
                {
                    "measure": "Concurrent validity",
                    "description": "Concurrent validity will be assessed through correlations between the domain, Overall Total, and calculated Comparison scores of the two ADOS-2 versions, as well as through comparison of total scores on the Social Communication Questionnaire (SCQ) and the Social Responsiveness Scale, Second Edition (SRS-2).",
                    "timeFrame": "Immediately after intervention"
                }
            ]
