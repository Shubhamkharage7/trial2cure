        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Omnigraft 3 month - 1 year Group 1",
                    "description": "Subjects will be enrolled between 3 months and 1 year post Simultaneous Pancreas Kidney (SPK) transplant. Blood sample to test Omnigraft will be collected every 3 months, at the time of rejection or when a biopsy is done."
                },
                {
                    "label": "Omnigraft 1 year - 3 years Group 2",
                    "description": "Subjects will be enrolled between 12 months and 3 year post SPK transplant. Blood sample to test Omnigraft will be collected every 3 months, at the time of rejection or when a biopsy is done."
                },
                {
                    "label": "Omnigraft for cause biopsy Group 3",
                    "description": "Subjects will be enrolled at any time if a biopsy is done between 3 months to 3 year post SPK transplant. Then, blood samples to test Omnigraft will be collected."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Number of participants that underwent SPK transplant",
                    "description": "Number of participants that underwent SPK transplant measured by Omnigraft testing. The Omnigraft test includes TruGraf GEP and TRAC dd-cfDNA.",
                    "timeFrame": "Up to 3 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Male or female adult (\u226518 years old) with a history of simultaneous pancreas kidney transplant between 3 months and 1 year prior to enrollment (cohort 1) or at the time of presentation for \"for-cause\" biopsy\n2. Adults able to consent\n\nExclusion Criteria:\n\n1. Recipient of an organ transplant other than a simultaneous pancreas kidney transplant\n2. Known to be pregnant\n3. Known to be infected with Human Immunodeficiency Virus\n4. Known to have active Polyoma (BK) nephropathy\n5. Participation in other biomarker studies",
