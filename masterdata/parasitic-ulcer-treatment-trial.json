        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Topical steroids",
                    "type": "EXPERIMENTAL",
                    "description": "Participants in this arm will receive anti-amoebic therapy plus topical steroids.",
                    "interventionNames": [
                        "Drug: Polyhexamethylene biguanide (PHMB)",
                        "Drug: Topical corticosteroid"
                    ]
                },
                {
                    "label": "Topical placebo",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "Participants in this arm will receive anti-amoebic therapy plus topical placebo.",
                    "interventionNames": [
                        "Drug: Polyhexamethylene biguanide (PHMB)",
                        "Other: Topical placebo"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Polyhexamethylene biguanide (PHMB)",
                    "description": "PHMB is a cationic antiseptic agent used for topical treatment of acanthamoeba keratitis (AK). Both treatment groups will take PHMB at least 4 times daily while on the allocated study drug.",
                    "armGroupLabels": [
                        "Topical placebo",
                        "Topical steroids"
                    ],
                    "otherNames": [
                        "Polyhexanide"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Topical corticosteroid",
                    "description": "Dexamethasone sodium phosphate, 0.1% ophthalmic solution will be administered 4 times daily for 4 weeks, then 2 times daily for 2 weeks, then 1 time daily for 2 weeks.",
                    "armGroupLabels": [
                        "Topical steroids"
                    ]
                },
                {
                    "type": "OTHER",
                    "name": "Topical placebo",
                    "description": "An artificial tear ophthalmic solution will be administered 4 times daily for 4 weeks, then 2 times daily for 2 weeks, then 1 time daily for 2 weeks.",
                    "armGroupLabels": [
                        "Topical placebo"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Vision",
                    "description": "Best corrected visual acuity",
                    "timeFrame": "6 Months"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Self-reported eye pain",
                    "description": "Pain visual analog scale (Likert scale from 0 to 10; 0=no pain, 10=worst pain)",
                    "timeFrame": "2 months"
                },
                {
                    "measure": "Clinical Resolution",
                    "description": "Time until clinical resolution (i.e., healed ocular surface and absence of inflammation)",
                    "timeFrame": "12 months"
                },
                {
                    "measure": "Multivariate Analysis",
                    "description": "The multivariate analysis will include the following outcomes measured at 6 months: best corrected visual acuity, corneal thinning on optical coherence tomography (OCT), scar density on Scheimpflug imaging, irregular astigmatism, glare, microbial clearance on confocal microscopy, pain score, time until clinical resolution",
                    "timeFrame": "6 Months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* AK on at least one of the following: culture, smear, PCR, shotgun sequencing, biopsy, or confocal microscopy\n* Ocular inflammation after 1 month of anti-amoebic treatment, defined as conjunctival, corneal, episcleral, or anterior chamber inflammation\n\nExclusion Criteria:\n\n* Evidence or history of interstitial keratitis\n* Known herpetic keratitis, as determined from history, exam, or microbiological testing\n* Known fungal keratitis, as demonstrated from corneal scrapings\n* Corneal perforation or impending corneal perforation\n* Prior therapeutic keratoplasty for acanthamoeba keratitis\n* Unwillingness or inability to follow-up\n* No light perception in the affected eye\n* Known hypertensive response to steroids\n* Corticosteroid allergy\n* Concurrent treatment with systemic corticosteroids\n* Concurrent granulomatous amoebic encephalitis",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "8 Years",
            "stdAges": [
                "CHILD",
                "ADULT",
                "OLDER_ADULT"
            ]
        },
