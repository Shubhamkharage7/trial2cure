                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Light goggles",
                    "description": "Blue light goggles designed to promote circadian entrainment will be worn for 1 hour each morning for two weeks.",
                    "armGroupLabels": [
                        "Light Googles then Orange Glasses",
                        "Orange Glasses then Light Googles"
                    ]
                },
                {
                    "type": "DEVICE",
                    "name": "Orange glasses",
                    "description": "Orange glasses that do not provide entrainment will be worn for 1 hour each morning to block morning light for two weeks.",
                    "armGroupLabels": [
                        "Light Googles then Orange Glasses",
                        "Orange Glasses then Light Googles"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Saliva samples for melatonin levels",
                    "description": "Serial saliva samples will be collected to calculate dim light melatonin onset.",
                    "timeFrame": "Baseline and 2-weeks post-intervention"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Light exposure using the Speck",
                    "description": "The Speck light meter will be used to collect overall light exposure.The Speck light meter uses a photosensor to measure the amount of light study participants are exposed to each day.",
                    "timeFrame": "worn all 12 study weeks"
                },
                {
                    "measure": "Sleep time using actigraphy",
                    "description": "Sleep time will be calculated from the actigraph. The wrist actigraph measures activity during the day and how well you sleep at night. The actigraph software will calculate the amount of time spent in bed sleeping.",
                    "timeFrame": "worn all 12 study weeks"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* healthy adults over the age of 55\n* score of 25 or greater on MoCA\n\nExclusion Criteria:\n\n* Severe sleep apnea\n* restless leg syndrome\n* obstructing cataracts\n* macular degeneration\n* diabetic retinopathy\n* glaucoma\n* use of sleep medications\n* brain disease",
