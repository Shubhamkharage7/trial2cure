        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Time until return of motor function following spinal placement",
                    "description": "This will be defined as full strength with both 1) foot plantar and dorsiflexion and 2) hip flexion and 3) knee flexion in the non-operative extremity.",
                    "timeFrame": "From anesthesia stop time until 7 days post-op"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Ambulation",
                    "description": "Time to first ambulation and distance at first ambulation",
                    "timeFrame": "from anesthesia stop time until 7 days post-op"
                },
                {
                    "measure": "Length of Stay",
                    "description": "based on anesthesia end time to time discharge order written",
                    "timeFrame": "Time from anesthesia end time to when discharge order is written, up to 30 days post-op"
                },
                {
                    "measure": "Side Effects",
                    "description": "Side effects such as Urinary retention, dizziness, symptomatic hypotension, transient neurologic symptoms",
                    "timeFrame": "0-48 hours post spinal placement"
                },
                {
                    "measure": "Pain Scores",
                    "description": "Visual Analog Pain Score 0-100 pain scores at rest, and with movement. A lower pain score means a better outcome.",
                    "timeFrame": "Pacu to 48 hours post-spinal placement"
                }
            ],
            "otherOutcomes": [
                {
                    "measure": "Preoperative opioid use",
                    "description": "Use of opioids in the pre-operative period converted to MME (morphine equivalent)",
                    "timeFrame": "within past 90 days at time of procedure"
                },
