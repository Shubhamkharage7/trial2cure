        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in qFibrosis",
                    "description": "Change in the continuous quantitative liver fibrosis (qFibrosis) score measured by second harmonic generation/two-photon excitation microscopy",
                    "timeFrame": "Baseline to week 52"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Improvement of non-alcoholic steatohepatitis clinical research network (NASH-CRN) fibrosis (F) stage by \u22651 stage without worsening of NASH",
                    "description": "Assessment will be based on the proportion of ALN-HSD treated participants relative to placebo achieving at least a 1 stage improvement in fibrosis by liver biopsy with no worsening of NASH",
                    "timeFrame": "Baseline to week 52"
                },
                {
                    "measure": "Resolution of NASH with no worsening of NASH-CRN fibrosis on liver biopsy",
                    "description": "Resolution of NASH (steatohepatitis) is defined as absent fatty liver disease or isolated or simple steatosis without steatohepatitis and a non-alcoholic fatty liver disease activity score (NAS) score of 0-1 for inflammation, 0 for ballooning, and any value for steatosis",
                    "timeFrame": "Baseline to week 52"
                },
