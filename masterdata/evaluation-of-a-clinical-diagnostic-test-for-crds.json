        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Pacing",
                    "type": "EXPERIMENTAL",
                    "description": "Separate ventricular and atrial pacing trains will be administered at different cycle lengths and the ventricular repolarization response on the first sinus beat following the subsequent pause will be evaluated.",
                    "interventionNames": [
                        "Diagnostic Test: Pacing"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "Pacing",
                    "description": "1. Ventricular 10 beat burst at 500ms (120bpm)\n2. Ventricular 10 beat burst at 400ms (150bpm)\n3. Atrial 10 beat burst at 500ms (120bpm)\n4. Atrial 10 beat burst at 400ms (150bpm).",
                    "armGroupLabels": [
                        "Pacing"
                    ]
                }
            ]
        },
        "outcomesModule": {
