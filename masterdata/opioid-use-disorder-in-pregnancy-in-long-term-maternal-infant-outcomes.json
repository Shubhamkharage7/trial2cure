        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Incidence of opioid misuse severity using assessments",
                    "description": "Incidence of opioid misuse severity and buprenorphine treatment response will be assessed by the treating physician using assessments such as Who Assist, TCU Motform, Adverse Childhood Experience Assessment, Ham-A and Ham-D.",
                    "timeFrame": "5 years"
                },
                {
                    "measure": "Pharmacological treatment for neonatal abstinence syndrome",
                    "description": "Incidence of Neonatal Abstinence Syndrome and need for pharmacological treatment.",
                    "timeFrame": "5 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Incidence of adverse effects of buprenorphine using MRI",
                    "description": "Incidence of adverse effects of buprenorphine use on fetal and placental changes using MRI.",
                    "timeFrame": "5 years"
                },
                {
                    "measure": "Concentration of long-term neurodevelopmental outcomes using assessments",
                    "description": "Concentration of long-term neurodevelopmental outcomes in neonates using the Ages and Stages Questionnaire and the Baileys follow-up assessment.",
                    "timeFrame": "5 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Age greater 18 years\n* Currently taking Buprenorphine for Opioid Use Disorder and enrolled in a prenatal opioid maintenance program in the antenatal clinic at IU Health University Hospital.\n* Pregnant with single baby\n* Planned delivery at Methodist, University or Riley Hospital\n\nExclusion Criteria:\n\n* Serious maternal medical illness as deemed by study physician or investigator.\n* Known or suspected major fetal/neonatal congenital abnormalities.",
