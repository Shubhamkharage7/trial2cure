        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Severe aortic stenosis",
                    "type": "EXPERIMENTAL",
                    "description": "Patients with echocardiographic evidence of severe aortic stenosis as defined by:\n\nAortic Vmax \u22654 m/s or mean \u0394P \u226540 mmHg AVA \u22641.0 cm2",
                    "interventionNames": [
                        "Other: MindMics earbud"
                    ]
                },
                {
                    "label": "Severe mitral regurgitation",
                    "type": "EXPERIMENTAL",
                    "description": "Patients with echocardiographic evidence of severe mitral regurgitation as defined by:\n\n* Central jet MR \\>40% LA or holosystolic eccentric jet MR\n* Vena contracta \u22650.7 cm\n* Regurgitant volume \u226560 mL\n* Regurgitant fraction \u226550%\n* ERO \u22650.40 cm2\n* Angiographic grade 3 to 4+",
                    "interventionNames": [
                        "Other: MindMics earbud"
                    ]
                },
                {
                    "label": "Heart Failure with Reduced EF <35%",
                    "type": "EXPERIMENTAL",
                    "description": "Patients with echocardiographic evidence of left ventricular ejection fraction of \\< or = to 35%",
                    "interventionNames": [
                        "Other: MindMics earbud"
                    ]
                },
                {
                    "label": "Pulmonary Hypertension",
                    "type": "EXPERIMENTAL",
                    "description": "Patients with echocardiographic evidence of a mean pulmonary artery pressure (mPAP; supine and at rest) \\>20mmHg",
                    "interventionNames": [
                        "Other: MindMics earbud"
                    ]
                },
                {
                    "label": "Suspected coronary artery disease",
                    "type": "EXPERIMENTAL",
                    "description": "Patients with plan to undergo elective left heart diagnostic catheterization for the assessment of coronary artery disease",
                    "interventionNames": [
                        "Other: MindMics earbud"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "OTHER",
