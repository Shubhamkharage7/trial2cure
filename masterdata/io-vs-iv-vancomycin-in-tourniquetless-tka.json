        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Intravenous Vancomycin Administration",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Patients will receive the Houston Methodist Hospital orthopedic surgery standard of care pre-operative antibiotic regimen for primary total knee arthroplasty patients. This includes IV antibiotics (typically ancef or cefepime and vancomycin) will be started in the pre-operative period approximately 1 hour prior to incision (vancomycin dose weight-based at approximately 15mg/kg \\[12,13\\] generally 1000-1750mg in 500mL NS).",
                    "interventionNames": [
                        "Drug: Intravenous Vancomycin"
                    ]
                },
                {
                    "label": "Intraosseous Vancomycin Administration",
                    "type": "EXPERIMENTAL",
                    "description": "* IV antibiotics (per physician's standard of care): Typically ancef or cefepime is started in pre-op within 1 hour of incision. IV Vancomycin will not be administered preoperatively in this group.\n* IO vancomycin is administered via an intraosseous cannulation device (Arrow EZ-IO; Teleflex, Morrisville, NC) in the OR after sterile prep and draping has occurred prior to skin incision (500mg in 150mL NS).\n* Injection will take place into the tibial tubercle (within a pre-specified region) immediately prior to incision.",
                    "interventionNames": [
                        "Drug: Intraosseous Vancomycin Injection"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "Intraosseous Vancomycin Injection",
                    "description": "\u2022 IO vancomycin is administered via an intraosseous cannulation device (Arrow EZ-IO; Teleflex, Morrisville, NC) in the OR after sterile prep and draping has occurred prior to skin incision (500mg in 150mL NS).",
                    "armGroupLabels": [
                        "Intraosseous Vancomycin Administration"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "Intravenous Vancomycin",
                    "description": "IV Vancomycin will be started in the pre-operative period approximately 1 hour prior to incision (vancomycin dose weight-based at approximately 15mg/kg generally 1000-1750mg in 500mL NS).",
                    "armGroupLabels": [
                        "Intravenous Vancomycin Administration"
                    ]
                }
            ]
        },
