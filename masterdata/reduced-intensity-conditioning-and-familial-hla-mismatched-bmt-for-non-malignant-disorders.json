        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "RIC Prep Regimen & GVHD Prophylaxis",
                    "type": "EXPERIMENTAL",
                    "description": "Single arm study. All patients receive the same Reduced Intensity Conditioning (RIC) regimen and GVHD prophylaxis regimen",
                    "interventionNames": [
                        "Drug: RIC regimen",
                        "Drug: GVHD prophylaxis regimen"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DRUG",
                    "name": "RIC regimen",
                    "description": "Days -60 to -21: hydroxyurea (30mg/kg/day po) \\>6hrs prior to 1st dose: alemtuzumab (3mg IV) Day -21: alemtuzumab (10mg IV or S/C) Day -20: alemtuzumab (15mg IV or S/C) (10mg if \\< 10kg) Day -19: alemtuzumab (20mg IV or S/C) (10mg if \\< 10kg) Days -8 to -4: fludarabine (30mg/m2/day IV) Day -4: thiotepa (8mg/kg IV) Day -3: melphalan (140mg/m2) Days -2 to -1: rest days/no therapy Day 0: bone marrow transplant",
                    "armGroupLabels": [
                        "RIC Prep Regimen & GVHD Prophylaxis"
                    ],
                    "otherNames": [
                        "Transplant Preparative Regimen",
                        "Transplant Conditioning Regimen"
                    ]
                },
                {
                    "type": "DRUG",
                    "name": "GVHD prophylaxis regimen",
                    "description": "Day +3 to +4: cyclophosphamide (50mg/kg/day IV) Day +5: Start of tacrolimus \\& Start of mycophenolate mofetil (MMF) Days +5, +14, +30, +60, +90: abatacept (IND) (10mg/kg/day IV) Days +120 to +390: abatacept (IND) monthly (5mg/kg/day IV)",
                    "armGroupLabels": [
                        "RIC Prep Regimen & GVHD Prophylaxis"
                    ],
                    "otherNames": [
                        "Graft versus Host Disease prophylaxis regimen"
                    ]
                }
            ]
        },
