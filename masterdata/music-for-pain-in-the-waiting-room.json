        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Pain Coping Information",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "A 4-minute audio recording about common pain coping techniques (e.g., ice, rest, medication, surgery).",
                    "interventionNames": [
                        "Behavioral: Pain Coping Information"
                    ]
                },
                {
                    "label": "Lyrical music",
                    "type": "EXPERIMENTAL",
                    "description": "A 4-minute audio recording of Leonard Cohen's song \"Hallelujah\" sung by Rufus Wainwright with choral accompaniment.",
                    "interventionNames": [
                        "Behavioral: Lyrical Music"
                    ]
                },
                {
                    "label": "Instrumental Music",
                    "type": "EXPERIMENTAL",
                    "description": "A 4-minute audio recording of instrumental, theta wave music.",
                    "interventionNames": [
                        "Behavioral: Instrumental Music"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Pain Coping Information",
