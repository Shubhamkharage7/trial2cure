                "type": "ESTIMATED"
            }
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Participants achieving DSM-5 remission, early or sustained, (yes/no) while on initial SUBLOCADE treatment by 6 months and 1, 1.5, 2, 2.5, and 3 years from study Baseline",
                    "description": "* Definition of early remission: None of the criteria for OUD has been met for at least 3 months but for less than 12 months with the exception that Criterion 4 (\"Craving, or a strong desire or urge to use opioids,\") may be met.\n* Definition of sustained remission: None of the criteria for OUD has been met for at least 12 months with the exception that Criterion 4 (\"Craving, or a strong desire or urge to use opioids,\") may be met.",
                    "timeFrame": "up to a maximum of 4 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Key Secondary: Participants with \"no symptoms\" or improving by \u22651 DSM-5 severity classification within the last 3 months (moderate to mild, or severe to mild or moderate) relative to study Baseline (yes/no) at Month 6 and Years 1, 2, and 3",
                    "description": "DSM-5-Related",
                    "timeFrame": "up to a maximum of 4 years"
                },
                {
                    "measure": "Among those starting SUBLOCADE within 3 months of study Baseline, time to achieve early remission for the first time while on initial SUBLOCADE treatment",
                    "description": "DSM-5-Related",
                    "timeFrame": "up to a maximum of 4 years"
                },
                {
                    "measure": "Participants achieving DSM-5 sustained remission (yes/no) by 1, 1.5, 2, 2.5, and 3 years while on initial SUBLOCADE treatment",
                    "description": "DSM-5-Related",
                    "timeFrame": "up to a maximum of 4 years"
                },
                {
                    "measure": "Time to achieve sustained remission for the first time while on initial SUBLOCADE treatment",
                    "description": "DSM-5-Related",
                    "timeFrame": "up to a maximum of 4 years"
                },
                {
                    "measure": "Time to discontinuing initial SUBLOCADE treatment and the corresponding reasons for discontinuation, eg, resolution of OUD symptoms, safety concerns, treatment failure, or seeking alternative MOUD treatment",
                    "description": "MOUD Use and Retention",
                    "timeFrame": "up to a maximum of 4 years"
                },
                {
                    "measure": "Participants switching from initial SUBLOCADE treatment to another MOUD (yes/no), and the corresponding reasons, eg, safety concerns, treatment failure, seeking alternative MOUD treatment",
                    "description": "MOUD Use and Retention",
                    "timeFrame": "up to a maximum of 4 years"
                },
                {
                    "measure": "The overall treatment duration for participants who stop initial SUBLOCADE treatment as the result of resolution of OUD symptoms",
                    "description": "MOUD Use and Retention",
                    "timeFrame": "up to a maximum of 4 years"
                },
                {
                    "measure": "Participants with any positive opioid use in the past month (yes/no) at Month 6 and Years 1, 2, and 3 while on initial SUBLOCADE treatment",
                    "description": "Illicit Opioid Use (Self-Reports)",
                    "timeFrame": "up to a maximum of 4 years"
                },
                {
                    "measure": "Participants with opioid overdose requiring intervention (yes/no) while on initial SUBLOCADE treatment at least once during the first 6 months, and during Years 1, 2, and 3",
                    "description": "Opioid Overdose Requiring Intervention",
                    "timeFrame": "up to a maximum of 4 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\nParticipants must meet all of the following criteria:\n\n* Is an adult \u2265 18 years of age who has signed the informed consent form\n* Plans to receive additional SUBLOCADE injections and\n\n  * Has initiated treatment for OUD/opioid dependence with SUBLOCADE within the last 3 months prior to Baseline Visit\n\n    * OR\n  * Has completed an Indivior SUBLOCADE study (eg, INDV-6000-401, INDV-6000-405) and has received the last dose of SUBLOCADE on that study within 3 months prior to Baseline Visit\n* Is not currently participating in any clinical trial requiring medical intervention\n* Is not currently using opioids to treat a diagnosis other than OUD/opioid dependence",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Participants may enroll if they have initiated treatment with SUBLOCADE within 3 months prior to signing the informed consent form (ICF), or have completed an Indivior SUBLOCADE study (eg, INDV-6000-401, INDV-6000-405) and received their last dose of SUBLOCADE on the study within 3 months prior to signing the ICF, and plan to receive additional SUBLOCADE injections.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
