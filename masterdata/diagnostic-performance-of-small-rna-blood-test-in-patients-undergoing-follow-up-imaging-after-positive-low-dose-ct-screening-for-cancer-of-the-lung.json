        "armsInterventionsModule": {
            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "miLungDx",
                    "description": "At study enrollment a blood sample will be drawn prior to any invasive diagnostic study or treatment. On this blood sample a novel lung cancer test relying on small RNA signatures will be performed and evaluated."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Primary Objective",
                    "description": "The study is a prospective, longitudinal, observational study to evaluate the performance of a novel diagnostic test for early-stage lung cancer. The primary endpoint is to detect the presence or absence of lung cancer, as determined by all follow-up imaging, and pathological data obtained throughout the duration of the study.",
                    "timeFrame": "1 year"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Adult male and female patients, aged 50 to 80 years\n* Received a lung RADS category 3 or 4 finding on a LDCT screening exam (first or subsequent) and are referred to undergo further diagnostic procedures for the detection of lung cancer\n* Has undergone LD-CT screening exam within last 90 days and allows for blood sampling within this period\n* Subject may not have undergone any invasive diagnostic procedure in relation to the suspicious nodule\n* Able and willing to provide informed consent\n\nExclusion Criteria:\n\n* Prior history of diagnosed lung cancer\n* History of gastrointestinal, hematological, breast, thyroid, and genitourinary cancer within the past 10 years\n* Active infectious diseases, such blood borne viral diseases (e.g. AIDS, hepatitis)\n* Immunosuppressive medication\n* Deemed not able to participate in the study by the investigator",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "50 Years",
            "maximumAge": "80 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Individuals, aged 50-80 who have received a lung-RADS category 3 or 4 result on a LDCT screening study and who are scheduled for follow-up diagnostic imaging study, biopsy, clinical consultation or surgical appointment at one of the participating hospitals.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
