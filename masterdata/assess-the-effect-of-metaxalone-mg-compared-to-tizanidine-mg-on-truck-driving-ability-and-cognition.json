        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change from baseline, Standard Deviation of Lateral Position",
                    "description": "Using the One Motion GT Elite Mini- Simulator (SimGear) with Carnet Soft Driving Simulation Software",
                    "timeFrame": "Baseline, Visit 2 dosing, (day 1), Visit 3 dosing, (day 8)"
                },
                {
                    "measure": "Change from baseline of participants' reasoning ability to recognize differences in six (6) computer generated shapes for 30 minutes",
                    "description": "Creyos Cognitive Test",
                    "timeFrame": "Visit 2 (Day 0), Visit 3 (Day 1). Visit 4 (Day 8)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change from baseline of participants' subjective report of drowsiness on a 10-point scale",
                    "description": "Karolinska Sleepiness Scale where 1 = extremely alert and 10 = extremely sleepy, can't keep awake",
                    "timeFrame": "Visit 2 (Day 0), Visit 3 (Day 1). Visit 4 (Day 8)"
                },
                {
                    "measure": "Percentage of participants unable to maintain Tandem Stand Position for more than 10 seconds (position 3 of the 4-Stage Balance Test)",
                    "description": "Validated 4-Stage Balance Test after completion of an FDA standardized high-fat meal. Place one foot in front of the other, heel touching toes.",
                    "timeFrame": "Visit 2 (Day 0), Visit 3 (Day 1). Visit 4 (Day 8)"
                },
                {
                    "measure": "The number of product related adverse events experienced by participants from Visit 1 through Visit 4",
                    "description": "Adverse and Serious Adverse Events",
                    "timeFrame": "Baseline, Visit 2 (Day 0), Visit 3 (Day 1), Visit 4 (Day 8), Visit 4 (Day 15)"
                }
            ]
        },
