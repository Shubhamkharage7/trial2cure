            "designInfo": {
                "allocation": "RANDOMIZED",
                "interventionModel": "PARALLEL",
                "interventionModelDescription": "Participants will be assigned to one of two conditions:1) aerobic exercise training on a stationary bicycle, or 2) stretching and balance training.",
                "primaryPurpose": "TREATMENT",
                "maskingInfo": {
                    "masking": "NONE"
                }
            },
            "enrollmentInfo": {
                "count": 40,
                "type": "ESTIMATED"
            }
        },
        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Exercise Training",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will exercise 3 times a week on a stationary bicycle. Exercise intensity will begin at low levels (50% of maximal heart rate reserve) and increase as tolerated to a maximum of 80% of maximal heart rate reserve. Exercise time will progress from an initial 20 minutes per session to a maximum of 45 minutes per session.",
                    "interventionNames": [
                        "Behavioral: Exercise"
                    ]
                },
                {
                    "label": "Stretching",
                    "type": "PLACEBO_COMPARATOR",
                    "description": "Participants will undergo progressive whole body stretching and balance exercises. This type of low intensity exercise is designed not to increase heart rate but rather to improve joint mobility and the ability to perform activities of daily living. This will serve as the control group to the exercise group. The control group will come in for stretching sessions 3 times per week for 20-45 minutes.",
                    "interventionNames": [
                        "Behavioral: Stretching"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Exercise",
                    "description": "Participants randomized to this arm will exercise on a stationary bicycle.",
                    "armGroupLabels": [
                        "Exercise Training"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Stretching",
                    "description": "Participants randomized to this arm will perform stretching and balance exercise.",
                    "armGroupLabels": [
                        "Stretching"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "change in dynamic cerebral autoregulation",
                    "description": "Mean arterial pressure and middle cerebral artery blood velocity data will be subjected to transfer function analysis to derive mean arterial pressure-middle cerebral artery blood velocity transfer function phase (radians) and gain (cm/s/mmHg). These indices reflect dynamic cerebral autoregulation.",
                    "timeFrame": "Mean arterial pressure-middle cerebral artery blood velocity transfer function phase (radians) and gain (cm/s/mmHg) will be compared at baseline (week 0) and after completing the 12-week intervention (week 13)."
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n\u2022 Individuals with chronic kidney disease stages III-IV.\n\nExclusion Criteria:\n\n* Heart failure\n* Chronic obstructive pulmonary disease\n* Uncontrolled hypertension or hypotension\n* Pregnancy or plans to become pregnant\n* Inability to exercise on a stationary bicycle\n* Current participation in exercise more than 20 minutes twice per week",
