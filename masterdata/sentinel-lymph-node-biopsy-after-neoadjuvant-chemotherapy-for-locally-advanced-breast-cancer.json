            ],
            "interventions": [
                {
                    "type": "PROCEDURE",
                    "name": "Sentinel Lymph Node Biopsy (SLNB)",
                    "description": "Sentinel Lymph Node Biopsy Patients with locally advanced breast cancer will undergo SLNB with dual tracer mapping followed by completion axillary lymph node dissection.",
