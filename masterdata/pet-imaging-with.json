        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Schizophrenia and other psychotic disorders",
                    "description": "For each \\[11C\\]APP311 PET scan, up to 20 mCi of \\[11C\\]APP311 will be administered by infusion pump, followed by up to 120 minutes of dynamic PET data acquisition.",
                    "interventionNames": [
                        "Radiation: [11C]APP311"
