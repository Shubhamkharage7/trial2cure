        "descriptionModule": {
            "briefSummary": "This randomized trial will use a Negative Pressure Wound Therapy device or standard wound closure, in participants with gastrointestinal neoplasms, to measure the rate of surgical site infections"
        },
        "conditionsModule": {
            "conditions": [
                "Surgical Wound Infection"
            ]
        },
        "designModule": {
            "studyType": "INTERVENTIONAL",
