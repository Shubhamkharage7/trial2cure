        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Balance targeted exercises group",
                    "type": "EXPERIMENTAL",
                    "description": "participants in this group will receive the balance targeted exercises for up to 6 weeks",
                    "interventionNames": [
                        "Behavioral: Balance targeted exercises"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Balance targeted exercises",
                    "description": "6-week exercise program, 3 sessions per week (2 sessions in person, 1 session of home exercises. Participants will receive a booklet with information about the home exercises and will receive a text message reminder), each session will last 60 minutes.",
                    "armGroupLabels": [
                        "Balance targeted exercises group"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in balance ability as measured by the BESTest",
                    "description": "The Balance Evaluation Systems Test (BESTest) will be used to measure balance ability. Items are scored from 0 (severe impairment) to 3 (no impairment), with a total score of 108. A higher score suggests better balance ability.",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in reaction time",
                    "description": "Measure in seconds",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in movement velocity",
                    "description": "Measure in degree per second",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in endpoint excursion percentage",
                    "description": "measure of endpoint excursion in a percentage out of a100",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in directional control percentage",
                    "description": "measure of directional control in a percentage out of a100",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in maximum excursion percentage",
                    "description": "measure of maximum excursion in a percentage out of a100",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in muscle responses latency as measured by MCT",
                    "description": "The Motor Control test (MCT) measures the subject's ability to generate a motor response latency to a sudden surface translation in a forward and backward direction at three different magnitudes (small, medium, and large).\n\nLatency: The time-lapse between the perturbation onset and the subject response for each limb. The latency is reported in milliseconds.",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in postural muscles responses latency as measured by Electromyography (EMG)",
                    "description": "Postural muscles responses onset latency in seconds",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in APA as measured by microvolts",
                    "description": "Postural muscles APA measured in microvolts",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in CPA as measured by microvolts",
                    "description": "Postural muscles CPA in microvolts",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Change in balance confidence as measured by ABC",
                    "description": "The Activities Specific Balance Confidence (ABC) scale is a self-report questionnaire of a subject's perceived level of balance confidence. It measures the subject's confidence in performing various ambulatory activities without falling. There are 16 items scored using a scale(0-100) with higher scores indicating more confidence in performing the tasks. A total score is reported by dividing the total ABC score by 16.",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                },
                {
                    "measure": "Change in quality of life as measured by the MSQoL-54",
                    "description": "Multiple Sclerosis Quality of Life - 54 (MSQoL-54) is a self-report questionnaire measuring subject perceived of quality of life. In total, it contains 54 items with a total score ranging from 0-100, with higher scores indicating a better quality of life",
                    "timeFrame": "Baseline, 6 weeks (post intervention)"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Must be18 to 70 years of age.\n2. Medical diagnosis of MS confirmed by a neurologist.\n3. Ability to stand independently without any aid for at least 3 minutes.\n4. A Patients determined disease steps (PDDS) score of 5 or lower.\n5. Normal or corrected-to-normal vision.\n6. English or Spanish speaking.\n\nExclusion Criteria:\n\n1. MS-related exacerbation or medication change in the past two months.\n2. Presence of concurrent neurological or orthopedic disorders.\n3. Unable to perform the experimental tasks.",
