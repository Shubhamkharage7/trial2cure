        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "LungCare Surveillance (500 Cases)",
                    "description": "Observational evaluation of diagnostic performance characteristics of AlloSure Lung to detect a spectrum of rejection and allograft infection."
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "ALAD",
                    "description": "Diagnostic performance characteristics (sensitivity, specificity, PPV, NPV) of AlloSure Lung to detect acute lung allograft dysfunction (ALAD).",
                    "timeFrame": "1 year post-transplant"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Chronic lung allograft dysfunction",
                    "description": "Clinical diagnostic performance characteristics (sensitivity, specificity, PPV and NPV) of AlloSure Lung to detect chronic lung allograft dysfunction.",
                    "timeFrame": "3 years post-transplant"
                },
                {
                    "measure": "Acute cellular rejection",
                    "description": "Clinical diagnostic performance characteristics (sensitivity, specificity, PPV and NPV) of AlloSure Lung to detect acute cellular rejection (ACR).",
                    "timeFrame": "3 years post-transplant"
                },
                {
                    "measure": "Antibody mediated rejection",
                    "description": "Clinical diagnostic performance characteristics (sensitivity, specificity, PPV and NPV) of AlloSure Lung to detect antibody mediated rejection (AMR).",
                    "timeFrame": "3 years post-transplant"
                },
                {
                    "measure": "Acute infection",
                    "description": "Clinical diagnostic performance characteristics (sensitivity, specificity, PPV and NPV) of AlloSure Lung to detect acute infection.",
                    "timeFrame": "3 years post-transplant"
                },
                {
                    "measure": "Subject Variability",
                    "description": "Define within-subject and between-subject variability of AlloSure Lung in clinically stable patients in order to establish reference change value RCV.",
                    "timeFrame": "3 years post-transplant"
                },
                {
                    "measure": "ALAD vs. Stability",
                    "description": "Evaluate the diagnostic performance characteristics of utilizing RCV to distinguish ALAD vs. Stability",
                    "timeFrame": "3 years post-transplant"
                },
                {
                    "measure": "Biopsy-proven rejections",
                    "description": "The proportion of biopsy-proven rejections with elevated AlloSure Lung versus those without elevated AlloSure-Lung.",
                    "timeFrame": "3 years post-transplant"
                },
                {
                    "measure": "AlloSure Lung Results",
                    "description": "The proportion of AlloSure Lung results obtained as surveillance or for-cause, that the physicians felt influenced their decisions around management of patients.",
                    "timeFrame": "3 years post-transplant"
                },
                {
                    "measure": "ALAD Free survival",
                    "description": "ALAD free survival in lung transplant recipients who received AlloSure Lung as part of post-transplant surveillance.",
                    "timeFrame": "3 years post-transplant"
                },
                {
                    "measure": "AlloSure Levels",
                    "description": "Evaluate AlloSure levels 30-90 days prior to acute rejection.",
                    "timeFrame": "3 years post-transplant"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Unilateral or Bilateral, deceased-donor, initial or re-transplant\n2. \u2264 90 days post-transplant\n3. Ability to understand and provide informed consent and adhere to laboratory surveillance schedule\n4. Patients who have LungCare initiated within 30 days of signing the informed consent form\n\nExclusion Criteria:\n\n1. Multi-organ transplants\n2. Pregnancy\n3. Active malignancy",
            "healthyVolunteers": true,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "All lung transplant patients without exclusion criteria will be eligible to enter the study. Patients will be approached by the study team at the time of listing or at the time of transplant or within 90 days post-transplant to discuss the trial, consent and enrollment.",
            "samplingMethod": "NON_PROBABILITY_SAMPLE"
        },
