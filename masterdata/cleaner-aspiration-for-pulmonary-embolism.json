        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Change in Right Ventricle (RV)/Left Ventricle (LV) Ratio",
                    "description": "Change in RV/LV ratio between baseline and 48 hours post procedure assessed by CTA.",
                    "timeFrame": "At 48 hours post-procedure"
                },
                {
                    "measure": "Rate of Major Adverse Events (MAEs)",
                    "description": "Rate of MAEs within the first 48 hours after the index procedure, defined as:\n\nDevice-related death Major bleeding, and Device-related Serious Adverse Events which includes clinical deterioration, pulmonary vascular injury, and cardiac injury.",
                    "timeFrame": "At 48 hours post-procedure"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Rate of device-related complications and device-related death",
                    "description": "Rate of device-related complications including major bleeding, clinical deterioration, pulmonary vascular injury, and cardiac injury, and device-related death within 48 hours of the index procedure.",
                    "timeFrame": "Within 48 hours of the procedure"
                },
                {
                    "measure": "Rate of device-related SAEs and all-cause mortality",
                    "description": "Rate of device-related serious adverse events (SAEs) and death for any cause within 30 days post-procedure.",
                    "timeFrame": "Within 30 days of procedure"
                },
                {
                    "measure": "Rate of Symptomatic PE Recurrence",
                    "description": "Rate of Symptomatic PE recurrence within 30 days.",
                    "timeFrame": "Within 30 days of the procedure"
                },
                {
                    "measure": "Volume of blood aspirated",
                    "description": "The amount of blood aspirated during the procedure.",
                    "timeFrame": "At index procedure"
                },
                {
                    "measure": "Use of thrombolytics",
                    "description": "Use of thrombolytics within 48 hours of the procedure.",
                    "timeFrame": "Within 48 hours of the procedure"
                },
                {
                    "measure": "Change in Modified Miller Index",
                    "description": "Change in Modified Miller Index between baseline and 48 hours post-procedure assessed by CTA. This index score quantifies arterial obstruction in pulmonary embolism. The higher the score, the more perfusion is reduced.",
                    "timeFrame": "At 48 hours post-procedure"
                },
                {
                    "measure": "Quality of Life assessed via PEmb-QoL",
