        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Postoperative incidence of serious adverse events probably or definitely related to GRANITE",
                    "timeFrame": "2 years"
                },
                {
                    "measure": "Postoperative incidence of SI joint pain in subject without SI joint pain at baseline",
                    "timeFrame": "2 years"
                },
                {
                    "measure": "Postoperative incidence of L5-S1 pseudoarthrosis",
                    "timeFrame": "2 years"
                },
                {
                    "measure": "Postoperative incidence of pelvic construct failure",
                    "description": "Failure is defined as:\n\n* Breakage of any implants placed into sacrum or ilium\n* Rod breakage in the lumbosacral region\n* Pelvic implant loosening that either causes new or worsened pain or requires surgical adjustment/revision",
                    "timeFrame": "2 years"
                },
                {
                    "measure": "Postoperative incidence of device failures related to GRANITE (e.g., rod disengagement, screw pullout)",
                    "timeFrame": "2 years"
                },
                {
                    "measure": "Radiographic",
                    "description": "Bony apposition to the implant at 12 and 24 months as assessed by CT scan",
                    "timeFrame": "12 month and 24 months"
                },
                {
                    "measure": "Change in Quality of Life PROs",
                    "description": "Scores on PROs at follow up timepoints compared to baseline",
                    "timeFrame": "6 month, 12 month and 24 months"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. \u2265 18 years of age at screening.\n2. Patient is scheduled for spinal fusion with pelvic fixation/fusion using iFuse Bedrock Granite\n3. Patient is willing and able to provide written informed consent\n\nExclusion Criteria:\n\n1. Any known anatomic issues or permanent implants already in the pelvis that could interfere with placement of the study device(s)\n2. Prominent neurologic or uncontrolled psychiatric condition that would interfere with study participation\n3. Known allergy to titanium or titanium alloys\n4. Current local or systemic infection that raises the risk of surgery\n5. Currently pregnant or planning pregnancy in the next 2 years\n6. Known or suspected active drug or alcohol abuse, including opioids\n7. Patient is a prisoner or ward of the state\n8. Patient is enrolled in an investigational clinical trial related to the spine",
            "healthyVolunteers": false,
