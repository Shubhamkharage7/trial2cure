            "armGroups": [
                {
                    "label": "Mild Hypothermic Circulatory Arrest",
                    "type": "EXPERIMENTAL",
                    "description": "During aortic hemiarch surgery, mild hypothermia (32\u00b0C) will be used during circulatory arrest.",
                    "interventionNames": [
