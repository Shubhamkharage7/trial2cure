        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Pneumonia",
                    "description": "Presence or absence of pneumonia based on adjudicator review of chart. The goal of the study is to create a diagnostic pathway for pneumonia in older adult emergency department patients.",
                    "timeFrame": "3 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n- Adults aged 65 and older presenting to the ED with suspected pneumonia defined as (1) either a physician order for CXR OR CT OR a chief complaint consistent with dyspnea; AND (2) physician suspicion of pneumonia prior to ED testing (score of 2-5 on Likert scale)\n\nExclusion Criteria:\n\n* inability to obtain consent from the patient or an LAR\n* active cancer\n* organ transplant\n* immunosuppression\n* trauma activation\n* suicidal ideation\n* incarceration\n* antibiotics in the last 14 days\n* a separate episode of lower respiratory illness in the last 30 days",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "65 Years",
