        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Effects of Carbaglu\u00ae on plasma ammonia levels",
                    "description": "Multiple plasma ammonia levels will be collected only during treatment with Carbaglu\u00ae according to prescribing information and routine medical practice in terms of visit frequency.",
                    "timeFrame": "Patients treated with Carbaglu\u00ae will be managed chronically (out-patient) or acutely (hospital in-patient). In both cases, data will be collected for approximately 1 year following discontinuation of Carbaglu treatment."
                },
                {
                    "measure": "Adverse Event frequency and severity",
                    "description": "Any Carbaglu\u00ae related adverse events will be be collected and reported",
                    "timeFrame": "Patients treated with Carbaglu\u00ae will be managed chronically (out-patient) or acutely (hospital in-patient). In both cases, data will be collected for approximately 1 year following discontinuation of Carbaglu treatment."
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Fetal Outcomes and Pregnancy Outcomes",
                    "description": "Pregnancy risks including maternal complications, adverse effects on the developing fetus and neonate, and adverse effects on the infant (through the first year of life).",
                    "timeFrame": "Collection of pregnancy information for patients who becomes pregnant while participating in the trial or at time of enrollment. Pregnancy reports and reports involving neonates and infants up to 1 year of age must be reported to RRD Pharmacovigilance."
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Provision of signed and dated informed consent/assent form\n2. Prescribed and treated with Carbaglu\u00ae\n3. Have an established diagnosis of PA or MMA defined as follows:\n\n   * Diagnosed with PA by semi quantitative urine organic acid analysis, defined as presence of elevated methylcitric acid and normal methylmalonic acid levels and no evidence of biotin related disorders in the organic acid analysis; OR\n   * Diagnosed with MMA by semi quantitative urine organic acid analysis, defined as elevation of methylmalonic acid and no evidence of vitamin B12 dependent disorder on plasma amino acid analysis (vitamin B12 dependency is defined by documented vitamin B12 responsiveness).\n\nAND/OR\n\n* Confirmation by molecular genetic testing\n\nExclusion Criteria:\n\n* None",
            "healthyVolunteers": false,
            "sex": "ALL",
