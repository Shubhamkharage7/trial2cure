        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Breathing and Attention Training (BAT)",
                    "type": "EXPERIMENTAL",
                    "description": "Participants will be asked to do 20-minutes of focused breathing and attention training, involving focusing on taking deep breaths and becoming aware of the changing body sensations associated with breathing (mindfulness).",
                    "interventionNames": [
                        "Behavioral: Breathing and Attention Training (BAT)"
                    ]
                },
                {
                    "label": "Controlled Deep Breathing",
                    "type": "ACTIVE_COMPARATOR",
                    "description": "Participants will be asked to do 20-minutes of deep breathing and letting the body relax.",
                    "interventionNames": [
                        "Behavioral: Breathing Control"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "BEHAVIORAL",
                    "name": "Breathing and Attention Training (BAT)",
                    "description": "BAT is an instructor guided breathing technique over 20 min that will be applied once in a training session and approximately one week later in a testing session.",
                    "armGroupLabels": [
                        "Breathing and Attention Training (BAT)"
                    ]
                },
                {
                    "type": "BEHAVIORAL",
                    "name": "Breathing Control",
                    "description": "Controlled Breathing (without mindfulness) is an instructor guided breathing technique over 20 min that will be applied once in a training session and approximately one week later in a testing session.",
                    "armGroupLabels": [
                        "Controlled Deep Breathing"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Ratings of Painful and Painless Sensory Stimuli",
                    "description": "Ratings of painful and non-painful stimuli in FM subjects and HC will be obtained before and after BAT or Deep Breathing. Painful stimuli will include pressure stimuli. Painless stimuli will consist of flashing light stimuli.",
                    "timeFrame": "2 hr"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Individuals diagnosed with fibromyalgia will have pain of duration \\> 6 months and meeting the 1990 Research Diagnostic Criteria for FM (ACR)\n* Healthy, pain-free age matched controls without chronic pain\n\nExclusion Criteria:\n\n* Personal or family history of photosensitive epilepsy\n* Prior history of cancer or diabetes\n* Patients must be willing to discontinue their analgesics, hypnotics, anxiolytics, or anti-depressants during the study period for at least 5 half-lives.",
