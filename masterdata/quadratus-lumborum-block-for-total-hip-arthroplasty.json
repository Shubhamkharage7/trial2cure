        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Pain score",
                    "description": "Pain scores will be assessed over 48hrs postop using the visual analog score",
                    "timeFrame": "Up to 48 hrs post op"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Opioid usage",
                    "description": "Total morphine-equivalent will be calculated at 0-24hrs and 24-48hrs postop.",
                    "timeFrame": "Up to 48 hrs post op"
                },
                {
                    "measure": "Time to first stand",
                    "description": "The period between the end of surgery and the patient's first successful attempt at standing, with or without the assistance of devices (i.e. walker, cane) or staff assistance.",
                    "timeFrame": "Up to 1 week post surgery"
                },
                {
                    "measure": "Time to first ambulation",
                    "description": "Time to first ambulation of 50 feet or more, with or without the help of assist devices (i.e. walker, cane) or staff assistance.",
                    "timeFrame": "Up to 1 week post surgery"
                },
                {
                    "measure": "Ambulation distance",
                    "description": "This data point will be measured in feet and will be determined by the distance ambulated during the patient's first ambulation attempt after surgery, regardless of the length of ambulation (This distance will be counted regardless of whether the patient reaches the 50 feet requirement to be counted as the \"time to first ambulation).",
                    "timeFrame": "Up to 1 week post surgery"
                },
                {
                    "measure": "Length of hospital stay",
                    "description": "Number of days in the hospital from postoperative day 0 to discharge",
                    "timeFrame": "Until discharge from hospital, up to 1 week post surgery"
                },
                {
                    "measure": "Patient satisfaction",
                    "description": "Patient satisfaction score will be assessed on day of discharge as measured by a 5-point scale with 1 = terrible, 2 = poor, 3 = satisfactory, 4 = good, and 5 = excellent.",
                    "timeFrame": "Until day of discharge from the hospital, up to 1 week post surgery"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Adult subjects undergoing elective primary total hip arthroplasty\n\nExclusion Criteria:\n\n* patients age \\< 18,\n* documented allergy to local anesthetic\n* presence of peripheral neuropathy\n* patients with current long term opioid use defined as receiving an opioid on most days in a 90 day period\n* patients with a contraindication to spinal and regional anesthesia such as coagulopathy or infection at the site of the regional technique.",
