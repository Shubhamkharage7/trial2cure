        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Standard care plus infrascans",
                    "description": "All participants will receive regular clinical neurological assessments as ordered by the clinical care team. In addition, a research team member will perform an infrascan. This process will be repeated hourly at the same timepoint as the clinical neurological assessments. If a patient's neurological status deteriorates at any time point, and the medical team orders an early (unplanned) CT scan, the neurological data collection will cease once the patient is sent to CT.\n\nInfrascan results will not be shared with the clinical care team, and will not guide the participants' care in any way.",
                    "interventionNames": [
                        "Device: Standard care plus infrascans"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DEVICE",
                    "name": "Standard care plus infrascans",
                    "description": "Hourly infrascans to detect expanding intracranial hematomas",
                    "armGroupLabels": [
                        "Standard care plus infrascans"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Diagnostic performance of the infrascanner",
                    "description": "Number of participants whose Infrascan results were accurate",
                    "timeFrame": "Month 24"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Time to detection of an enlarging hematoma",
                    "description": "Difference in time to detection of hematoma enlargement between Infrascan and second CT across the cohort",
                    "timeFrame": "0-12 hours"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n1. Age 15 or older\n2. Admission CT Scan shows intracranial hemorrhage\n3. Glasgow Coma Scale less than 15\n4. Neurosurgery service determines initial care is nonoperative\n5. Admitted for observation for TBI, to either step-down unit or intensive care unit\n\nExclusion Criteria:\n\n1. Inability to provide informed consent or lack of a legally authorized representative (LAR)\n2. Admitted and observed for \\>2 hours prior to screening (i.e., has already received second neurological check)\n3. Other injuries deemed non-survivable\n4. Diagnosed with hemorrhagic shock or receives a large volume transfusion (\\>3 units of any blood product within any 1 hour)\n5. Planned surgical interventions/ procedures during study period (before the second CT scan)\n6. Known prisoner",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "15 Years",
            "stdAges": [
                "CHILD",
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "This study will enroll up to 440 patients over 14 months, at 10 trauma centers. Currently, there are no high quality published datasets or literature to document the mean or variability in time to the detection of hematoma expansion that could be used for meaningful sample size projections. Therefore, this study has been designed to provide reasonable estimates of the future study outcomes.",
            "samplingMethod": "PROBABILITY_SAMPLE"
        },
