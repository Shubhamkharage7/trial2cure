        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Acceptability of the newborn massage intervention",
                    "description": "Acceptability of the intervention will be defined by the 1-sided 95% confidence interval for the proportion of the intervention group performing massage at least once to include 80% and for the 1-sided 95% confidence interval for at the proportion performing massage as directed to include 70%.",
                    "timeFrame": "Through study completion, up to one year"
                },
                {
                    "measure": "Feasibility of recruitment",
                    "description": "Feasibility of recruitment will be defined by at least 30% of approached, eligible participants consenting to participate.",
                    "timeFrame": "Ascertained following completion of recruitment of 30 study participants. Through study completion, up to an average of one year."
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Difference between transcutaneous bilirubin level and AAP phototherapy threshold on the day of discharge",
                    "description": "delta-TcSB, which is calculated by subtracting infant's transcutaneous bilirubin level from their phototherapy treatment threshold at that hour of age (in mg/dL)",
                    "timeFrame": "Value at obtained at hospital discharge will be evaluated at the end of study completion, up to one year"
                },
                {
                    "measure": "Number of stools in prior 24 hours (at hospital discharge)",
                    "timeFrame": "Value at obtained at hospital discharge will be evaluated at the end of study completion, up to one year"
                },
                {
                    "measure": "Timing of first transitional (non-meconium) stool passage",
                    "timeFrame": "Ascertained at 1-2 week survey"
                },
                {
                    "measure": "Parental satisfaction with newborn massage",
                    "description": "Parental satisfaction with newborn massage assessed by survey",
                    "timeFrame": "7-14 day survey"
                },
                {
                    "measure": "Proportion of participants who read with baby in the last 24 hours",
                    "description": "Proportion of participants who read with baby in the last 24 hours assessed by survey",
                    "timeFrame": "1-2 week survey"
                },
                {
                    "measure": "Proportion of participants who read with baby in the last week",
                    "description": "Proportion of participants who read with baby in the last week assessed by survey",
                    "timeFrame": "1-2 week survey"
                },
                {
                    "measure": "Breastfeeding at 1 week of age",
                    "description": "Infant received any human milk in the prior 24 hours",
                    "timeFrame": "1-2 week survey"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Newborn admitted to the well newborn/postpartum care unit at UC Davis Medical Center\n* Less than 48 hours of age at enrollment\n\nExclusion Criteria:\n\n* Infant has already received phototherapy\n* Mother/birthing person is incarcerated\n* Infant unexpected to discharge into parental care\n* Parental inability to speak or read in English\n* Infant without parent rooming-in with them in the hospital",
