            }
        },
        "armsInterventionsModule": {
            "armGroups": [
                {
                    "label": "Retrospective patient cohort",
                    "interventionNames": [
                        "Diagnostic Test: assessment of pulmonary hemodynamics during exercise by right heart catheterization"
                    ]
                },
                {
                    "label": "Prospective patient cohort",
                    "interventionNames": [
                        "Diagnostic Test: assessment of pulmonary hemodynamics during exercise by right heart catheterization"
                    ]
                }
            ],
            "interventions": [
                {
                    "type": "DIAGNOSTIC_TEST",
                    "name": "assessment of pulmonary hemodynamics during exercise by right heart catheterization",
                    "description": "all patients undergo right heart catheterization during exercise due to clinical reasons. Results of this and other Routine clinical tests will be included in the registry",
                    "armGroupLabels": [
                        "Prospective patient cohort",
                        "Retrospective patient cohort"
                    ]
                }
            ]
        },
        "outcomesModule": {
            "primaryOutcomes": [
                {
                    "measure": "Number of Participants who die during observation",
                    "description": "The investigators aim to assess the prognostic relevance of pulmonary hemodynamics during exercise as assessed by right heart catheterization by using a multi-centre approach and to identify independent predictors of adverse events. Separate data sets will be analysed for the retrospective and prospective Evaluation.",
                    "timeFrame": "6 years"
                },
                {
                    "measure": "Number of Participants undergoing lung Transplantation during observation",
                    "description": "Occurrences of lung transplantations",
                    "timeFrame": "6 years"
                }
            ],
            "secondaryOutcomes": [
                {
                    "measure": "Number of Participants being hospitalized during observation",
                    "description": "Number of hospitalizations",
                    "timeFrame": "6 years"
                },
                {
                    "measure": "Number of Participants who develop pulmonary Hypertension as assessed by right heart catheterization",
                    "description": "Diagnosis of pulmonary hypertension",
                    "timeFrame": "6 years"
                },
                {
                    "measure": "Number of Participants with newly initiated pulmonary arterial Hypertension medication during observation",
                    "description": "Evaluation of medication initiations",
                    "timeFrame": "6 years"
                }
            ]
        },
        "eligibilityModule": {
            "eligibilityCriteria": "Inclusion Criteria:\n\n* Patients (females and males; age: above 18yrs) with intermediate or high echocardiographic probability of PH and/or unexplained dyspnea, and/or associated conditions for PAH as clinical indication for RHC at rest and exercise\n* Written informed consent of participating subjects after being fully briefed (for prospective analysis)\n\nExclusion Criteria:\n\n* Patients with incomplete hemodynamic data at rest or exercise\n* Patients without sufficient follow-up data (information on survival / lung transplantation)\n* advanced tumour disease or other diseases with a short life expectancy, except pulmonary vascular diseases\n* advanced heart failure with pulmonary arterial wedge pressure (PAWP) \\> 18 mmHg at rest\n* uncontrolled systemic arterial hypertension (RR values \\> 160/100 mmHg at rest)\n* FEV1\\<50% predicted\n* TLC\\<60% predicted",
            "healthyVolunteers": false,
            "sex": "ALL",
            "minimumAge": "18 Years",
            "stdAges": [
                "ADULT",
                "OLDER_ADULT"
            ],
            "studyPopulation": "Patients (females and males; age: above 18yrs) with intermediate or high echocardiographic probability of PH and/or unexplained dyspnea, and/or associated conditions for PAH as clinical indication for right heart catheterization at rest and exercise",
            "samplingMethod": "PROBABILITY_SAMPLE"
        },
